    "AVFoundation",
    "CFNetwork",
    "CoreGraphics",
    "CoreMedia",
    "CoreLocation",
    "CoreVideo",
    "CoreImage",
    "CoreTelephony",
    "Foundation",
    "ImageIO",
    "MessageUI",
    "MobileCoreServices",
    "OpenGLES",
    "QuartzCore",
    "Social",
    "SystemConfiguration",
    "Security",
    "StoreKit",
    "Twitter",
    "UIKit"
