    "tag": "1.2.0"
  },
  "platforms": {
    "ios": "8.0"
  },
  "default_subspecs": [
    "Picker",
    "Browser"
  ],
  "subspecs": [
    {
      "name": "Picker",
      "source_files": [
        "XMNPhoto/Classes/XMNPhotoPicker/*.{h,m}",
        "XMNPhoto/Classes/XMNPhotoPicker/**/*.{h,m}"
      ],
      "resource_bundles": {
        "XMNPhotoPicker": [
          "XMNPhoto/Assets/XMNPhotoPicker/*.png",
          "XMNPhoto/Assets/XMNPhotoPicker/*.xib"
        ]
      }
    },
    {
      "name": "Browser",
      "source_files": [
        "XMNPhoto/Classes/XMNPhotoBrowser/*.{h,m}",
        "XMNPhoto/Classes/XMNPhotoBrowser/**/*.{h,m}"
      ],
      "dependencies": {
        "YYWebImage": [
          "1.0.5"
        ]
      }
    }
  ]
}
