{
  "name": "paymentSDK",
  "version": "1.0.6",
  "summary": "Wirecard Payment Platform SDK - online credit card payments",
  "description": "                       The library allows online credit card payment processing. \n                       Major Credit and Debit Card payment processing (Master, Visa, American Express)\n                       \n                       Fully integrated with licensed Wirecard Payment infrastructure.\n",
  "homepage": "https://github.com/mposSVK/paymentSDK",
  "license": "MIT",
  "authors": {
    "Radoslav Danko": "radoslav.danko@wirecard.com",
    "Martin Dzuro": "martin.dzuro@wirecard.com",
    "Slavomir Berenda": "slavomir.berenda@wirecard.com"
  },
  "source": {
    "git": "https://github.com/mposSVK/paymentSDK.git",
    "tag": "1.0.6"
  },
  "platforms": {
    "ios": "7.0"
  },
  "requires_arc": true,
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "$(SDKROOT)/usr/include/libxml2"
  },
  "libraries": [
    "stdc++.6",
    "xml2"
  ],
  "frameworks": [
    "Foundation",
    "SystemConfiguration"
  ],
  "source_files": "Pod/Classes/include/*.{h}",
  "public_header_files": "**/*.{h}",
  "vendored_libraries": "Pod/Classes/lib/*.{a}",
  "resources": [
    "Pod/Assets/*.xml",
    "Pod/Classes/lib/PaymentResources.bundle"
  ],
  "dependencies": {
    "KissXML": [
      "~> 5.0"
    ]
  }
}
