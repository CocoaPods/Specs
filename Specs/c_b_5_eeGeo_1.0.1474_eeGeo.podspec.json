{
  "name": "eeGeo",
  "version": "1.0.1474",
  "summary": "Stunning, Interactive 3D Maps",
  "description": "The eeGeo SDK, a C++11 OpenGL-based library for beautiful and customisable 3D maps. Includes Objective-C bindings for iOS.",
  "homepage": "http://www.eegeo.com",
  "license": "eeGeo SDK License",
  "authors": {
    "eeGeo": "support@eegeo.com"
  },
  "source": {
    "http": "https://s3.amazonaws.com/eegeo-static/api/ios/api.package.1.0.1474.tar.gz",
    "flatten": true
  },
  "platforms": {
    "ios": "7.0"
  },
  "requires_arc": false,
  "source_files": "**/*.{h,m,mm,cpp}",
  "public_header_files": "api/public/*.h",
  "vendored_libraries": [
    "libapps-on-maps-cpp11.a",
    "libapps-on-maps-cpp11-sim.a",
    "libturbojpeg.a",
    "libuv.a",
    "libcurl.a"
  ],
  "module_name": "eeGeo",
  "frameworks": [
    "QuartzCore",
    "CoreLocation",
    "MobileCoreServices",
    "SystemConfiguration",
    "CFNetwork",
    "GLKit",
    "OpenGLES",
    "UIKit",
    "Foundation",
    "CoreGraphics",
    "CoreData"
  ],
  "libraries": [
    "c++",
    "z"
  ],
  "xcconfig": {
    "OTHER_CPLUSPLUSFLAGS": "-DCOMPILE_CPP_11=1",
    "CLANG_CXX_LANGUAGE_STANDARD": "c++11",
    "CLANG_CXX_LIBRARY": "libc++"
  },
  "dependencies": {
    "SMCalloutView": [
      "~> 2.1"
    ]
  }
}
