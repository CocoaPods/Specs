{
  "name": "PageSegment",
  "version": "0.3",
  "summary": "A pages control tool.",
  "description": "这是一个关于承载多个页面的聚合页，并且可以自定义top栏每个item的样式",
  "homepage": "https://github.com/sunhua163/PageSegment",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "sunhua": "sunhua@yizijob.com"
  },
  "source": {
    "git": "https://github.com/sunhua163/PageSegment.git",
    "tag": "0.3"
  },
  "source_files": [
    "Classes",
    "PageSegment/PageEnum框架/**/*.{h,m}"
  ],
  "resources": "PageSegment/PageEnum框架/**/*.{xib}",
  "resource_bundles": {
    "PageSegment": [
      "PageSegment/PageEnum框架/**/*.{xib}"
    ]
  },
  "exclude_files": "Classes/Exclude",
  "frameworks": "UIKit",
  "prefix_header_contents": [
    "#import <UIKit/UIKit.h>",
    "#import <Foundation/Foundation.h>"
  ],
  "requires_arc": true,
  "platforms": {
    "ios": "8.0"
  }
}
