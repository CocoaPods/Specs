{
  "name": "ClearBlade-iOS-API",
  "platforms": {
    "ios": "7.0",
    "osx": "10.8",
    "tvos": "9.0"
  },
  "version": "1.17.0",
  "summary": "iOS API libraries for the ClearBlade Platform",
  "ios": {
    "frameworks": [
      "MobileCoreServices",
      "SystemConfiguration",
      "Security",
      "CoreGraphics"
    ]
  },
  "osx": {
    "frameworks": [
      "CoreServices",
      "SystemConfiguration",
      "Security"
    ]
  },
  "tvos": {
    "frameworks": [
      "MobileCoreServices",
      "SystemConfiguration",
      "Security"
    ]
  },
  "homepage": "https://github.com/ClearBlade/iOS-API",
  "license": {
    "type": "EPL",
    "file": "LICENSE"
  },
  "authors": {
    "ClearBlade, Inc.": "support@clearblade.com"
  },
  "source": {
    "git": "https://github.com/ClearBlade/iOS-API.git",
    "tag": "1.17.0"
  },
  "header_dir": "libmosquitto",
  "source_files": "ClearBladeAPI/**/*.{h,m,c}",
  "exclude_files": "ClearBladeAPI/libmosquitto/*.c",
  "requires_arc": true,
  "subspecs": [
    {
      "name": "libmosquitto",
      "source_files": "ClearBladeAPI/libmosquitto/*.{c,h}",
      "requires_arc": false,
      "compiler_flags": [
        "-DWITH_THREADING",
        "-fno-objc-arc"
      ]
    }
  ]
}
