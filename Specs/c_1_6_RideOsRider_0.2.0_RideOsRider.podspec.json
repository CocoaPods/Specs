{
  "name": "RideOsRider",
  "version": "0.2.0",
  "summary": "rideOS SDK for building ridehailing rider apps",
  "description": "rideOS SDK for building ridehailing rider apps.",
  "homepage": "https://github.com/rideOS/rideos-sdk-ios",
  "license": {
    "type": "Apache License, Version 2.0",
    "file": "LICENSE"
  },
  "authors": {
    "rideOS": "support@rideos.ai"
  },
  "source": {
    "git": "https://github.com/rideOS/rideos-sdk-ios.git",
    "tag": "0.2.0"
  },
  "platforms": {
    "ios": "11.0"
  },
  "swift_versions": "5.0",
  "dependencies": {
    "RideOsApi": [
      "~> 0.1"
    ],
    "RideOsCommon": [
      "~> 0.1"
    ],
    "RxCocoa": [
      "~> 4.4"
    ],
    "RxSwift": [
      "~> 4.4"
    ],
    "RxSwiftExt": [
      "~> 3.4"
    ],
    "RxOptional": [
      "~> 3.6"
    ],
    "SideMenu": [
      "~> 5.0"
    ],
    "Pulley": [
      "~> 2.7"
    ]
  },
  "source_files": "RideOsRider/**/*.{swift, h, m}",
  "resource_bundles": {
    "RideOsRider": [
      "RideOsRider/Assets.xcassets",
      "RideOsRider/*.lproj"
    ]
  },
  "pod_target_xcconfig": {
    "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited) GPB_USE_PROTOBUF_FRAMEWORK_IMPORTS=1",
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES"
  },
  "swift_version": "5.0"
}
