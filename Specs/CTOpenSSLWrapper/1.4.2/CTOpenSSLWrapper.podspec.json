{
  "name": "CTOpenSSLWrapper",
  "version": "1.4.2",
  "license": "MIT",
  "source": {
    "git": "https://github.com/ebf/CTOpenSSLWrapper.git",
    "tag": "1.4.2"
  },
  "frameworks": "Foundation",
  "requires_arc": true,
  "homepage": "https://github.com/ebf/CTOpenSSLWrapper",
  "summary": "Objc OpenSSL wrapper.",
  "authors": {
    "Oliver Letterer": "oliver.letterer@gmail.com"
  },
  "platforms": {
    "ios": "8.0",
    "osx": "10.11",
    "tvos": "9.0"
  },
  "default_subspecs": "CTOpenSSLWrapper_iOS",
  "subspecs": [
    {
      "name": "CTOpenSSLWrapper_iOS",
      "source_files": [
        "CTOpenSSLWrapper/*.{h,m}",
        "CTOpenSSLWrapper/Framework Additions/**/**/*.{h,m}",
        "CTOpenSSLWrapper/**/*.{h,m}"
      ],
      "dependencies": {
        "CTOpenSSLWrapper/OpenSSL_iOS": [

        ]
      }
    },
    {
      "name": "CTOpenSSLWrapper_tvOS",
      "source_files": [
        "CTOpenSSLWrapper/*.{h,m}",
        "CTOpenSSLWrapper/Framework Additions/**/**/*.{h,m}",
        "CTOpenSSLWrapper/**/*.{h,m}"
      ],
      "dependencies": {
        "CTOpenSSLWrapper/OpenSSL_tvOS": [

        ]
      }
    },
    {
      "name": "CTOpenSSLWrapper_osx",
      "source_files": [
        "CTOpenSSLWrapper/*.{h,m}",
        "CTOpenSSLWrapper/Framework Additions/**/**/*.{h,m}",
        "CTOpenSSLWrapper/**/*.{h,m}"
      ],
      "dependencies": {
        "CTOpenSSLWrapper/OpenSSL_osx": [

        ]
      }
    },
    {
      "name": "OpenSSL_iOS",
      "source_files": "include/openssl/*.h",
      "public_header_files": "include/openssl/*.h",
      "header_dir": "openssl",
      "ios": {
        "vendored_libraries": [
          "lib/libcrypto_iOS.a",
          "lib/libssl_iOS.a"
        ]
      },
      "osx": {
        "vendored_libraries": [
          "lib/libcrypto_Mac.a",
          "lib/libssl_Mac.a"
        ]
      },
      "tvos": {
        "vendored_libraries": [
          "lib/libcrypto_tvOS.a",
          "lib/libssl_tvOS.a"
        ]
      }
    },
    {
      "name": "OpenSSL_tvOS",
      "source_files": "include/openssl/*.h",
      "public_header_files": "include/openssl/*.h",
      "header_dir": "openssl",
      "ios": {
        "vendored_libraries": [
          "lib/libcrypto_iOS.a",
          "lib/libssl_iOS.a"
        ]
      },
      "osx": {
        "vendored_libraries": [
          "lib/libcrypto_Mac.a",
          "lib/libssl_Mac.a"
        ]
      },
      "tvos": {
        "vendored_libraries": [
          "lib/libcrypto_tvOS.a",
          "lib/libssl_tvOS.a"
        ]
      }
    },
    {
      "name": "OpenSSL_osx",
      "source_files": "include/openssl/*.h",
      "public_header_files": "include/openssl/*.h",
      "header_dir": "openssl",
      "ios": {
        "vendored_libraries": [
          "lib/libcrypto_iOS.a",
          "lib/libssl_iOS.a"
        ]
      },
      "osx": {
        "vendored_libraries": [
          "lib/libcrypto_Mac.a",
          "lib/libssl_Mac.a"
        ]
      },
      "tvos": {
        "vendored_libraries": [
          "lib/libcrypto_tvOS.a",
          "lib/libssl_tvOS.a"
        ]
      }
    }
  ]
}
