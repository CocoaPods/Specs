    {
      "name": "Installations",
      "dependencies": {
        "KSCrash/Recording": [

        ],
        "KSCrash/Reporting": [

        ]
      },
      "source_files": "Source/KSCrash/Installations/**/*.{h,m,mm,c,cpp}"
