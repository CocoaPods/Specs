          "name": "MessageUI"
        },
        {
          "name": "Sinks",
          "ios": {
            "frameworks": "MessageUI"
          },
          "dependencies": {
            "KSCrash/Reporting/Filters": [

            ],
            "KSCrash/Reporting/Tools": [

            ]
