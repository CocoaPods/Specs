  "license": {
    "type": "KSCrash license agreement",
    "file": "LICENSE"
  },
  "authors": {
    "Karl Stenerud": "kstenerud@gmail.com"
  },
  "platforms": {
    "ios": "6.0",
    "osx": "10.8",
    "tvos": "9.0",
    "watchos": "2.0"
  },
  "source": {
    "git": "https://github.com/kstenerud/KSCrash.git",
    "tag": "1.13.0"
