{
  "name": "AlipaySDK_ZF",
  "version": "15.2.0",
  "summary": "Alipay SDK for iOS. You can create alipay order or sign orders with `Order` subspec.",
  "homepage": "https://b.alipay.com/newIndex.htm",
  "license": {
    "type": "Copyright",
    "text": "      支付宝(中国)网络技术有限公司 ^? 版权所有.\n"
  },
  "authors": {
    "AliPay": "http://www.alipay.com/"
  },
  "requires_arc": true,
  "description": "this podspec will update by alipaySDK, it's include alipaySDK. good for iosdeveloper who user alipay.",
  "source": {
    "git": "https://git.oschina.net/zify/AlipaySDK_ZF.git",
    "tag": "15.2.0"
  },
  "platforms": {
    "ios": "7.0"
  },
  "frameworks": [
    "CoreTelephony",
    "SystemConfiguration",
    "CoreMotion"
  ],
  "libraries": [
    "z",
    "c++"
  ],
  "default_subspecs": "Core",
  "subspecs": [
    {
      "name": "Core",
      "source_files": "AlipaySDK.framework/Headers/**/*.h",
      "resources": "AlipaySDK.bundle",
      "vendored_frameworks": "AlipaySDK.framework",
      "public_header_files": "AlipaySDK.framework/Headers/**/*.h"
    }
  ]
}
