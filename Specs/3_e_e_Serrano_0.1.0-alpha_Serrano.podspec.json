{
  "name": "Serrano",
  "version": "0.1.0-alpha",
  "license": {
    "type": "MIT"
  },
  "homepage": "https://github.com/pcpLiu/Serrano",
  "authors": {
    "Tim Liu": "pcpliu.dev@gmail.com"
  },
  "summary": "Graph computation library for iOS",
  "source": {
    "git": "https://github.com/pcpLiu/Serrano.git",
    "tag": "v0.1.0-alpha"
  },
  "module_name": "Serrano",
  "documentation_url": "http://serrano-lib.org",
  "platforms": {
    "ios": "10.0"
  },
  "requires_arc": true,
  "ios": {
    "frameworks": "Accelerate",
    "weak_frameworks": "Metal",
    "resource_bundles": {
      "Serrano": "Source/**/*.{metal}"
    }
  },
  "source_files": "Source/**/*.{swift,c,h}",
  "public_header_files": "Source/Supporting Files/Serrano.h",
  "pod_target_xcconfig": {
    "SWIFT_VERSION": "3.2",
    "SWIFT_INCLUDE_PATHS": "$(SRCROOT)/Serrano/Source/library/FBSUtil/flatcc"
  },
  "preserve_paths": "Source/library/module.map",
  "pushed_with_swift_version": "3.0"
}
