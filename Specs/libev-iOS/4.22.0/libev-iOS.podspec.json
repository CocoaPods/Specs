{
  "name": "libev-iOS",
  "version": "4.22.0",
  "summary": "Libev for Cocoapods",
  "description": "This is a binary distribution of the libev library built for iOS and OSX.\nThe library uses configure which makes it a bit tricky to cross compile.",
  "homepage": "https://github.com/icodesign/libev.git",
  "authors": {
    "iCodesign": "leimagnet@gmail.com"
  },
  "license": "BSD",
  "platforms": {
    "ios": "6.0",
    "osx": "10.9"
  },
  "source": {
    "git": "https://github.com/icodesign/libev.git",
    "tag": "4.22.0"
  },
  "source_files": "include/*.h",
  "vendored_libraries": "lib/libev.a",
  "preserve_paths": [
    "include",
    "lib"
  ],
  "requires_arc": true,
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "$(PODS_ROOT)/libev/include/"
  }
}
