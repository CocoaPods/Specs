{
  "name": "STXImageCache",
  "version": "1.0.2",
  "summary": "A lightweight and pure-Swift library for downloading and caching images.",
  "description": "STXImageCache is a lightweight, pure-Swift, easy to use library for downloading and caching images. It provides convenient UI extensions. It's inspired by popular libraries like Kingfisher and SDWebImage.",
  "homepage": "https://github.com/stxnext/STXImageCache",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Norbert Sroczy≈Ñski": "norbert.sroczynski@stxnext.pl"
  },
  "platforms": {
    "ios": "9.0",
    "tvos": "9.0",
    "osx": "10.10",
    "watchos": "2.0"
  },
  "source": {
    "git": "https://github.com/stxnext/STXImageCache.git",
    "tag": "1.0.2"
  },
  "ios": {
    "source_files": [
      "Source/*.swift",
      "STXImageCache.h"
    ],
    "exclude_files": [
      "Source/STXImageCache+NSButtonExtensions.swift"
    ]
  },
  "watchos": {
    "source_files": [
      "Source/*.swift",
      "STXImageCache.h"
    ],
    "exclude_files": [
      "Source/STXImageCache+NSButtonExtensions.swift",
      "Source/STXImageCache+UIButtonExtensions.swift"
    ]
  },
  "tvos": {
    "source_files": [
      "Source/*.swift",
      "STXImageCache.h"
    ],
    "exclude_files": [
      "Source/STXImageCache+NSButtonExtensions.swift"
    ]
  },
  "osx": {
    "source_files": [
      "Source/*.swift",
      "STXImageCache.h"
    ],
    "exclude_files": [
      "Source/STXImageCache+UIButtonExtensions.swift"
    ]
  },
  "public_header_files": [
    "STXImageCache.h"
  ],
  "requires_arc": true,
  "pod_target_xcconfig": {
    "SWIFT_VERSION": "3.0"
  },
  "pushed_with_swift_version": "3.0"
}
