{
  "name": "RxSwiftly",
  "platforms": {
    "ios": "9.0"
  },
  "version": "1.2.1",
  "summary": "An E-sites collection of RxSwift extensions.",
  "authors": {
    "Bas van Kuijck": "bas@e-sites.nl"
  },
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "homepage": "https://github.com/e-sites/RxSwiftly",
  "source": {
    "git": "https://github.com/e-sites/RxSwiftly.git",
    "tag": "v1.2.1"
  },
  "source_files": "Source/*.h",
  "requires_arc": true,
  "swift_versions": [
    "4.2",
    "5.0"
  ],
  "dependencies": {
    "RxSwift": [

    ],
    "RxCocoa": [

    ]
  },
  "frameworks": [
    "Foundation",
    "UIKit"
  ],
  "default_subspecs": [
    "Core",
    "UIKit",
    "CoreMotion"
  ],
  "swift_version": "5.0",
  "subspecs": [
    {
      "name": "Core",
      "source_files": "Source/Core/*.swift"
    },
    {
      "name": "UIKit",
      "dependencies": {
        "RxSwiftly/Core": [

        ]
      },
      "subspecs": [
        {
          "name": "UIApplication",
          "source_files": "Source/UIKit/UIApplication/*.{h,swift}"
        },
        {
          "name": "UITextField",
          "source_files": "Source/UIKit/UITextField/*.{h,swift}"
        }
      ]
    },
    {
      "name": "CoreMotion",
      "frameworks": "CoreMotion",
      "dependencies": {
        "RxSwiftly/Core": [

        ]
      },
      "subspecs": [
        {
          "name": "CMMotionManager",
          "source_files": "Source/CoreMotion/CMMotionManager/*.{h,swift}"
        }
      ]
    }
  ]
}
