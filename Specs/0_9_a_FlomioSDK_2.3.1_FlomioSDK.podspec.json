{
  "name": "FlomioSDK",
  "version": "2.3.1",
  "summary": "An iOS library for FLomio's line of NFC, BLE, and UHF RFID readers.",
  "homepage": "http://www.flomio.com",
  "license": {
    "type": "Copyright",
    "text": "        Copyright 2018 Flomio, Inc. All rights reserved.\n"
  },
  "requires_arc": true,
  "authors": {
    "scott@flomio.com": "condronscott@gmail.com"
  },
  "source": {
    "git": "https://github.com/flomio/flomio_mw_releases.git",
    "tag": "2.3.1"
  },
  "vendored_frameworks": "FlomioSDK/FlomioSDK.framework",
  "source_files": [
    "FlomioSDK/FlomioSDK.framework/Headers/*"
  ],
  "platforms": {
    "ios": "8.0"
  },
  "libraries": "z",
  "xcconfig": {
    "LIBRARY_SEARCH_PATHS": "\"$(PODS_ROOT)/FlomioSDK\"",
    "HEADER_SEARCH_PATHS": "\"${PODS_ROOT}/Headers/FlomioSDK\"",
    "OTHER_LDFLAGS": "\"-lc++\"\n        \"-ObjC\"",
    "ENABLE_BITCODE": "NO"
  },
  "resource_bundles": {
    "FlomioSDK": [
      "FlomioSDK/Assets/*"
    ]
  }
}
