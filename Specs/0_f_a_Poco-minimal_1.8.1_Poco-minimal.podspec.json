{
  "name": "Poco-minimal",
  "version": "1.8.1",
  "summary": "Powerful open source C++ class libraries.",
  "description": "Powerful open source C++ class libraries (https://pocoproject.org). 迷你版,只打入基本库(Foundation、Net、XML、JSON、Util)",
  "homepage": "https://github.com/shepherdlazy/Poco-Pod",
  "license": "Boost Software License",
  "authors": {
    "Raven Du": "Raven.Du@hotmail.com"
  },
  "platforms": {
    "ios": "7.0"
  },
  "source": {
    "http": "https://github.com/shepherdlazy/Poco-Pod/releases/download/v1.8.1/poco-1.8.1-minimal-sdk11.2.tar.gz",
    "sha256": "62dd8442d88d02dfa687807a196406c7746fb3340a4b29b5dc2f314f1bd75a8c"
  },
  "header_dir": "Poco",
  "vendored_libraries": "lib/*.a",
  "requires_arc": false,
  "subspecs": [
    {
      "name": "Foundation",
      "header_dir": "Poco",
      "source_files": "POCOIncludes/Poco/*.h",
      "public_header_files": "POCOIncludes/Poco/*.h"
    },
    {
      "name": "DOM",
      "header_dir": "Poco/DOM",
      "source_files": "POCOIncludes/Poco/DOM/*.h",
      "public_header_files": "POCOIncludes/Poco/DOM/*.h"
    },
    {
      "name": "Dynamic",
      "header_dir": "Poco/Dynamic",
      "source_files": "POCOIncludes/Poco/Dynamic/*.h",
      "public_header_files": "POCOIncludes/Poco/Dynamic/*.h"
    },
    {
      "name": "SAX",
      "header_dir": "Poco/SAX",
      "source_files": "POCOIncludes/Poco/SAX/*.h",
      "public_header_files": "POCOIncludes/Poco/SAX/*.h"
    },
    {
      "name": "Redis",
      "header_dir": "Poco/Redis",
      "source_files": "POCOIncludes/Poco/Redis/*.h",
      "public_header_files": "POCOIncludes/Poco/Redis/*.h"
    },
    {
      "name": "Net",
      "header_dir": "Poco/Net",
      "source_files": "POCOIncludes/Poco/Net/*.h",
      "public_header_files": "POCOIncludes/Poco/Net/*.h"
    },
    {
      "name": "XML",
      "header_dir": "Poco/XML",
      "source_files": "POCOIncludes/Poco/XML/*.h",
      "public_header_files": "POCOIncludes/Poco/XML/*.h"
    },
    {
      "name": "JSON",
      "header_dir": "Poco/JSON",
      "source_files": "POCOIncludes/Poco/JSON/*.h",
      "public_header_files": "POCOIncludes/Poco/JSON/*.h"
    },
    {
      "name": "Util",
      "header_dir": "Poco/Util",
      "source_files": "POCOIncludes/Poco/Util/*.h",
      "public_header_files": "POCOIncludes/Poco/Util/*.h"
    }
  ]
}
