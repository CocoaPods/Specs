{
  "name": "SwiftToolsKit",
  "version": "0.0.1",
  "summary": "SwiftToolsKit makes code in Swift easy to use.",
  "homepage": "https://github.com/diaodepeng/SwiftToolsKit",
  "license": {
    "type": "MIT"
  },
  "authors": {
    "depler": "diaodepeng@foxmail.com"
  },
  "requires_arc": true,
  "swift_versions": "5.0",
