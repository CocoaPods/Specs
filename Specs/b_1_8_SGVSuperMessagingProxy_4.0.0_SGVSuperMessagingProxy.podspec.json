    "tag": "4.0.0"
  },
  "social_media_url": "https://twitter.com/sanekgusev",
  "swift_version": "4.0",
  "platforms": {
    "ios": "8.0",
    "osx": "10.9",
    "watchos": "2.0",
    "tvos": "9.0"
  },
