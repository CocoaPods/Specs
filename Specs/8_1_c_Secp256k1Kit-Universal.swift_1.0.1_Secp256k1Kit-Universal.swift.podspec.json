{
  "name": "Secp256k1Kit-Universal.swift",
  "module_name": "Secp256k1Kit",
  "version": "1.0.1",
  "summary": "Crypto secp256k1 library for Swift. Supports iOS & OSX",
  "description": "CryptoSecp256k1 includes crypto functions for signing transactions Swift. It supports secp256k1.",
  "homepage": "https://github.com/cuhte3/secp256k1-kit-universal",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Horizontal Systems": "hsdao@protonmail.ch",
    "cuhte3": "farid@getportal.co"
  },
  "source": {
    "git": "https://github.com/cuhte3/secp256k1-kit-universal.git",
    "tag": "1.0.1"
  },
  "social_media_url": "http://horizontalsystems.io/",
  "swift_versions": "5",
  "preserve_paths": [
    "Secp256k1Kit/Libraries"
  ],
  "platforms": {
    "ios": "11.0",
    "osx": "10.14"
  },
  "ios": {
    "source_files": [
      "Secp256k1Kit/Classes/**/*",
      "Secp256k1Kit/Libraries/ios/include/*.h"
    ],
    "vendored_libraries": "Secp256k1Kit/Libraries/ios/lib/libsecp256k1.a",
    "pod_target_xcconfig": {
      "HEADER_SEARCH_PATHS": "\"${PODS_TARGET_SRCROOT}/Secp256k1Kit/Libraries/ios/include\"",
      "LIBRARY_SEARCH_PATHS": "\"${PODS_TARGET_SRCROOT}/Secp256k1Kit/Libraries/ios/lib\""
    }
  },
  "osx": {
    "source_files": [
      "Secp256k1Kit/Classes/**/*",
      "Secp256k1Kit/Libraries/osx/include/*.h"
    ],
    "vendored_libraries": "Secp256k1Kit/Libraries/osx/lib/libsecp256k1.a",
    "pod_target_xcconfig": {
      "HEADER_SEARCH_PATHS": "\"${PODS_TARGET_SRCROOT}/Secp256k1Kit/Libraries/osx/include\"",
      "LIBRARY_SEARCH_PATHS": "\"${PODS_TARGET_SRCROOT}/Secp256k1Kit/Libraries/osx/lib\""
    }
  },
  "swift_version": "5"
}
