{
  "name": "OHMWPhotoBrowser",
  "version": "3.0.0",
  "license": "MIT",
  "summary": "A fork of MWPhotoBrowser. A simple iOS photo and video browser with optional captions for iOS 13+.",
  "description": "MWPhotoBrowser can display one or more images or videos by providing either UIImage\nobjects, web images/videos or local files.\nThe photo browser handles the downloading and caching of photos from the web seamlessly.\nPhotos can be zoomed and panned, and optional (customisable) captions can be displayed.",
  "screenshots": [
    "https://raw.github.com/mwaterfall/MWPhotoBrowser/master/Screenshots/MWPhotoBrowser1.png",
    "https://raw.github.com/mwaterfall/MWPhotoBrowser/master/Screenshots/MWPhotoBrowser2.png",
    "https://raw.github.com/mwaterfall/MWPhotoBrowser/master/Screenshots/MWPhotoBrowser3.png",
    "https://raw.github.com/mwaterfall/MWPhotoBrowser/master/Screenshots/MWPhotoBrowser4.png",
    "https://raw.github.com/mwaterfall/MWPhotoBrowser/master/Screenshots/MWPhotoBrowser5.png",
    "https://raw.github.com/mwaterfall/MWPhotoBrowser/master/Screenshots/MWPhotoBrowser6.png"
  ],
  "homepage": "https://github.com/owjhart/MWPhotoBrowser",
  "authors": {
    "owjhart": "owenjhart@gmail.com"
  },
  "source": {
    "git": "https://github.com/owjhart/MWPhotoBrowser.git",
    "tag": "3.0.0"
  },
  "platforms": {
    "ios": "13.0"
  },
  "source_files": "Pod/Classes/**/*",
  "resource_bundles": {
    "MWPhotoBrowser": [
      "Pod/Assets/*.png"
    ]
  },
  "requires_arc": true,
  "frameworks": [
    "ImageIO",
    "QuartzCore",
    "AVKit"
  ],
  "weak_frameworks": "Photos",
  "dependencies": {
    "MBProgressHUD": [
      "~> 1.1"
    ],
    "DACircularProgress": [
      "~> 2.3"
    ],
    "SDWebImage": [
      "~> 5.2"
    ]
  }
}
