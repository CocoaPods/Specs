  "homepage": "https://github.com/arkenthera/mopub-ios-mediation",
  "license": {
    "type": "New BSD",
    "file": "LICENSE"
  },
  "authors": {
    "MoPub": "support@mopub.com"
  },
  "source": {
    "git": "https://github.com/arkenthera/mopub-ios-mediation.git",
