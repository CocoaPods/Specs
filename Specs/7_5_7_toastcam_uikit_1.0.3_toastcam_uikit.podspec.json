      "source_files": "toastcam_uikit/Classes/**/*",
      "resources": "toastcam_uikit/Assets/**/*",
      "pod_target_xcconfig": {
        "OTHER_SWIFT_FLAGS": "-DAPP_EXTENSIONS"
      }
    }
  ]
