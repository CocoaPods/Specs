{
  "name": "OCFuntime",
  "version": "0.3.0",
  "summary": "OCFuntime is a toolkit for objective-c runtime.",
  "homepage": "https://github.com/belkevich/ocfuntime",
  "license": {
    "type": "MIT",
    "file": "LICENSE.txt"
  },
  "social_media_url": "https://twitter.com/okolodev",
  "authors": {
    "Alexey Belkevich": "belkevich.alexey@gmail.com"
  },
  "source": {
    "git": "https://github.com/belkevich/ocfuntime.git",
    "tag": "0.3.0"
  },
  "requires_arc": true,
  "platforms": {
    "ios": "5.0",
    "osx": "10.7"
  },
  "default_subspecs": "All",
  "subspecs": [
    {
      "name": "Core",
      "source_files": "Classes/Core/**/*.{h,m}"
    },
    {
      "name": "Methods",
      "source_files": "Classes/Methods/**/*.{h,m}",
      "dependencies": {
        "OCFuntime/Core": [

        ]
      }
    },
    {
      "name": "Properties",
      "source_files": "Classes/Properties/**/*.{h,m}",
      "dependencies": {
        "OCFuntime/Core": [

        ]
      }
    },
    {
      "name": "Protocols",
      "source_files": "Classes/Protocols/**/*.{h,m}",
      "dependencies": {
        "OCFuntime/Core": [

        ]
      }
    },
    {
      "name": "Shared",
      "source_files": "Classes/Shared/*.{h,m}",
      "dependencies": {
        "OCFuntime/Core": [

        ],
        "ABMultiton": [

        ]
      }
    },
    {
      "name": "NSObject+OCFMethods",
      "source_files": "Classes/Categories/Methods/**/*.{h,m}",
      "dependencies": {
        "OCFuntime/Methods": [

        ],
        "OCFuntime/Shared": [

        ]
      }
    },
    {
      "name": "NSObject+OCFProperties",
      "source_files": "Classes/Categories/Properties/**/*.{h,m}",
      "dependencies": {
        "OCFuntime/Properties": [

        ],
        "OCFuntime/Shared": [

        ]
      }
    },
    {
      "name": "All",
      "source_files": "Classes/All/*.h",
      "dependencies": {
        "OCFuntime/NSObject+OCFMethods": [

        ],
        "OCFuntime/NSObject+OCFProperties": [

        ],
        "OCFuntime/Protocols": [

        ]
      }
    }
  ]
}
