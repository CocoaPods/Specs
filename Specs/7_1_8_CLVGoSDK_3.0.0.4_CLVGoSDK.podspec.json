{
  "name": "CLVGoSDK",
  "version": "3.0.0.4",
  "summary": "CLVGo SDK",
  "description": "Clover Go SDK provides an interface to connect to CloverGo readers (RP350, RP450) and process payments by Swipe, EMV and Tap",
  "homepage": "https://github.com/CloverGoSDK/CloverGoSDKv2.1.git",
  "license": {
    "type": "Apache License 2.0",
    "file": "LICENSE"
  },
  "authors": {
    "CloverGo Dev Team": "clovergodev@gmail.com"
  },
  "platforms": {
    "ios": "9.0",
    "osx": "10.10"
  },
  "source": {
    "git": "https://github.com/CloverGoSDK/CloverGoSDKv2.1.git",
    "tag": "3.0.0.4"
  },
  "pod_target_xcconfig": {
    "HEADER_SEARCH_PATHS": "$(PODS_ROOT)/CloverGoSDK",
    "SWIFT_VERSION": "4.0"
  },
  "preserve_paths": [
    "module.modulemap",
    "G4XSwiper.framework",
    "LandiSDK_BLE.framework",
    "RUA_BLE.framework"
  ],
  "module_map": "module.modulemap",
  "frameworks": [
    "Foundation",
    "MediaPlayer",
    "AVFoundation",
    "AudioToolbox"
  ],
  "libraries": "stdc++.6.0.9",
  "dependencies": {
    "Alamofire": [
      "4.2"
    ],
    "ObjectMapper": [
      "3.0.0"
    ],
    "RealmSwift": [
      "2.10.0"
    ]
  },
  "ios": {
    "vendored_frameworks": [
      "clovergoclient.framework",
      "CloverGoReaderSDK.framework",
      "CloverGoLogger.framework"
    ]
  },
  "pushed_with_swift_version": "4.0"
}
