{
  "name": "UIImage+BPG",
  "version": "0.0.1",
  "source": {
    "git": "https://github.com/hoppenichu/UIImage-BPG.git",
    "tag": "0.0.1"
  },
  "summary": "BPG image decoder for iOS",
  "homepage": "https://github.com/hoppenichu/UIImage-BPG",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Takeru Chuganji": "takeru@hoppenichu.com"
  },
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "source_files": "UIImage+BPG/*.{h,m}",
  "default_subspecs": "libbpg",
  "subspecs": [
    {
      "name": "libbpg",
      "source_files": "UIImage+BPG/libbpg/include/*.h",
      "vendored_libraries": "UIImage+BPG/libbpg/lib/*.a",
      "header_dir": "libbpg"
    }
  ]
}
