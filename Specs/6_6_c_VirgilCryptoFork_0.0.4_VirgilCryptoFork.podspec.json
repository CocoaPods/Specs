  "homepage": "https://github.com/SanjoDeundiak/virgil-foundation-x",
  "cocoapods_version": ">= 0.36",
  "license": {
    "type": "BSD",
    "file": "LICENSE"
  },
  "authors": {
    "Oleksandr Deundiak": "deundiak@gmail.com"
  },
  "platforms": {
    "osx": "10.10"
  },
  "source": {
    "git": "https://github.com/SanjoDeundiak/virgil-foundation-x.git",
