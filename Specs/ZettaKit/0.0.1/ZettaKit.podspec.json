  "homepage": "https://github.com/zettajs/ZettaKit",
  "license": "MIT",
  "authors": {
    "Matthew Dobson": "matt@apigee.com"
  },
  "source": {
    "git": "https://github.com/zettajs/ZettaKit.git",
    "tag": "0.0.1"
  },
  "social_media_url": "https://twitter.com/zettajs",
