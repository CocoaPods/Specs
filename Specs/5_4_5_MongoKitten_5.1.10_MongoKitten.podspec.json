        "MongoKitten/Core": [

        ]
      }
    },
    {
      "name": "Mobile",
      "dependencies": {
        "mongo_embedded": [
          "~> 4.0"
        ],
        "MongoKitten/Core": [

        ]
      }
    },
    {
      "name": "Core",
      "source_files": "Sources/MongoKitten/**/*",
      "dependencies": {
        "MongoKitten/_MongoKittenCrypto": [

        ]
      }
    }
  ]
}
