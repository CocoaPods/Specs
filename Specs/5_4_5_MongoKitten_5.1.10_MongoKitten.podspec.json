  "swift_version": "4.2",
  "homepage": "https://github.com/OpenKitten/MongoKitten",
  "license": {
    "type": "MIT",
    "file": "LICENSE.md"
  },
  "authors": {
    "joannis": "joannis@orlandos.nl"
  },
  "source": {
    "git": "https://github.com/OpenKitten/MongoKitten.git",
    "tag": "5.1.10"
  },
  "social_media_url": "https://twitter.com/joannisorlandos",
  "dependencies": {
    "BSON": [
      "~> 6.0.2"
    ]
  },
