{
  "name": "WaSPV",
  "version": "0.1",
  "license": "GPL",
  "summary": "A native Bitcoin SPV client library for iOS with BIP32 support.",
  "homepage": "https://github.com/keeshux/WaSPV",
  "authors": {
    "Davide De Rosa": "keeshux@gmail.com"
  },
  "source": {
    "git": "https://github.com/keeshux/WaSPV.git",
    "tag": "0.1"
  },
  "platforms": {
    "ios": "7.0"
  },
  "source_files": "WaSPV/Sources/**/*.{h,m}",
  "resource_bundles": {
    "WaSPV": "WaSPV/Resources/*"
  },
  "exclude_files": [
    "WaSPVDemo",
    "WaSPVTests"
  ],
  "requires_arc": true,
  "frameworks": "CoreData",
  "dependencies": {
    "OpenSSL-Universal": [
      "~> 1.0.1.h"
    ],
    "CocoaLumberjack": [
      "~> 1.9.0"
    ],
    "CocoaAsyncSocket": [
      "~> 7.3.5"
    ],
    "AutoCoding": [
      "~> 2.2"
    ]
  }
}