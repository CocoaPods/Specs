{
  "name": "FyberSDKWrapper",
  "version": "1.0.0",
  "summary": "The FyberSDK compatibility wrapper.",
  "description": "                   The Fyber Wrapper SDK will allow you to integrate FyberSDK 8.x without migrating your FyberSDK 7.x integration code.\n",
  "homepage": "http://developer.fyber.com/",
  "license": {
    "type": "Copyright",
    "text": "Copyright 2011 - 2015, Fyber GmbH, all rights reserved"
  },
  "authors": "Fyber GmbH",
  "social_media_url": "https://twitter.com/Fyber",
  "platforms": {
    "ios": "6.0"
  },
  "source": {
    "http": "https://bintray.com/artifact/download/fyber/mobile-sdk/Fyber_iOS_SDK_Wrapper_v.1.0.0.zip"
  },
  "public_header_files": "Fyber_iOS_SDK_Wrapper_v.1.0.0/fyber-wrapper-lib/*.h",
  "vendored_libraries": "Fyber_iOS_SDK_Wrapper_v.1.0.0/fyber-wrapper-lib/libFyberSDKWrapper-*.a",
  "preserve_paths": "Fyber_iOS_SDK_Wrapper_v.1.0.0/fyber-wrapper-lib/**",
