{
  "name": "TSCubeEngineFoundationKit",
  "version": "1.0.2",
  "summary": "Foundation framework",
  "description": "A foundation framework of CubeEngine",
  "homepage": "https://github.com/AshineReal/CubeEngineFoundationKit",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Ashine": "758988371@qq.com"
  },
  "source": {
    "git": "https://github.com/AshineReal/CubeEngineFoundationKit.git",
    "tag": "1.0.2"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source_files": "CubeEngineFoundationKit/Classes/**/*",
  "vendored_frameworks": "CubeEngineFoundationKit/Frameworks/*.framework",
  "requires_arc": true,
  "xcconfig": {
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES",
    "FRAMEWORK_SEARCH_PATHS": "$(inherited)",
    "HEADER_SEARCH_PATHS": "$(inherited)",
    "ARCHS": "$(inherited)",
    "OTHER_LDFLAGS": "$(inherited) -undefined dynamic_lookup",
    "VALID_ARCHS": "$(inherited)"
  }
}
