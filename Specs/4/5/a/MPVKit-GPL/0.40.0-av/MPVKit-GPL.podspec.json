{
  "name": "MPVKit-GPL",
  "version": "0.40.0-av",
  "summary": "MPVKit with AVFoundation video output for iOS/tvOS",
  "description": "MPVKit fork with AVFoundation video output (vo_avfoundation) support.\nFeatures Picture-in-Picture, hardware-accelerated VideoToolbox decoding,\ncomposite OSD for subtitles, and HDR/Dolby Vision support.",
  "homepage": "https://github.com/Alexk2309/MPVKit",
  "license": {
    "type": "GPL-3.0",
    "text": "GPL-3.0. See https://www.gnu.org/licenses/gpl-3.0.html"
  },
  "authors": {
    "Alexk2309": "https://github.com/Alexk2309"
  },
  "source": {
    "http": "https://github.com/Alexk2309/MPVKit/releases/download/0.40.0-av/MPVKit-GPL-Frameworks.zip"
  },
  "platforms": {
    "ios": "13.0",
    "tvos": "13.0"
  },
  "static_framework": true,
  "requires_arc": true,
  "vendored_frameworks": [
    "MPVKit-GPL-Frameworks/Frameworks/Libmpv.xcframework",
    "MPVKit-GPL-Frameworks/Frameworks/Libavcodec.xcframework",
    "MPVKit-GPL-Frameworks/Frameworks/Libavdevice.xcframework",
    "MPVKit-GPL-Frameworks/Frameworks/Libavfilter.xcframework",
    "MPVKit-GPL-Frameworks/Frameworks/Libavformat.xcframework",
    "MPVKit-GPL-Frameworks/Frameworks/Libavutil.xcframework",
    "MPVKit-GPL-Frameworks/Frameworks/Libswresample.xcframework",
    "MPVKit-GPL-Frameworks/Frameworks/Libswscale.xcframework",
    "MPVKit-GPL-Frameworks/Frameworks/Libssl.xcframework",
    "MPVKit-GPL-Frameworks/Frameworks/Libcrypto.xcframework",
    "MPVKit-GPL-Frameworks/Frameworks/Libass.xcframework",
    "MPVKit-GPL-Frameworks/Frameworks/Libfreetype.xcframework",
    "MPVKit-GPL-Frameworks/Frameworks/Libfribidi.xcframework",
    "MPVKit-GPL-Frameworks/Frameworks/Libharfbuzz.xcframework",
    "MPVKit-GPL-Frameworks/Frameworks/Libunibreak.xcframework",
    "MPVKit-GPL-Frameworks/Frameworks/MoltenVK.xcframework",
    "MPVKit-GPL-Frameworks/Frameworks/Libshaderc_combined.xcframework",
    "MPVKit-GPL-Frameworks/Frameworks/lcms2.xcframework",
    "MPVKit-GPL-Frameworks/Frameworks/Libplacebo.xcframework",
    "MPVKit-GPL-Frameworks/Frameworks/Libdovi.xcframework",
    "MPVKit-GPL-Frameworks/Frameworks/Libsmbclient.xcframework",
    "MPVKit-GPL-Frameworks/Frameworks/gmp.xcframework",
    "MPVKit-GPL-Frameworks/Frameworks/nettle.xcframework",
    "MPVKit-GPL-Frameworks/Frameworks/hogweed.xcframework",
    "MPVKit-GPL-Frameworks/Frameworks/gnutls.xcframework",
    "MPVKit-GPL-Frameworks/Frameworks/Libdav1d.xcframework",
    "MPVKit-GPL-Frameworks/Frameworks/Libuavs3d.xcframework",
    "MPVKit-GPL-Frameworks/Frameworks/Libuchardet.xcframework",
    "MPVKit-GPL-Frameworks/Frameworks/Libbluray.xcframework"
  ],
  "frameworks": [
    "AVFoundation",
    "AudioToolbox",
    "CoreAudio",
    "CoreVideo",
    "CoreFoundation",
    "CoreMedia",
    "Metal",
    "VideoToolbox"
  ],
  "libraries": [
    "bz2",
    "iconv",
    "expat",
    "resolv",
    "xml2",
    "z",
    "c++"
  ],
  "pod_target_xcconfig": {
    "VALID_ARCHS": "arm64",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "i386",
    "EXCLUDED_ARCHS[sdk=appletvsimulator*]": "i386 x86_64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "i386",
    "EXCLUDED_ARCHS[sdk=appletvsimulator*]": "i386 x86_64"
  }
}
