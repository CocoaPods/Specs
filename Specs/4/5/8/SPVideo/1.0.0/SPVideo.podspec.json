{
  "name": "SPVideo",
  "version": "1.0.0",
  "summary": "iOS SDK for Real time Video Chat from Superpro.ai",
  "description": "This SDK lets developers integrate a ready made video call solution in their iOS apps with zero development/integration of video call libraries.",
  "homepage": "https://github.com/superproai/SPVideo",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "aravindhu-gloify": "aravindhu@gloify.com"
  },
  "source": {
    "git": "https://github.com/superproai/SPVideo.git",
    "tag": "1.0.0"
  },
  "platforms": {
    "ios": "11.0"
  },
  "swift_versions": "5.0",
  "resources": "SPVideo/**/*.{storyboard,xib,xcassets}",
  "source_files": "SPVideo/**/*.{h,m,swift}",
  "resource_bundles": {
    "SPVideo": [
      "SPVideo/Assets/**/*.{xcassets,mp3,json,png,ttf,plist}"
    ]
  },
  "dependencies": {
    "Analytics": [
      "~> 4.1"
    ],
    "FirebaseFirestore": [

    ],
    "Firebase/Crashlytics": [

    ],
    "HMSVideo": [
      "~> 0.10.0"
    ]
  },
  "static_framework": true,
  "info_plist": {
    "CFBundleIdentifier": "com.superpro.VideoCall-IOS"
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "swift_version": "5.0"
}
