  "platforms": {
    "ios": "8.0"
  },
  "source_files": "YSTCommonKit/Classes/**/**/*.{h,m,pch}",
  "resource_bundles": {
    "YSTCommonKit": [
      "YSTCommonKit/Assets/*.{bundle,xcassets,imageset,png}",
      "YSTCommonKit/Classes/**/*.{storyboard,xib,json,plist,cer}"
    ]
  },
  "public_header_files": "YSTCommonKit/Classes/PublicClasses/**/*.h",
  "prefix_header_file": "YSTCommonKit/Classes/PublicClasses/Common.pch",
  "frameworks": [
    "UIKit",
    "AVFoundation",
    "Foundation",
    "SystemConfiguration",
    "Security",
    "CoreTelephony",
    "SystemConfiguration"
  ],
  "libraries": [
    "xml2",
    "resolv",
    "iconv",
    "z",
    "sqlite3"
  ],
  "vendored_frameworks": "YSTCommonKit/Classes/PublicClasses/Frameworks/*.framework",
  "xcconfig": {
    "ENABLE_BITCODE": "YES",
    "GCC_PRECOMPILE_PREFIX_HEADER": "YES",
    "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited)",
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES"
  },
  "pod_target_xcconfig": {
    "OTHER_LDFLAGS": "$(inherited)",
    "SWIFT_VERSION": "4.0",
    "HEADER_SEARCH_PATHS": "$(inherited) $(SDKROOT)/usr/include/libxml2 $(SDKROOT)/usr/include/libresolv $(inherited)$(PROJECT_DIR) /usr/local/include",
    "CLANG_CXX_LANGUAGE_STANDARD": "c++11",
    "CLANG_CXX_LIBRARY": "libc++"
  },
