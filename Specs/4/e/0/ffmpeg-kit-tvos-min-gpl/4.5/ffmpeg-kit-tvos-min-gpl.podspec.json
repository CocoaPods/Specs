{
  "name": "ffmpeg-kit-tvos-min-gpl",
  "version": "4.5",
  "summary": "FFmpeg Kit tvOS Min GPL Static Framework",
  "description": "Includes FFmpeg with libvid.stab, x264, x265 and xvidcore libraries enabled.",
  "homepage": "https://github.com/tanersener/ffmpeg-kit",
  "authors": {
    "Taner Sener": "tanersener@gmail.com"
  },
  "license": {
    "type": "GPL-3.0",
    "file": "ffmpegkit.xcframework/tvos-arm64/ffmpegkit.framework/LICENSE"
  },
  "platforms": {
    "tvos": "10.2"
  },
  "requires_arc": true,
  "libraries": [
    "z",
    "bz2",
    "c++",
    "iconv"
  ],
  "source": {
    "http": "https://github.com/tanersener/ffmpeg-kit/releases/download/v4.5/ffmpeg-kit-min-gpl-4.5-tvos-xcframework.zip"
  },
  "tvos": {
    "frameworks": [
      "AudioToolbox",
      "VideoToolbox",
      "CoreMedia"
    ],
    "vendored_frameworks": [
      "ffmpegkit.xcframework",
      "libavcodec.xcframework",
      "libavdevice.xcframework",
      "libavfilter.xcframework",
      "libavformat.xcframework",
      "libavutil.xcframework",
      "libswresample.xcframework",
      "libswscale.xcframework",
      "libvidstab.xcframework",
      "x264.xcframework",
      "x265.xcframework",
      "xvidcore.xcframework"
    ]
  }
}
