{
  "name": "Tropicalytics",
  "version": "0.1.1",
  "summary": "Tropicalytics is a lightweight, flexible library made for analytics tracking in your iOS app.",
  "description": "Tropicalytics is a lightweight configurable analytics tool that allows you to send self describing JSON to your own server so you are in control of your data.",
  "homepage": "https://github.com/tilteng/Tropicalytics",
  "license": "MIT",
  "authors": {
    "Tilt.com Inc": "tech@tilt.com"
  },
  "source": {
    "git": "https://github.com/tilteng/Tropicalytics.git",
    "tag": "0.1.1"
  },
  "social_media_url": "https://twitter.com/tilteng",
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "source_files": "Pod/Classes/**/*",
  "resource_bundles": {
    "Tropicalytics": [
      "Pod/Assets/*.png"
    ]
  },
  "public_header_files": "Pod/Classes/**/*.h",
  "dependencies": {
    "MAObjCRuntime": [
      "~> 0.0.1"
    ]
  },
  "resources": "Pod/Classes/Tropicalytics.xcdatamodeld",
  "preserve_paths": "Pod/Classes/Tropicalytics.xcdatamodeld",
  "frameworks": "CoreData"
}
