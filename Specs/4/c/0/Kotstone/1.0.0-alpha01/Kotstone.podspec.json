{
  "name": "Kotstone",
  "version": "1.0.0-alpha01",
  "summary": "High-level Multiplatform wrapper for the Capstone disassembly engine",
  "description": "Kotstone Kotlin Multiplatform Mobile library packaged as a framework for iOS, macOS, tvOS, and watchOS. High-level wrapper for the Capstone disassembly engine.",
  "homepage": "https://github.com/alisalimik/kotstone",
  "license": {
    "type": "Apache-2.0",
    "file": "LICENSE"
  },
  "authors": {
    "Ali Salimi": "alisalimik2002@gmail.com"
  },
  "source": {
    "http": "https://github.com/alisalimik/kotstone/releases/download/v1.0.0-alpha01/KotstoneKit.xcframework.zip"
  },
  "platforms": {
    "ios": "14.0",
    "tvos": "14.0",
    "watchos": "7.0",
    "osx": "12.0"
  },
  "vendored_frameworks": "KotstoneKit.xcframework",
  "static_framework": true,
  "requires_arc": true,
  "pod_target_xcconfig": {
    "ENABLE_BITCODE": "NO",
    "GIT_REPO": "https://github.com/alisalimik/kotstone.git",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "i386",
    "EXCLUDED_ARCHS[sdk=appletvsimulator*]": "i386",
    "EXCLUDED_ARCHS[sdk=watchsimulator*]": "i386"
  },
  "user_target_xcconfig": {
    "ENABLE_BITCODE": "NO",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "i386",
    "EXCLUDED_ARCHS[sdk=appletvsimulator*]": "i386",
    "EXCLUDED_ARCHS[sdk=watchsimulator*]": "i386"
  },
  "swift_versions": "5.0",
  "swift_version": "5.0"
}
