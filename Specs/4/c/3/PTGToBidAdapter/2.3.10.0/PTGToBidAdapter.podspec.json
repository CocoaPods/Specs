{
  "name": "PTGToBidAdapter",
  "version": "2.3.10.0",
  "summary": "PTG mediation adapter for ToBid SDK.",
  "description": "PTGToBidAdapter is a mediation adapter that enables PTGAdFramework\nto integrate with the ToBid advertising SDK.",
  "homepage": "https://github.com/PTGAd/PTGToBidAdapter",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "fancy": "ptg_all@fancydigital.com.cn"
  },
  "source": {
    "git": "https://github.com/PTGAd/PTGToBidAdapter.git",
