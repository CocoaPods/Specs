    {
      "name": "all",
      "source_files": "frameworks/Headers/**/*.{h,hpp,ipp}",
      "vendored_frameworks": [
        "frameworks/boost_atomic.xcframework",
        "frameworks/boost_chrono.xcframework",
        "frameworks/boost_container.xcframework",
