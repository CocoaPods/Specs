  "platforms": {
    "osx": "11.0",
    "ios": "13.4"
  },
  "osx": {
    "pod_target_xcconfig": {
      "ONLY_ACTIVE_ARCH": "YES"
    }
  },
  "ios": {
    "pod_target_xcconfig": {
      "ONLY_ACTIVE_ARCH": "YES"
    }
  },
  "static_framework": true,
  "prepare_command": "sh scripts/build.sh",
  "source": {
    "git": "https://github.com/apotocki/boost-iosx.git",
    "tag": "1.80.0.2"
  },
  "header_mappings_dir": "frameworks/Headers",
  "default_subspecs": "all",
