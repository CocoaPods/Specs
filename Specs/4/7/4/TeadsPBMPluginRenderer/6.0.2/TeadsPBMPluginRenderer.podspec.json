{
  "name": "TeadsPBMPluginRenderer",
  "version": "6.0.2",
  "summary": "PrebidMobile Plugin renderer for Teads' iOS SDK",
  "module_name": "TeadsPBMPluginRenderer",
  "description": "Use this plugin to request and render Teads demand source in your PrebidMobile integration",
  "homepage": "https://github.com/teads/TeadsSDK-iOS",
  "documentation_url": "https://support.teads.tv/support/solutions/articles/36000459748",
  "license": {
    "type": "Copyright",
    "text": "Copyright Teads 2021"
  },
  "authors": {
    "Teads": "support-sdk@teads.tv"
  },
  "source": {
    "git": "https://github.com/teads/TeadsSDK-iOS.git",
    "tag": "6.0.2"
  },
  "platforms": {
    "ios": "12.0"
  },
  "static_framework": true,
  "requires_arc": true,
  "source_files": [
    "MediationAdapters/TeadsPluginRenderer/**/*{.swift}",
    "MediationAdapters/Common/*{.swift}"
  ],
  "exclude_files": [

  ],
  "dependencies": {
    "TeadsSDK": [
      "6.0.2"
    ],
    "PrebidMobile": [
      ">= 3.0.2"
    ]
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "i386"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "i386"
  }
}
