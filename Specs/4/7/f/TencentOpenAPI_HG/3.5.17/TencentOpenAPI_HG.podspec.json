{
  "name": "TencentOpenAPI_HG",
  "version": "3.5.17",
  "summary": "TencentOpenAPI for cocoapods",
  "homepage": "https://github.com/lengzz",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "zhuangzhi.leng": "leng_zhuangzhi@163.com"
  },
  "source": {
    "git": "https://github.com/lengzz/TencentOpenAPI_HG.git",
    "tag": "3.5.17"
  },
  "platforms": {
    "ios": "7.0"
  },
  "static_framework": true,
  "requires_arc": true,
  "frameworks": [
    "Security",
    "SystemConfiguration",
    "CoreGraphics",
    "CoreTelephony",
    "WebKit"
  ],
  "libraries": [
    "iconv",
    "sqlite3",
    "c++",
    "z"
  ],
  "vendored_frameworks": "*.framework",
  "source_files": "TencentOpenAPI.framework/Headers/*",
  "resources": "*.bundle",
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64",
    "SWIFT_INCLUDE_PATHS": "$(PODS_TARGET_SRCROOT)/TencentOpenAPI.framework/Headers/",
    "DEFINES_MODULE": "YES"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  }
}
