{
  "name": "AdpSDK",
  "version": "6.1.9",
  "summary": "adpsdk",
  "homepage": "https://ddd.com",
  "license": "MIT",
  "authors": {
    "lee": "123456@qq.com"
  },
  "platforms": {
    "ios": "12.0"
  },
  "source": {
    "git": "https://github.com/suhc/AdpSDK.git",
    "tag": "6.1.9"
  },
  "frameworks": [
    "AVFoundation",
    "AdSupport",
    "CoreLocation",
    "CoreML",
    "CoreMedia",
    "CoreMotion",
    "CoreTelephony",
    "DeviceCheck",
    "JavaScriptCore",
    "MessageUI",
    "SafariServices",
    "StoreKit",
    "SystemConfiguration",
    "WebKit"
  ],
  "libraries": [
    "bz2",
    "c++",
    "resolv.9",
    "sqlite3",
    "xml2",
    "c++abi"
  ],
  "pod_target_xcconfig": {
    "OTHER_LDFLAGS": "-ObjC -l\"c++\" -l\"c++abi\" -l\"sqlite3\" -l\"z\""
  },
  "default_subspecs": "Core",
  "subspecs": [
    {
      "name": "Core",
      "vendored_frameworks": [
        "base/AdpSDK.framework",
        "base/**/*.xcframework"
      ],
      "source_files": [
        "base/**/*.h"
      ],
      "resources": [
        "base/IpaAdSDK.bundle"
      ],
      "vendored_libraries": [
        "base/**/*.a"
      ],
      "frameworks": [
        "UIKit",
        "Foundation"
      ],
      "requires_arc": true,
      "pod_target_xcconfig": {
        "VALID_ARCHS": "x86_64 armv7 arm64"
      }
    }
  ]
}
