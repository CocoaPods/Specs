{
  "name": "HYAsyncDisplayKit",
  "version": "0.3.0",
  "license": {
    "type": "BSD"
  },
  "homepage": "https://github.com/huboceanLi/AsyncDisplayKit",
  "authors": {
    "li437277219@gmail.com": "li437277219@gmail.com"
  },
  "summary": "Smooth asynchronous user interfaces for iOS apps.",
  "source": {
    "git": "https://github.com/huboceanLi/AsyncDisplayKit.git",
    "tag": "0.3.0"
  },
  "weak_frameworks": [
    "Photos",
    "MapKit",
    "AssetsLibrary"
  ],
  "requires_arc": true,
  "platforms": {
    "ios": "14.0"
  },
  "static_framework": true,
  "user_target_xcconfig": {
    "OTHER_LDFLAGS": "-ObjC",
    "GCC_WARN_ABOUT_DEPRECATED_FUNCTIONS": "NO"
  },
  "source_files": "HYAsyncDisplayKit/Classes/**/*",
  "libraries": [
    "c++",
    "z"
  ],
  "pod_target_xcconfig": {
    "DEFINES_MODULE": "YES",
    "CLANG_ENABLE_MODULES": "YES",
    "CLANG_MODULES_AUTOLINK": "YES",
    "PRODUCT_MODULE_NAME": "HYAsyncDisplayKit",
    "GCC_WARN_ABOUT_DEPRECATED_FUNCTIONS": "NO",
    "CLANG_WARN_DEPRECATED_OBJC_IMPLEMENTATIONS": "NO",
    "GCC_TREAT_WARNINGS_AS_ERRORS": "NO",
    "CLANG_WARN_OBJC_IMPLICIT_RETAIN_SELF": "NO",
    "CLANG_CXX_LANGUAGE_STANDARD": "c++17",
    "CLANG_CXX_LIBRARY": "libc++",
    "OTHER_CPLUSPLUSFLAGS": "-std=c++17 -Werror -stdlib=libc++",
    "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited) MINIMAL_ASDK=1",
    "HEADER_SEARCH_PATHS": "\"${PODS_ROOT}/HYAsyncDisplayKit/Source\" \"${PODS_ROOT}/HYAsyncDisplayKit/Source/PublicHeaders\""
  },
  "frameworks": [
    "UIKit",
    "Foundation",
    "QuartzCore",
    "CoreMedia",
    "CoreText",
    "CoreGraphics"
  ]
}
