{
  "name": "SDWebImageSVGNativeCoder",
  "version": "0.1.0",
  "summary": "SVG-Native vector image coder plugin for SDWebImage.",
  "description": "This SDWebImage coder plugin, use adobe svg-native-viewer library to provide SVG-Native (subset of SVG 1.1) vector image support",
  "homepage": "https://github.com/SDWebImage/SDWebImageSVGNativeCoder",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "dreampiggy": "lizhuoli1126@126.com"
  },
  "source": {
    "git": "https://github.com/SDWebImage/SDWebImageSVGNativeCoder.git",
    "tag": "0.1.0"
  },
  "platforms": {
    "ios": "9.0",
    "osx": "10.11",
    "tvos": "9.0",
    "watchos": "2.0"
  },
  "source_files": "SDWebImageSVGNativeCoder/Classes/**/*",
  "pod_target_xcconfig": {
    "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited) BOOST_VARIANT_DETAIL_NO_SUBSTITUTE=1",
    "HEADER_SEARCH_PATHS": "$(inherited) ${PODS_ROOT}/svgnative/svg-native-viewer/third_party/boost_variant_property_tree"
  },
  "dependencies": {
    "SDWebImage": [
      "~> 5.10"
    ],
    "svgnative": [
      ">= 0.1.0-beta"
    ]
  }
}
