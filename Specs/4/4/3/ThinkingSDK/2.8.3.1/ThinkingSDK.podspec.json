        "exclude_files": "ThinkingSDK/Source/Util/Toast/*.{h,m}"
      },
      "dependencies": {
        "ThinkingSDK/Core": [

        ]
      }
    },
    {
      "name": "Extension",
      "source_files": "ThinkingSDK/Source/Extension/**/*.{h,m}",
      "dependencies": {
        "ThinkingSDK/Core": [

        ]
      }
    },
    {
      "name": "Main",
      "ios": {
        "dependencies": {
          "ThinkingSDK/iOS": [

          ]
        }
      },
      "osx": {
        "dependencies": {
          "ThinkingSDK/OSX": [

          ]
        }
      }
