    {
      "name": "Base",
      "source_files": "ThinkingSDK/Source/AppLifeCycle/**/*.{h,m}",
      "dependencies": {
        "ThinkingSDK/Util": [

        ],
        "ThinkingSDK/Core": [

        ],
        "ThinkingSDK/Extension": [

