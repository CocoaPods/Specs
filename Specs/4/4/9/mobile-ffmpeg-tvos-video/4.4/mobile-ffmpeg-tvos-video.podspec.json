  },
  "requires_arc": true,
  "libraries": [
    "z",
    "bz2",
    "c++",
    "iconv"
  ],
  "source": {
    "http": "https://github.com/tanersener/mobile-ffmpeg/releases/download/v4.4/mobile-ffmpeg-video-4.4-tvos-framework.zip"
