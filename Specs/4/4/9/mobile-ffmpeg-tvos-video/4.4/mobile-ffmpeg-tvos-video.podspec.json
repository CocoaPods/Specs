      "freetype.framework",
      "fribidi.framework",
      "giflib.framework",
      "jpeg.framework",
      "kvazaar.framework",
      "libaom.framework",
      "libass.framework",
      "libogg.framework",
      "libpng.framework",
      "libtheora.framework",
      "libtheoradec.framework",
      "libtheoraenc.framework",
      "libvorbis.framework",
      "libvorbisenc.framework",
      "libvorbisfile.framework",
      "libvpx.framework",
      "libwebp.framework",
      "libwebpmux.framework",
      "libwebpdemux.framework",
      "snappy.framework",
      "tiff.framework"
