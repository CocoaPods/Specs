  },
  "authors": {
    "Oliver Borchert": "borchero@icloud.com"
  },
  "source": {
    "git": "https://github.com/borchero/BxGesture.git",
    "tag": "1.0.1"
  },
  "platforms": {
    "ios": "11.0"
  },
  "source_files": "BxGesture/**/*",
  "dependencies": {
    "RxSwift": [
      "~> 4.0"
    ],
    "RxCocoa": [
      "~> 4.0"
    ],
    "RxGesture": [
      "~> 1.2"
    ]
  },
  "frameworks": "UIKit"
