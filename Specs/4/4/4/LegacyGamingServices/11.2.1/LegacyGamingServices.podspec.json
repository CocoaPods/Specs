{
  "name": "LegacyGamingServices",
  "version": "11.2.1",
  "summary": "The legacy Objective-C implementation of FBSDKGamingServicesKit that will be converted to Swift.",
  "description": "The legacy Objective-C implementation of FBSDKGamingServicesKit that will be converted to Swift.\nThis will not contain interfaces for new features written in Swift.\nIf you are looking for the FacebookGamingServices SDK please use `pod FacebookGamingServices`.",
  "homepage": "https://developers.facebook.com/docs/ios/",
  "license": {
    "type": "Facebook Platform License",
    "file": "LICENSE"
  },
  "authors": "Facebook",
  "platforms": {
    "ios": "9.0"
  },
  "swift_versions": "5.0",
  "source": {
    "git": "https://github.com/facebook/facebook-ios-sdk.git",
    "tag": "v11.2.1"
  },
  "weak_frameworks": [
    "Accounts",
    "Social",
    "Security",
    "Foundation",
    "CoreGraphics"
  ],
  "requires_arc": true,
  "pod_target_xcconfig": {
    "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited) FBSDKCOCOAPODS=1",
    "OTHER_SWIFT_FLAGS": "$(inherited) -Xcc -DFBSDKCOCOAPODS",
    "DEFINES_MODULE": "YES"
  },
  "source_files": "FBSDKGamingServicesKit/LegacyGamingServices/**/*.{h,m}",
  "public_header_files": "FBSDKGamingServicesKit/LegacyGamingServices/*.{h}",
  "exclude_files": "FBSDKGamingServicesKit/LegacyGamingServices/include/**/*",
  "dependencies": {
    "FBSDKCoreKit_Basics": [
      "~> 11.2.1"
    ],
    "FBSDKCoreKit": [
      "~> 11.2.1"
    ]
  },
  "swift_version": "5.0"
}
