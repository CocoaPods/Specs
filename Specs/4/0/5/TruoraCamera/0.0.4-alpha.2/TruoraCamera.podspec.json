{
  "name": "TruoraCamera",
  "version": "0.0.4-alpha.2",
  "summary": "Truora camera module to capture input for biometric validations",
  "description": "TruoraCamera is a camera module based on AVFoundation, with facial detection (Vision/CoreML)\nused by TruoraValidationsSDK for passive capture and biometric flows.",
  "homepage": "https://bitbucket.org/truora/truora-sdks",
  "license": {
    "type": "Proprietary",
    "text": "Copyright Truora. All rights reserved."
  },
  "authors": {
    "Truora": "truora-apps@truora.com"
  },
  "platforms": {
    "ios": "13.0"
  },
  "swift_versions": "5.9",
  "static_framework": true,
  "source": {
    "git": "https://bitbucket.org/truora/truora-sdks.git",
    "tag": "0.0.4-alpha.2"
  },
  "source_files": [
    "ios/validations/TruoraCamera/Sources/**/*.{swift}",
    "ios/validations/TensorFlowLite/Sources/**/*.{swift}",
    "ios/validations/Derived/Sources/TuistAssets+TruoraCamera.swift",
    "ios/validations/Derived/Sources/TuistBundle+TruoraCamera.swift"
  ],
  "resource_bundles": {
    "TruoraCameraResources": [
      "ios/validations/TruoraCamera/Sources/Assets.xcassets",
      "ios/validations/TruoraCamera/Resources/**/*.tflite"
    ]
  },
  "vendored_frameworks": [
    "ios/validations/XCFrameworks/TensorFlowLiteC.xcframework"
  ],
  "frameworks": [
    "AVFoundation",
    "UIKit",
    "CoreGraphics",
    "Vision",
    "CoreML"
  ],
  "pod_target_xcconfig": {
    "PRODUCT_MODULE_NAME": "TruoraCamera",
    "DEFINES_MODULE": "YES",
    "SWIFT_VERSION": "5.9",
    "SWIFT_ACTIVE_COMPILATION_CONDITIONS": "COCOAPODS"
  },
  "swift_version": "5.9"
}
