{
  "name": "smaato-ios-sdk-mediation-admob",
  "version": "7.69.0.0",
  "summary": "Google mobile ads adapter used for mediation with the Smaato SDK",
  "homepage": "https://www.smaato.com/",
  "license": {
    "type": "MIT",
    "file": "LICENSE.txt"
  },
  "authors": {
    "Smaato": "ankur.shivani@smaato.com"
  },
  "platforms": {
    "ios": "9.0"
  },
  "requires_arc": true,
  "source": {
    "http": "https://smaato-sdk-releases.s3.amazonaws.com/iOSMediation/admob-ios-sdk/7.69.0.0/smaato-ios-sdk-mediation-admob.zip"
  },
  "default_subspecs": "Adapters",
  "static_framework": true,
  "dependencies": {
    "Google-Mobile-Ads-SDK": [
      "7.69.0"
    ]
  },
  "subspecs": [
    {
      "name": "Adapters",
      "dependencies": {
        "smaato-ios-sdk": [
          ">= 21.5.2"
        ],
        "smaato-ios-sdk-mediation-admob/Banner": [

        ],
        "smaato-ios-sdk-mediation-admob/Interstitial": [

        ],
        "smaato-ios-sdk-mediation-admob/RewardedAds": [

        ],
        "smaato-ios-sdk-mediation-admob/Native": [

        ]
      }
    },
    {
      "name": "Banner",
      "dependencies": {
        "smaato-ios-sdk/Banner": [
          ">= 21.5.2"
        ]
      },
      "source_files": "SmaatoSDKAdapters/AdMob/Banner/*.{h,m}"
    },
    {
      "name": "Interstitial",
      "dependencies": {
        "smaato-ios-sdk/Interstitial": [
          ">= 21.5.2"
        ]
      },
      "source_files": "SmaatoSDKAdapters/AdMob/Interstitial/*.{h,m}"
    },
    {
      "name": "RewardedAds",
      "dependencies": {
        "smaato-ios-sdk/RewardedAds": [
          ">= 21.5.2"
        ]
      },
      "source_files": "SmaatoSDKAdapters/AdMob/RewardedVideo/*.{h,m}"
    },
    {
      "name": "Native",
      "dependencies": {
        "smaato-ios-sdk/Native": [
          ">= 21.5.2"
        ]
      },
      "source_files": "SmaatoSDKAdapters/AdMob/Native/*.{h,m}"
    },
    {
      "name": "CSM",
      "subspecs": [
        {
          "name": "Banner",
          "dependencies": {
            "smaato-ios-sdk/Banner": [
              ">= 21.5.2"
            ]
          },
          "vendored_frameworks": "SmaatoSDKAdMobCSMBannerAdapter.framework"
        },
        {
          "name": "Interstitial",
          "dependencies": {
            "smaato-ios-sdk/Interstitial": [
              ">= 21.5.2"
            ]
          },
          "vendored_frameworks": "SmaatoSDKAdMobCSMInterstitialAdapter.framework"
        },
        {
          "name": "RewardedVideo",
          "dependencies": {
            "smaato-ios-sdk/RewardedAds": [
              ">= 21.5.2"
            ]
          },
          "vendored_frameworks": "SmaatoSDKAdMobCSMRewardedVideoAdapter.framework"
        }
      ]
    }
  ]
}
