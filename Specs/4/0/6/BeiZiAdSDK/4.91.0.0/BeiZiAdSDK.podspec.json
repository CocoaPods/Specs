{
  "name": "BeiZiAdSDK",
  "version": "4.91.0.0",
  "summary": "BeiZiAdSDK 广告平台, 用于请求广告的iOS SDK.",
  "description": "支持开屏, banner, 原生, 激励, 插屏, 原生插屏, 自定义类型广告请求。",
  "homepage": "https://gitee.com/adscope/beiziadsdk.git",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "lvjunxue": "lvjunxue@hubcloud.com.cn"
  },
  "source": {
    "git": "https://gitee.com/adscope/beiziadsdk.git",
    "tag": "4.91.0.0"
  },
  "platforms": {
    "ios": "10.0"
  },
  "default_subspecs": [
    "BeiZiAdSDK"
  ],
  "libraries": [
    "z",
    "c++",
    "sqlite3",
    "xml2",
    "c++abi",
    "iconv",
    "c"
  ],
  "frameworks": [
    "AdSupport",
    "AVFoundation",
    "AppTrackingTransparency",
    "CoreData",
    "CoreLocation",
    "CoreMotion",
    "CoreTelephony",
    "QuartzCore",
    "SystemConfiguration",
    "Security",
    "StoreKit",
    "WebKit"
  ],
  "xcconfig": {
    "OTHER_LDFLAGS": "-ObjC"
  },
  "subspecs": [
    {
      "name": "BeiZiAdSDK",
      "vendored_frameworks": [
        "BeiZiSDK.xcframework",
        "BeiZiFoundation.xcframework"
      ]
    }
  ]
}
