{
  "name": "MockingbirdFramework",
  "module_name": "Mockingbird",
  "version": "0.20.0",
  "summary": "A Swifty mocking framework for Swift and Objective-C.",
  "homepage": "https://github.com/birdrides/mockingbird",
  "license": {
    "type": "MIT",
    "file": "LICENSE.md"
  },
  "authors": {
    "Andrew Chang": "typealiased@gmail.com"
  },
  "source": {
    "git": "https://github.com/birdrides/mockingbird.git",
    "tag": "0.20.0"
  },
  "platforms": {
    "ios": "9.0",
    "osx": "10.10",
    "tvos": "9.0",
    "watchos": "7.4"
  },
  "swift_versions": "5.0",
  "frameworks": "XCTest",
  "user_target_xcconfig": {
    "FRAMEWORK_SEARCH_PATHS": "$(PLATFORM_DIR)/Developer/Library/Frameworks"
  },
  "pod_target_xcconfig": {
    "ENABLE_BITCODE": "NO",
    "ENABLE_TESTABILITY": "YES",
    "COPY_PHASE_STRIP": "NO",
    "DEFINES_MODULE": "YES"
  },
  "source_files": [
    "Sources/MockingbirdFramework/**/*.{swift,h,m,mm}",
    "Sources/MockingbirdCommon/**/*.swift"
  ],
  "preserve_paths": [
    "README.md",
    "LICENSE.md",
    "mockingbird",
    "Sources/MockingbirdAutomationCli/Resources/CodesigningRequirements/*",
    "Sources/MockingbirdFramework/Info.plist",
    "Sources/MockingbirdCli/Info.plist"
  ],
  "swift_version": "5.0"
}
