{
  "name": "WebSocketRocket",
  "version": "0.5.2",
  "summary": "A conforming WebSocket (RFC 6455) client library for iOS, macOS and tvOS.",
  "homepage": "https://github.com/facebook/SocketRocket",
  "authors": {
    "Nikita Lutsenko": "nlutsenko@me.com",
    "Dan Federman": "federman@squareup.com",
    "Mike Lewis": "mikelikespie@gmail.com",
    "Dmitry Lizin": "sdkdimon@gmail.com"
  },
  "license": "BSD",
  "source": {
    "git": "https://github.com/sdkdimon/SocketRocket.git",
    "tag": "0.5.2"
  },
  "requires_arc": true,
  "source_files": "SocketRocket/**/*.{h,m}",
  "public_header_files": "SocketRocket/*.h",
  "exclude_files": "SocketRocket/SocketRocket.h",
  "platforms": {
    "ios": "9.0",
    "osx": "10.9",
    "tvos": "9.0"
  },
  "ios": {
    "frameworks": [
      "CFNetwork",
      "Security"
    ]
  },
  "osx": {
    "frameworks": [
      "CoreServices",
      "Security"
    ]
  },
  "tvos": {
    "frameworks": [
      "CFNetwork",
      "Security"
    ]
  },
  "libraries": "icucore"
}
