{
  "name": "TanxSDK",
  "platforms": {
    "ios": "12.0"
  },
  "version": "3.7.21",
  "summary": "an adSDK for media",
  "description": "TanxSDK is a product provided by Alibaba Group for monetizing advertisements for external media.",
  "authors": {
    "tongyuecheng": "wb-tyc866331@alibabapictures.com"
  },
  "homepage": "https://github.com/Alimama-TanxSDK/TanxSDK.git",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "source": {
    "git": "https://github.com/Alimama-TanxSDK/TanxSDK.git",
    "tag": "3.7.21"
  },
  "vendored_frameworks": [
    "TanxSDK/TanxSDK.library/TanxSDK.xcframework",
    "TanxSDK/TanxSDK.library/TNXASDK.xcframework",
    "TanxSDK/TanxSDK.library/TanxMonitor.xcframework"
  ],
  "pod_target_xcconfig": {
    "VALID_ARCHS[sdk=iphonesimulator*]": "x86_64 arm64",
    "EXCLUDED_ARCHS[sdk=iphoneos*]": "i386 armv7 armv7s",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "armv7 armv7s",
    "OTHER_LDFLAGS": "-ObjC"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "armv7 armv7s",
    "OTHER_LDFLAGS": "$(inherited) -ObjC"
  },
  "resources": [
    "TanxSDK/TanxSDK.library/TanxSDK.xcframework/ios-arm64/TanxSDK.framework/TanxID.bundle/**/*"
  ],
  "preserve_paths": [
    "TanxSDK/TanxSDK.library/TanxSDK.xcframework",
    "TanxSDK/TanxSDK.library/TNXASDK.xcframework",
    "TanxSDK/TanxSDK.library/TanxMonitor.xcframework"
  ],
  "exclude_files": "TanxSDK/TanxSDK.library/**/*.md",
  "source_files": [
    "TanxSDK/TanxSDK.library/TanxSDK.xcframework/ios-arm64/TanxSDK.framework/Headers/*.h",
    "TanxSDK/TanxSDK.library/TanxSDK.xcframework/ios-arm64_x86_64-simulator/TanxSDK.framework/Headers/*.h"
  ]
}
