{
  "name": "AdHubAdNetwork",
  "version": "2.1",
  "summary": "BaiduSDK, AFPSDK, GDTSDK, IFLY 广告SDK.",
  "description": "AdHubAdNetwork 聚合Baidu，GDT，AFP，科大讯飞 等移动广告平台。",
  "homepage": "https://github.com/songMW/AdHubAdNetwork",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "songMW": "songshoubing7664@163.com"
  },
  "source": {
    "git": "https://github.com/songMW/AdHubAdNetwork.git",
    "tag": "2.1"
  },
  "platforms": {
    "ios": "8.0"
  },
  "frameworks": [
    "SystemConfiguration",
    "CoreGraphics",
    "CoreTelephony",
    "Security",
    "CoreLocation",
    "JavaScriptCore",
    "UIKit",
    "StoreKit",
    "CoreMotion",
    "AVFoundation",
    "AudioToolbox",
    "MediaPlayer",
    "CoreGraphics",
    "WebKit",
    "SafariServices",
    "MessageUI",
    "AdSupport",
    "CoreMedia"
  ],
  "libraries": [
    "iconv",
    "sqlite3",
    "stdc++",
    "z",
    "xml2"
  ],
  "subspecs": [
    {
      "name": "GDT",
      "source_files": "AdHubAdNetwork/Classes/GDT/*",
      "vendored_libraries": "AdHubAdNetwork/Classes/GDT/*.a"
    },
    {
      "name": "Baidu",
      "source_files": "AdHubAdNetwork/Classes/Baidu/*",
      "vendored_libraries": "AdHubAdNetwork/Classes/Baidu/*.a",
      "resources": "AdHubAdNetwork/Classes/Baidu/*.bundle"
    },
    {
      "name": "AFP",
      "source_files": "AdHubAdNetwork/Classes/AFP/*",
      "vendored_frameworks": "AdHubAdNetwork/Classes/AFP/*.framework",
      "vendored_libraries": "AdHubAdNetwork/Classes/AFP/*.a",
      "resources": "AdHubAdNetwork/Classes/AFP/*.bundle"
    },
    {
      "name": "IFLY",
      "source_files": "AdHubAdNetwork/Classes/IFLY/*",
      "vendored_libraries": "AdHubAdNetwork/Classes/IFLY/*.a",
      "resources": "AdHubAdNetwork/Classes/IFLY/*.bundle"
    }
  ]
}
