{
  "name": "UdeskSDK",
  "version": "1.1",
  "license": "MIT",
  "summary": "Udesk SDK for iOS",
  "homepage": "https://github.com/xushichen/UdeskSDK",
  "authors": {
    "xuchen ": "xuc@udesk.cn"
  },
  "source": {
    "git": "https://github.com/xushichen/UdeskSDK.git",
    "tag": "1.1"
  },
  "source_files": [
    "UdeskSDK/UDChatMessage/**/*.{h,m}",
    "UdeskSDK/SDK/*.{h}"
  ],
  "platforms": {
    "ios": "6.0"
  },
  "requires_arc": true,
  "frameworks": [
    "AVFoundation",
    "CoreTelephony",
    "SystemConfiguration",
    "MobileCoreServices"
  ],
  "libraries": [
    "z",
    "xml2",
    "resolv",
    "sqlite3"
  ],
  "resources": "UdeskSDK/Resource/UdeskBundle.bundle",
  "vendored_libraries": "UdeskSDK/SDK/libUdesk.a",
  "xcconfig": {
    "OTHER_LDFLAGS": "-ObjC",
    "HEADER_SEARCH_PATHS": "$(SDKROOT)/usr/include/libxml2"
  }
}
