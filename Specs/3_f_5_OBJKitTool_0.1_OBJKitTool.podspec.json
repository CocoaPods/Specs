{
  "name": "OBJKitTool",
  "version": "0.1",
  "summary": "A Objective-c extenesions and utils classes for iOS .",
  "homepage": "https://github.com/CoderLawrence/OBJKitTool",
  "license": {
    "type": "MIT",
    "file": "./LICENSE.md"
  },
  "authors": {
    "Lawrence": "791785648@qq.com"
  },
  "platforms": {
    "ios": "7.0"
  },
  "source": {
    "git": "https://github.com/CoderLawrence/OBJKitTool.git",
    "tag": "0.1",
    "submodules": true
  },
  "default_subspecs": [
    "Default",
    "UIKit",
    "Utils"
  ],
  "requires_arc": true,
  "subspecs": [
    {
      "name": "Default",
      "source_files": [
        "OBJKitTool/OBJKitTool.h",
        "OBJKitTool/**/*{h,m}"
      ],
      "public_header_files": [
        "OBJKitTool/OBJKitTool.h",
        "OBJKitTool/**/*.h"
      ]
    },
    {
      "name": "UIKit",
      "source_files": "OBJKitTool/UIKit/*.{h,m}",
      "public_header_files": "OBJKitTool/UIKit/*.{h}"
    },
    {
      "name": "Utils",
      "source_files": "OBJKitTool/Utils/*.{h,m}",
      "public_header_files": "OBJKitTool/Utils/*.{h}"
    }
  ]
}
