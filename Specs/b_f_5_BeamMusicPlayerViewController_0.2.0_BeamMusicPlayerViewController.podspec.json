{
  "name": "BeamMusicPlayerViewController",
  "version": "0.2.0",
  "summary": "An iPhone/iPad view controller to visualize and control music playback.",
  "homepage": "https://github.com/BeamApp/MusicPlayerViewController",
  "license": {
    "type": "New BSD",
    "file": "LICENSE"
  },
  "authors": {
    "Moritz Haarmann": "http://momo.brauchtman.net",
    "Heiko Behrens": "http://HeikoBehrens.net"
  },
  "source": {
    "git": "https://github.com/BeamApp/MusicPlayerViewController.git",
    "tag": "0.2.0"
  },
  "platforms": {
    "ios": "5.0"
  },
  "source_files": "Source/*.{h,m}",
