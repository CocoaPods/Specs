{
  "name": "XCFit",
  "version": "0.1.4",
  "summary": "Full Stack BDD for iOS and macOS Apps with Swift, Xcode using XCUITest, Cucumberish, FitNesse and friends.",
  "description": "XCFit is a full stack BDD framework for iOS and macOS apps with Swift using Cucumberish and Fitnesse. Native Xcode implementation, No Appium, No Calabash, No Ruby or any other non iOS language.",
  "homepage": "https://github.com/Shashikant86/XCFit",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Shashikant86": "shashikant.jagtap@icloud.com"
  },
  "source": {
    "git": "https://github.com/Shashikant86/XCFit.git",
    "tag": "0.1.4"
  },
  "social_media_url": "https://twitter.com/Shashikant86",
  "platforms": {
    "ios": "9.0"
  },
  "source_files": [
    "XCFit/Classes/Cucumberish/*.{h,m}",
    "XCFit/Classes/Cucumberish/Core/Managers/*.{h,m}",
    "XCFit/Classes/Cucumberish/Core/Models/*.{h,m}",
    "XCFit/Classes/Cucumberish/Utils/*.{h,m}",
    "XCFit/Classes/Cucumberish/Dependencies/Gherkin",
    "XCFit/Classes/Cucumberish/Core/CCIBlockDefinitions.h"
  ],
  "public_header_files": [
    "XCFit/Classes/Cucumberish/Cucumberish.h",
    "XCFit/Classes/Cucumberish/Core/Managers/CCIStepsManager.h",
    "XCFit/Classes/Cucumberish/Core/CCIBlockDefinitions.h",
    "XCFit/Classes/Cucumberish/Core/Models/CCIScenarioDefinition.h",
    "XCFit/Classes/Cucumberish/Core/Models/CCIExample.h",
    "XCFit/Classes/Cucumberish/Core/Models/CCIStep.h",
    "XCFit/Classes/Cucumberish/Core/Models/CCILocation.h",
    "XCFit/Classes/Cucumberish/Core/Models/CCIArgument.h"
  ],
  "resource_bundles": {
    "GherkinLanguages": [
      "XCFit/Classes/Cucumberish/Dependencies/Gherkin/gherkin-languages.json"
    ]
  },
  "frameworks": "XCTest",
  "dependencies": {
    "OCSlimProject": [
      "1.3"
    ],
    "OCSlimProjectTestBundleSupport": [
      "1.3"
    ]
  },
  "xcconfig": {
    "GCC_PREPROCESSOR_DEFINITIONS": "SRC_ROOT=@\\\"$(SRCROOT)\\\""
  }
}
