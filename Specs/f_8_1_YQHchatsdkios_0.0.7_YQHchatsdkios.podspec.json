  "vendored_libraries": [
    "Pod/Classes/Utils/VoiceConvert/opencore-amrnb/libopencore-amrnb.a",
    "Pod/Classes/Utils/VoiceConvert/opencore-amrwb/libopencore-amrwb.a"
  ],
  "resource_bundles": {
  },
  "dependencies": {
    "Masonry": [
      "~> 1.1.0"
    ],
    "SDWebImage": [
      "~> 4.4.6"
    ]
