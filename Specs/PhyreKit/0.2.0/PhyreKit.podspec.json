{
  "name": "PhyreKit",
  "version": "0.2.0",
  "summary": "Common utilities for Phyre Inc.",
  "homepage": "http://rallyapp.io",
  "license": "MIT",
  "authors": {
    "Matt Ricketson": "matt@phyreup.com"
  },
  "source": {
    "git": "git@bitbucket.org:phyre/phyrekit.git",
    "tag": "0.2.0"
  },
  "social_media_url": "https://twitter.com/phyreup",
  "platforms": {
    "ios": "7.0"
  },
  "requires_arc": true,
  "source_files": "Classes/*.{h,m}",
  "subspecs": [
    {
      "name": "Foundation",
      "source_files": "Classes/Foundation/*.{h,m}",
      "requires_arc": true
    },
    {
      "name": "UIKit",
      "source_files": "Classes/UIKit/*.{h,m}",
      "requires_arc": true,
      "dependencies": {
        "PhyreKit/Foundation": [

        ]
      }
    },
    {
      "name": "CoreLocation",
      "source_files": "Classes/CoreLocation/*.{h,m}",
      "requires_arc": true,
      "frameworks": "CoreLocation"
    },
    {
      "name": "Vendor",
      "source_files": "Classes/Vendor/*.{h,m}",
      "requires_arc": true
    }
  ]
}
