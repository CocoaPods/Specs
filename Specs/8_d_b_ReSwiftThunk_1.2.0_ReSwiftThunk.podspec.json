  "license": {
    "type": "MIT",
    "file": "LICENSE.md"
  },
  "authors": "ReSwift",
  "platforms": {
    "ios": "8.0",
    "osx": "10.10",
    "watchos": "2.0",
    "tvos": "9.0"
  },
  "module_name": "ReSwiftThunk",
  "swift_versions": [
    "5.0",
    "4.2",
    "4.1",
    "5.0"
  ],
  "source": {
    "git": "https://github.com/ReSwift/ReSwift-Thunk.git",
    "tag": "1.2.0"
  },
  "default_subspecs": "Core",
  "dependencies": {
    "ReSwift": [
      "~> 5.0"
    ]
  },
  "swift_version": "5.0",
