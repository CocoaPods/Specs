      "source_files": "ReSwift-Thunk"
    },
    {
      "name": "ExpectThunk",
      "platforms": {
        "ios": "8.0",
        "osx": "10.10",
        "tvos": "9.0"
      },
      "dependencies": {
        "ReSwiftThunk/Core": [

        ]
      },
      "pod_target_xcconfig": {
