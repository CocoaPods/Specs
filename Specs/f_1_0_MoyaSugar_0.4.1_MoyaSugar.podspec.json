{
  "name": "MoyaSugar",
  "version": "0.4.1",
  "summary": "🍯 Syntactic sugar for Moya",
  "homepage": "https://github.com/devxoul/MoyaSugar",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Suyeol Jeon": "devxoul@gmail.com"
  },
  "source": {
    "git": "https://github.com/devxoul/MoyaSugar.git",
    "tag": "0.4.1"
  },
  "platforms": {
    "ios": "8.0",
    "osx": "10.11",
    "tvos": "9.0",
    "watchos": "2.0"
  },
  "pod_target_xcconfig": {
    "SWIFT_VERSION": "3.1"
  },
  "default_subspecs": "Core",
  "pushed_with_swift_version": "3.1",
  "subspecs": [
    {
      "name": "Core",
      "source_files": "Sources/MoyaSugar/*.swift",
      "frameworks": "Foundation",
      "dependencies": {
        "Moya": [
          ">= 8.0.0"
        ]
      }
    },
    {
      "name": "RxSwift",
      "source_files": "Sources/RxMoyaSugar/*.swift",
      "dependencies": {
        "MoyaSugar/Core": [

        ],
        "Moya/RxSwift": [
          ">= 8.0.0"
        ],
        "RxSwift": [
          ">= 3.0.0"
        ]
      }
    }
  ]
}
