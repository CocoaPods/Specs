{
  "name": "LFNetworking",
  "version": "0.0.5",
  "license": "MIT",
  "summary": "A lightweight networking framework.",
  "homepage": "https://github.com/zhangwei100/LFNetworking",
  "authors": {
    "Wei Zhang": "zhangwei100@gmail.com"
  },
  "source": {
    "git": "https://github.com/zhangwei100/LFNetworking.git",
    "tag": "0.0.5",
    "submodules": false
  },
  "requires_arc": true,
  "dependencies": {
    "AFNetworking": [
      "~> 2.5.1"
    ]
  },
  "platforms": {
    "ios": "7.0",
    "osx": "10.9"
  },
  "source_files": [
    "LFNetworking/*",
    "LFNetworking/ThirdParty/*"
  ],
  "exclude_files": "Example",
  "ios": {
    "frameworks": [
      "MobileCoreServices",
      "CoreGraphics",
      "Security"
    ]
  },
  "osx": {
    "frameworks": [
      "CoreServices",
      "Security"
    ]
  }
}
