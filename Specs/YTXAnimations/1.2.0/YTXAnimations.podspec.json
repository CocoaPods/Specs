{
  "name": "YTXAnimations",
  "version": "1.2.0",
  "summary": "提供UIView的动画",
  "description": "The iOS animation implementation of CSS3",
  "homepage": "https://github.com/baidao/YTXAnimations",
  "license": "MIT",
  "authors": {
    "caojun": "78612846@qq.com"
  },
  "source": {
    "git": "https://github.com/baidao/YTXAnimations.git",
    "tag": "1.2.0"
  },
  "platforms": {
    "ios": "7.0"
  },
  "requires_arc": true,
  "source_files": "Pod/Classes/**/*",
  "resource_bundles": {
