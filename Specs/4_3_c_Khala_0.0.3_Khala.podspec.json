{
  "name": "Khala",
  "version": "0.0.3",
  "license": "MIT",
  "summary": "For we are bound by the Khala.(神圣的卡拉连接着我们.)",
  "homepage": "https://github.com/linhay/Khala",
  "authors": {
    "linhey": "is.linhay@outlook.com"
  },
  "source": {
    "git": "https://github.com/linhay/Khala.git",
    "tag": "0.0.3"
  },
  "swift_version": "4.2",
  "platforms": {
    "ios": "8.0",
    "tvos": "9.0",
    "osx": "10.10",
    "watchos": "2.0"
  },
  "dependencies": {
    "DarkTemplar": [
      "0.0.3"
    ]
  },
  "requires_arc": true,
  "subspecs": [
    {
      "name": "core",
      "source_files": [
        "Sources/core/*.{swift,h}"
      ],
      "public_header_files": [
        "Sources/core/Khala.h"
      ]
    },
    {
      "name": "extension",
      "dependencies": {
        "Khala/core": [

        ]
      },
      "source_files": "Sources/extension/*.swift",
      "osx": {
        "exclude_files": [
          "Sources/extension/Khala+UIKit.swift"
        ]
      },
      "watchos": {
        "exclude_files": [
          "Sources/extension/Khala+AppKit.swift",
          "Sources/extension/Khala+UIKit.swift"
        ]
      },
      "ios": {
        "exclude_files": [
          "Sources/extension/Khala+AppKit.swift"
        ]
      },
      "tvos": {
        "exclude_files": [
          "Sources/extension/Khala+AppKit.swift"
        ]
      }
    }
  ]
}
