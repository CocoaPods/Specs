{
  "name": "NimbleMockSix",
  "version": "0.1.0",
  "license": "MIT",
  "summary": "Nimble matchers for MockSix",
  "description": "With NimbleMockSix, you can easily make expectations on method invocations on MockSix mock objects in Swift.",
  "homepage": "https://github.com/lvsti/NimbleMockSix",
  "social_media_url": "https://twitter.com/cocoagrinder",
  "authors": {
    "Tamas Lustyik": "elveestei@gmail.com"
  },
  "source": {
    "git": "https://github.com/lvsti/NimbleMockSix.git",
    "tag": "v0.1.0"
  },
  "platforms": {
    "ios": "8.4",
    "osx": "10.10",
    "tvos": "9.2"
  },
  "source_files": "NimbleMockSix/*.{swift,h}",
  "public_header_files": "NimbleMockSix/NimbleMockSix.h",
  "dependencies": {
    "MockSix": [
      "~> 0.1.1"
    ],
    "Nimble": [
      "~> 5.1.1"
    ]
  },
  "frameworks": [
    "MockSix",
    "Nimble"
  ],
  "weak_frameworks": "XCTest",
  "pod_target_xcconfig": {
    "ENABLE_BITCODE": "NO",
    "OTHER_LDFLAGS": "-weak-lswiftXCTest",
    "FRAMEWORK_SEARCH_PATHS": "$(inherited) \"$(PLATFORM_DIR)/Developer/Library/Frameworks\""
  },
  "pushed_with_swift_version": "3.0"
}
