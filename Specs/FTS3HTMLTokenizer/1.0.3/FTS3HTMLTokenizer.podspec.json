{
  "name": "FTS3HTMLTokenizer",
  "version": "1.0.3",
  "summary": "FTS3 HTML Tokenizer",
  "license": "MIT",
  "authors": "Stephan Heilner",
  "homepage": "https://github.com/stephanheilner/FTS3HTMLTokenizer",
  "source": {
    "git": "https://github.com/stephanheilner/FTS3HTMLTokenizer.git",
    "tag": "1.0.3"
  },
  "source_files": "src/*.{h,c}",
  "requires_arc": true,
  "dependencies": {
    "sqlite3/fts": [

    ],
    "snowball": [

    ]
  },
  "platforms": {
    "ios": "6.0",
    "osx": "10.8"
  },
  "default_subspecs": [
    "fts3",
    "stopwords"
  ],
  "subspecs": [
    {
      "name": "fts3",
      "source_files": "src/fts/*.{h,c}"
    },
    {
      "name": "stopwords",
      "libraries": "stdc++",
      "source_files": "src/stopwords/*.{h,cpp}",
      "resources": "src/stopwords/stopwords/*.txt"
    }
  ]
}
