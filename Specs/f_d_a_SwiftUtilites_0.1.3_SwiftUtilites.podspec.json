{
  "name": "SwiftUtilites",
  "version": "0.1.3",
  "summary": "SwiftUtilites for iOS applications.",
  "description": "SwiftUtilites for iOS applications. Using ReactiveSwift. Contain Foundation utilites, UI utilites, Reactive utilites. Some base classes, like keyboard handler, Alerts etc.",
  "homepage": "https://bitbucket.org/Khalbaevdm/swiftutilites",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Dmitriy Khalbaev": "dkhalbaev@sinergo.ru"
  },
  "source": {
    "git": "https://bitbucket.org/Khalbaevdm/swiftutilites",
    "tag": "0.1.3"
  },
  "social_media_url": "https://twitter.com/KhalbaevDmitriy",
  "platforms": {
    "ios": "9.0"
  },
  "swift_version": "4",
  "subspecs": [
    {
      "name": "UIKit",
      "source_files": "SwiftUtilites/Classes/UIKit/**/*",
      "frameworks": "UIKit"
    },
    {
      "name": "Foundation",
      "source_files": "SwiftUtilites/Classes/Foundation/*",
      "frameworks": "Foundation"
    },
    {
      "name": "Classes",
      "source_files": "SwiftUtilites/Classes/Classes/*",
      "frameworks": "UIKit"
    },
    {
      "name": "Alert",
      "source_files": "SwiftUtilites/Classes/Alert/*",
      "frameworks": "Foundation"
    },
    {
      "name": "Reactive",
      "source_files": "SwiftUtilites/Classes/Reactive/*",
      "frameworks": "UIKit",
      "dependencies": {
        "ReactiveSwift": [
          "~> 3.1.0"
        ],
        "ReactiveCocoa": [
          "~> 7.1.0"
        ],
        "Result": [
          "~> 3.2.4"
        ]
      }
    }
  ]
}
