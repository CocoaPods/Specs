        ]
      }
    },
    {
      "name": "WebP",
      "source_files": "GQImageViewer/**/*.{h,m}",
      "dependencies": {
        "GQImageDownloader/WebP": [

        ]
      }
    },
    {
      "name": "CoreSD",
      "source_files": "GQImageViewer/**/*.{h,m}",
      "dependencies": {
        "SDWebImage/Core": [

        ]
      }
    },
    {
      "name": "WebPSD",
      "source_files": "GQImageViewer/**/*.{h,m}",
      "dependencies": {
        "SDWebImage/WebP": [

        ]
      }
    }
  ]
}
