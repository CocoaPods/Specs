{
  "name": "LDSDKManager",
  "version": "1.0.8",
  "summary": "iOS第三方聚合库",
  "description": "聚合QQ、微信、微博、支付宝、钉钉、Telegram等第三方库，抽象封装分享、授权、支付功能，以便其他开发者能快速接入。",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "littleplayer": "mailjiancheng@163.com"
  },
  "homepage": "https://github.com/poholo/LDSDKManager_IOS",
  "source": {
    "git": "https://github.com/poholo/LDSDKManager_IOS.git",
    "tag": "1.0.8"
  },
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "default_subspecs": "All",
  "xcconfig": {
    "VALID_ARCHS": "arm64 x86_64",
    "USER_HEADER_SEARCH_PATHS": "${PROJECT_DIR}/Pods/**"
  },
  "pod_target_xcconfig": {
    "VALID_ARCHS": "arm64 x86_64"
  },
  "frameworks": [
    "UIKit",
    "CoreGraphics",
    "Foundation"
  ],
  "subspecs": [
    {
      "name": "Core",
      "source_files": [
        "SDK/Core/**/*.{h,m,mm}",
        "SDK/Core/*.{h,m,mm}"
      ],
      "public_header_files": "SDK/Core/**/*.h",
      "dependencies": {
        "MCBase/Dto": [

        ],
        "MCBase/Log": [

        ]
      }
    },
    {
      "name": "QQ",
      "source_files": "SDK/QQPlatform/*.{h,m,mm}",
      "public_header_files": "SDK/QQPlatform/*.h",
      "dependencies": {
        "LDSDKManager/Core": [

        ],
        "MCTencentOpenAPI": [

        ]
      }
    },
    {
      "name": "Wechat",
      "source_files": [
        "SDK/WechatPlatform/**/*.{h,m,mm}",
        "SDK/WechatPlatform/*.{h,m,mm}"
      ],
      "public_header_files": "SDK/WechatPlatform/*.h",
      "dependencies": {
        "LDSDKManager/Core": [

        ],
        "WechatOpenSDK": [

        ]
      }
    },
    {
      "name": "Weibo",
      "source_files": "SDK/WeiboPlatform/*{h,m,mm}",
      "public_header_files": "SDK/WeiboPlatform/*.h",
      "dependencies": {
        "LDSDKManager/Core": [

        ],
        "Weibo_SDK": [

        ]
      }
    },
    {
      "name": "Alipay",
      "source_files": "SDK/AlipayPlatform/*{h,m,mm}",
      "public_header_files": "SDK/AlipayPlatform/*.h",
      "dependencies": {
        "AlipaySDK-iOS": [

        ],
        "LDSDKManager/Core": [

        ],
        "APOpenSdk": [

        ]
      }
    },
    {
      "name": "DingTalk",
      "source_files": "SDK/DingTalkPlatform/*{h,m,mm}",
      "public_header_files": "SDK/DingTalkPlatform/*.h",
      "dependencies": {
        "LDSDKManager/Core": [

        ],
        "MCDingTalk": [

        ]
      }
    },
    {
      "name": "Telegram",
      "source_files": "SDK/TelegramPlatform/*{h,m,mm}",
      "public_header_files": "SDK/TelegramPlatform/*.h",
      "dependencies": {
        "LDSDKManager/Core": [

        ]
      }
    },
    {
      "name": "All",
      "dependencies": {
        "LDSDKManager/QQ": [

        ],
        "LDSDKManager/Wechat": [

        ],
        "LDSDKManager/Weibo": [

        ],
        "LDSDKManager/Alipay": [

        ],
        "LDSDKManager/DingTalk": [

        ],
        "LDSDKManager/Telegram": [

        ]
      }
    }
  ]
}
