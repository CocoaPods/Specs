{
  "name": "box-ios-preview-sdk",
  "version": "1.0.2",
  "summary": "iOS Preview SDK.",
  "homepage": "https://github.com/box/box-ios-preview-sdk",
  "license": {
    "type": "Box Software Development Kit License Agreement",
    "file": "LICENSE"
  },
  "authors": "Box",
  "source": {
    "http": "https://raw.githubusercontent.com/box/box-ios-preview-sdk/v1.0.2/box-ios-preview-sdk.zip"
  },
  "platforms": {
    "ios": "7.0"
  },
  "ios": {
    "source_files": "BoxPreviewSDK.framework/Versions/A/Headers/**/*.h",
    "vendored_frameworks": "BoxPreviewSDK.framework",
    "header_dir": "BoxPreviewSDK"
  },
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "$(SDKROOT)/usr/include/libxml2"
  },
  "preserve_paths": [
    "BoxPreviewSDK.framework",
    "LICENSE"
  ],
  "requires_arc": true,
  "dependencies": {
    "box-ios-sdk": [

    ]
  },
  "resource_bundles": {
    "BoxPreviewSDKResources": [
      "BoxPreviewSDK.framework/BoxPreviewSDKResources/MDWebView.bundle",
      "BoxPreviewSDK.framework/BoxPreviewSDKResources/*Images/*.*",
      "PSPDFKit.bundle"
    ]
  },
  "libraries": [
    "z",
    "sqlite3",
    "xml2",
    "c++"
  ],
  "frameworks": [
    "QuartzCore",
    "CoreText",
    "CoreMedia",
    "MediaPlayer",
    "AVFoundation",
    "ImageIO",
    "MessageUI",
    "CoreGraphics",
    "Foundation",
    "CFNetwork",
    "MobileCoreServices",
    "SystemConfiguration",
    "AssetsLibrary",
    "Security",
    "UIKit",
    "AudioToolbox",
    "QuickLook",
    "CoreTelephony",
    "Accelerate",
    "JavaScriptCore",
    "GLKit",
    "OpenGLES",
    "CoreImage"
  ]
}
