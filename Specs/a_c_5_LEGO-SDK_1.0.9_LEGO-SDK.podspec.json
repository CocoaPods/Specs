            },
            {
              "name": "Pasteboard",
              "source_files": "SDK/Modules/Native/Pasteboard/*.{h,m}"
            },
            {
              "name": "UserDefaults",
              "source_files": "SDK/Modules/Native/UserDefaults/*.{h,m}"
            }
