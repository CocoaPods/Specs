{
  "name": "Pale",
  "version": "0.1.1",
  "summary": "Addressable Moya provider.",
  "description": "Pale is just a small addition on top of Moya to be able to use addressable providers,\nthis is, providers with a configurable base URL.",
  "homepage": "https://github.com/OPENinput/Pale",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "José González": "jose.gonzalez@openinput.com"
  },
  "source": {
    "git": "https://github.com/OPENinput/Pale.git",
    "tag": "0.1.1"
  },
  "platforms": {
    "ios": "10.0"
  },
  "swift_versions": "5.0",
  "default_subspecs": "Core",
  "subspecs": [
    {
      "name": "Core",
      "source_files": "Pale/main/classes/core/**/*",
      "dependencies": {
        "Moya": [
          "14.0.0-alpha.1"
        ]
      }
    },
    {
      "name": "RxSwift",
      "source_files": "Pale/main/classes/rx/**/*",
      "dependencies": {
        "Pale/Core": [

        ],
        "Moya/RxSwift": [
          "14.0.0-alpha.1"
        ],
        "RxSwift": [
          "~> 5.0"
        ]
      }
    }
  ],
  "swift_version": "5.0"
}
