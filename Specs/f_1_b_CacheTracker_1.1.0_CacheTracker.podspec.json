  "platforms": {
    "ios": "9.0"
  },
  "source": {
    "git": "https://github.com/ladeiko/CacheTracker.git",
    "tag": "1.1.0"
  },
  "requires_arc": true,
  "default_subspecs": "Core",
  "subspecs": [
    {
      "name": "Core",
