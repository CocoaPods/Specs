{
  "name": "DOUAudioStreamer",
  "version": "0.2.5",
  "license": {
    "type": "BSD",
    "file": "LICENSE"
  },
  "summary": "A Core Audio based streaming audio player for iOS/Mac.",
  "homepage": "https://github.com/douban/DOUAudioStreamer",
  "authors": {
    "Chongyu Zhu": "lembacon@gmail.com"
  },
  "source": {
    "git": "https://github.com/douban/DOUAudioStreamer.git",
    "tag": "0.2.5"
  },
  "source_files": "src/*.{h,m}",
  "requires_arc": true,
  "platforms": {
    "ios": "4.3",
    "osx": "10.7"
  },
  "ios": {
    "frameworks": [
      "Accelerate",
      "CFNetwork",
      "CoreAudio",
      "AudioToolbox",
      "QuartzCore",
      "OpenGLES",
      "MobileCoreServices"
    ]
  },
  "osx": {
    "frameworks": [
      "Accelerate",
      "CFNetwork",
      "CoreAudio",
      "AudioToolbox",
      "AudioUnit",
      "CoreServices"
    ]
  }
}