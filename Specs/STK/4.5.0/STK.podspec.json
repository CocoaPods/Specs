{
  "name": "STK",
  "version": "4.5.0",
  "summary": "The Synthesis ToolKit in C++ is a set of open source audio signal processing and algorithmic synthesis classes.",
  "homepage": "https://ccrma.stanford.edu/software/stk/",
  "license": {
    "type": "MIT"
  },
  "authors": {
    "Gary Scavone": "gary@ccrma.stanford.edu",
    "Perry Cook": "prc@CS.Princeton.EDU"
  },
  "source": {
    "git": "https://github.com/thestk/stk.git",
    "tag": "4.5.0"
  },
  "platforms": {
    "ios": "5.1",
    "osx": "10.7"
  },
  "source_files": [
    "src/*.cpp",
    "include/*.h"
  ],
  "public_header_files": [
    "include/*.h",
    "include/SKINI.msg",
    "include/SKINI.tbl"
  ],
  "exclude_files": [
    "include/Thread.h",
    "src/Thread.cpp",
    "include/Mutex.h",
    "src/Mutex.cpp",
    "include/UdpSocket.h",
    "src/UdpSocket.cpp",
    "include/Socket.h",
    "src/Socket.cpp",
    "include/TcpClient.h",
    "src/TcpClient.cpp",
    "include/TcpServer.h",
    "src/TcpServer.cpp",
    "include/InetWvIn.h",
    "src/InetWvIn.cpp",
    "include/InetWvOut.h",
    "src/InetWvOut.cpp",
    "include/RtAudio.h",
    "src/RtAudio.cpp",
    "include/RtMidi.h",
    "src/RtMidi.cpp",
    "include/RtWvIn.h",
    "src/RtWvIn.cpp",
    "include/RtWvOut.h",
    "src/RtWvOut.cpp",
    "include/RtError.h"
  ],
  "preserve_paths": "README.MD",
  "resource_bundles": {
    "rawwaves": "rawwaves/*.raw"
  },
  "requires_arc": true
}
