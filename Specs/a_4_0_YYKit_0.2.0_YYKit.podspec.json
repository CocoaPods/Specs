{
  "name": "YYKit",
  "version": "0.2.0",
  "summary": "Utilities for iOS",
  "homepage": "https://github.com/ibireme/YYKit",
  "license": "MIT",
  "authors": {
    "ibireme": "ibireme@gmail.com"
  },
  "source": {
    "git": "https://github.com/ibireme/YYKit.git",
    "tag": "0.2.0"
  },
  "platforms": {
    "ios": "6.0"
  },
  "requires_arc": true,
  "source_files": "YYKit/*.{h,m}",
