  },
  "frameworks": [
    "UIKit",
    "QuartzCore",
    "CoreGraphics",
    "CoreText",
    "ImageIO",
    "Accelerate",
    "Security",
    "MobileCoreServices"
  ],
  "libraries": "z",
  "subspecs": [
    {
      "name": "no-arc",
      "requires_arc": false,
      "source_files": "YYKit/NSObject+YYAddForARC.{h,m}"
    }
  ]
}
