{
  "name": "LTXiOSUtils",
  "version": "0.0.4",
  "platforms": {
    "ios": "10.0"
  },
  "summary": "通用工具类以及组件的整合、封装以及使用介绍",
  "homepage": "https://github.com/Coder-Star/LTXiOSUtils",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "CoderStar": "1340529758@qq.com"
  },
  "source": {
    "git": "https://github.com/Coder-Star/LTXiOSUtils.git",
    "tag": "0.0.4"
  },
  "requires_arc": true,
  "swift_versions": "5.0",
  "pod_target_xcconfig": {
    "DEFINES_MODULE": "YES"
  },
  "subspecs": [
    {
      "name": "Extension",
      "source_files": "Sources/LTXiOSUtils/Classes/Extension/**/*.swift"
    },
    {
      "name": "Util",
      "source_files": "Sources/LTXiOSUtils/Classes/Util/**/*",
      "subspecs": [
        {
          "name": "Log",
          "source_files": "Sources/LTXiOSUtils/Classes/Util/Log/*.swift"
        }
      ]
    },
    {
      "name": "PropertyWrapper",
      "source_files": "Sources/LTXiOSUtils/Classes/PropertyWrapper/**/*.swift"
    },
    {
      "name": "Component",
      "dependencies": {
        "LTXiOSUtils/Extension": [

        ]
      },
      "source_files": "Sources/LTXiOSUtils/Classes/Component/**/*.swift",
      "subspecs": [
        {
          "name": "Resources",
          "resource_bundles": {
            "LTXiOSUtilsComponent": "Sources/LTXiOSUtils/Classes/Component/Resources/Resource/*"
          }
        }
      ]
    }
  ],
  "swift_version": "5.0"
}
