{
  "name": "ffmpeg-kit-macos-video",
  "version": "4.5.1.LTS",
  "summary": "FFmpeg Kit macOS Video Shared Framework",
  "description": "Includes FFmpeg with dav1d, fontconfig, freetype, fribidi, kvazaar, libass, libtheora, libvpx, snappy, libwebp and zimg libraries enabled.",
  "homepage": "https://github.com/tanersener/ffmpeg-kit",
  "authors": {
    "Taner Sener": "tanersener@gmail.com"
  },
  "license": {
    "type": "LGPL-3.0",
    "file": "ffmpegkit.framework/Resources/LICENSE"
  },
  "platforms": {
    "osx": "10.12"
  },
  "requires_arc": true,
  "libraries": [
    "z",
    "bz2",
    "c++",
    "iconv"
  ],
  "source": {
    "http": "https://github.com/tanersener/ffmpeg-kit/releases/download/v4.5.1.LTS/ffmpeg-kit-video-4.5.1.LTS-macos-framework.zip"
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=macosx*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=macosx*]": "arm64"
  },
  "osx": {
    "frameworks": [
      "AudioToolbox",
      "CoreAudio",
      "CoreImage",
      "CoreMedia",
      "OpenCL",
      "OpenGL",
      "VideoToolbox"
    ],
    "vendored_frameworks": [
      "ffmpegkit.framework",
      "libavcodec.framework",
      "libavdevice.framework",
      "libavfilter.framework",
      "libavformat.framework",
      "libavutil.framework",
      "libswresample.framework",
      "libswscale.framework"
    ]
  }
}
