{
  "name": "ffmpeg-kit-macos-video",
  "version": "4.5",
  "summary": "FFmpeg Kit macOS Video Static Framework",
  "description": "Includes FFmpeg with dav1d, fontconfig, freetype, fribidi, kvazaar, libass, libtheora, libvpx, snappy and libwebp libraries enabled.",
  "homepage": "https://github.com/tanersener/ffmpeg-kit",
  "authors": {
    "Taner Sener": "tanersener@gmail.com"
  },
  "license": {
    "type": "LGPL-3.0",
    "file": "ffmpegkit.xcframework/macos-arm64_x86_64/ffmpegkit.framework/LICENSE"
  },
  "platforms": {
    "osx": "10.15"
  },
  "requires_arc": true,
  "libraries": [
    "z",
    "bz2",
    "c++",
    "iconv"
  ],
  "source": {
    "http": "https://github.com/tanersener/ffmpeg-kit/releases/download/v4.5/ffmpeg-kit-video-4.5-macos-xcframework.zip"
  },
  "osx": {
    "frameworks": [
      "AudioToolbox",
      "AVFoundation",
      "CoreAudio",
      "CoreImage",
      "CoreMedia",
      "OpenCL",
      "OpenGL",
      "VideoToolbox"
    ],
    "vendored_frameworks": [
      "ffmpegkit.xcframework",
      "libavcodec.xcframework",
      "libavdevice.xcframework",
      "libavfilter.xcframework",
      "libavformat.xcframework",
      "libavutil.xcframework",
      "libswresample.xcframework",
      "libswscale.xcframework",
      "dav1d.xcframework",
      "expat.xcframework",
      "fontconfig.xcframework",
      "freetype.xcframework",
      "fribidi.xcframework",
      "giflib.xcframework",
      "harfbuzz.xcframework",
      "jpeg.xcframework",
      "kvazaar.xcframework",
      "libass.xcframework",
      "libogg.xcframework",
      "libpng.xcframework",
      "libtheora.xcframework",
      "libtheoradec.xcframework",
      "libtheoraenc.xcframework",
      "libvorbis.xcframework",
      "libvorbisenc.xcframework",
      "libvorbisfile.xcframework",
      "libvpx.xcframework",
      "libwebp.xcframework",
      "libwebpmux.xcframework",
      "libwebpdemux.xcframework",
      "snappy.xcframework",
      "tiff.xcframework"
    ]
  }
}
