{
  "name": "Starlift",
  "version": "1.0.7",
  "summary": "Starlift is a library that intends to improve the general feedback and reviews received by a certain app.",
  "description": "Starlift framework helps developers get user feedback and react based on the certain feedback. This was proven to have a positive impact over app reviews.",
  "homepage": "http://starlift.3pillarglobal.com/",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "3Pillar Global": "starlift-support@3pillarglobal.com"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source": {
    "git": "https://github.com/3PillarGlobal/Starlift-iOS.git",
    "tag": "1.0.7"
  },
  "source_files": [
    "Starlift",
    "Starlift/**/*.swift"
  ],
  "resource_bundles": {
    "Starlift": [
      "Starlift/**/*.html",
      "Starlift/**/*.png",
      "Starlift/**/Starlift.storyboard",
      "Starlift/**/*.xib"
    ]
  },
  "frameworks": [
    "UIKit",
    "CFNetwork",
    "Security",
    "SystemConfiguration",
    "CoreTelephony",
    "WebKit",
    "MessageUI",
    "JavaScriptCore"
  ]
}
