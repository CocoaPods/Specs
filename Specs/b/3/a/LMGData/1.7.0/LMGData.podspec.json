  "homepage": "https://github.com/loopmediagroup/loopmediagroup-ios",
  "license": {
    "type": "Copyright",
    "file": "LICENSE"
  },
  "authors": {
    "Loop Media Group": "dev@loopmediagroup.com"
  },
