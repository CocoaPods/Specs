{
  "name": "AIRexLib",
  "version": "0.8.8",
  "summary": "iOS Sdk helps you to track user events in your app and display notifications",
  "homepage": "https://www.appier.com/en/index.html",
  "documentation_url": "https://docs.aiqua.appier.com/docs",
  "license": "MIT",
  "authors": {
    "appier": "rex.chen@appier.com"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source": {
    "git": "https://github.com/chunta/HeyRexLib.git",
    "tag": "0.8.8"
  },
  "default_subspecs": "ios-sdk",
  "requires_arc": true,
  "static_framework": true,
  "subspecs": [
    {
      "name": "ios-sdk",
      "libraries": "z",
      "source_files": "NewLib/Classes/**/*.{h, m, swift}",
      "vendored_libraries": "NewLib/Classes/**/*.a",
      "frameworks": [
        "AdSupport",
        "CoreTelephony",
        "SystemConfiguration",
        "CoreLocation",
        "ImageIO",
        "MobileCoreServices"
      ]
    }
  ]
}
