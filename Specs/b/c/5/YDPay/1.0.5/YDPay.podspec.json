{
  "name": "YDPay",
  "version": "1.0.5",
  "summary": "优点付费SDK",
  "description": "优点付费SDK，集成了微信、支付宝、银联等主流的支付SDK，为开发者提供统一的支付接入体验。",
  "homepage": "https://www.dataeye.com",
  "license": "MIT",
  "authors": {
    "xqwang": "wxq491216@163.com"
  },
  "source": {
    "git": "https://github.com/DataEye/YDPay.git"
  },
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "source_files": [
    "YDPay/*.h",
    "YDPay/SPaySDK/include/*.h",
    "YDPay/wechat/*.h"
  ],
  "vendored_libraries": [
    "YDPay/*.a",
    "YDPay/SPaySDK/libSPaySDK.a",
    "YDPay/wechat/libWeChatSDK.a"
  ],
  "vendored_frameworks": "YDPay/AlipaySDK.framework",
  "resources": [
    "YDPay/AlipaySDK.bundle",
    "YDPay/YDPay.bundle",
    "YDPay/SPaySDK/SPaySDKResource.bundle"
  ],
  "frameworks": [
    "CoreMotion",
    "CFNetwork",
    "Foundation",
    "UIKit",
    "CoreGraphics",
    "CoreText",
    "QuartzCore",
    "AdSupport",
    "Security",
    "CoreTelephony",
    "CoreLocation",
    "SystemConfiguration",
    "AVFoundation"
  ],
  "libraries": [
    "z",
    "c++",
    "sqlite3"
  ],
  "dependencies": {
    "AFNetworking": [
      "~> 3.0"
    ]
  }
}
