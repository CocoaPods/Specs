{
  "name": "GTVSdk",
  "version": "2.0.0",
  "summary": "GTV SDK cho iOS",
  "description": "SDK hỗ trợ login, notification, tracking và quảng cáo cho ứng dụng iOS.",
  "homepage": "https://github.com/gtv-source/gtv-ios-dev-sdk.git",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "gtv-source": "dev-team@gtv.vn"
  },
  "source": {
    "git": "https://github.com/gtv-source/gtv-ios-dev-sdk.git",
    "tag": "2.0.0"
  },
  "platforms": {
    "ios": "15.0"
  },
  "swift_versions": [
    "5.0",
    "5.5",
    "5.9"
  ],
  "static_framework": false,
  "default_subspecs": "Core",
  "subspecs": [
    {
      "name": "Core",
      "vendored_frameworks": "Core/GTVSdkIos.xcframework",
      "dependencies": {
        "Firebase/Messaging": [
          "~> 12.3.0"
        ],
        "airbridge-ios-sdk": [
          "~> 4.8.1"
        ],
        "Firebase/Analytics": [
          "~> 12.3.0"
        ]
      }
    },
    {
      "name": "Full",
      "vendored_frameworks": "Full/GTVSdkIos.xcframework",
      "dependencies": {
        "Google-Mobile-Ads-SDK": [
          "~> 12.11.0"
        ],
        "Firebase/Messaging": [
          "~> 12.3.0"
        ],
        "Firebase/Analytics": [
          "~> 12.3.0"
        ],
        "airbridge-ios-sdk": [
          "~> 4.8.1"
        ]
      }
    }
  ],
  "swift_version": "5.9"
}
