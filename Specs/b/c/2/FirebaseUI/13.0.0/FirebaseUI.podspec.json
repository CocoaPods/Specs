{
  "name": "FirebaseUI",
  "version": "13.0.0",
  "summary": "UI binding libraries for Firebase.",
  "homepage": "https://github.com/firebase/FirebaseUI-iOS",
  "license": {
    "type": "Apache 2.0",
    "file": "LICENSE"
  },
  "source": {
    "git": "https://github.com/firebase/FirebaseUI-iOS.git",
    "tag": "v13.0.0"
  },
  "authors": "Firebase",
  "platforms": {
    "ios": "12.0"
  },
  "ios": {
    "frameworks": "UIKit"
  },
  "requires_arc": true,
  "public_header_files": "FirebaseUI.h",
  "source_files": "FirebaseUI.h",
  "swift_versions": "5.3",
  "cocoapods_version": ">= 1.8.0",
  "pod_target_xcconfig": {
    "HEADER_SEARCH_PATHS": "\"${PODS_TARGET_SRCROOT}\""
  },
  "subspecs": [
    {
      "name": "Database",
      "dependencies": {
        "FirebaseDatabaseUI": [
          "~> 13.0"
        ]
      }
    },
    {
      "name": "Firestore",
      "dependencies": {
        "FirebaseFirestoreUI": [
          "~> 13.0"
        ]
      }
    },
    {
      "name": "Storage",
      "dependencies": {
        "FirebaseStorageUI": [
          "~> 13.0"
        ]
      }
    },
    {
      "name": "Auth",
      "dependencies": {
        "FirebaseAuthUI": [
          "~> 13.0"
        ]
      }
    },
    {
      "name": "Anonymous",
      "dependencies": {
        "FirebaseAnonymousAuthUI": [
          "~> 13.0"
        ]
      }
    },
    {
      "name": "Email",
      "dependencies": {
        "FirebaseEmailAuthUI": [
          "~> 13.0"
        ]
      }
    },
    {
      "name": "Facebook",
      "dependencies": {
        "FirebaseFacebookAuthUI": [
          "~> 13.0"
        ]
      }
    },
    {
      "name": "Google",
      "dependencies": {
        "FirebaseGoogleAuthUI": [
          "~> 13.0"
        ]
      }
    },
    {
      "name": "OAuth",
      "dependencies": {
        "FirebaseOAuthUI": [
          "~> 13.0"
        ]
      }
    },
    {
      "name": "Phone",
      "dependencies": {
        "FirebasePhoneAuthUI": [
          "~> 13.0"
        ]
      }
    }
  ],
  "swift_version": "5.3"
}
