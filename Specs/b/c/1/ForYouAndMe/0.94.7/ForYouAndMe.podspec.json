{
  "name": "ForYouAndMe",
  "version": "0.94.7",
  "summary": "Framework for research studies apps",
  "description": "ForYouAndMe is a framework aimed to easily develop an app for research study",
  "homepage": "https://github.com/4YouandMeData/4YouandMeiOS",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Giuseppe Lapenta": "giuseppe@balzo.eu"
  },
  "source": {
    "git": "https://github.com/4YouandMeData/4YouandMeiOS.git",
    "tag": "0.94.7"
  },
  "cocoapods_version": ">= 1.6.0",
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "x86_64 arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "platforms": {
    "ios": "15.6"
  },
  "swift_versions": "5.2",
  "source_files": "ForYouAndMe/Classes/**/*",
  "resource_bundles": {
    "ForYouAndMe": [
      "ForYouAndMe/Assets/**/*.{json,xcassets}"
    ]
  },
  "vendored_frameworks": "ForYouAndMe/Frameworks/MirSmartDevice.framework",
  "xcconfig": {
    "FRAMEWORK_SEARCH_PATHS": "$(PODS_ROOT)/ForYouAndMe/Frameworks/**"
  },
  "frameworks": "UIKit",
  "dependencies": {
    "Moya/RxSwift": [
      "~> 15.0.0"
    ],
    "Moya/ReactiveSwift": [
      "~> 15.0.0"
    ],
    "AlamofireImage": [
      "~> 4.3.0"
    ],
    "RxCocoa": [
      "~> 6.0"
    ],
    "PureLayout": [
      "~> 3.1.9"
    ],
    "SVProgressHUD": [
      "~> 2.3.1"
    ],
    "TPKeyboardAvoiding": [
      "~> 1.3.3"
    ],
    "OAuthSwift": [
      "~> 2.1.2"
    ],
    "ReachabilitySwift": [
      "~> 5.2.4"
    ],
    "Validator": [
      "~> 3.2.1"
    ],
    "Firebase/Analytics": [
      "~> 11.7.0"
    ],
    "Firebase/Crashlytics": [
      "~> 11.7.0"
    ],
    "Firebase/Messaging": [
      "~> 11.7.0"
    ],
    "PhoneNumberKit": [
      "3.3.1"
    ],
    "CountryPickerView": [
      "~> 3.1.2"
    ],
    "UberSignature": [
      "~> 1.0.3"
    ],
    "RxSwiftExt": [
      "~> 6.2.1"
    ],
    "FYAMResearchKit": [
      "~> 3.0.0"
    ],
    "StepSlider": [
      "~> 1.8.0"
    ],
    "BalzoGPUImage2": [
      "~> 0.2.1"
    ],
    "JJFloatingActionButton": [
      "~> 3.0.1"
    ]
  },
  "static_framework": true,
  "swift_version": "5.2"
}
