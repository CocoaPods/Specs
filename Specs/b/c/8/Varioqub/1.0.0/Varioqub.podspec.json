  "source": {
    "git": "https://github.com/appmetrica/varioqub-sdk-ios.git",
    "tag": "1.0.0"
  },
  "swift_versions": "5.7",
  "pod_target_xcconfig": {
    "APPLICATION_EXTENSION_API_ONLY": "YES",
