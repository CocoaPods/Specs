    {
      "name": "Core",
      "source_files": [
        "Sources/Varioqub/**/*.swift",
        "Sources/VarioqubLogger/**/*.swift",
        "Sources/VarioqubNetwork/**/*.swift",
        "Sources/VarioqubUtils/**/*.swift"
      ],
      "dependencies": {
        "SwiftProtobuf": [

        ]
      }
    },
    {
      "name": "ObjC",
      "source_files": [
        "Sources/VarioqubObjC/**/*.swift"
      ],
      "dependencies": {
        "Varioqub/Core": [

        ]
      }
    }
  ],
  "swift_version": "5.7"
