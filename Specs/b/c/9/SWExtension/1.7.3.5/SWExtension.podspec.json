  "subspecs": [
    {
      "name": "Core",
      "source_files": "SWExtension/SWExtension/Core/*.{h,m}",
      "subspecs": [
        {
          "name": "UIKit",
          "source_files": "SWExtension/SWExtension/Core/UIKit/*.{h,m}"
