{
  "name": "KJCategories",
  "version": "1.0.6",
  "summary": "Common categories for daily development. Such as UIKit, Foundation, etc.",
  "homepage": "https://github.com/yangKJ/KJCategories",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "yangkejun": "ykj310@126.com"
  },
  "source": {
    "git": "https://github.com/yangKJ/KJCategories.git",
    "tag": "1.0.6"
  },
  "platforms": {
    "ios": "9.0"
  },
  "swift_versions": "5.0",
  "requires_arc": true,
  "static_framework": true,
  "frameworks": [
    "UIKit",
    "Foundation"
  ],
  "default_subspecs": "Core",
  "ios": {
    "source_files": "Sources/KJCategories.h"
  },
  "subspecs": [
    {
      "name": "Core",
      "source_files": "Sources/Core/*.h",
      "subspecs": [
        {
          "name": "UIKit",
          "source_files": "Sources/Core/UIKit/**/*.{h,m}"
        },
        {
          "name": "Foundation",
          "source_files": "Sources/Core/Foundation/**/*.{h,m}"
        }
      ]
    },
    {
      "name": "C7",
      "source_files": "Sources/UIKit/*.h",
      "subspecs": [
        {
          "name": "UIBezierPath",
          "source_files": "Sources/UIKit/UIBezierPath/*.{h,m}"
        },
        {
          "name": "UIButton",
          "source_files": "Sources/UIKit/UIButton/*.{h,m}"
        },
        {
          "name": "UICollectionView",
          "source_files": "Sources/UIKit/UICollectionView/*.{h,m}"
        },
        {
          "name": "UIColor",
          "source_files": "Sources/UIKit/UIColor/*.{h,m}"
        },
        {
          "name": "UIDevice",
          "source_files": "Sources/UIKit/UIDevice/*.{h,m}"
        },
        {
          "name": "UIImage",
          "source_files": "Sources/UIKit/UIImage/*.{h,m}"
        },
        {
          "name": "UILabel",
          "source_files": "Sources/UIKit/UILabel/*.{h,m}"
        },
        {
          "name": "UINavigation",
          "source_files": "Sources/UIKit/UINavigation/*.{h,m}"
        },
        {
          "name": "UIResponder",
          "source_files": "Sources/UIKit/UIResponder/*.{h,m}"
        },
        {
          "name": "UISlider",
          "source_files": "Sources/UIKit/UISlider/*.{h,m}"
        },
        {
          "name": "UITabBar",
          "source_files": "Sources/UIKit/UITabBar/*.{h,m}"
        },
        {
          "name": "UITextView",
          "source_files": "Sources/UIKit/UITextView/*.{h,m}"
        },
        {
          "name": "UIView",
          "source_files": "Sources/UIKit/UIView/*.{h,m}"
        },
        {
          "name": "UIViewController",
          "source_files": "Sources/UIKit/UIViewController/*.{h,m}"
        }
      ]
    },
    {
      "name": "Foundation",
      "source_files": "Sources/Foundation/*.h",
      "subspecs": [
        {
          "name": "NSArray",
          "source_files": "Sources/Foundation/NSArray/*.{h,m}"
        },
        {
          "name": "NSData",
          "source_files": "Sources/Foundation/NSData/*.{h,m}"
        },
        {
          "name": "NSDate",
          "source_files": "Sources/Foundation/NSDate/*.{h,m}"
        },
        {
          "name": "NSDictionary",
          "source_files": "Sources/Foundation/NSDictionary/*.{h,m}"
        },
        {
          "name": "NSNumber",
          "source_files": "Sources/Foundation/NSNumber/*.{h,m}"
        },
        {
          "name": "NSObject",
          "source_files": "Sources/Foundation/NSObject/*.{h,m}"
        },
        {
          "name": "NSString",
          "source_files": "Sources/Foundation/NSString/*.{h,m}"
        },
        {
          "name": "NSTimer",
          "source_files": "Sources/Foundation/NSTimer/*.{h,m}"
        }
      ]
    },
    {
      "name": "Customized",
      "subspecs": [
        {
          "name": "GradientSlider",
          "source_files": "Sources/Customized/GradientSlider/*.{h,m}"
        },
        {
          "name": "EmitterAnimation",
          "source_files": "Sources/Customized/EmitterAnimation/*.{h,m}"
        },
        {
          "name": "ProjectionView",
          "source_files": "Sources/Customized/ProjectionView/*.swift"
        },
        {
          "name": "ShadowView",
          "source_files": "Sources/Customized/ShadowView/*.swift"
        }
      ]
    }
  ],
  "swift_version": "5.0"
}
