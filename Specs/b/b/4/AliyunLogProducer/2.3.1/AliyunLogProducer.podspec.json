        "SystemConfiguration",
        "CoreGraphics"
      ],
      "libraries": [
        "z",
        "c++"
      ],
      "pod_target_xcconfig": {
        "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64",
        "OTHER_LDFLAGS": "-ObjC"
      },
      "user_target_xcconfig": {
        "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
      }
