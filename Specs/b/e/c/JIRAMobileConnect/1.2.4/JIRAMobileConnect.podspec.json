{
  "name": "JIRAMobileConnect",
  "version": "1.2.4",
  "summary": "Enables JIRA to collect user feedback and real-time crash reports.",
  "description": "JIRA Mobile Connect enables JIRA to collect user feedback and real-time crash reports for your mobile apps. Key features include:\n\n* In-app User feedback- Get feedback from your mobile users or testers\n* 2-way Communications -Developers can follow up with users or testers for additional feedback on your app or notify them that their issue has been resolved!\n* Rich Data Collection-  Capture text and audio comments, annotated screenshots, and map any custom application data to fields in JIRA",
  "homepage": "https://bitbucket.org/atlassian/jiraconnect-ios",
  "license": "Apache License, Version 2.0",
  "authors": {
    "Nick Pellow": "http://twitter.com/niick",
    "Thomas Dohmke": "http://twitter.com/ashtom",
    "Stefan Saasen": "http://twitter.com/stefansaasen",
    "Shihab Hamid": "http://twitter.com/shihabhamid",
    "Erik Romijn": "http://twitter.com/erikpub",
    "Bindu Wavell": "http://twitter.com/binduwavell",
    "Theodora Tse": ""
  },
  "platforms": {
    "ios": "8.0"
  },
  "source": {
    "git": "https://bitbucket.org/atlassian/jiraconnect-apple.git",
    "tag": "1.2.4"
  },
  "source_files": [
    "JIRAConnect/JMCClasses/Base/**/*.{h,m,mm}",
    "JIRAConnect/JMCClasses/Core/**/*.{h,m,mm}"
  ],
  "public_header_files": [
    "JIRAConnect/JMCClasses/Base/**/*.{h}",
    "JIRAConnect/JMCClasses/Core/attachments/**/*.{h}",
    "JIRAConnect/JMCClasses/Core/sketch/**/*.{h}",
    "JIRAConnect/JMCClasses/Core/transport/**/*.{h}",
    "JIRAConnect/JMCClasses/Core/model/**/*.{h}",
    "JIRAConnect/JMCClasses/Core/queue/**/*.{h}",
    "JIRAConnect/JMCClasses/Core/audio/**/*.{h}"
  ],
  "resources": [
    "JIRAConnect/JMCClasses/Base/**/*.{xib}",
    "JIRAConnect/JMCClasses/Core/**/*.{xib}",
    "JIRAConnect/JMCClasses/Resources/**/*.{png,bundle}"
  ],
  "frameworks": [
    "CFNetwork",
    "SystemConfiguration",
    "MobileCoreServices",
    "CoreGraphics",
    "AVFoundation",
    "CoreLocation"
  ],
  "libraries": "sqlite3",
  "vendored_frameworks": "JIRAConnect/JMCClasses/libraries/CrashReporter.framework",
  "requires_arc": true,
  "pod_target_xcconfig": {
    "ENABLE_BITCODE": "NO"
  }
