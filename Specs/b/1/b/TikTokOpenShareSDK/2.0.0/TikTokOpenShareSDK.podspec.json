{
  "name": "TikTokOpenShareSDK",
  "version": "2.0.0",
  "summary": "TikTok OpenSDK Share",
  "description": "This is TikTok OpenSDK for video and image sharing.",
  "homepage": "https://github.com/tiktok/tiktok-opensdk-ios",
  "license": {
    "file": "LICENSE"
  },
  "authors": {
    "TikTok OpenPlatform": "tiktok-openplatform@bytedance.com"
  },
  "source": {
    "git": "https://github.com/tiktok/tiktok-opensdk-ios.git",
    "tag": "v2.0.0"
  },
  "platforms": {
    "ios": "11.0"
  },
  "pod_target_xcconfig": {
    "GCC_PRECOMPILE_PREFIX_HEADER": "NO",
    "DEBUG_INFORMATION_FORMAT": "DWARF with dSYM",
    "CLANG_ENABLE_MODULES": "YES",
    "GCC_GENERATE_DEBUGGING_SYMBOLS": "YES"
  },
  "default_subspecs": "Share",
  "frameworks": [
    "UIKit",
    "Foundation"
  ],
  "swift_versions": "5.0",
  "dependencies": {
    "TikTokOpenSDKCore": [
      "2.0.0"
    ]
  },
  "testspecs": [
    {
      "name": "UnitTests",
      "test_type": "unit",
      "requires_app_host": true,
      "source_files": "TikTokOpenShareSDK/Classes/Tests/**/*.swift"
    }
  ],
  "subspecs": [
    {
      "name": "Share",
      "source_files": "TikTokOpenShareSDK/Classes/Core/**/*.swift"
    }
  ],
  "swift_version": "5.0"
}
