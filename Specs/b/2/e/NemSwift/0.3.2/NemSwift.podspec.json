{
  "name": "NemSwift",
  "version": "0.3.2",
  "summary": "client library for easy use of NEM API",
  "description": "- NemSwift wraps HTTP requests to NIS(NEM Infrastructure Server) and HTTP responses from NIS.\n- NemSwift provides crypt related utilities like key pair generation signing and verifying.",
  "homepage": "https://github.com/ryuta46/nem-swift",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "ryuta46": "ryuta46g@gmail.com"
  },
  "social_media_url": "https://twitter.com/ryuta461",
  "platforms": {
    "ios": "9.0"
  },
  "source": {
    "git": "https://github.com/ryuta46/nem-swift.git",
    "tag": "0.3.2",
    "submodules": true
  },
  "source_files": [
    "NemSwift/Sources/ed25519_sha3_512.{c,h}",
    "NemSwift/Sources/message_encryption.{c,h}",
    "NemSwift/Sources/ed25519/src/*.{c,h}",
    "NemSwift/Sources/RHash/librhash/{ripemd-160,byte_order,sha3}.c",
    "NemSwift/Sources/RHash/librhash/{ripemd-160,byte_order,sha3,ustd}.h",
    "NemSwift/NemSwift.h",
    "NemSwift/**/*.swift"
  ],
  "public_header_files": [
    "NemSwift/Sources/ed25519_sha3_512.h",
    "NemSwift/Sources/message_encryption.h",
    "NemSwift/Sources/ed25519/src/ed25519.h",
    "NemSwift/NemSwift.h"
  ],
  "requires_arc": true,
  "xcconfig": {
    "GCC_PREPROCESSOR_DEFINITIONS": "\"USE_KECCAK\""
  },
  "dependencies": {
    "APIKit": [
      "~> 3.1"
    ],
    "CryptoSwift": [

    ]
  }
}
