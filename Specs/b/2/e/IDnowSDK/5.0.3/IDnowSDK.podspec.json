{
  "name": "IDnowSDK",
  "version": "5.0.3",
  "summary": "Provides functionality for instantiating a video and/or photo identification.",
  "license": {
    "type": "Commercial",
    "file": "idnow-sdk/LICENSE.md"
  },
  "authors": {
    "Armin Bauer": "armin.bauer@idnow.de"
  },
  "source": {
    "http": "https://downloads.idnow.de/ios/idnow-ios-5.0.3.tar.gz"
  },
  "homepage": "https://www.idnow.de",
  "platforms": {
    "ios": "9.0"
  },
  "swift_versions": [
    "4.0",
    "4.2",
    "5.0",
    "5.1",
    "5.2",
    "5.3"
  ],
  "requires_arc": true,
  "source_files": [
    "idnow-sdk/include/*.h",
    "idnow-sdk/resources/*.swift"
  ],
  "resources": [
    "idnow-sdk/resources/*.jpg",
    "idnow-sdk/resources/*.bundle",
    "idnow-sdk/resources/*.mp4"
  ],
  "vendored_libraries": "idnow-sdk/libidnow-sdk-universal.a",
  "preserve_paths": "**/*.*",
  "module_map": "idnow-sdk/include/module.modulemap",
  "frameworks": [
    "StoreKit",
    "Accelerate",
    "WebKit",
    "AudioToolbox",
    "AVFoundation",
    "CoreMedia",
    "GLKit",
    "OpenGLES",
    "SystemConfiguration",
    "UserNotifications",
    "VideoToolbox"
  ],
  "libraries": "z",
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "$(PODS_ROOT)/IDnowSDK/idnow-sdk/include"
  },
  "static_framework": true,
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "dependencies": {
    "Masonry": [
      "~> 1.1.0"
    ],
    "SocketRocket": [
      "~> 0.5.1"
    ],
    "libPhoneNumber-iOS": [
      "~> 0.9"
    ],
    "FLAnimatedImage": [
      "~> 1.0"
    ],
    "AFNetworking": [
      "~> 4.0"
    ]
  },
  "swift_version": "5.3"
}
