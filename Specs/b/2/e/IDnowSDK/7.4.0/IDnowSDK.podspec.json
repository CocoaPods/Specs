{
  "name": "IDnowSDK",
  "version": "7.4.0",
  "summary": "Provides functionality for instantiating a video and/or photo identification.",
  "license": {
    "type": "Commercial",
    "file": "LICENSE.md"
  },
  "authors": {
    "Armin Bauer": "armin.bauer@idnow.de"
  },
  "source": {
    "http": "https://github.com/idnow/de.idnow.ios/releases/download/v7.4.0/idnow-ios-7.4.0.tar.gz"
  },
  "homepage": "https://www.idnow.de",
  "platforms": {
    "ios": "11.0"
  },
  "swift_versions": [
    "4.0",
    "4.2",
    "5.0",
    "5.1",
    "5.2",
    "5.3"
  ],
  "requires_arc": true,
  "source_files": [
    "include/*.h",
    "resources/*.swift"
  ],
  "resources": [
    "resources/*.jpg",
    "resources/*.bundle"
  ],
  "vendored_libraries": "libidnow-sdk-universal.a",
  "preserve_paths": "**/*.*",
  "module_map": "include/module.modulemap",
  "frameworks": [
    "StoreKit",
    "Accelerate",
    "WebKit",
    "AudioToolbox",
    "AVFoundation",
    "CoreMedia",
    "GLKit",
    "OpenGLES",
    "SystemConfiguration",
    "UserNotifications",
    "VideoToolbox",
    "CoreTelephony"
  ],
  "libraries": "z",
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "$(PODS_ROOT)/IDnowSDK/include"
  },
  "static_framework": true,
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "swift_version": "5.3"
}
