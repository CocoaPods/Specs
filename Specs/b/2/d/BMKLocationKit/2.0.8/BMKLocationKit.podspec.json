{
  "name": "BMKLocationKit",
  "version": "2.0.8",
  "summary": "Baidu IOS Location SDK",
  "description": "百度地图iOS定位SDK，是百度地图开放平台面向广大iOS端移动应用开发者全新推出的一款定位服务产品。开发者在自己的iOS移动应用中集成定位SDK，可轻松实现获取当前位置信息的功能。百度地图iOS定位SDK以iOS系统定位能力为基础，在此基础之上，面向广大开发者提供了更多丰富功能，包括：地址解析、位置描述、移动热点识别、地理围栏、国内外位置判断等功能。",
  "homepage": "http://lbsyun.baidu.com/index.php?title=ios-locsdk",
  "license": {
    "type": "Copyright",
    "text": "Copyright (c) 2020 BaiduLBS"
  },
  "authors": {
    "baidu": "loc-rd@baidu.com"
  },
  "platforms": {
    "ios": "7.0"
  },
  "source": {
    "http": "https://lbsyun-baidu.cdn.bcebos.com/iossdk/location/2.0.8/BMKLocationKitV2.0.8.zip"
  },
  "source_files": "BMKLocationKit/*.framework/Headers/*.h",
  "public_header_files": "BMKLocationKit/*.framework/Headers/*.h",
  "vendored_frameworks": "BMKLocationKit/*.framework",
  "frameworks": [
    "CoreLocation",
    "Foundation",
    "UIKit",
    "SystemConfiguration",
    "AdSupport",
    "Security",
    "CoreTelephony"
  ],
  "libraries": [
    "sqlite3.0",
    "c++"
  ],
  "requires_arc": true,
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  }
}
