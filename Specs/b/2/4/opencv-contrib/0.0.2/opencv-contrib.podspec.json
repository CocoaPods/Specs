{
  "name": "opencv-contrib",
  "version": "0.0.2",
  "summary": "c++ opencv2 framework for ios",
  "description": "This pod serves as just includer of c++ opencv2 library, but it doesn't contain any swift or obj-c code.",
  "homepage": "https://gitlab.com/martingeorgiu/opencv-contrib-ios",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "martingeorgiu": "martin.georgiu@gmail.com"
  },
  "source": {
    "git": "https://gitlab.com/martingeorgiu/opencv-contrib-ios",
    "tag": "0.0.2"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "platforms": {
    "ios": "9.0"
  },
  "swift_versions": "4.0",
  "preserve_paths": "opencv2.framework",
  "vendored_frameworks": "opencv2.framework",
  "libraries": "c++",
  "xcconfig": {
    "OTHER_LDFLAGS": "-framework opencv2"
  },
  "source_files": "**/*{.h,.hpp}",
  "public_header_files": "**/*{.h,.hpp}",
  "swift_version": "4.0"
}
