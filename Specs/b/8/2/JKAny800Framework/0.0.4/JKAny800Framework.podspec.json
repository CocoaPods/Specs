    "file": "LICENSE"
  },
  "authors": {
    "ilucklyzhengwen@163.com": "jerry.gu@9client.com"
  },
  "source": {
    "git": "https://github.com/jerryzhengwen/JKAny800Framework.git",
    "tag": "0.0.4"
  },
  "platforms": {
    "ios": "9.0"
  },
  "libraries": [
    "resolv",
    "xml2",
    "icucore"
  ],
  "public_header_files": "JKAny800Framework/**/Classes/**/*.h",
  "source_files": [
    "JKAny800Framework/**/Classes/**/*.{h,m}",
    "JKAny800Framework/**/UI/**/*.{h,m}",
    "JKAny800Framework/**/Classes/**/*.h"
  ],
  "ios": {
