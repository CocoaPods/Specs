  },
  "resources": "JKAny800Framework/**/UIKit/**/{JKDialogeModel.xcdatamodeld,JKFace.plist,JKIMImage.bundle,style.css}",
  "xcconfig": {
    "VALID_ARCHS": "arm64 x86_64 armv7 i386"
  },
  "frameworks": [
    "UIKit",
    "MapKit"
  ],
  "requires_arc": [
    "JKAny800Framework/**/Classes/*.{h,m}"
