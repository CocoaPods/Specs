{
  "name": "AppsFlyerRPC",
  "version": "1.0.0",
  "summary": "AppsFlyer iOS SDK RPC Interface",
  "description": "AppsFlyerRPC provides a unified JSON-based RPC interface for the AppsFlyer iOS SDK,\ndesigned to simplify integration for mobile plugins (React Native, Flutter, Unity, Cordova).\nIt replaces per-plugin native bindings with a consistent, maintainable communication layer.",
  "homepage": "https://www.appsflyer.com",
  "license": {
    "type": "Proprietary",
    "text": "Copyright 2024 AppsFlyer Ltd. All rights reserved."
  },
  "authors": {
    "AppsFlyer": "support@appsflyer.com"
  },
  "source": {
    "git": "https://github.com/AppsFlyerSDK/appsflyer-apple-rpc.git",
    "tag": "1.0.0"
  },
  "requires_arc": true,
  "platforms": {
    "ios": "13.0"
  },
  "swift_versions": "5.9",
  "default_subspecs": "Main",
  "subspecs": [
    {
      "name": "Main",
      "ios": {
        "preserve_paths": "AppsFlyerRPC.xcframework",
        "vendored_frameworks": "AppsFlyerRPC.xcframework",
        "resource_bundles": {
          "AppsFlyerRPC_Privacy": [
            "Resources/PrivacyInfo.xcprivacy"
          ]
        }
      },
      "dependencies": {
        "AppsFlyerFramework": [
          "6.17.8"
        ]
      }
    }
  ],
  "swift_version": "5.9"
}
