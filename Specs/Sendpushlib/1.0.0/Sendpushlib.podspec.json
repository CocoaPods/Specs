{
  "name": "Sendpushlib",
  "version": "1.0.0",
  "summary": "Library for use with Sendpush",
  "description": "This library needs to be included in your iOS app to allow sendpush functionality.",
  "homepage": "http://sendpush.co/",
  "license": "MIT",
  "authors": {
    "Bob Axford": "bob@sendpush.co",
    "Timothy Marks": "tim@sendpush.co"
  },
  "source": {
    "git": "https://github.com/sendpush/sendpush-ios-lib.git",
    "tag": "1.0.0"
  },
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "source_files": "Pod/Classes/**/*",
  "resource_bundles": {
    "Sendpushlib": [
      "Pod/Assets/*.png"
    ]
  },
  "dependencies": {
    "JSONWebToken": [
      "1.4.0"
    ]
  }
}
