{
  "name": "CalcLib",
  "version": "1.0",
  "summary": "Example iOS Static Library - Calculator. ",
  "description": "                  This iOS Static library is used to explain how to distribute iOS Static library via cocoapods\n",
  "homepage": "https://github.com/bharath2020/CalcLibrary/blob/master/README.md",
  "license": {
    "type": "BSD",
    "file": "LICENSE"
  },
  "authors": {
    "Bharath Booshan": "bharath2020@gmail.com"
  },
  "social_media_url": "http://twitter.com/bharath2020",
  "platforms": {
    "ios": "6.0"
  },
  "source": {
    "git": "https://github.com/bharath2020/CalcLibrary.git",
    "tag": "1.0"
  },
  "source_files": "include/*.h",
  "public_header_files": "include/*.h",
  "preserve_paths": "libCalculator.a",
  "ios": {
    "vendored_libraries": "libCalculator.a"
  },
  "libraries": [
    "stdc++",
    "Calculator"
  ],
  "requires_arc": true
}
