{
  "name": "DLSFTPClient",
  "version": "1.0.4",
  "summary": "DLSFTPClient is an SFTP Client library for iOS, using libssh2",
  "homepage": "https://github.com/dleehr/DLSFTPClient",
  "social_media_url": "https://twitter.com/leehro",
  "license": {
    "type": "BSD",
    "file": "LICENSE"
  },
  "authors": {
    "Dan Leehr": "dan@hammockdistrict.com"
  },
  "source": {
    "git": "https://github.com/dleehr/DLSFTPClient.git",
    "tag": "1.0.4"
  },
  "platforms": {
    "ios": "6.0"
  },
  "requires_arc": true,
  "source_files": [
    "DLSFTPClient/Classes/*.{h,m}",
    "DLSFTPClient/Libraries/include/**/*.h"
