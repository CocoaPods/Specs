{
  "name": "HiPay",
  "version": "0.7.0",
  "summary": "Payment util with swift",
  "description": "移动应用支付接口。\n使移动支付更简单。\n支持微信支付，银联支付，支付宝支付",
  "homepage": "https://github.com/wuyingminhui/HiPay",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "jasonwoo": "wuyingminhui@gmail.com"
  },
  "source": {
    "git": "https://github.com/wuyingminhui/HiPay.git",
    "tag": "0.7.0"
  },
  "requires_arc": true,
  "default_subspecs": [
    "Core",
    "AliPay",
    "WxPay",
    "UnionPay"
  ],
  "platforms": {
    "ios": "8.0"
  },
  "subspecs": [
    {
      "name": "Core",
      "source_files": "HiPay/Classes/HiPayCore",
      "ios": {
        "libraries": [
          "c++",
          "z"
        ]
      },
      "frameworks": [
        "CoreTelephony",
        "SystemConfiguration",
        "CFNetwork"
      ],
      "xcconfig": {
        "OTHER_LDFLAGS": "-ObjC"
      }
    },
    {
      "name": "AlipayUtil",
      "source_files": [
        "HiPay/Classes/AlipaySDk/Util/*.h",
        "HiPay/Classes/AlipaySDk/Util/openssl/*.h"
      ],
      "public_header_files": "HiPay/Classes/AlipaySDk/Util/*.h",
      "header_dir": "openssl"
    },
    {
      "name": "AliPay",
      "dependencies": {
        "HiPay/AlipayUtil": [

        ],
        "HiPay/Core": [

        ]
      },
      "source_files": [
        "HiPay/Classes/AlipaySDk/*.swift",
        "HiPay/Classes/AlipaySDk/AlipaySDK.framework/Headers/*"
      ],
      "vendored_frameworks": "HiPay/Classes/AlipaySDk/AlipaySDK.framework",
      "vendored_libraries": "HiPay/Classes/AlipaySDk/*.a",
      "resources": "HiPay/Classes/AlipaySDk/AlipaySDK.bundle",
      "public_header_files": "HiPay/Classes/AlipaySDk/AlipaySDK.framework/Headers/**/*.h",
      "frameworks": [
        "CoreMotion",
        "CoreGraphics",
        "CoreText",
        "QuartzCore"
      ]
    },
    {
      "name": "WxPay",
      "source_files": "HiPay/Classes/WxSDK",
      "vendored_libraries": "HiPay/Classes/WxSDK/*.a",
      "ios": {
        "libraries": "sqlite3.0"
      },
      "dependencies": {
        "HiPay/Core": [

        ]
      }
    },
    {
      "name": "UnionPay",
      "source_files": "HiPay/Classes/UPPaySDK",
      "vendored_libraries": "HiPay/Classes/UPPaySDK/*.a",
      "public_header_files": "HiPay/Classes/UPPaySDK/*.h",
      "dependencies": {
        "HiPay/Core": [

        ]
      }
    }
  ]
}
