{
  "name": "JGSourceBase",
  "version": "0.1.0",
  "summary": "JGSourceCode 通用定义、功能模块",
  "description": "JGSourceCode 通用定义、功能模块\n\n功能包括：\n1、通用日志控制模式\n2、通用重用标示快速定义\n3、Block循环引用常用定义weak、strong快捷处理",
  "homepage": "https://github.com/dengni8023/JGSourceBase",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "source": {
    "git": "https://github.com/dengni8023/JGSourceBase.git",
    "tag": "0.1.0"
  },
  "authors": [
    {
      "等你8023": "945835664@qq.com"
    },
    {
      "git": "https://github.com/dengni8023/JGSourceBase.git",
      "tag": "0.1.0"
    }
  ],
  "platforms": {
    "ios": "8.0"
  },
  "source_files": "JGSourceBase/*.{h,m}",
  "public_header_files": "JGSourceBase/*.h",
  "requires_arc": true,
  "default_subspecs": "Common",
  "subspecs": [
    {
      "name": "Common",
      "source_files": "JGSourceBase/Common/*.{h,m}",
      "public_header_files": "JGSourceBase/Common/*.h",
      "xcconfig": {
        "OTHER_LDFLAGS": "$(inherited) -ObjC"
      }
    },
    {
      "name": "NSObject",
      "source_files": "JGSourceBase/NSObject/*.{h,m}",
      "public_header_files": "JGSourceBase/NSObject/*.h",
      "libraries": "objc",
      "xcconfig": {
        "OTHER_LDFLAGS": "$(inherited) -ObjC",
        "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited) JGSC_NSObject"
      }
    }
  ]
}
