        ]
      }
    },
    {
      "name": "Menu",
      "source_files": [
        "AlertTransition/Transitions/MenuTransition/*.swift"
      ],
      "dependencies": {
        "AlertTransition/Core": [

        ]
      }
    },
    {
      "name": "Trolley",
      "source_files": [
        "AlertTransition/Transitions/TrolleyTransition.swift"
      ],
      "dependencies": {
        "AlertTransition/Core": [

        ]
      }
    }
  ]
}
