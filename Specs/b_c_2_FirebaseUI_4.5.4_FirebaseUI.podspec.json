{
  "name": "FirebaseUI",
  "version": "4.5.4",
  "summary": "UI binding libraries for Firebase.",
  "homepage": "https://github.com/firebase/FirebaseUI-iOS",
  "license": {
    "type": "Apache 2.0",
    "file": "FirebaseUIFrameworks/LICENSE"
  },
  "source": {
    "http": "https://github.com/firebase/FirebaseUI-iOS/releases/download/v4.5.4/FirebaseUIFrameworks.zip"
  },
  "authors": "Firebase",
  "platforms": {
    "ios": "9.0"
  },
  "ios": {
    "frameworks": "UIKit",
    "vendored_frameworks": "FirebaseUIFrameworks/*/Frameworks/*.framework"
  },
  "requires_arc": true,
  "default_subspecs": "All",
  "subspecs": [
    {
      "name": "All",
      "platforms": {
        "ios": "9.0"
      },
      "dependencies": {
        "FirebaseUI/Database": [

        ],
        "FirebaseUI/Storage": [

        ],
        "FirebaseUI/Auth": [

        ],
        "FirebaseUI/Facebook": [

        ],
        "FirebaseUI/Google": [

        ],
        "FirebaseUI/Phone": [

        ],
        "FirebaseUI/Twitter": [

        ],
        "FirebaseUI/Firestore": [

        ]
      }
    },
    {
      "name": "Database",
      "platforms": {
        "ios": "8.0"
      },
      "vendored_frameworks": [
        "FirebaseUIFrameworks/FirebaseDatabaseUI/Frameworks/FirebaseDatabaseUI.framework"
      ],
      "dependencies": {
        "Firebase/Database": [
          "~> 4.0"
        ]
      }
    },
    {
      "name": "Firestore",
      "platforms": {
        "ios": "8.0"
      },
      "vendored_frameworks": [
        "FirebaseUIFrameworks/FirebaseFirestoreUI/Frameworks/FirebaseFirestoreUI.framework"
      ],
      "dependencies": {
        "Firebase/Firestore": [

        ]
