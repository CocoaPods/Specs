        ],
        "FBSDKLoginKit": [
          "~> 4.0"
        ]
      },
      "resource_bundles": {
        "FirebaseFacebookAuthUI": [
          "FirebaseUIFrameworks/FirebaseFacebookAuthUI/Frameworks/FirebaseFacebookAuthUI.framework/*.nib",
