{
  "name": "SZFontAwesomeKit",
  "version": "2.2.2",
  "summary": "Icon font library for iOS. Currently supports Font-Awesome, Foundation icons, Zocial, and ionicons",
  "homepage": "https://github.com/jaja-digitalspaceexplorer/SZFontAwesomeKit.git",
  "license": "MIT",
  "authors": {
    "Pride Chung": "pride.chung@gmail.com"
  },
  "source": {
    "git": "https://github.com/jaja-digitalspaceexplorer/SZFontAwesomeKit.git",
    "tag": "2.2.2"
  },
  "platforms": {
    "ios": "6.0"
  },
  "ios": {
    "frameworks": [
      "UIKit",
      "CoreText"
    ]
  },
  "source_files": "SZFontAwesomeKit/FontAwesomeKit.h",
  "requires_arc": true,
  "subspecs": [
    {
      "name": "Core",
      "source_files": "SZFontAwesomeKit/FAKIcon.{h,m}"
    },
    {
      "name": "FontAwesome",
      "dependencies": {
        "SZFontAwesomeKit/Core": [

        ]
      },
      "source_files": "SZFontAwesomeKit/FAKFontAwesome.{h,m}",
      "resources": "SZFontAwesomeKit/FontAwesome.otf"
    },
    {
      "name": "FoundationIcons",
      "dependencies": {
        "SZFontAwesomeKit/Core": [

        ]
      },
      "source_files": "SZFontAwesomeKit/FAKFoundationIcons.{h,m}",
      "resources": "SZFontAwesomeKit/foundation-icons.ttf"
    },
    {
      "name": "Zocial",
      "dependencies": {
        "SZFontAwesomeKit/Core": [

        ]
      },
      "source_files": "SZFontAwesomeKit/FAKZocial.{h,m}",
      "resources": "SZFontAwesomeKit/zocial-regular-webfont.ttf"
    },
    {
      "name": "IonIcons",
      "dependencies": {
        "SZFontAwesomeKit/Core": [

        ]
      },
      "source_files": "SZFontAwesomeKit/FAKIonIcons.{h,m}",
      "resources": "SZFontAwesomeKit/ionicons.ttf"
    },
    {
      "name": "Octicons",
      "dependencies": {
        "SZFontAwesomeKit/Core": [

        ]
      },
      "source_files": "SZFontAwesomeKit/FAKOcticons.{h,m}",
      "resources": "SZFontAwesomeKit/octicons.ttf"
    },
    {
      "name": "Material",
      "dependencies": {
        "SZFontAwesomeKit/Core": [

        ]
      },
      "source_files": "SZFontAwesomeKit/FAKMaterialIcons.{h,m}",
      "resources": "SZFontAwesomeKit/Material-Design-Iconic-Font.ttf"
    }
  ]
}
