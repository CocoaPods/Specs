{
  "name": "RxAlgebra",
  "version": "0.1.0",
  "summary": "Algebraic expression support for Rx style programming",
  "description": "Algebraic expression support for Rx style programming. In an attempt to make the code\nmore concise (1) and readable (2). This library provides operators enabling adopter to wield the power of Algebra.",
  "homepage": "https://github.com/myste1tainn/RxAlgebra",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "myste1tainn": "a.keereena@gmail.com"
  },
  "source": {
    "git": "https://github.com/myste1tainn/RxAlgebra.git",
    "tag": "0.1.0"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source_files": "RxAlgebra/Core/**/*",
  "swift_version": "4.2",
  "default_subspecs": [
    "RxSwift"
  ],
  "subspecs": [
    {
      "name": "RxSwift",
      "source_files": [
        "RxAlgebra/Core/**/*",
        "RxAlgebra/RxSwift/**/*"
      ],
      "dependencies": {
        "RxSwift": [
          "~> 4.3.0"
        ],
        "SwiftExpansion": [
          "~> 0.1.0"
        ],
        "RxSwiftExpansion": [
          "~> 0.1.0"
        ]
      }
    },
    {
      "name": "RxCocoa",
      "source_files": [
        "RxAlgebra/Core/**/*",
        "RxAlgebra/RxSwift/**/*",
        "RxAlgebra/RxCocoa/**/*"
      ],
      "dependencies": {
        "RxSwift": [
          "~> 4.3.0"
        ],
        "RxCocoa": [
          "~> 4.3.0"
        ],
        "SwiftExpansion": [
          "~> 0.1.0"
        ],
        "RxSwiftExpansion": [
          "~> 0.1.0"
        ]
      }
    }
  ]
}
