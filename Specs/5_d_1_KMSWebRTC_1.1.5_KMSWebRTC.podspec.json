  "ios": {
    "vendored_frameworks": "KMSWebRTC/vendor/WebRTC/WebRTC.framework"
  },
  "xcconfig": {
    "FRAMEWORK_SEARCH_PATHS": "\"${PODS_ROOT}/KMSWebRTC/KMSWebRTC/vendor/WebRTC\""
  },
  "dependencies": {
    "KMSClient": [
      "1.1.2"
    ],
    "ReactiveObjC": [
      "3.0.0"
    ]
  }
}
