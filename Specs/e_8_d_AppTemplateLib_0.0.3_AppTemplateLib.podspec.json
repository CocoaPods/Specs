      "dependencies": {
        "AppTemplateLib/Utility": [

        ]
      },
      "pod_target_xcconfig": {
        "GCC_PREPROCESSOR_DEFINITIONS": "ATAppTemplateLibModel"
      }
    },
    {
      "name": "UI",
      "source_files": "AppTemplateLib/UI/*.{h,m}",
      "dependencies": {
        "AppTemplateLib/Utility": [

        ]
      }
    }
  ]
