{
  "name": "YouboraFreewheelAdapter",
  "version": "6.0.0-beta",
  "summary": "Adapter to use YouboraLib on Freewheel ads",
  "description": "<<-DESC\n                      YouboraFreewheelAdapter is an AdsAdapter for Freewheel ads\n                     DESC",
  "homepage": "http://developer.nicepeopleatwork.com/",
  "license": {
    "type": "MIT",
    "file": "LICENSE.md"
  },
  "authors": {
    "Nice People at Work": "support@nicepeopleatwork.com"
  },
  "platforms": {
    "ios": "8.0",
    "tvos": "9.0"
  },
  "source": {
    "git": "https://bitbucket.org/npaw/freewheel-adapter-ios.git",
    "tag": "6.0.0-beta"
  },
  "source_files": "YouboraFreewheelAdapter/**/*.{h,m}",
  "public_header_files": "YouboraFreewheelAdapter/**/*.h",
  "requires_arc": true,
  "pod_target_xcconfig": {
    "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited) YOUBORAFREEWHEELADAPTER_VERSION=6.0.0-beta"
