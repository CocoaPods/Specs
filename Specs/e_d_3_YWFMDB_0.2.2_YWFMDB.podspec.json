    {
      "name": "SQLCipher",
      "dependencies": {
        "FMDB": [

        ],
        "SQLCipher": [

        ]
      },
      "source_files": "YWFMDB/*",
      "xcconfig": {
        "OTHER_CFLAGS": "$(inherited) -DSQLITE_HAS_CODEC -DHAVE_USLEEP=1",
        "HEADER_SEARCH_PATHS": "SQLCipher"
      }
    }
  ]
