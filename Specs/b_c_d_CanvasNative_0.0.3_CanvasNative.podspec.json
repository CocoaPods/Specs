{
  "name": "CanvasNative",
  "version": "0.0.3",
  "summary": "A Canvas library",
  "homepage": "https://github.com/triniwiz/canvas-native-ios",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Osei Fortune": "fortune.osei@yahoo.com"
  },
  "platforms": {
    "ios": "11.0"
  },
  "source": {
    "git": "https://github.com/triniwiz/canvas-native-ios.git",
    "tag": "0.0.3"
  },
  "source_files": "CanvasNative/**/*.{swift,m,h,modulemap}",
  "preserve_paths": [
    "CanvasNative/include/*.h",
    "CanvasNative/include/**/*.h",
    "CanvasNative/libs/*.a"
  ],
  "pod_target_xcconfig": {
    "USER_HEADER_SEARCH_PATHS": "\"${PODS_ROOT}/CanvasNative/include\"",
    "LIBRARY_SEARCH_PATHS": "\"${PODS_ROOT}/CanvasNative/libs\"",
    "HEADER_SEARCH_PATHS": "\"${PODS_ROOT}/CanvasNative/include\"",
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES"
  },
  "swift_versions": [
    "4.0",
    "4.2",
    "5.0",
    "5.0"
  ],
  "vendored_libraries": "CanvasNative/libs/*.a",
  "public_header_files": "CanvasNative/include/*.h",
  "libraries": "c++",
  "swift_version": "5.0"
}
