{
  "name": "ReactantUI",
  "version": "0.4.0",
  "summary": "Reactant extension for UI declaration in XML",
  "description": "Reactant UI is an extension for Reactant allowing you to declare views and layout using XML. Don't worry, there's no runtime overhead, as all those declarations are precompiled into Swift. Reactant then uses the generated code to create your UI.",
  "homepage": "http://reactant.tech",
  "license": "MIT",
  "authors": {
    "Tadeas Kriz": "tadeas@brightify.org",
    "Matous Hybl": "matous@brightify.org",
    "Filip Dolnik": "filip@brightify.org"
  },
  "source": {
    "git": "https://github.com/Brightify/ReactantUI.git",
    "tag": "0.4.0"
  },
  "social_media_url": "https://twitter.com/BrightifyOrg",
  "requires_arc": true,
  "platforms": {
    "ios": "10.0",
    "tvos": "10.0"
  },
  "swift_versions": "5.1",
  "source_files": [
    "Sources/Common/**/*.swift",
    "Sources/Runtime/**/*.swift"
  ],
  "preserve_paths": [
    "Sources/**/*",
    "Package.swift",
    "Package.resolved"
  ],
  "prepare_command": "curl -Lo reactant-ui https://github.com/Brightify/ReactantUI/releases/download/0.4.0/reactant-ui\nchmod +x reactant-ui",
  "swift_version": "5.1"
}
