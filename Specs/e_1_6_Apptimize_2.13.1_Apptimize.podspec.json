{
  "name": "Apptimize",
  "version": "2.13.1",
  "summary": "Apptimize Library for AB testing on iOS.",
  "description": "Anything you can program, you can test with Apptimize.",
  "homepage": "http://apptimize.com/",
  "authors": "Apptimize, Inc.",
  "license": {
    "type": "commercial",
    "text": "See https://apptimize.com/admin/terms"
  },
  "source": {
    "http": "http://staging-sdk.apptimize.com/ios/apptimize-ios-2.13.1.zip"
  },
  "platforms": {
    "ios": "5.1"
  },
  "prepare_command": "mv \"apptimize-ios-2.13.1\"/Apptimize.framework ./Apptimize.framework",
  "source_files": "**/*.h",
  "frameworks": [
    "Apptimize",
    "Security",
    "Foundation",
    "CFNetwork",
    "UIKit",
    "CoreGraphics"
  ],
  "xcconfig": {
    "OTHER_LDFLAGS": "-ObjC",
    "FRAMEWORK_SEARCH_PATHS": "\"$(PODS_ROOT)/Apptimize\""
