{
  "name": "AVOSCloud-OSX",
  "version": "3.1.6.3",
  "summary": "LeanCloud OS X SDK for Mac platform.",
  "homepage": "https://leancloud.cn",
  "documentation_url": "https://leancloud.cn/docs/api/iOS/index.html",
  "license": {
    "type": "Commercial",
    "text": "Copyright 2015 LeanCloud, Inc. See https://leancloud.cn/terms.html"
  },
  "authors": {
    "LeanCloud": "support@leancloud.cn"
  },
  "platforms": {
    "osx": 10.8
  },
  "requires_arc": true,
  "source": {
    "http": "http://download.leancloud.cn/sdk/OSX/release-v3.1.6.3/AVOSCloud.framework.zip"
  },
  "preserve_paths": "OSX/release-v3.1.6.3/AVOSCloud.framework",
  "public_header_files": "OSX/release-v3.1.6.3/AVOSCloud.framework/**/*.h",
  "vendored_frameworks": "OSX/release-v3.1.6.3/AVOSCloud.framework",
  "resources": "OSX/release-v3.1.6.3/AVOSCloud.framework",
  "frameworks": [
    "CoreServices",
    "SystemConfiguration",
    "CoreLocation"
  ],
  "libraries": [
    "icucore",
    "sqlite3",
    "z",
    "curl"
  ],
  "xcconfig": {
    "OTHER_LDFLAGS": "$(inherited) -ObjC",
    "FRAMEWORK_SEARCH_PATHS": "\"${PODS_ROOT}/AVOSCloud-OSX/**\"",
    "LD_RUNPATH_SEARCH_PATHS": "@loader_path/../Frameworks"
  }
}
