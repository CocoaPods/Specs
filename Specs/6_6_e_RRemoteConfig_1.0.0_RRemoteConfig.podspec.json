{
  "name": "RRemoteConfig",
  "version": "1.0.0",
  "authors": "Rakuten Ecosystem Mobile",
  "summary": "Rakuten's Remote Config module.",
  "homepage": "https://github.com/rakutentech/ios-remote-config",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "source": {
    "git": "https://github.com/rakutentech/ios-remote-config.git",
    "tag": "1.0.0"
  },
  "platforms": {
    "ios": "10.0"
  },
  "swift_versions": "5.0",
  "requires_arc": true,
  "documentation_url": "https://rakutentech.github.io/ios-remote-config/",
  "pod_target_xcconfig": {
    "CLANG_ENABLE_MODULES": "YES",
    "CLANG_MODULES_AUTOLINK": "YES",
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES",
    "GCC_C_LANGUAGE_STANDARD": "gnu11",
    "OTHER_CFLAGS": "'-DRPT_SDK_VERSION=1.0.0'"
  },
  "user_target_xcconfig": {
    "CLANG_ENABLE_MODULES": "YES",
    "CLANG_MODULES_AUTOLINK": "YES",
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES"
  },
  "weak_frameworks": [
    "Foundation"
  ],
  "source_files": "RRemoteConfig/**/*.{Swift,m}",
  "swift_version": "5.0"
}
