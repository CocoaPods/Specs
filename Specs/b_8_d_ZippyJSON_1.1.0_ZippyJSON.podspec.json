  "homepage": "https://github.com/michaeleisel/ZippyJSON",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "michaeleisel": "michael.eisel@gmail.com"
  },
  "source": {
    "git": "https://github.com/michaeleisel/ZippyJSON.git",
    "tag": "1.1.0"
  },
  "platforms": {
    "ios": "11.0",
    "osx": "10.13"
  },
  "source_files": "Sources/**/*.{h,hh,mm,m,c,cpp,swift}",
  "dependencies": {
    "ZippyJSONCFamily": [
      "~> 1.1.0"
    ],
    "JJLISO8601DateFormatter": [
      "~> 0.1.2"
    ]
  },
  "swift_versions": "5.0",
  "testspecs": [
    {
