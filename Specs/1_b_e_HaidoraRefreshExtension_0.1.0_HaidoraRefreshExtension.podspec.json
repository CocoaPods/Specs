  "homepage": "https://github.com/Haidora/HaidoraRefreshExtension",
  "license": "MIT",
  "authors": {
    "mrdaios": "mrdaios@gmail.com"
  },
  "source": {
    "git": "https://github.com/Haidora/HaidoraRefreshExtension.git",
