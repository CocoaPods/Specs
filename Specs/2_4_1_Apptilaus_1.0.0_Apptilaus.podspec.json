  "homepage": "https://github.com/Apptilaus/",
  "social_media_url": "https://facebook.com/apptilaus",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Apptilaus": "ios-sdk@apptilaus.com"
  },
  "source": {
    "git": "https://github.com/Apptilaus/ios_subscriptions_sdk.git",
