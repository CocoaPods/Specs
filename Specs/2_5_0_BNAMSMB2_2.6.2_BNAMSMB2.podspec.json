{
  "name": "BNAMSMB2",
  "version": "2.6.2",
  "summary": "Swift framework to connect SMB2/3 shares",
  "description": "This is small Swift library for iOS which wraps libsmb2 and allows to connect a SMB2/3 share and do file operation.",
  "homepage": "https://github.com/amosavian/AMSMB2",
  "license": {
    "type": "LGPL-2.1",
    "file": "LICENSE"
  },
  "authors": {
    "Amir Abbas Mousavian": "a.mosavian@gmail.com"
  },
  "social_media_url": "https://twitter.com/amosavian",
  "swift_versions": "5.0",
  "platforms": {
    "ios": "9.0",
    "osx": "10.11",
    "tvos": "9.0"
  },
  "source": {
    "git": "https://github.com/kasimok/BNAMSMB2.git",
    "tag": "2.6.2"
  },
  "ios": {
    "vendored_libraries": "libsmb2/lib/libsmb2-ios.a"
  },
  "osx": {
    "vendored_libraries": "libsmb2/lib/libsmb2-macos.a"
  },
  "tvos": {
    "vendored_libraries": "libsmb2/lib/libsmb2-tvos.a"
  },
  "watchos": {
    "vendored_libraries": "libsmb2/lib/libsmb2-watchos.a"
  },
  "source_files": "AMSMB2/**/*.swift",
  "pod_target_xcconfig": {
    "LIBRARY_SEARCH_PATHS": "$(PODS_TARGET_SRCROOT)/libsmb2/lib",
    "SWIFT_INCLUDE_PATHS": "${PODS_TARGET_SRCROOT}/libsmb2/**"
  },
  "xcconfig": {
    "LIBRARY_SEARCH_PATHS": "$(PODS_ROOT)/AMSMB2/libsmb2/lib",
    "SWIFT_INCLUDE_PATHS": "${PODS_ROOT}/AMSMB2/libsmb2/**"
  },
  "preserve_paths": "libsmb2/include/**",
  "frameworks": "Foundation",
  "swift_version": "5.0"
}
