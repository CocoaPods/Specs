  },
  "documentation_url": "https://docs.megacool.co",
  "platforms": {
    "ios": "7.0"
  },
  "xcconfig": {
    "FRAMEWORK_SEARCH_PATHS": "\"$(PODS_ROOT)/Megacool/**\"",
