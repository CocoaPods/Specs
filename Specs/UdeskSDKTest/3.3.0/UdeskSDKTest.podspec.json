{
  "name": "UdeskSDKTest",
  "version": "3.3.0",
  "license": "MIT",
  "summary": "Udesk SDK for iOS",
  "homepage": "https://github.com/xushichen/UdeskSDKTest",
  "authors": {
    "xuchen ": "xuc@udesk.cn"
  },
  "source": {
    "git": "https://github.com/xushichen/UdeskSDKTest.git",
    "tag": "3.3.0"
  },
  "platforms": {
    "ios": "6.0"
  },
  "requires_arc": true,
  "vendored_libraries": "UdeskSDK/SDK/*.a",
  "source_files": [
    "UdeskSDK/SDK/UDManager.h",
    "UdeskSDK/UDChatMessage/**/*.{h,m}"
  ],
  "frameworks": [
    "AVFoundation",
    "CoreTelephony",
    "SystemConfiguration",
    "MobileCoreServices"
  ],
  "libraries": [
    "z",
    "xml2",
    "resolv",
    "sqlite3"
  ],
  "resources": "UdeskSDK/Resource/UdeskBundle.bundle",
  "xcconfig": {
    "OTHER_LDFLAGS": "-lxml2",
    "HEADER_SEARCH_PATHS": "$(SDKROOT)/usr/include/libxml2"
  }
}
