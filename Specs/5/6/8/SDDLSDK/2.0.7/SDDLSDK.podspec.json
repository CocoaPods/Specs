{
  "name": "SDDLSDK",
  "version": "2.0.7",
  "summary": "iOS SDK for SDDL deep links integration",
  "description": "SDDLSDK helps to integrate deep links from SDDL easily in iOS apps.",
  "homepage": "https://github.com/nonanerz/ssdl-ios-sdk",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Eduard": "bulavaeduard@gmail.com"
  },
  "platforms": {
    "ios": "12.0"
  },
  "source": {
    "git": "https://github.com/nonanerz/ssdl-ios-sdk.git",
    "tag": "2.0.7"
  },
  "source_files": "SDDLSDK/**/*.{h,swift}",
  "module_map": "SDDLSDK/module.modulemap",
  "exclude_files": [
    "SDDLSDK/SDDLSDK.docc",
    ".idea",
    ".git",
    ".DS_Store"
  ],
  "frameworks": [
    "Foundation",
    "UIKit"
  ],
  "requires_arc": true,
  "static_framework": true,
  "swift_versions": [
    "5.0"
  ],
  "pod_target_xcconfig": {
    "BUILD_LIBRARY_FOR_DISTRIBUTION": "YES",
    "DEFINES_MODULE": "YES"
  },
  "swift_version": "5.0"
}
