    "tvos": "9.0",
    "osx": "10.10",
    "watchos": "2.0"
  },
  "source": {
    "git": "https://github.com/Meniny/Imagery.git",
    "tag": "1.2.0"
  },
  "source_files": [
    "Imagery/*.swift",
