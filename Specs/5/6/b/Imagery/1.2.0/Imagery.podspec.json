{
  "name": "Imagery",
  "version": "1.2.0",
  "summary": "A lightweight library for downloading and cacheing image from the web.",
  "homepage": "https://meniny.cn/",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Elias Abel": "meniny@qq.com"
