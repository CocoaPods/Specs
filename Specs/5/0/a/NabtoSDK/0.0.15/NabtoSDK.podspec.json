{
  "name": "NabtoSDK",
  "version": "0.0.15",
  "summary": "Native C-based Nabto Client SDK to securely access devices using the Nabto P2P communication platform.",
  "description": "This is the low level native C Nabto Client SDK. Normally you would use the iOS client api that provides better abstractions.",
  "homepage": "https://www.nabto.com",
  "license": {
    "type": "Commercial",
    "file": "LICENSE"
  },
  "authors": {
    "nabto": "apps@nabto.com"
  },
  "source": {
    "http": "https://download.nabto.com/ug/nabto-ios-sdk-3.0.15.zip"
  },
  "social_media_url": "https://twitter.com/nabto_com",
  "platforms": {
    "ios": "5.0"
  },
  "vendored_libraries": [
    "SDK/lib/libnabto_client_api_static.a",
    "SDK/lib/libnabto_static_external.a"
  ],
  "source_files": "SDK/include/nabto_client_api.h",
  "pod_target_xcconfig": {
    "OTHER_LDFLAGS": "-ObjC -lstdc++"
  },
  "resources": "SDK/share/nabto/*",
  "preserve_paths": "SDK/**/*.*"
}
