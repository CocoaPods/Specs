{
  "name": "AblyDeltaCodec",
  "version": "1.3.3",
  "summary": "Cocoa VCDiff decoder",
  "homepage": "https://ably.com",
  "authors": {
    "Ably": "support@ably.com"
  },
  "license": "MIT",
  "social_media_url": "https://twitter.com/ablyrealtime",
  "description": "Cocoa VCDiff decoder. Uses Xdelta version 3, a C library for delta compression using VCDIFF/RFC 3284 streams.",
  "source": {
    "git": "https://github.com/ably/delta-codec-cocoa.git",
    "tag": "1.3.3",
    "submodules": true
  },
  "platforms": {
    "ios": "10.0",
    "tvos": "9.0",
    "watchos": "2.0",
    "osx": "10.12"
  },
  "requires_arc": true,
  "source_files": [
    "DeltaCodec/*.{h}",
    "source/*.{h,hh,m,mm}",
    "xdelta/xdelta3/xdelta3.c",
    "xdelta/xdelta3/xdelta3.h",
    "xdelta/xdelta3/xdelta3-blkcache.h",
    "xdelta/xdelta3/xdelta3-cfgs.h",
    "xdelta/xdelta3/xdelta3-decode.h",
    "xdelta/xdelta3/xdelta3-djw.h",
    "xdelta/xdelta3/xdelta3-fgk.h",
    "xdelta/xdelta3/xdelta3-hash.h",
    "xdelta/xdelta3/xdelta3-internal.h",
    "xdelta/xdelta3/xdelta3-list.h",
    "xdelta/xdelta3/xdelta3-lzma.h",
    "xdelta/xdelta3/xdelta3-main.h",
    "xdelta/xdelta3/xdelta3-merge.h",
    "xdelta/xdelta3/xdelta3-second.h"
  ],
  "public_header_files": [
    "DeltaCodec/*.{h}",
    "source/ARTDeltaCodec.h"
  ],
  "private_header_files": [
    "source/config.h",
    "xdelta/xdelta3/xdelta3.h",
    "xdelta/xdelta3/xdelta3-blkcache.h",
    "xdelta/xdelta3/xdelta3-cfgs.h",
    "xdelta/xdelta3/xdelta3-decode.h",
    "xdelta/xdelta3/xdelta3-djw.h",
    "xdelta/xdelta3/xdelta3-fgk.h",
    "xdelta/xdelta3/xdelta3-hash.h",
    "xdelta/xdelta3/xdelta3-internal.h",
    "xdelta/xdelta3/xdelta3-list.h",
    "xdelta/xdelta3/xdelta3-lzma.h",
    "xdelta/xdelta3/xdelta3-main.h",
    "xdelta/xdelta3/xdelta3-merge.h",
    "xdelta/xdelta3/xdelta3-second.h"
  ],
  "xcconfig": {
    "GCC_PREPROCESSOR_DEFINITIONS": "HAVE_DELTA_CONFIG_H=1"
  }
}
