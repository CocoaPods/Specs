{
  "name": "YFSwiftKit",
  "version": "0.4.0",
  "summary": "YFSwiftKit: a collection of useful swift extension",
  "description": "YFSwiftKit: a collection of useful swift extension.",
  "homepage": "https://github.com/yonfong/YFSwiftKit",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "yonfong": "yongfongzhang@163.com"
  },
  "source": {
    "git": "https://github.com/yonfong/YFSwiftKit.git",
    "tag": "0.4.0"
  },
  "platforms": {
    "ios": "12.0"
  },
  "swift_versions": "5.0",
  "default_subspecs": "SwiftKit",
  "subspecs": [
    {
      "name": "SwiftKit",
      "source_files": "YFSwiftKit/Classes/**/*"
    },
    {
      "name": "Define",
      "source_files": "YFSwiftKit/Classes/Define/**/*"
    },
    {
      "name": "UIKit",
      "source_files": "YFSwiftKit/Classes/UIKit/**/*",
      "subspecs": [
        {
          "name": "Color",
          "source_files": "YFSwiftKit/Classes/UIKit/Color/**/*"
        },
        {
          "name": "CollectionView",
          "source_files": "YFSwiftKit/Classes/UIKit/CollectionView/**/*"
        },
        {
          "name": "TableView",
          "source_files": "YFSwiftKit/Classes/UIKit/TableView/**/*"
        }
      ]
    }
  ],
  "swift_version": "5.0"
}
