{
  "name": "XLAudioFormatConvertTool",
  "version": "1.1.0",
  "summary": "轻量级音频转码器",
  "description": "基于ffmpeg-kit",
  "homepage": "https://gitee.com/ilongge/XLAudioFormatConvertTool",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "10150820867@qq.com": "1015820857@qq.com"
  },
  "source": {
    "git": "https://gitee.com/ilongge/XLAudioFormatConvertTool.git",
    "tag": "1.1.0"
  },
  "platforms": {
    "ios": "11.0"
  },
  "static_framework": true,
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64",
    "GENERATE_INFOPLIST_FILE": "YES"
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64",
    "GENERATE_INFOPLIST_FILE": "YES"
  },
  "source_files": [
    "XLAudioFormatConvertTool/Classes/Public/*.{h,m,mm}"
  ],
  "public_header_files": [
    "XLAudioFormatConvertTool/Classes/Public/*.h"
  ],
  "dependencies": {
    "ffmpeg-kit-ios-audio": [
      "~> 5.1.LTS"
    ]
  }
}
