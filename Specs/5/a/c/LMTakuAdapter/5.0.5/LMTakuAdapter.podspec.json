{
  "name": "LMTakuAdapter",
  "version": "5.0.5",
  "summary": "LMTakuAdapter - LitemizeSDK 的 Taku/AnyThink 适配器",
  "description": "LMTakuAdapter 是 LitemizeSDK 的第三方广告平台适配器，用于将 LitemizeSDK 接入到 Taku/AnyThink SDK。",
  "homepage": "https://github.com/aimagicat/Litemize",
  "license": {
    "type": "MIT"
  },
  "authors": {
    "Litemob": "@example.com"
  },
  "source": {
    "git": "https://github.com/aimagicat/Litemize.git",
    "tag": "v5.0.5"
  },
  "platforms": {
    "ios": "11.0"
  },
  "requires_arc": true,
  "source_files": "LMTakuAdapter/**/*.{h,m}",
  "public_header_files": "LMTakuAdapter/**/*.h",
  "dependencies": {
    "LitemizeSDK": [
      "~> 5.0.5"
    ],
    "AnyThinkiOS": [
      "6.4.93"
    ]
  },
  "frameworks": [
    "Foundation",
    "UIKit"
  ],
  "pod_target_xcconfig": {
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES"
  }
}
