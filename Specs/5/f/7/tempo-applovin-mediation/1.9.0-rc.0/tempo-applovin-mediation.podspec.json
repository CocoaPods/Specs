{
  "name": "tempo-applovin-mediation",
  "version": "1.9.0-rc.0",
  "swift_versions": "5.6.1",
  "authors": {
    "Tempo Engineering": "development@tempoplatform.com"
  },
  "license": {
    "type": "Apache License, Version 2.0",
    "file": "LICENSE"
  },
  "homepage": "https://www.tempoplatform.com",
  "readme": "https://github.com/Tempo-Platform/tempo-ios-applovin-mediation-adapter/blob/main/README.md",
  "source": {
    "git": "https://github.com/Tempo-Platform/tempo-ios-applovin-mediation-adapter.git",
    "tag": "1.9.0-rc.0"
  },
  "summary": "Tempo AppLovin iOS Mediation Adapter.",
  "description": "Using this adapter you will be able to integrate Tempo SDK via AppLovin mediation",
  "platforms": {
    "ios": "11.0"
  },
  "source_files": "TempoAdapter/*.*",
  "resource_bundles": {
    "TempoAdapter": [
      "TempoAdapter/Resources/**/*"
    ]
  },
  "dependencies": {
    "TempoSDK": [
      "1.8.0"
    ],
    "AppLovinSDK": [
      "< 14.0"
    ]
  },
  "requires_arc": true,
  "frameworks": [
    "Foundation",
    "UIKit"
  ],
  "static_framework": true,
  "pod_target_xcconfig": {
    "PRODUCT_BUNDLE_IDENTIFIER": "com.tempoplatform.applovin-adapter-sdk"
  },
  "user_target_xcconfig": {
    "VALID_ARCHS": "arm64 armv7 x86_64",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "swift_version": "5.6.1"
}
