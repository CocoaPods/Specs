{
  "name": "PredictionKeyboard",
  "version": "1.0.45",
  "summary": "Intelligent next-word prediction for iOS keyboards",
  "description": "PredictionKeyboard provides fast, accurate next-word prediction and word completion \nfor custom iOS keyboards. Built on Realm database with n-grams for context-aware \nsuggestions. Database is downloaded on first launch.",
  "homepage": "https://github.com/carloskekwa/Custom-Keyboard-Prediction",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Carlos Kekwa": "carlos_kek@hotmail.com"
  },
  "source": {
    "http": "https://youtakeadvantage.s3.eu-central-1.amazonaws.com/PredictionKeyboard.zip"
  },
  "platforms": {
    "ios": "12.0"
  },
  "swift_versions": [
    "5.0",
    "5.1",
    "5.2",
    "5.3",
    "5.4",
    "5.5"
  ],
  "vendored_frameworks": "PredictionKeyboard.xcframework",
  "frameworks": [
    "Foundation",
    "UIKit",
    "Security"
  ],
  "libraries": [
    "c++",
    "compression",
    "z"
  ],
  "dependencies": {
    "Realm": [
      "~> 10.0"
    ],
    "RealmSwift": [
      "~> 10.0"
    ]
  },
  "compiler_flags": "-fmodules -fcxx-modules",
  "requires_arc": true,
  "pod_target_xcconfig": {
    "DEFINES_MODULE": "YES",
    "CODE_SIGN_IDENTITY": "",
    "CODE_SIGNING_REQUIRED": "NO",
    "CODE_SIGNING_ALLOWED": "NO"
  },
  "swift_version": "5.5"
}
