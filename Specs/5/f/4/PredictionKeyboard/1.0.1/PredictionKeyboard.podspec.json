{
  "name": "PredictionKeyboard",
  "version": "1.0.1",
  "summary": "Intelligent next-word prediction for iOS keyboards",
  "description": "PredictionKeyboard provides fast, accurate next-word prediction and word completion \nfor custom iOS keyboards. Built on a 600MB Realm database with millions of n-grams \nfor context-aware suggestions.",
  "homepage": "https://github.com/carloskekwa/Custom-Keyboard-Prediction",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Carlos Kekwa": "carlos.kekwa@example.com"
  },
  "source": {
    "git": "https://github.com/carloskekwa/Custom-Keyboard-Prediction.git",
    "tag": "1.0.1"
  },
  "platforms": {
    "ios": "12.0"
  },
  "swift_versions": [
    "5.0",
    "5.1",
    "5.2",
    "5.3",
    "5.4",
    "5.5"
  ],
  "source_files": "Sources/**/*.{h,m,swift}",
  "public_header_files": "Sources/**/*.h",
  "resource_bundles": {
    "PredictionKeyboard": [
      "Resources/**/*"
    ]
  },
  "frameworks": [
    "Foundation",
    "UIKit"
  ],
  "dependencies": {
    "Realm": [
      "~> 10.0"
    ],
    "RealmSwift": [
      "~> 10.0"
    ]
  },
  "requires_arc": true,
  "pod_target_xcconfig": {
    "DEFINES_MODULE": "YES"
  },
  "swift_version": "5.5"
}
