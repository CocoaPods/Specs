{
  "name": "EasyPermissionX",
  "version": "0.0.2",
  "summary": "An easy permission library for iOS.",
  "homepage": "https://github.com/arcticfox1919/EasyPermissionX",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "arcticfox": "flutterdev@88.com"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source": {
    "git": "https://github.com/arcticfox1919/EasyPermissionX.git",
    "tag": "0.0.2"
  },
  "requires_arc": true,
  "default_subspecs": "All",
  "subspecs": [
    {
      "name": "All",
      "source_files": "permissions/*.{h,m}"
    },
    {
      "name": "Base",
      "source_files": [
        "permissions/EasyPermissionSetting.{h,m}",
        "permissions/EasyPermission.{h,m}"
      ]
    },
    {
      "name": "Camera",
      "source_files": "permissions/EasyPermissionCamera.{h,m}"
    },
    {
      "name": "Photo",
      "source_files": "permissions/EasyPermissionPhotos.{h,m}"
    },
    {
      "name": "Contact",
      "source_files": "permissions/EasyPermissionContacts.{h,m}"
    },
    {
      "name": "Location",
      "source_files": "permissions/EasyPermissionLocation.{h,m}"
    },
    {
      "name": "Reminder",
      "source_files": "permissions/EasyPermissionReminders.{h,m}"
    },
    {
      "name": "Calendar",
      "source_files": "permissions/EasyPermissionCalendar.{h,m}"
    },
    {
      "name": "Microphone",
      "source_files": "permissions/EasyPermissionMicrophone.{h,m}"
    },
    {
      "name": "Health",
      "source_files": "permissions/EasyPermissionHealth.{h,m}"
    },
    {
      "name": "Net",
      "source_files": [
        "permissions/EasyPermissionNet.{h,m}",
        "permissions/NetReachability.{h,m}",
        "permissions/EasyPermissionData.{h,m}"
      ]
    },
    {
      "name": "Tracking",
      "source_files": "permissions/EasyPermissionTracking.{h,m}"
    },
    {
      "name": "Media",
      "source_files": "permissions/EasyPermissionMediaLibrary.{h,m}"
    },
    {
      "name": "Notification",
      "source_files": "permissions/EasyPermissionNotification.{h,m}"
    },
    {
      "name": "Bluetooth",
      "source_files": "permissions/EasyPermissionBluetooth.{h,m}"
    }
  ]
}
