{
  "name": "BlueConicClient",
  "version": "2.8.2",
  "license": {
    "type": "Commercial"
  },
  "swift_versions": "5.2",
  "homepage": "https://blueconic.com",
  "authors": {
    "BlueConic": "info@blueconic.com"
  },
  "summary": "The BlueConicClient Framework provides the basis for communication with BlueConic.",
  "platforms": {
    "ios": "8.0"
  },
  "source": {
    "http": "https://download.blueconic.com/cocoapod/BlueConic-iOS-SDK-2.8.2-Source.zip",
    "flatten": true
  },
  "source_files": "BlueConicClient/BlueConicClient.h",
  "default_subspecs": "main",
  "resources": [
    "BlueConicClient/Resources/*.{png}",
    "BlueConicClient/Resources/**/*.{png}"
  ],
  "resource_bundles": {
    "Assets": [
      "BlueConicClient/Resources/*.{png}",
      "BlueConicClient/Resources/**/*.{png}"
    ]
  },
  "subspecs": [
    {
      "name": "main",
      "source_files": "BlueConicClient/**/*",
      "exclude_files": [
        "BlueConicClient/Info.plist",
        "BlueConicClient/Resources/**/*"
      ]
    }
  ],
  "swift_version": "5.2"
}
