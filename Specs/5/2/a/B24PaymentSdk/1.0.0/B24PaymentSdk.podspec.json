  },
  "exclude_files": "Classes/Exclude",
  "dependencies": {
    "Alamofire": [
      "~> 5.4"
    ],
    "Socket.IO-Client-Swift": [
      "~>16.1.0"
    ]
  },
  "swift_version": "5.0"
}
