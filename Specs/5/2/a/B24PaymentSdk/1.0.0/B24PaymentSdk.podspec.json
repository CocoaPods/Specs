{
  "name": "B24PaymentSdk",
  "version": "1.0.0",
  "summary": "Easy connect to payment with multiple payment method just one time integration",
  "description": "This instructions will get you a conplete guide on how to integrate with our sdk to allow your consumer to payment via bill24 finacial partner .",
  "homepage": "https://github.com/Bill24CoLtd/IOS-B24PaymentSdk",
  "license": {
    "type": "MIT"
  },
  "authors": {
    "Bill24CoLtd": "bill24.net@gmail.com"
  },
  "platforms": {
    "ios": "13.0"
  },
  "swift_versions": "5.0",
  "source": {
    "git": "https://github.com/Bill24CoLtd/IOS-B24PaymentSdk.git",
    "tag": "1.0.0"
  },
  "source_files": [
    "B24PaymentSdk/**/*.swift",
    "B24PaymentSdk/**/**/*.swift"
