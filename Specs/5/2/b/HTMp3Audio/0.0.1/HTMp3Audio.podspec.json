      "libraries": "mp3lame",
      "xcconfig": {
        "HEADER_SEARCH_PATHS": "${PODS_ROOT}/HTMp3Audio/HTMp3Audio/lame/lame.h"
      },
      "user_target_xcconfig": {
        "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
      },
      "pod_target_xcconfig": {
        "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
      }
    },
    {
      "name": "Record",
      "platforms": {
        "ios": "9.0"
      },
      "ios": {
        "dependencies": {
          "HTMp3Audio/Lame": [

          ]
        }
      },
      "source_files": "HTMp3Audio/HTMp3AudioRecorder.{h,m}"
    }
  ]
}
