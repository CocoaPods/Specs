{
  "name": "DiffableDataSources",
  "version": "0.4.0",
  "authors": {
    "ra1028": "r.fe51028.r@gmail.com"
  },
  "homepage": "https://github.com/ra1028/DiffableDataSources",
  "documentation_url": "https://ra1028.github.io/DiffableDataSources",
  "summary": "A library for backporting UITableView/UICollectionViewDiffableDataSource.",
  "source": {
    "git": "https://github.com/ra1028/DiffableDataSources.git",
    "tag": "0.4.0"
  },
  "license": {
    "type": "Apache 2.0",
    "file": "LICENSE"
  },
  "requires_arc": true,
  "swift_versions": [
    "5.0",
    "5.1"
  ],
  "ios": {
    "dependencies": {
      "DifferenceKit/UIKitExtension": [
        "~> 1.1"
      ]
    },
    "source_files": "Sources/UIKit/*.swift",
    "frameworks": "UIKit"
  },
  "tvos": {
    "dependencies": {
      "DifferenceKit/UIKitExtension": [
        "~> 1.1"
      ]
    },
    "source_files": "Sources/UIKit/*.swift",
    "frameworks": "UIKit"
  },
  "osx": {
    "dependencies": {
      "DifferenceKit/AppKitExtension": [
        "~> 1.1"
      ]
    },
    "source_files": "Sources/AppKit/*.swift",
    "frameworks": "Appkit"
  },
  "source_files": [
    "Sources/Internal/*.swift",
    "Sources/*.swift"
  ],
  "platforms": {
    "ios": "9.0",
    "tvos": "9.0",
    "osx": "10.11"
  },
  "swift_version": "5.1"
}
