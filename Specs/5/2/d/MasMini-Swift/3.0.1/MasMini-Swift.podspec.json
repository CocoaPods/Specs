    "file": "LICENSE"
  },
  "authors": {
    "MásMóvil": "info@grupomasmovil.com"
  },
  "source": {
    "git": "https://github.com/masmovil/masmini-swift.git",
    "tag": "v3.0.1"
  },
  "social_media_url": "https://twitter.com/masmovil",
  "platforms": {
    "ios": "14.1",
    "osx": "11.0",
    "tvos": "13.0"
  },
  "frameworks": "Foundation",
  "default_subspecs": "Core",
  "module_name": "Mini",
  "subspecs": [
    {
      "name": "Core",
      "ios": {
        "source_files": [
          "Sources/*.swift",
          "Sources/Utils/**/*.swift"
        ]
      },
      "osx": {
        "source_files": [
          "Sources/*.swift",
          "Sources/Utils/**/*.swift"
        ]
      },
      "tvos": {
        "source_files": [
          "Sources/*.swift",
          "Sources/Utils/**/*.swift"
        ]
      }
    }
  ],
  "swift_version": "5.0"
}
