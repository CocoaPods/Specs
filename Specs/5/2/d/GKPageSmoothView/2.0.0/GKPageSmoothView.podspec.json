      "source_files": "Sources/GKPageSmoothViewSwift/*.swift",
      "libraries": "swiftCoreGraphics",
      "xcconfig": {
        "LIBRARY_SEARCH_PATHS": "$(SDKROOT)/usr/lib/swift"
      },
      "resource_bundles": {
        "GKPageSmoothView": "Sources/GKPageSmoothViewSwift/PrivacyInfo.xcprivacy"
      }
    }
