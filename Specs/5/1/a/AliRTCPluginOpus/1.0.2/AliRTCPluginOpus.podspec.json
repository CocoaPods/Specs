{
  "name": "AliRTCPluginOpus",
  "version": "1.0.2",
  "summary": "AliRTCPluginOpus_iOS",
  "description": "It's an Plugin SDK for AliRTC, which implement by Objective-C.",
  "homepage": "https://github.com/aliyunvideo/AliRtcSDK_iOS",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "aliyunvideo": "videosdk@service.aliyun.com"
  },
  "source": {
    "http": "https://alivc-demo-cms.alicdn.com/versionProduct/sourceCode/rtc/PluginOpus/1.0.2/PluginOpus.library.zip"
  },
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "pod_target_xcconfig": {
    "ENABLE_BITCODE": "NO",
    "OTHER_LDFLAGS": "lObjC"
  },
  "xcconfig": {
    "ARCHS": "$(ARCHS_STANDARD)"
  },
  "vendored_frameworks": "PluginOpus.framework",
  "frameworks": [
    "AudioToolbox",
    "VideoToolbox",
    "CoreVideo",
    "CoreMedia",
    "OpenGLES",
    "AVFoundation",
    "UIKit",
    "CoreTelephony",
    "SystemConfiguration"
  ],
  "libraries": [
    "c++",
    "resolv"
  ]
}
