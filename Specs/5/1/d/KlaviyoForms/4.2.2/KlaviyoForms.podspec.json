{
  "name": "KlaviyoForms",
  "version": "4.2.2",
  "summary": "Klaviyo forms is a new way to engage with your app users",
  "description": "Use Klaviyo forms to include in app forms in your app and engage user with marketing content",
  "homepage": "https://github.com/klaviyo/klaviyo-swift-sdk",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Mobile @ Klaviyo": "mobile@klaviyo.com"
  },
  "source": {
    "git": "https://github.com/klaviyo/klaviyo-swift-sdk.git",
    "tag": "4.2.2"
  },
  "swift_versions": "5.7",
  "platforms": {
    "ios": "13.0"
  },
  "source_files": "Sources/KlaviyoForms/**/*.swift",
  "resource_bundles": {
    "KlaviyoFormsResources": [
      "Sources/KlaviyoForms/InAppForms/Assets/*.{html}",
      "Tests/KlaviyoFormsTests/Assets/*.{html}"
    ]
  },
  "pod_target_xcconfig": {
    "OTHER_SWIFT_FLAGS": "-package-name KlaviyoSwift"
  },
  "dependencies": {
    "KlaviyoSwift": [
      "~> 4.2.2"
    ]
  },
  "swift_version": "5.7"
}
