{
  "name": "LMSigmobAdapter",
  "version": "5.1",
  "summary": "LMSigmobAdapter - LitemizeSDK 的 ToBid 适配器",
  "description": "LMSigmobAdapter 是 LitemizeSDK 的第三方广告平台适配器，用于将 LitemizeSDK 接入到 ToBid SDK。",
  "homepage": "https://github.com/aimagicat/Litemize",
  "license": {
    "type": "MIT"
  },
  "authors": {
    "Litemob": "@example.com"
  },
  "source": {
    "git": "https://github.com/aimagicat/Litemize.git",
    "tag": "v5.1"
  },
  "platforms": {
    "ios": "12.0"
  },
  "requires_arc": true,
  "source_files": "LMSigmobAdapter/**/*.{h,m}",
  "public_header_files": "LMSigmobAdapter/**/*.h",
  "dependencies": {
    "LitemizeSDK": [
      "~> 5.1"
    ],
    "ToBid-iOS": [
      "~> 4.6.85"
    ]
  },
  "frameworks": [
    "Foundation",
    "UIKit"
  ],
  "pod_target_xcconfig": {
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES"
  }
}
