{
  "name": "LMSigmobAdapter",
  "version": "5.0.8",
  "summary": "LMSigmobAdapter - LitemobSDK 的 ToBid 适配器",
  "description": "LMSigmobAdapter 是 LitemobSDK 的第三方广告平台适配器，用于将 LitemobSDK 接入到 ToBid SDK。",
  "homepage": "https://github.com/aimagicat/Litemize",
  "license": {
    "type": "MIT"
  },
  "authors": {
    "Litemob": "shibao@litemob.com"
  },
  "source": {
    "git": "https://github.com/aimagicat/Litemize.git",
    "tag": "LMSigmobAdapter-v5.0.8"
  },
  "platforms": {
    "ios": "11.0"
  },
  "requires_arc": true,
  "source_files": "LMSigmobAdapter/**/*.{h,m}",
  "public_header_files": "LMSigmobAdapter/**/*.h",
  "dependencies": {
    "LitemobSDK": [
      "~> 5.0.8"
    ],
    "ToBid-iOS": [
      "4.7.0"
    ]
  },
  "frameworks": [
    "Foundation",
    "UIKit"
  ],
  "pod_target_xcconfig": {
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES"
  }
}
