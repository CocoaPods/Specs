{
  "name": "SwiftyDropboxObjC",
  "version": "10.0.4",
  "summary": "Objective-C Wrapper for Dropbox Swift SDK for API v2",
  "homepage": "https://dropbox.com/developers/",
  "license": "MIT",
  "authors": {
    "Stephen Cobbe": "scobbe@dropbox.com"
  },
  "source": {
    "git": "https://github.com/dropbox/SwiftyDropbox.git",
    "tag": "10.0.4"
  },
  "source_files": "Source/SwiftyDropboxObjC/Shared/**/*.{swift,h,m}",
  "osx": {
