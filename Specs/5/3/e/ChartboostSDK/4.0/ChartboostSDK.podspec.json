{
  "name": "ChartboostSDK",
  "version": "4.0",
  "license": "Commercial",
  "summary": "ChartboostSDK for showing ads and more.",
  "homepage": "https://chartboost.com/",
  "authors": {
    "Chartboost": "https://chartboost.com/"
  },
  "source": {
    "git": "https://github.com/ChartBoost/iOS-SDK-Pod.git",
    "tag": "4.0"
  },
  "description": "ChartboostSDK for showing ads and more apps pages, and tracking analytics and in-app purchase revenue.",
  "platforms": {
    "ios": null
  },
  "source_files": "**/*.h",
  "preserve_paths": "**/*.a",
