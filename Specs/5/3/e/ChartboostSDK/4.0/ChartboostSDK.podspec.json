  "xcconfig": {
    "LIBRARY_SEARCH_PATHS": "\"$(PODS_ROOT)/ChartboostSDK/Chartboost\""
  },
  "weak_frameworks": "AdSupport",
  "frameworks": [
    "QuartzCore",
    "SystemConfiguration",
    "CoreGraphics",
    "StoreKit"
  ],
  "requires_arc": false
}
