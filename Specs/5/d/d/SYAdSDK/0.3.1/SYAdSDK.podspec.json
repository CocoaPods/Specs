{
  "name": "SYAdSDK",
  "version": "0.3.1",
  "summary": "A short description of SYAdSDK.",
  "description": "Shiyu Ad SDK for iOS",
  "homepage": "http://gitlab.huina365.com:10080/iOS/SYAdSDK",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Thinkman Wang": "wangxf1985@gmail.com"
  },
  "source": {
    "git": "ssh://git@gitlab.huina365.com:10022/iOS/SYAdSDK.git",
    "tag": "0.3.1"
  },
  "platforms": {
    "ios": "9.0"
  },
  "source_files": "SYAdSDK/Classes/**/*.{h,m}",
  "public_header_files": "SYAdSDK/Classes/**/*.h",
  "static_framework": true,
  "dependencies": {
    "Bytedance-UnionAD": [
      "~> 3.3.1.5"
    ],
    "AFNetworking": [
      "~> 2.3"
    ]
  },
  "frameworks": [
    "UIKit",
    "StoreKit",
    "MobileCoreServices",
    "WebKit",
    "MediaPlayer",
    "CoreMedia",
    "CoreLocation",
    "AVFoundation",
    "CoreTelephony",
    "SystemConfiguration",
    "AdSupport",
    "CoreMotion",
    "Accelerate",
    "Security"
  ],
  "xcconfig": {
    "OTHER_LINK_FLAG": "$(inherited) -ObjC -l\"bz2\" -l\"c++\" -l\"c++abi\" -l\"resolv\" -l\"sqlite3\" -l\"xml2\" -l\"z\" "
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  }
}
