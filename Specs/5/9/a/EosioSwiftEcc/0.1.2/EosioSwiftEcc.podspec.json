{
  "name": "EosioSwiftEcc",
  "version": "0.1.2",
  "summary": "Elliptical Curve Cryptography (ECC) functions for EOSIO.",
  "homepage": "https://github.com/EOSIO/eosio-swift-ecc",
  "license": {
    "type": "MIT",
    "text": "                           Copyright (c) 2017-2019 block.one and its contributors.  All rights reserved.\n"
  },
  "authors": {
    "Todd Bowden": "todd.bowden@block.one",
    "Serguei Vinnitskii": "serguei.vinnitskii@block.one",
    "Farid Rahmani": "farid.rahmani@block.one",
    "Brandon Fancher": "brandon.fancher@block.one",
    "Steve McCoole": "steve.mccoole@objectpartners.com",
    "Ben Martell": "ben.martell@objectpartners.com"
  },
  "source": {
    "git": "https://github.com/EOSIO/eosio-swift-ecc.git",
    "tag": "v0.1.2"
  },
  "swift_versions": "4.2",
  "platforms": {
    "ios": "11.0"
  },
  "source_files": [
    "EosioSwiftEcc/**/*.{c,h,m,cpp,hpp}",
    "EosioSwiftEcc/**/*.swift"
  ],
  "libraries": "c++",
  "pod_target_xcconfig": {
    "CLANG_CXX_LANGUAGE_STANDARD": "gnu++17",
    "CLANG_CXX_LIBRARY": "libc++",
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES",
    "CLANG_ENABLE_MODULES": "YES",
    "SWIFT_COMPILATION_MODE": "wholemodule",
    "ENABLE_BITCODE": "YES"
  },
  "ios": {
    "dependencies": {
      "GRKOpenSSLFramework": [
        "~> 1.0"
      ],
      "EosioSwift": [
        "~> 0.1.2"
      ]
    }
  },
  "swift_version": "4.2"
}
