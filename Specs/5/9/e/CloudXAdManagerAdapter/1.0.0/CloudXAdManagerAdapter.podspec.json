  },
  "platforms": {
    "ios": "13.0"
  },
  "swift_versions": "5.0",
  "module_name": "CloudXAdManagerAdapter",
  "source": {
    "http": "https://github.com/cloudx-xenoss/CloudXiOSAdManagerAdapter/releases/download/1.0.0/CloudXAdManagerAdapter.xcframework.zip",
    "type": "zip",
    "flatten": false
