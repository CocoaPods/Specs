{
  "name": "Alexandria",
  "version": "4.1.0",
  "summary": "Library of useful iOS Swift extensions.",
  "description": "Alexandria provides a library of useful iOS Swift extensions.",
  "homepage": "https://github.com/jlandon/Alexandria",
  "license": "MIT",
  "authors": {
    "John Daub": "hsoi@hsoi.com",
    "Jonathan Landon": "jlandon@me.com",
    "Ben Kreeger": "ben.kreeger@icloud.com",
    "Chris Voss": "chrisvoss@gmail.com",
    "Steven Schobert": "spschobert@gmail.com",
    "Jayson Lane": "jaysonlane@gmail.com",
    "Claire Knight": "claire.knight@krider.co.uk",
    "Alex Corcoran": "alex.p.corcoran@gmail.com"
  },
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "swift_versions": [
    "5.0",
    "5.1"
  ],
  "documentation_url": "https://jlandon.github.io/Alexandria",
  "source": {
    "git": "https://github.com/jlandon/Alexandria.git",
    "tag": "4.1.0"
  },
  "frameworks": [
    "Foundation",
    "UIKit"
  ],
  "subspecs": [
    {
      "name": "Core",
      "source_files": [
        "Sources/Alexandria/*.{swift,h,m}"
      ]
    },
    {
      "name": "StoreKit",
      "source_files": "Sources/Alexandria/StoreKit",
      "weak_frameworks": "StoreKit"
    },
    {
      "name": "ImageEffects",
      "source_files": "Sources/Alexandria/ImageEffects",
      "weak_frameworks": "Accelerate"
    }
  ],
  "swift_version": "5.1"
}
