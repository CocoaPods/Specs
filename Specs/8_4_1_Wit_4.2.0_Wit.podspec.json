{
  "name": "Wit",
  "version": "4.2.0",
  "summary": "Wit.AI Official SDK",
  "description": "Official Wit SDK, https://wit.ai/docs/ios-tutorial/",
  "homepage": "https://github.com/wit-ai/wit-ios-sdk",
  "authors": {
    "Willy Blandin": "willy@wit.ai"
  },
  "source": {
    "git": "https://github.com/wit-ai/wit-ios-sdk.git",
    "tag": "4.2.0"
  },
  "platforms": {
    "ios": "7.0"
  },
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "requires_arc": true,
  "frameworks": [
    "QuartzCore",
    "CoreTelephony",
    "AudioToolbox"
  ],
  "weak_frameworks": "Speech",
  "dependencies": {
    "GCNetworkReachability": [
      "~> 1.3"
    ]
  },
  "source_files": [
    "Classes",
    "Classes/**/*.{h,m}",
    "Wit/*.{h,m}"
  ],
  "preserve_paths": "WitResources/Images",
  "resources": [
    "WitResources/Images/*.png"
  ]
}
