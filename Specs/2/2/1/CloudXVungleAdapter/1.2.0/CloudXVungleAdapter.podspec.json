{
  "name": "CloudXVungleAdapter",
  "version": "1.2.0",
  "summary": "CloudX Vungle Adapter - Static Framework",
  "description": "Vungle/Liftoff adapter for CloudX iOS SDK - binary distribution",
  "homepage": "https://github.com/cloudx-io/cloudx-ios",
  "license": {
    "type": "Business Source License 1.1"
  },
  "authors": {
    "CloudX": "support@cloudx.io"
  },
  "source": {
    "git": "https://github.com/cloudx-io/cloudx-ios.git",
    "tag": "v1.2.0-vungle"
