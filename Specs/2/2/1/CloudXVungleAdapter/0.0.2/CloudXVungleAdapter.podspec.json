{
  "name": "CloudXVungleAdapter",
  "version": "0.0.2",
  "summary": "Mobile SDK for CloudX iOS Vungle Adapter",
  "description": "iOS adapter add-on to the CloudX iOS SDK for Vungle bidder supporting all ad formats",
  "homepage": "https://github.com/cloudx-io/cloudx-ios",
  "license": {
    "type": "Business Source License 1.1",
    "file": "LICENSE"
  },
  "authors": {
    "CloudX": "support@cloudx.com"
  },
  "source": {
    "http": "https://github.com/cloudx-io/cloudx-ios/releases/download/v0.0.2-vungle/CloudXVungleAdapter-v0.0.2.xcframework.zip",
    "type": "zip",
    "flatten": false
  },
  "platforms": {
    "ios": "14.0"
  },
  "vendored_frameworks": "CloudXVungleAdapter.xcframework",
  "preserve_paths": "CloudXVungleAdapter.xcframework",
  "dependencies": {
    "CloudXCore": [

    ],
    "VungleAds": [
      "~> 7.4.0"
    ]
  },
  "frameworks": [
    "Foundation",
    "UIKit",
    "WebKit",
    "AVFoundation",
    "CoreMedia",
    "AudioToolbox",
    "CFNetwork",
    "CoreGraphics",
    "CoreTelephony",
    "SystemConfiguration",
    "StoreKit"
  ],
  "pod_target_xcconfig": {
    "FRAMEWORK_SEARCH_PATHS": "$(PODS_ROOT)/CloudXVungleAdapter",
    "OTHER_LDFLAGS": "-framework CloudXVungleAdapter",
    "DEFINES_MODULE": "YES",
    "CLANG_ENABLE_MODULES": "YES",
    "OTHER_CFLAGS": "-fmodules",
    "ENABLE_USER_SCRIPT_SANDBOXING": "NO"
  },
  "user_target_xcconfig": {
    "OTHER_LDFLAGS": "-ObjC",
    "ENABLE_USER_SCRIPT_SANDBOXING": "NO"
  },
  "requires_arc": true,
  "swift_versions": [
    "5.0",
    "5.1",
    "5.2",
    "5.3",
    "5.4",
    "5.5",
    "5.6",
    "5.7",
    "5.8",
    "5.9",
    "6.0",
    "6.1",
    "6.2"
  ],
  "swift_version": "6.2"
}
