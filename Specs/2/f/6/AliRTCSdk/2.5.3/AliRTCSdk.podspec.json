{
  "name": "AliRTCSdk",
  "version": "2.5.3",
  "summary": "AliRTCSdk_iOS",
  "description": "It's an SDK for aliyun video rtc, which implement by Objective-C.",
  "homepage": "https://github.com/aliyunvideo/AliRtcSDK_iOS",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "aliyunvideo": "videosdk@service.aliyun.com"
  },
  "source": {
    "http": "https://alivc-demo-cms.alicdn.com/versionProduct/sourceCode/rtc/2.5.3/AliRTCSdk_2.5.3(iOS_Pod).zip"
  },
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "pod_target_xcconfig": {
    "ENABLE_BITCODE": "NO",
    "OTHER_LDFLAGS": "lObjC"
  },
  "xcconfig": {
    "ARCHS": "$(ARCHS_STANDARD)"
  },
  "vendored_frameworks": "AliRTCSdk.framework",
  "frameworks": [
    "AudioToolbox",
    "VideoToolbox",
    "CoreVideo",
    "CoreMedia",
    "OpenGLES",
    "AVFoundation",
    "UIKit",
    "CoreTelephony",
    "SystemConfiguration"
  ],
  "libraries": [
    "c++",
    "resolv"
  ]
}
