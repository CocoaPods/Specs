{
  "name": "AliRTCSdk",
  "version": "0.0.2-SNAPSHOT",
  "summary": "AliRTCSdk_iOS",
  "description": "It's an SDK for aliyun video rtc, which implement by Objective-C.",
  "homepage": "https://github.com/aliyunvideo/AliRtcSDK_iOS",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "aliyunvideo": "videosdk@service.aliyun.com"
  },
  "source": {
    "http": "https://alivc-demo-cms.alicdn.com/versionProduct/sourceCode/rtc/2.4.1/AliRTCSdk_2.4.1(iOS).zip"
  },
  "platforms": {
    "ios": "9.0"
  },
  "requires_arc": true,
  "pod_target_xcconfig": {
    "ENABLE_BITCODE": "NO",
    "OTHER_LDFLAGS": "lObjC",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "xcconfig": {
    "ARCHS": "$(ARCHS_STANDARD)"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "vendored_frameworks": "Full_ARCH_Framework/AliRTCSdk.framework",
  "frameworks": [
    "AudioToolbox",
    "VideoToolbox",
    "CoreVideo",
    "CoreMedia",
    "OpenGLES",
    "AVFoundation",
    "UIKit",
    "CoreTelephony",
    "SystemConfiguration"
  ],
  "libraries": [
    "c++",
    "resolv"
  ]
}
