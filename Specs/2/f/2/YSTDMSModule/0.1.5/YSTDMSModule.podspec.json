{
  "name": "YSTDMSModule",
  "version": "0.1.5",
  "summary": "首页、VIP页面、二级EPG页面",
  "description": "YSTDMS系统相关页面包括 首页、VIP页面、二级EPG页面",
  "homepage": "http://192.168.1.10:8181/ios-mobile/modules/YSTDMSModule.git",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Allen": "caifeng@ysten.com"
  },
  "source": {
    "git": "http://192.168.1.10:8181/ios-mobile/modules/YSTDMSModule.git",
    "tag": "0.1.5"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source_files": "YSTDMSModule/Classes/**/*.{h,pch,m}",
  "public_header_files": "YSTDMSModule/Classes/PublicClasses/**/*.h",
  "prefix_header_file": "YSTDMSModule/Classes/PrivateClasses/OtherFiles/YSTDMSPch.pch",
  "xcconfig": {
    "ENABLE_BITCODE": "YES",
    "GCC_PRECOMPILE_PREFIX_HEADER": "YES",
    "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited)",
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES"
  },
  "pod_target_xcconfig": {
    "OTHER_LDFLAGS": "$(inherited)",
    "SWIFT_VERSION": "4.0",
    "HEADER_SEARCH_PATHS": "$(inherited) $(SDKROOT)/usr/include/libxml2 $(SDKROOT)/usr/include/libresolv $(inherited)$(PROJECT_DIR) /usr/local/include",
    "CLANG_CXX_LANGUAGE_STANDARD": "c++11",
    "CLANG_CXX_LIBRARY": "libc++"
  },
  "resource_bundles": {
    "YSTDMSModule": [
      "YSTDMSModule/Assets/*.{bundle,xcassets,imageset,png}",
      "YSTDMSModule/Classes/**/*.{storyboard,xib,cer,json,plist}"
    ]
  },
  "frameworks": "UIKit",
  "dependencies": {
    "Masonry": [

    ],
    "MJExtension": [

    ],
    "SDWebImage": [

    ],
    "ReactiveObjC": [

    ],
    "AvoidCrash": [
      "~> 2.3.0-beta"
    ],
    "YSTNetKit": [

    ],
    "YSTMRLog": [

    ],
    "MJRefresh": [

    ],
    "YSTCommonKit": [

    ]
  }
}
