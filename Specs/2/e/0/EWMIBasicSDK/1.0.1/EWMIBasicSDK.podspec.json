{
  "name": "EWMIBasicSDK",
  "version": "1.0.1",
  "summary": "微逆蓝牙联网SDK",
  "description": "易能科技出品的微逆蓝牙联网 SDK，用于设备蓝牙连接与数据交互。",
  "homepage": "http://www.sheway.com",
  "license": {
    "type": "Commercial",
    "text": "Copyright © 2025 艺唯科技股份有限公司. All rights reserved."
  },
  "authors": {
    "keli": "888@kelijun.com"
  },
  "platforms": {
    "ios": "12.0"
  },
  "source": {
    "git": "https://github.com/kelijun/EWMIBasicSDK.git",
    "tag": "1.0.1"
  },
  "vendored_frameworks": "EWMIBasicSDK.framework",
  "source_files": "EWMIBasicSDK.framework/**/*.{h,m}",
  "public_header_files": "EWMIBasicSDK.framework/Headers/**/*.h",
  "frameworks": [
    "Foundation",
    "UIKit",
    "CoreBluetooth",
    "SystemConfiguration",
    "Network"
  ],
  "libraries": [
    "z",
    "c++",
    "sqlite3"
