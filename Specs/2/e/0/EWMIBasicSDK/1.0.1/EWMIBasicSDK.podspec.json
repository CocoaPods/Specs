  "pod_target_xcconfig": {
    "ARCHS": "x86_64 arm64",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "HEADER_SEARCH_PATHS": "\"${PODS_ROOT}/EWMIBasicSDK/EWMIBasicSDK.framework/Headers\"",
    "OTHER_LDFLAGS": "-ObjC",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "static_framework": true,
  "dependencies": {
    "EWBluetoothSDK": [
      "0.1.7"
    ]
  }
