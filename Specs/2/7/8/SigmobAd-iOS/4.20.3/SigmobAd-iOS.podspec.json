{
  "name": "SigmobAd-iOS",
  "version": "4.20.3",
  "summary": "SigmobAd-iOS is a SDK from Sigmob providing AD service.",
  "description": "SigmobAd-iOS provides ADs which include native、splash、RewardVideo、Interstitial etc.",
  "homepage": "https://www.sigmob.com/",
  "license": {
    "type": "MIT",
    "file": "wind-sdk-ios/LICENSE"
  },
  "authors": {
    "Codi": "codi.zhao@sigmob.com"
  },
  "platforms": {
    "ios": "9.0"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "source": {
    "http": "https://sdkres.sigmob.cn/wind/ios/4.20.3_a6cc083294067f3eee17b3b6bbac3905/wind_release_ios_4.20.3_20250922.zip"
  },
  "xcconfig": {
    "VALID_ARCHS": "arm64 x86_64",
    "VALID_ARCHS[sdk=iphoneos*]": "arm64",
    "VALID_ARCHS[sdk=iphonesimulator*]": "x86_64",
    "OTHER_LDFLAGS": [
      "-lObjC"
    ]
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "requires_arc": true,
  "default_subspecs": "WindSDK",
  "subspecs": [
    {
      "name": "WindSDK",
      "frameworks": [
        "ImageIO",
        "StoreKit",
        "CFNetwork",
        "CoreMedia",
        "AdSupport",
        "CoreMotion",
        "MediaPlayer",
        "CoreGraphics",
        "AVFoundation",
        "CoreLocation",
        "CoreTelephony",
        "SafariServices",
        "MobileCoreServices",
        "SystemConfiguration",
        "AudioToolbox"
      ],
      "weak_frameworks": [
        "WebKit",
        "UIKit",
        "Foundation"
      ],
      "libraries": [
        "c++",
        "z",
        "sqlite3"
      ],
      "vendored_frameworks": "wind-sdk-ios/*.xcframework",
      "preserve_paths": "wind-sdk-ios/*.xcframework"
    }
  ]
}
