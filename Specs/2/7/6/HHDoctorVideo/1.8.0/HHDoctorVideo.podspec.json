{
  "name": "HHDoctorVideo",
  "version": "1.8.0",
  "summary": "HHDoctorVideo-iOS版互联网医院",
  "description": "互联网医院iOS版本测试，Objective- C版本",
  "homepage": "https://gitee.com/neighourchild/hhdoctor-hospital-i-os",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "thechildren": "565959276@qq.com"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source": {
    "git": "https://gitee.com/neighourchild/hhdoctor-hospital-i-os.git",
    "tag": "1.8.0"
  },
  "resources": "HHDoctorVideo.bundle",
  "pod_target_xcconfig": {
    "VALID_ARCHS": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": ""
  },
  "ios": {
    "vendored_frameworks": "HHDoctorVideo.framework"
  },
  "dependencies": {
    "JXBWebKit": [
      "1.3.0"
    ],
    "AFNetworking": [
      "3.0"
    ],
    "Masonry": [
      "1.0.1"
    ],
    "MBProgressHUD": [
      "1.1.0"
    ],
    "IQKeyboardManager": [

    ],
    "SVProgressHUD": [
      "2.2.5"
    ],
    "FMDB": [
      "2.7.5"
    ],
    "GPUImage": [
      "0.1.7"
    ],
    "SDWebImage": [
      "3.8"
    ],
    "MJRefresh": [

    ],
    "NIMSDK": [

    ]
  },
  "default_subspecs": "Base",
  "subspecs": [
    {
      "name": "Base",
      "vendored_frameworks": "HHDoctorSDK/*.framework",
      "resources": "HHDoctorSDK/resources/*.bundle",
      "frameworks": [
        "SystemConfiguration",
        "MobileCoreServices",
        "AVFoundation",
        "CoreTelephony",
        "VideoToolbox",
        "AudioToolbox",
        "CoreMedia"
      ],
      "libraries": [
        "z",
        "sqlite3.0",
        "c++",
        "resolv.9"
      ]
    }
  ]
