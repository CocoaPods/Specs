  },
  "platforms": {
    "ios": "7.0"
  },
  "source": {
    "git": "https://github.com/xinlidexiaoman/JOProjectBaseSDK.git",
    "tag": "1.1.19"
  },
  "source_files": "JOBaseSDK/**/JOProjectBaseSDK.h",
  "resources": "JOBaseSDK/Resources/**/*.{png,txt,a}",
