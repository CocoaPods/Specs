{
  "name": "VeloniumSDK",
  "version": "1.0.0",
  "summary": "Velonium Mobile Advertising SDK for iOS",
  "description": "The Velonium SDK provides header bidding functionality for mobile apps.\nThis is a repackaged version of VeloniumSDK Mobile SDK customized for Velonium.",
  "homepage": "https://velonium.com",
  "license": {
    "type": "Apache 2.0",
    "text": "Copyright 2025 Velonium"
  },
  "authors": {
    "Velonium": "support@velonium.com"
  },
  "source": {
    "git": "https://github.com/VeloniumLTD/Velonium-SDK-iOS.git",
    "tag": "1.0.0"
  },
  "platforms": {
    "ios": "12.0"
  },
  "swift_versions": "5.0",
  "vendored_frameworks": [
    "VeloniumSDK.xcframework",
    "libs/OMSDK-Static_Prebidorg.xcframework"
  ],
  "frameworks": [
    "CoreTelephony",
    "SystemConfiguration",
    "UIKit",
    "Foundation",
    "AVFoundation",
    "CoreGraphics",
    "CoreMedia",
    "CoreLocation",
    "AdSupport",
    "WebKit"
  ],
  "weak_frameworks": [
    "EventKit",
    "EventKitUI",
    "WatchConnectivity",
    "AdSupport"
  ],
  "xcconfig": {
    "OTHER_LDFLAGS": "-ObjC",
    "ENABLE_BITCODE": "NO"
  },
  "module_name": "VeloniumSDK",
  "pod_target_xcconfig": {
    "DEFINES_MODULE": "YES",
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES"
  },
  "user_target_xcconfig": {
    "FRAMEWORK_SEARCH_PATHS": "$(inherited) $(PROJECT_DIR)/Frameworks $(PROJECT_DIR)/Frameworks/**",
    "HEADER_SEARCH_PATHS": "$(inherited) $(PROJECT_DIR)/Frameworks/**"
  },
  "swift_version": "5.0"
}
