    "UIKit",
    "AVFoundation"
  ],
  "dependencies": {
    "TensorFlowLiteObjC": [
      "~> 2.4.0"
    ]
  },
  "pod_target_xcconfig": {
    "VALID_ARCHS": "x86_64 armv7 arm64"
  },
  "user_target_xcconfig": {
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES"
  },
  "requires_arc": true
}
