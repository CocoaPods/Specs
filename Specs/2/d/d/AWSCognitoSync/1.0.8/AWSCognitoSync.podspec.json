{
  "name": "AWSCognitoSync",
  "version": "1.0.8",
  "summary": "Amazon Cognito SDK for iOS",
  "description": "Amazon Cognito offers multi device data synchronization with offline access",
  "homepage": "http://aws.amazon.com/cognito",
  "license": "Amazon Software License",
  "authors": {
    "Amazon Web Services": "amazonwebservices"
  },
  "platforms": {
    "ios": "7.0"
  },
  "source": {
    "git": "https://github.com/aws/amazon-cognito-ios.git",
    "tag": "1.0.8"
  },
  "dependencies": {
    "AWSiOSSDKv2": [
      "~> 2.0.16"
    ]
  },
  "requires_arc": true,
  "subspecs": [
    {
      "name": "Cognito",
      "source_files": [
        "CognitoSync/*.{h,m}",
        "Cognito/*.{h,m}",
        "Cognito/**/*.{h,m}"
      ],
      "public_header_files": [
        "Cognito/*.h",
        "CognitoSync/*.{h,m}"
      ],
      "resources": [
        "CognitoSync/Resources/*.json"
      ]
    }
  ]
}
