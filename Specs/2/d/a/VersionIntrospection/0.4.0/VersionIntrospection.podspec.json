{
  "name": "VersionIntrospection",
  "version": "0.4.0",
  "summary": "Simple tool to expose versions of dependencies by parsing Podfile.lock",
  "homepage": "https://github.com/sprylab/VersionIntrospection",
  "license": "MIT",
  "authors": {
    "Claus Weymann": "claus.weymann@sprylab.com"
  },
  "source": {
    "git": "https://github.com/sprylab/VersionIntrospection.git",
    "tag": "0.4.0"
  },
  "platforms": {
    "ios": "7.0",
    "osx": "10.7"
  },
  "requires_arc": true,
  "source_files": "Pod/Classes/**/*.{h,m}",
  "ios": {
    "resources": "Pod/Classes/ios/**/*.{xib}",
    "dependencies": {
      "TSMarkdownParser": [
        "~> 1.0.18"
      ]
    }
  },
  "osx": {
    "exclude_files": "Pod/Classes/ios/**/*.*"
  }
}
