{
  "name": "Backtrace",
  "version": "1.5.0",
  "summary": "Backtrace's integration with iOS, macOS and tvOS",
  "description": "Backtrace's integration with iOS and macOS for handling crashes",
  "homepage": "https://backtrace.io/",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Backtrace I/O": "info@backtrace.io"
  },
  "source": {
    "git": "https://github.com/backtrace-labs/backtrace-cocoa.git",
    "tag": "1.5.0"
  },
  "platforms": {
    "ios": "10.0",
    "osx": "10.10",
    "tvos": "10.0"
  },
  "ios": {
    "source_files": [
      "Sources/**/*.{swift}",
      "Backtrace-iOS/**/*.{h*,swift}"
    ],
    "public_header_files": [
      "Backtrace-iOS/**/*.h*"
    ]
  },
  "osx": {
    "source_files": [
      "Sources/**/*.{swift}",
      "Backtrace-macOS/**/*.{h*,swift}"
    ],
    "public_header_files": [
      "Backtrace-macOS/**/*.h*"
    ]
  },
  "tvos": {
    "source_files": [
      "Sources/**/*.{swift}",
      "Backtrace-tvOS/**/*.{h*,swift}"
    ],
    "public_header_files": [
      "Backtrace-tvOS/**/*.h*"
    ]
  },
  "dependencies": {
    "Backtrace-PLCrashReporter": [

    ]
  },
  "resources": "Sources/**/*.xcdatamodeld",
  "swift_versions": "4.2"
}
