{
  "name": "PiwikPROSDK",
  "version": "1.1.10",
  "summary": "Piwik PRO SDK for iOS",
  "description": "SDK for iOS to measure your apps with Piwik PRO",
  "homepage": "https://piwik.pro",
  "license": {
    "type": "MIT",
    "file": "LICENSE.md"
  },
  "authors": {
    "Piwik PRO": "contact+cocoapods@piwik.pro"
  },
  "social_media_url": "http://twitter.com/piwikpro/",
  "platforms": {
    "ios": "9.0"
  },
  "source": {
    "git": "https://github.com/PiwikPRO/piwik-pro-sdk-framework-ios.git",
    "branch": "master",
    "tag": "1.1.10"
  },
  "source_files": "PiwikPROSDK.xcframework/**/*.{h,m}",
  "public_header_files": "PiwikPROSDK.xcframework/**/*.h",
  "preserve_paths": "PiwikPROSDK.xcframework/**/*",
  "documentation_url": "http://developers.piwik.pro/en/latest/sdk/Piwik_PRO_SDK_for_iOS.html",
  "frameworks": "SystemConfiguration",
  "ios": {
    "vendored_frameworks": "PiwikPROSDK.xcframework"
  },
  "requires_arc": false,
  "pod_target_xcconfig": {
    "HEADER_SEARCH_PATHS": "$(PODS_ROOT)/**"
  },
  "xcconfig": {
    "FRAMEWORK_SEARCH_PATHS": "$(PODS_ROOT)/",
    "USER_HEADER_SEARCH_PATHS": "$(PODS_ROOT)/**",
    "OTHER_LDFLAGS": "$(inherited) -ObjC"
  }
}
