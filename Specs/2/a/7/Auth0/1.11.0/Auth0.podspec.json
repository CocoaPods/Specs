      "Auth0/AuthSession.swift",
      "Auth0/SafariSession.swift",
      "Auth0/SafariAuthenticationSession.swift",
      "Auth0/SafariAuthenticationCallback.swift",
      "Auth0/SilentSafariViewController.swift",
