{
  "name": "Auth0",
  "version": "2.15.0",
  "summary": "Auth0 SDK for Apple platforms",
  "description": "Auth0 SDK for iOS, macOS, tvOS, watchOS and visionOS apps.",
  "homepage": "https://github.com/auth0/Auth0.swift",
  "license": "MIT",
  "authors": {
    "Auth0": "support@auth0.com",
    "Rita Zerrizuela": "rita.zerrizuela@auth0.com"
  },
  "source": {
    "git": "https://github.com/auth0/Auth0.swift.git",
    "tag": "2.15.0"
  },
  "social_media_url": "https://twitter.com/auth0",
  "source_files": "Auth0/**/*.swift",
  "resource_bundles": {
    "Auth0": "Auth0/PrivacyInfo.xcprivacy"
  },
  "swift_versions": [
    "5.0"
  ],
  "dependencies": {
    "SimpleKeychain": [
      "1.3.0"
    ],
    "JWTDecode": [
      "3.3.0"
    ]
  },
  "platforms": {
    "ios": "14.0",
    "osx": "11.0",
    "tvos": "14.0",
    "watchos": "7.0",
    "visionos": "1.0"
  },
  "ios": {
    "pod_target_xcconfig": {
      "SWIFT_ACTIVE_COMPILATION_CONDITIONS": "WEB_AUTH_PLATFORM PASSKEYS_PLATFORM"
    }
  },
  "osx": {
    "pod_target_xcconfig": {
      "SWIFT_ACTIVE_COMPILATION_CONDITIONS": "WEB_AUTH_PLATFORM PASSKEYS_PLATFORM"
    }
  },
  "visionos": {
    "pod_target_xcconfig": {
      "SWIFT_ACTIVE_COMPILATION_CONDITIONS": "WEB_AUTH_PLATFORM PASSKEYS_PLATFORM"
    }
  },
  "swift_version": "5.0"
}
