  "osx": {
    "exclude_files": [
      "Auth0/MobileWebAuth.swift"
    ],
    "pod_target_xcconfig": {
      "SWIFT_ACTIVE_COMPILATION_CONDITIONS": "WEB_AUTH_PLATFORM"
    }
