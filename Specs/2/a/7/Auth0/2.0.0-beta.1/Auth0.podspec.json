      "~> 2.0"
    ]
  },
  "platforms": {
    "ios": "12.0",
    "osx": "10.15",
    "tvos": "12.0",
    "watchos": "6.2"
  },
  "ios": {
    "exclude_files": [
      "Auth0/DesktopWebAuth.swift"
    ],
    "pod_target_xcconfig": {
      "SWIFT_ACTIVE_COMPILATION_CONDITIONS": "WEB_AUTH_PLATFORM"
    }
