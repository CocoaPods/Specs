{
  "name": "JsonRPC.swift",
  "version": "0.1.1",
  "summary": "Cross-plaform Json RPC library for Swift with WebSocket support",
  "description": "Cross-plaform Json RPC library for Swift with WebSocket support. Supports all Apple platforms and Linux.",
  "homepage": "https://github.com/tesseract-one/JsonRPC.swift",
  "license": {
    "type": "Apache-2.0",
    "file": "LICENSE"
  },
  "authors": {
    "Tesseract Systems, Inc.": "info@tesseract.one"
  },
  "source": {
    "git": "https://github.com/tesseract-one/JsonRPC.swift.git",
    "tag": "0.1.1"
  },
  "platforms": {
    "ios": "10.0",
    "osx": "10.12",
    "tvos": "10.0",
    "watchos": "6.0"
  },
  "swift_versions": "5.4",
  "module_name": "JsonRPC",
  "default_subspecs": "RPC",
  "subspecs": [
    {
      "name": "RPC",
      "source_files": "Sources/**/*.swift",
      "exclude_files": "Sources/Service/Serializable.swift",
      "dependencies": {
        "TesseractWebSocket": [
          "~> 0.2.0"
        ]
      }
    },
    {
      "name": "Serializable",
      "dependencies": {
        "JsonRPC.swift/RPC": [

        ],
        "Serializable.swift": [
          "~> 0.2.3"
        ]
      },
      "source_files": "Sources/Service/Serializable.swift",
      "testspecs": [
        {
          "name": "Tests",
          "test_type": "unit",
          "platforms": {
            "ios": "10.0",
            "osx": "10.12",
            "tvos": "10.0"
          },
          "source_files": "Tests/JsonRPCTests/*.swift",
          "dependencies": {
            "Serializable.swift": [
              "~> 0.2.3"
            ]
          }
        }
      ]
    }
  ],
  "swift_version": "5.4"
}
