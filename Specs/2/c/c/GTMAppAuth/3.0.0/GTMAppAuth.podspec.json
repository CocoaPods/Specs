    "file": "LICENSE"
  },
  "authors": "Google LLC",
  "source": {
    "git": "https://github.com/google/GTMAppAuth.git",
    "tag": "3.0.0"
  },
  "prefix_header_file": false,
  "source_files": "GTMAppAuth/Sources/**/*.swift",
  "platforms": {
    "ios": "10.0",
    "osx": "10.12",
    "tvos": "10.0",
    "watchos": "6.0"
  },
  "frameworks": "Security",
  "dependencies": {
    "GTMSessionFetcher/Core": [
      ">= 2.1",
      "< 4.0"
    ],
    "AppAuth/Core": [
