      },
      "source_files": [
        "GTMAppAuth/Tests/Unit/**/*.swift",
        "GTMAppAuth/Tests/Helpers/**/*.swift"
      ],
      "dependencies": {
        "AppAuth/Core": [

        ]
      },
      "requires_app_host": true
    },
    {
      "name": "objc-api-integration",
      "test_type": "unit",
      "platforms": {
        "ios": "10.0",
        "osx": "10.12",
        "tvos": "10.0"
      },
      "source_files": [
        "GTMAppAuth/Tests/ObjCIntegration/**/*.m",
        "GTMAppAuth/Tests/Helpers/**/*.swift"
      ],
      "dependencies": {
        "AppAuth/Core": [

        ]
      },
      "requires_app_host": true
    }
  ],
  "swift_version": "4.0"
