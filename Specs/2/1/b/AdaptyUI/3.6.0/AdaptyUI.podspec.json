  "homepage": "https://adapty.io/",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Adapty": "contact@adapty.io"
  },
  "source": {
    "git": "https://github.com/adaptyteam/AdaptySDK-iOS.git",
    "tag": "3.6.0"
  },
  "documentation_url": "https://docs.adapty.io",
  "platforms": {
    "ios": "13.0"
  },
  "swift_versions": "5.9",
  "source_files": "AdaptyUI/**/*.swift",
  "resource_bundles": {
    "AdaptyUI": [
      "AdaptyUI/PrivacyInfo.xcprivacy"
    ]
  },
  "dependencies": {
    "Adapty": [
      "3.6.0"
    ]
  },
  "frameworks": "SwiftUI",
