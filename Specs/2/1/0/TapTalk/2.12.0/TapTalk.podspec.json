  "resource_bundles": {
    "TapTalk": [
      "Pod/**/*.xib",
      "Pod/**/*.storyboard",
      "Pod/**/*.{png,jpeg,jpg,xcassets,ttf,otf,caf}",
      "TapTalk/**/*.xib",
      "TapTalk/**/*.storyboard",
      "TapTalk/**/*.{png,jpeg,jpg,xcassets,ttf,otf,caf}",
      "TapTalk/*.lproj/*.strings"
    ]
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  }
