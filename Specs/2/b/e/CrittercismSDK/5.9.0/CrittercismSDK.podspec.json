{
  "name": "CrittercismSDK",
  "version": "5.9.0",
  "summary": "iOS and tvOS SDK for applicaton performance management.",
  "description": "SDK for monitoring iOS and tvOS app performance, including crashes, API latency, and more.\nFor more info visit https://www.apteligent.com",
  "homepage": "https://www.apteligent.com/",
  "license": {
    "type": "Commercial",
    "text": "See https://www.apteligent.com/service-terms-and-conditions/"
  },
  "authors": {
    "crittercism": "support@apteligent.com"
  },
  "source": {
    "http": "https://app.apteligent.com/images/Crittercism_v5_9_0.zip"
  },
  "platforms": {
    "ios": "8.0",
    "tvos": "9.0"
  },
  "ios": {
    "public_header_files": "iOS/Crittercism.framework/Headers/*.h",
    "source_files": "iOS/Crittercism.framework/Headers/*.h",
    "vendored_frameworks": "iOS/Crittercism.framework"
  },
  "tvos": {
    "public_header_files": "tvOS/Crittercism.framework/Headers/*.h",
    "source_files": "tvOS/Crittercism.framework/Headers/*.h",
    "vendored_frameworks": "tvOS/Crittercism.framework"
  },
  "preserve_paths": [
    "iOS/*",
    "tvOS/*",
    "dsym_upload.sh",
    "dsym_upload.xcfilelist"
  ],
  "requires_arc": true,
  "frameworks": [
    "Foundation",
    "CoreData",
    "SystemConfiguration"
  ]
}
