  "homepage": "https://github.com/checkout/frames-ios.git",
  "swift_versions": "5.0",
  "license": "MIT",
  "authors": {
    "Checkout.com Integration": "integration@checkout.com"
  },
  "platforms": {
    "ios": "10.0"
  },
  "source": {
