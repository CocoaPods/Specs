{
  "name": "iSpiral",
  "module_name": "iSpiralUI",
  "version": "1.12.7",
  "summary": "Face detection and recognition",
  "homepage": "https://github.com/AppliedRecognition",
  "license": {
    "type": "COMMERCIAL",
    "file": "LICENCE.txt"
  },
  "authors": "Jakub Dolejs",
  "platforms": {
    "ios": "11.0"
  },
  "swift_versions": "5",
  "documentation_url": "https://appliedrecognition.github.io/Ver-ID-Core-Apple",
  "source": {
    "git": "https://github.com/AppliedRecognition/Ver-ID-UI-iOS.git",
    "tag": "v1.12.7"
  },
  "default_subspecs": [
    "Core",
    "UI"
  ],
  "static_framework": true,
  "cocoapods_version": ">= 1.10",
  "script_phases": {
    "name": "Check Ver-ID fully downloaded",
    "script": "filesize=$(wc -c <\"${PODS_XCFRAMEWORKS_BUILD_DIR}/iSpiral/VerIDCore.framework/VerIDCore\"); if [ $filesize -lt 1000000 ]; then echo \"error: iSpiral framework files not fully downloaded. Please install Git LFS, clear the iSpiral pod cache using pod cache clean iSpiral and run pod install.\"; exit 1; else echo \"iSpiral installed successfully\"; fi",
    "execution_position": "before_compile"
  },
  "subspecs": [
    {
      "name": "UI",
      "source_files": "VerIDUI/*.swift",
      "resource_bundles": {
        "VerIDUIResources": [
          "VerIDUI/Resources/Video/*.mp4",
          "VerIDUI/Resources/Localization/*.xml",
          "VerIDUI/Resources/*.xcassets",
          "VerIDUI/Resources/**.{storyboard,xib}"
        ]
      },
      "dependencies": {
        "iSpiral/Core": [

        ]
      },
      "pod_target_xcconfig": {
        "ENABLE_BITCODE": "YES"
      }
    },
    {
      "name": "Core",
      "dependencies": {
        "OpenCV2": [
          "~> 4.1"
        ],
        "ZIPFoundation": [
          "~> 0.9"
        ],
        "RxSwift": [
          "~> 5"
        ],
        "RxCocoa": [
          "~> 5"
        ]
      },
      "vendored_frameworks": "Frameworks/iSpiralCore/bitcode/VerIDCore.xcframework"
    },
    {
      "name": "UI-no-bitcode",
      "source_files": "VerIDUI/*.swift",
      "resource_bundles": {
        "VerIDUIResources": [
          "VerIDUI/Resources/Video/*.mp4",
          "VerIDUI/Resources/Localization/*.xml",
          "VerIDUI/Resources/*.xcassets",
          "VerIDUI/Resources/**.{storyboard,xib}"
        ]
      },
      "dependencies": {
        "iSpiral/Core-no-bitcode": [

        ]
      },
      "pod_target_xcconfig": {
        "ENABLE_BITCODE": "NO"
      }
    },
    {
      "name": "Core-no-bitcode",
      "dependencies": {
        "OpenCV2": [
          "~> 4.1"
        ],
        "ZIPFoundation": [
          "~> 0.9"
        ],
        "RxSwift": [
          "~> 5"
        ],
        "RxCocoa": [
          "~> 5"
        ]
      },
      "vendored_frameworks": "Frameworks/iSpiralCore/no-bitcode/VerIDCore.xcframework"
    }
  ],
  "swift_version": "5"
}
