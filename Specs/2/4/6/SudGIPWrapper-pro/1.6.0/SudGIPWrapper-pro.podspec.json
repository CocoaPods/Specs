{
  "name": "SudGIPWrapper-pro",
  "version": "1.6.0",
  "summary": "This is a Wrapper for SudGIP SDK",
  "description": "This is a Wrapper for SudGIP SDK, To enable developers to quickly access the SudGIP SDK",
  "homepage": "https://github.com/SudTechnology/SudGIPWrapper",
  "license": {
    "type": "MIT",
    "file": "SudGIPWrapper/LICENSE"
  },
  "authors": {
    "sud.tech": "dev@sud.tech"
  },
  "source": {
    "git": "https://github.com/SudTechnology/SudMGPWrapper.git",
    "tag": "v1.6.0"
  },
  "frameworks": [
    "Foundation",
    "UIKit"
  ],
  "requires_arc": true,
  "platforms": {
    "ios": "11.0"
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "public_header_files": "SudGIPWrapper/**/*.h",
  "source_files": [
    "SudGIPWrapper/*.{h,m,mm,cpp,c,hpp,cc,swift}",
    "SudGIPWrapper/Decorator/**/*.{h,m,mm,cpp,c,hpp,cc,swift}",
    "SudGIPWrapper/Model/**/*.{h,m,mm,cpp,c,hpp,cc,swift}",
    "SudGIPWrapper/State/**/*.{h,m,mm,cpp,c,hpp,cc,swift}"
  ],
  "dependencies": {
    "SudGIP-pro": [

    ],
    "MJExtension": [
      "~> 3.4.1"
    ]
  }
}
