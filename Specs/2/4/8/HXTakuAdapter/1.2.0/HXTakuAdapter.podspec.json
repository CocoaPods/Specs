{
  "name": "HXTakuAdapter",
  "version": "1.2.0",
  "summary": "HXSDK 适配器, 用于在TopOn聚合SDK请求广告",
  "homepage": "https://gitee.com/octmon/hxtaku-adapter-ios.git",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "OctMon": "octmon@qq.com"
  },
  "source": {
    "git": "https://gitee.com/octmon/hxtaku-adapter-ios.git",
    "tag": "1.2.0"
  },
  "platforms": {
    "ios": "11.0"
  },
  "static_framework": true,
  "ios": {
    "source_files": "HXTakuAdapter/Classes/**/*.{h,m}",
    "public_header_files": "HXTakuAdapter/Classes/**/*.h"
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64",
    "OTHER_LINK_FLAG": "$(inherited) -ObjC"
  },
  "dependencies": {
    "AnyThinkiOS": [
      "6.4.92"
    ],
    "HXSDK": [
      "4.8.0"
    ]
  }
}
