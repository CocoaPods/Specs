    "type": "LGPL",
    "file": "LICENSE"
  },
  "platforms": {
    "ios": "6.0"
  },
  "requires_arc": true,
  "source": {
    "git": "https://github.com/crazyhf/TencentOpenAPIV2_3.git",
    "tag": "v2.3"
  },
  "resources": "TencentOpenAPI/TencentOpenApi_IOS_Bundle.bundle",
