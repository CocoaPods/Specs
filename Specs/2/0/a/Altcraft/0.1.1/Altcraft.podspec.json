{
  "name": "Altcraft",
  "version": "0.1.1",
  "summary": "Altcraft iOS SDK.",
  "description": "Altcraft iOS SDK is a library that provides seamless integration with the Altcraft API, enabling iOS applications to interact with the Altcraft marketing platform efficiently.",
  "license": {
    "type": "EULA",
    "file": "LICENSE.md"
  },
  "homepage": "https://github.com/altcraft/ios-sdk",
  "source": {
    "git": "https://github.com/altcraft/ios-sdk.git",
    "tag": "0.1.1"
  },
  "authors": {
    "Altcraft": "contact@altcraft.com"
  },
  "platforms": {
    "ios": "13.0"
  },
  "swift_versions": [
    "5.10"
  ],
  "static_framework": true,
  "source_files": "Altcraft/**/*.{swift}",
  "exclude_files": "AltcraftTests/**",
  "resource_bundles": {
    "AltcraftResources": [
      "Altcraft/DataBase/**/*.{xcdatamodeld,xcdatamodel,momd,mom}"
    ]
  },
  "frameworks": [
    "UIKit",
    "Foundation",
    "CoreData",
    "CryptoKit",
    "BackgroundTasks",
    "Network",
    "UserNotifications",
    "MobileCoreServices"
  ],
  "weak_frameworks": [
    "AdSupport"
  ],
  "requires_arc": true,
  "user_target_xcconfig": {
    "ENABLE_USER_SCRIPT_SANDBOXING": "NO"
  },
  "pod_target_xcconfig": {
    "ENABLE_USER_SCRIPT_SANDBOXING": "NO"
  },
  "swift_version": "5.10"
}
