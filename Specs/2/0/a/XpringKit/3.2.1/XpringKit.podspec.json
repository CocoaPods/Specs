      "~> 4.9.0"
    ],
    "BigInt": [
      "~> 5.0.0"
    ],
    "SwiftGRPC": [
      "~> 0.9.1"
    ],
    "SwiftProtobuf": [
      "~> 1.5.0"
    ]
  },
  "frameworks": "Foundation",
  "testspecs": [
    {
      "name": "Tests",
      "test_type": "unit",
      "source_files": [
        "Tests/**/*.swift"
      ],
      "dependencies": {
        "BigInt": [
          "~> 5.0.0"
        ]
      },
      "frameworks": "Foundation"
    }
  ],
  "swift_version": "5.1"
