{
  "name": "BUAdSDK",
  "version": "0.1.0.25",
  "license": "MIT",
  "summary": "Advertising interface layer",
  "description": "As an existing interface layer, it is used to provide the ability to load ads",
  "homepage": "https://github.com/bytedance/Bytedance-UnionAD",
  "authors": {
    "zhangtianhao": "zhangtianhao.1230@bytedance.com"
  },
  "platforms": {
    "ios": "9.0"
  },
  "source": {
    "http": "https://sf16-fe-tos-sg.i18n-pglstatp.com/obj/bu-rely-framework-sg/interface/0.1.0.25/BUAdSDK.zip"
  },
  "frameworks": [
    "UIKit",
    "CoreFoundation",
    "WebKit",
    "StoreKit",
    "CoreServices",
    "MediaPlayer",
    "CoreMedia",
    "CoreLocation",
    "AVFoundation",
    "CoreTelephony",
    "SystemConfiguration",
    "AdSupport",
    "CoreMotion",
    "Accelerate",
    "Security"
  ],
  "weak_frameworks": "AppTrackingTransparency",
  "libraries": [
    "z",
    "bz2",
    "resolv.9",
    "c++",
    "sqlite3",
    "xml2",
    "iconv"
  ],
  "requires_arc": true,
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64",
    "OTHER_LDFLAGS": "-ObjC",
    "COMPILER_INDEX_STORE_ENABLE": "NO",
    "LLVM_LTO[config=Debug][sdk=*][arch=*]": "NO",
    "LLVM_LTO[config=Release][sdk=*][arch=*]": "YES_THIN",
    "GCC_OPTIMIZATION_LEVEL[config=Debug][sdk=*][arch=*]": "0",
    "GCC_OPTIMIZATION_LEVEL[config=Release][sdk=*][arch=*]": "z",
    "ASSETCATALOG_COMPILER_OPTIMIZATION": "space",
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES",
    "GCC_WARN_INHIBIT_ALL_WARNINGS": "YES",
    "CLANG_WARN_DOCUMENTATION_COMMENTS": "NO"
  },
  "default_subspecs": [
    "BUAdSDK"
  ],
  "subspecs": [
    {
      "name": "BUAdSDK",
      "vendored_frameworks": [
        "BUAdSDK/BUAdSDK.framework"
      ],
      "preserve_paths": [
        "BUAdSDK/BUAdSDK.framework"
      ]
    }
  ]
}
