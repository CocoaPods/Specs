  "homepage": "https://github.com/helfyz/HFLibs",
  "license": "MIT",
  "authors": {
    "helfy": "562812743@qq.com"
  },
  "platforms": {
    "ios": "7.0"
  },
  "source": {
    "git": "https://github.com/helfyz/HFLibs.git",
    "tag": "0.8.3",
    "submodules": true
  },
  "source_files": "HFLibs/HFLibs.h",
  "public_header_files": "HFLibs/HFLibs.h",
  "frameworks": [
    "Foundation",
    "CoreGraphics",
    "UIKit"
  ],
  "requires_arc": true,
  "dependencies": {
    "Masonry": [
