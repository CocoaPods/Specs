    "CoreGraphics",
    "QuartzCore",
    "SystemConfiguration",
    "MediaPlayer",
    "CoreMotion",
    "EventKitUI",
    "EventKit",
    "AdSupport",
    "StoreKit",
    "CoreLocation",
    "Accelerate",
    "CoreTelephony",
    "MessageUI"
  ],
