  "platforms": {
    "osx": "10.7"
  },
  "source": {
    "git": "https://github.com/sstigler/SubRipForCocoa.git",
    "tag": "1.0.1"
  },
  "source_files": [
    "SubRip/*",
    "TagSupport/**/*",
    "External/**/*"
