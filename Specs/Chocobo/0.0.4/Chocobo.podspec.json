{
  "name": "Chocobo",
  "version": "0.0.4",
  "summary": "Backbone-ish up your iOS models",
  "description": "                    Add async functions to your iOS models so you don't have to worry about another library.\n",
  "homepage": "https://github.com/PetroFeed/chocobo",
  "license": "MIT",
  "authors": {
    "markhazlett": "mark@petrofeed.com"
  },
  "source": {
    "git": "https://github.com/PetroFeed/chocobo.git",
    "tag": "0.0.4"
  },
  "platforms": {
    "ios": "7.0"
  },
  "requires_arc": true,
  "source_files": "Classes/ios/**",
  "ios": {
    "exclude_files": "Classes/osx"
  },
  "public_header_files": "Classes/**/*.h",
  "dependencies": {
    "AFNetworking": [
      "~> 2.0"
    ]
  }
}
