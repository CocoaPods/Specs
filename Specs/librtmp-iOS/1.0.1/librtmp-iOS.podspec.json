{
  "name": "librtmp-iOS",
  "version": "1.0.1",
  "license": "MIT",
  "summary": "librtmp build for iOS",
  "homepage": "https://github.com/ifactorylab/librtmp-iOS",
  "authors": {
    "Min Kim": "minsikzzang@gmail.com"
  },
  "source": {
    "git": "https://github.com/ifactorylab/librtmp-iOS.git",
    "tag": "1.0.1",
    "submodules": true
  },
  "requires_arc": false,
  "platforms": {
    "ios": "6.0",
    "osx": "10.8"
  },
  "public_header_files": [
    "include/librtmp/{*}.h",
    "include/openssl/{*}.h"
  ],
  "vendored_libraries": [
    "lib/libcrypto.a",
    "lib/libssl.a",
    "lib/librtmp.a"
  ],
  "preserve_paths": [
    "include/librtmp/*.h",
    "include/openssl/*.h"
  ],
  "libraries": [
    "ssl",
    "crypto",
    "rtmp"
  ],
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "${PODS_ROOT}/librtmp-iOS/include/**",
    "LIBRARY_SEARCH_PATHS": "${PODS_ROOT}/librtmp-iOS/lib/**"
  }
}