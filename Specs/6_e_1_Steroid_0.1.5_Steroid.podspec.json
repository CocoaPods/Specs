    {
      "name": "UIKit13",
      "platforms": {
        "ios": "13.0"
      },
      "source_files": [
        "Sources/SteroidUIKit/iOS8/**/*.{swift}",
        "Sources/SteroidUIKit/iOS11/**/*.{swift}",
        "Sources/SteroidUIKit/iOS13/**/*.{swift}"
