{
  "name": "XLCUtils",
  "version": "0.1.2",
  "summary": "Some Objective-C Utilities",
  "homepage": "https://github.com/xlc/XLCUtils",
  "license": "MIT",
  "authors": {
    "Xiliang Chen": "xlchen1291@gmail.com"
  },
  "source": {
    "git": "https://github.com/xlc/XLCUtils.git",
