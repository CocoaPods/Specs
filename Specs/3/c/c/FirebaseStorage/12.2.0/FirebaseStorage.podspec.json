{
  "name": "FirebaseStorage",
  "version": "12.2.0",
  "summary": "Firebase Storage",
  "description": "Firebase Storage provides robust, secure file uploads and downloads from Firebase SDKs, powered by Google Cloud Storage.",
  "homepage": "https://firebase.google.com",
  "license": {
    "type": "Apache-2.0",
    "file": "LICENSE"
  },
  "authors": "Google, Inc.",
  "source": {
    "git": "https://github.com/firebase/firebase-ios-sdk.git",
    "tag": "CocoaPods-12.2.0"
  },
  "social_media_url": "https://twitter.com/Firebase",
  "platforms": {
    "ios": "15.0",
    "osx": "10.15",
    "tvos": "15.0",
    "watchos": "7.0"
  },
  "swift_versions": "5.9",
  "cocoapods_version": ">= 1.12.0",
  "prefix_header_file": false,
  "source_files": [
    "FirebaseStorage/Sources/**/*.swift",
    "FirebaseStorage/Typedefs/*.h"
  ],
  "dependencies": {
    "FirebaseAppCheckInterop": [
      "~> 12.2.0"
    ],
    "FirebaseAuthInterop": [
      "~> 12.2.0"
    ],
    "FirebaseCore": [
      "~> 12.2.0"
    ],
    "FirebaseCoreExtension": [
      "~> 12.2.0"
    ],
    "GTMSessionFetcher/Core": [
      ">= 3.4",
      "< 6.0"
    ],
    "GoogleUtilities/Environment": [
      "~> 8.1"
    ]
  },
  "testspecs": [
    {
      "name": "ObjCIntegration",
      "test_type": "unit",
      "scheme": {
        "code_coverage": true
      },
      "platforms": {
        "ios": "15.0",
        "osx": "10.15",
        "tvos": "15.0"
      },
      "source_files": [
        "FirebaseStorage/Tests/ObjCIntegration/*.{m,mm}"
      ],
      "requires_app_host": true,
      "resources": [
        "FirebaseStorage/Tests/Integration/Resources/1mb.dat",
        "FirebaseStorage/Tests/Integration/Resources/GoogleService-Info.plist"
      ],
      "dependencies": {
        "FirebaseAuth": [
          "~> 12.2.0"
        ]
      },
      "pod_target_xcconfig": {
        "HEADER_SEARCH_PATHS": "\"${PODS_TARGET_SRCROOT}\""
      }
    },
    {
      "name": "unit",
      "test_type": "unit",
      "scheme": {
        "code_coverage": true
      },
      "platforms": {
        "ios": "15.0",
        "osx": "10.15",
        "tvos": "15.0"
      },
      "source_files": "FirebaseStorage/Tests/Unit/StorageAPITests.swift",
      "requires_app_host": true
    },
    {
      "name": "integration",
      "test_type": "unit",
      "scheme": {
        "code_coverage": true
      },
      "platforms": {
        "ios": "15.0",
        "osx": "10.15",
        "tvos": "15.0"
      },
      "source_files": "FirebaseStorage/Tests/Integration/*.swift",
      "requires_app_host": true,
      "resources": [
        "FirebaseStorage/Tests/Integration/Resources/1mb.dat",
        "FirebaseStorage/Tests/Integration/Resources/GoogleService-Info.plist",
        "FirebaseStorage/Tests/Integration/Resources/HomeImprovement.numbers"
      ],
      "dependencies": {
        "FirebaseAuth": [
          "~> 12.2.0"
        ]
      }
    }
  ],
  "swift_version": "5.9"
}
