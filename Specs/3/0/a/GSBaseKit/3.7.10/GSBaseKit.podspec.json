  "homepage": "https://github.com/net263/GSBaseKit.git",
  "license": "MIT",
  "authors": {
    "net263": "277715243@qq.com"
  },
  "source": {
    "git": "https://github.com/net263/GSBaseKit.git",
    "tag": "3.7.10"
