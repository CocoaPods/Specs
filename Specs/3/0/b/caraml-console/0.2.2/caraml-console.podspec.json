{
  "name": "caraml-console",
  "version": "0.2.2",
  "summary": "ANSI console addon for caraml",
  "description": "An ANSI console view for use with caraml and LiquidCore.",
  "homepage": "https://github.com/LiquidPlayer/caraml-console",
  "license": {
    "type": "MIT",
    "file": "LICENSE.md"
  },
  "authors": {
    "Eric Lange": "eric@flicket.tv"
  },
  "platforms": {
    "ios": "11.0"
  },
  "source": {
    "git": "https://github.com/LiquidPlayer/caraml-console.git",
    "tag": "0.2.2"
  },
  "source_files": [
    "src/*.{cpp,h}",
    "caraml-console/caraml-console/*.{h,m,mm}"
  ],
  "public_header_files": [
    "caraml-console/caraml-console/caraml_console.h"
  ],
  "xcconfig": {
    "CLANG_WARN_DOCUMENTATION_COMMENTS": "NO",
    "OTHER_CFLAGS": "-Wno-nullability-completeness",
    "HEADER_SEARCH_PATHS": "${PODS_CONFIGURATION_BUILD_DIR}/LiquidCore-headers/LiquidCore_headers.framework/PrivateHeaders/v8 ${PODS_CONFIGURATION_BUILD_DIR}/LiquidCore-headers/LiquidCore_headers.framework/PrivateHeaders/node"
  },
  "resources": [
    "caraml-console/caraml-console/ConsoleView.xib"
  ],
  "dependencies": {
    "caraml-core": [

    ],
    "LiquidCore": [

    ],
    "LiquidCore-headers": [

    ]
  }
}
