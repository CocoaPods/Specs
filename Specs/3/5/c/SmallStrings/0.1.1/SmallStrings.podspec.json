{
  "name": "SmallStrings",
  "version": "0.1.1",
  "summary": "A minifier for localized .strings files",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": "Emerge Tools",
  "source": {
    "git": "https://github.com/EmergeTools/SmallStrings"
  },
  "homepage": "https://www.emergetools.com/",
  "source_files": [
    "Source/*.{m,h}"
  ],
  "preserve_paths": [
    "compress",
    "localize.rb",
    "localize.sh"
  ],
  "prepare_command": "clang -O3 compress.m -framework Foundation -lcompression -o compress",
  "libraries": "compression",
  "platforms": {
    "osx": null,
    "ios": null,
    "tvos": null,
    "watchos": null
  }
}
