{
  "name": "AlamofireRSSParser",
  "version": "1.0.0",
  "summary": "An RSS parser response handler for Alamofire",
  "description": "An RSS parser plugin for Alamofire.  Adds a \"responseRSS()\" responseHandler to Alamofire.",
  "homepage": "https://github.com/AdeptusAstartes/AlamofireRSSParser",
  "license": "MIT",
  "authors": {
    "Don Angelillo": "dangelillo@gmail.com"
  },
  "source": {
    "git": "https://github.com/AdeptusAstartes/AlamofireRSSParser.git",
    "tag": "1.0.0"
  },
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "source_files": "Pod/Classes/**/*",
  "resource_bundles": {
    "AlamofireRSS": [
      "Pod/Assets/*.png"
    ]
  },
  "dependencies": {
    "Alamofire": [
      "> 3.0.0"
    ]
  }
}
