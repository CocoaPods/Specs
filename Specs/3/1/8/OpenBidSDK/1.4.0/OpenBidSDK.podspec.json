      "source_files": "Common/Headers/*.h",
      "vendored_frameworks": "Common/Common.framework",
      "public_header_files": "Common/Headers/*.h"
    },
    {
      "name": "Webrendering",
      "dependencies": {
        "OpenBidSDK/Common": [

        ]
      },
      "source_files": "Webrendering/Headers/*.h",
      "public_header_files": "Webrendering/Headers/*.h",
      "vendored_frameworks": "Webrendering/Webrendering.framework",
      "resources": "Webrendering/Resources/*.{js}"
    },
    {
      "name": "Video",
      "dependencies": {
        "OpenBidSDK/Common": [

        ]
      },
      "vendored_frameworks": "Video/Video.framework",
      "resources": "Video/Resources/*.{bundle}"
    },
    {
      "name": "Measurement",
      "dependencies": {
        "OpenBidSDK/Common": [

        ]
      },
      "vendored_frameworks": [
        "OMSDK/OMSDK.framework",
        "OMSDK/Frameworks/OMSDK_Pubmatic.framework"
      ],
      "resources": "OMSDK/Resources/*.{js}"
    },
    {
      "name": "OpenBid",
      "dependencies": {
        "OpenBidSDK/Webrendering": [

        ],
        "OpenBidSDK/Video": [

        ],
        "OpenBidSDK/Measurement": [

        ],
        "OpenBidSDK/Monitor": [

        ]
      },
      "vendored_frameworks": "OpenBid/OpenBid.framework",
      "source_files": "OpenBid/Headers/*.h",
      "public_header_files": "OpenBid/Headers/*.h"
    },
    {
      "name": "OpenBid-Monitor-Excluded",
      "dependencies": {
        "OpenBidSDK/Webrendering": [

        ],
        "OpenBidSDK/Video": [

        ],
        "OpenBidSDK/Measurement": [

        ]
      },
      "vendored_frameworks": "OpenBid/OpenBid.framework",
      "source_files": "OpenBid/Headers/*.h",
      "public_header_files": "OpenBid/Headers/*.h"
    },
    {
      "name": "Monitor",
      "dependencies": {
        "OpenBidSDK/Common": [

        ],
        "OpenBidSDK/Webrendering": [

        ],
        "OpenBidSDK/Video": [

        ]
      },
