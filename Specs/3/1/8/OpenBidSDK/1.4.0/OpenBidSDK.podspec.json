  },
  "platforms": {
    "ios": "9.0"
  },
  "default_subspecs": "OpenBid",
  "frameworks": [
    "Foundation",
    "UIKit",
    "MessageUI",
    "CoreLocation",
    "EventKitUI",
    "EventKit",
    "AdSupport",
    "SystemConfiguration",
    "CoreGraphics",
    "CoreTelephony",
    "WebKit"
