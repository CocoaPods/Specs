  "homepage": "https://github.com/nickswalker/ASCIIfy",
  "license": "MIT",
  "authors": {
    "Nick Walker": "nick@nickwalker.us"
  },
  "source": {
    "git": "https://github.com/nickswalker/ASCIIfy.git",
