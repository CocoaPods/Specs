{
  "name": "SubstrateSdk",
  "version": "1.4.0",
  "summary": "Utility library that implements clients specific logic to interact with substrate based networks",
  "homepage": "https://github.com/nova-wallet/substrate-sdk-ios",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "ERussel": "ruslan@novawallet.io"
  },
  "source": {
    "git": "https://github.com/nova-wallet/substrate-sdk-ios.git",
    "tag": "1.4.0"
  },
  "swift_versions": "5.0",
  "platforms": {
    "ios": "11.0"
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64",
    "VALID_ARCHS": "x86_64 armv7 arm64"
  },
  "source_files": "SubstrateSdk/Classes/**/*",
  "dependencies": {
    "IrohaCrypto/sr25519": [
      "~> 0.9.0"
    ],
    "IrohaCrypto/ed25519": [
      "~> 0.9.0"
    ],
    "IrohaCrypto/secp256k1": [
      "~> 0.9.0"
    ],
    "IrohaCrypto/Scrypt": [
      "~> 0.9.0"
    ],
    "IrohaCrypto/ss58": [
      "~> 0.9.0"
    ],
    "ReachabilitySwift": [
      "~> 5.0"
    ],
    "RobinHood": [
      "~> 2.6.0"
    ],
    "Starscream": [

    ],
    "TweetNacl": [
      "~> 1.0.0"
    ],
    "BigInt": [
      "~> 5.0"
    ],
    "xxHash-Swift": [
      "~> 1.0.0"
    ],
    "keccak.c": [
      "~> 0.1.0"
    ]
  },
  "testspecs": [
    {
      "name": "Tests",
      "test_type": "unit",
      "source_files": "Tests/**/*.swift",
      "resources": [
        "Tests/**/*.json",
        "Tests/**/*-metadata"
      ]
    }
  ],
  "swift_version": "5.0"
}
