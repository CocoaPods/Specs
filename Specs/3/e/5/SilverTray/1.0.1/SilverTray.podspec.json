    "vendored_libraries": "SilverTray/Libraries/Opus/Binary/watchOS/libopus.a"
  },
  "osx": {
    "vendored_libraries": "SilverTray/Libraries/Opus/Binary/macOS/libopus.a"
  },
  "preserve_paths": [
    "SilverTray/Libraries/**",
    "SilverTray/SilverTray.modulemap"
  ],
  "module_map": "SilverTray/SilverTray.modulemap",
  "libraries": "c++",
  "dependencies": {
    "NattyLog": [
      "1.1.1"
    ]
  },
  "xcconfig": {
    "OTHER_LDFLAGS": "-Xlinker -w"
  },
  "swift_version": "5.1"
