    "tag": "1.0.1"
  },
  "documentation_url": "https://developers.nugu.co.kr",
  "platforms": {
    "ios": "10.0",
    "tvos": "13.0",
    "watchos": "6.0",
    "osx": "10.15.0"
  },
  "swift_versions": "5.1",
  "source_files": [
    "SilverTray/Classes/**/*",
