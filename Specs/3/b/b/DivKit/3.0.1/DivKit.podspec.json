{
  "name": "DivKit",
  "version": "3.0.1",
  "summary": "DivKit framework",
  "description": "Backend-Driven UI",
  "homepage": "https://divkit.tech",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "divkit": "divkit-cocoapod@yandex-team.ru"
  },
  "source": {
    "git": "https://github.com/divkit/divkit.git",
    "tag": "3.0.1"
  },
  "swift_versions": "5",
  "requires_arc": true,
  "prefix_header_file": false,
  "platforms": {
    "ios": "10.0"
  },
  "dependencies": {
    "DivKit_Base": [
      "3.0.1"
    ],
    "DivKit_BaseTiny": [
      "3.0.1"
    ],
    "DivKit_BaseUI": [
      "3.0.1"
    ],
    "DivKit_LayoutKit": [
      "3.0.1"
    ],
    "DivKit_LayoutKitInterface": [
      "3.0.1"
    ],
    "DivKit_CommonCore": [
      "3.0.1"
    ],
    "DivKit_CommonCoreTiny": [
      "3.0.1"
    ],
    "DivKit_Serialization": [
      "3.0.1"
    ],
    "DivKit_TemplatesSupport": [
      "3.0.1"
    ],
    "DivKit_Networking": [
      "3.0.1"
    ]
  },
  "pod_target_xcconfig": {
    "OTHER_SWIFT_FLAGS": "-DOFFICIAL_BUILD -application-extension",
    "OTHER_CFLAGS": "-DOFFICIAL_BUILD",
    "HEADER_SEARCH_PATHS": "$PODS_ROOT/Headers/Public/DivKit_Base $PODS_ROOT/Headers/Public/DivKit_CommonCoreTiny $PODS_ROOT/Headers/Public/DivKit_CommonCore $PODS_ROOT/Headers/Public/DivKit_LayoutKit $PODS_ROOT/Headers/Public/DivKit_Serialization",
    "DEFINES_MODULE": "YES"
  },
  "source_files": [
    "client/ios/DivKit/**/*"
  ],
  "swift_version": "5"
}
