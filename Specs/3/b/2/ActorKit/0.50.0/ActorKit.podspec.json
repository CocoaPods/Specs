    "ios": "8.0",
    "watchos": "2.0",
    "tvos": "9.0",
    "osx": "10.9"
  },
  "requires_arc": true,
  "source": {
    "git": "https://github.com/jkrumow/ActorKit.git",
    "tag": "0.50.0"
  },
  "default_subspecs": "Core",
