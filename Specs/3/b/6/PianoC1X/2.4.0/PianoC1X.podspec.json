  "homepage": "https://github.com/tinypass/piano-sdk-for-ios",
  "license": {
    "type": "Apache License, Version 2.0",
    "file": "LICENSE"
  },
  "authors": "Piano Inc.",
  "platforms": {
    "ios": "9.0"
  },
  "source": {
    "git": "https://github.com/tinypass/piano-sdk-for-ios.git",
    "tag": "2.4.0"
  },
  "source_files": [
