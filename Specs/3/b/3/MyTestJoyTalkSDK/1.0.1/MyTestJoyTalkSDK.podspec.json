{
  "name": "MyTestJoyTalkSDK",
  "version": "1.0.1",
  "summary": "MyTestJoyTalkSDK - 一个测试用的SDK",
  "description": "MyTestJoyTalkSDK 是一个用于测试的SDK框架。\n提供了基础的初始化功能。",
  "homepage": "https://github.com/Wangyaxiong/MyTestJoyTalkSDK",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Wangyaxiong": "tinyxion@qq.com"
  },
  "platforms": {
    "ios": "15.0"
  },
  "source": {
    "git": "https://github.com/Wangyaxiong/MyTestJoyTalkSDK.git",
    "tag": "1.0.1"
  },
  "vendored_frameworks": "MyTestJoyTalkSDK.framework",
  "static_framework": true,
  "frameworks": [
    "Foundation",
    "UIKit",
    "AVFoundation"
  ],
  "requires_arc": true,
  "pod_target_xcconfig": {
    "CODE_SIGNING_REQUIRED": "NO",
    "CODE_SIGNING_ALLOWED": "NO",
    "VALID_ARCHS": "x86_64 arm64",
    "ARCHS": "x86_64 arm64",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64",
    "PRIVATE_HEADERS_FOLDER_PATH": "MyTestJoyTalkSDK.framework/Headers/Private",
    "PUBLIC_HEADERS_FOLDER_PATH": "MyTestJoyTalkSDK.framework/Headers/Public",
    "ENABLE_BITCODE": "NO"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64",
    "VALID_ARCHS": "x86_64 arm64",
    "ENABLE_BITCODE": "NO"
  }
}
