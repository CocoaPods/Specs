{
  "name": "QTPopups",
  "version": "0.0.1",
  "summary": "QTPopups",
  "description": "Qingting iOS QTPopups Framework",
  "homepage": "https://git2.qingtingfm.com/CocoaPods/QTPopups",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "xupeng": "xupeng@qingting.fm"
  },
  "source": {
    "git": "https://git2.qingtingfm.com/CocoaPods/qtpopups.git",
    "tag": "0.0.1"
  },
  "platforms": {
    "ios": "9.0"
  },
  "requires_arc": true,
  "public_header_files": "Source/Classes/QTPopups.h",
  "source_files": "Source/Classes/QTPopups.h",
  "resource_bundles": {
    "QTPopups": [
      "Source/Assets/images/*.png"
    ]
  },
  "subspecs": [
    {
      "name": "AlertView",
      "source_files": [
        "Source/Classes/AlertView/*",
        "Source/Classes/Base/*"
      ]
    },
    {
      "name": "SheetView",
      "source_files": [
        "Source/Classes/SheetView/*",
        "Source/Classes/Base/*"
      ]
    }
  ]
}
