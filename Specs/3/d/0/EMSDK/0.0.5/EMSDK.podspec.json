{
  "name": "EMSDK",
  "summary": "Easy Mobile SDK",
  "version": "0.0.5",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "description": "Easy Mobile SDK",
  "homepage": "https://www.yd-mobile.cn",
  "authors": {
    "徐畅": "xuchang@supcon.com"
  },
  "source": {
    "git": "https://github.com/team-ezm/EMSDK.git",
    "tag": "0.0.5"
  },
  "platforms": {
    "ios": "9.0"
  },
  "requires_arc": true,
  "static_framework": true,
  "pod_target_xcconfig": {
    "ENABLE_BITCODE": "NO",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "ENABLE_BITCODE": "NO",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "prepare_command": "sh prepare_build.sh",
  "default_subspecs": "Core",
  "subspecs": [
    {
      "name": "Core",
      "source_files": "*.{h,m}",
      "resources": [
        "Frameworks/*.bundle",
        "Resources/**"
      ],
      "vendored_frameworks": "Frameworks/*.framework",
      "dependencies": {
        "YYCache": [

        ],
        "FDFullscreenPopGesture": [

        ],
        "YYCategories": [

        ],
        "MBProgressHUD": [

        ],
        "SAMKeychain": [

        ],
        "Reachability": [

        ],
        "AFNetworking": [

        ],
        "SDWebImage": [

        ],
        "IQKeyboardManager": [

        ],
        "Masonry": [

        ]
      }
    },
    {
      "name": "AppInfo",
      "vendored_frameworks": "Frameworks/AppInfo/*.framework",
      "dependencies": {
        "EMSDK/Core": [

        ]
      }
    },
    {
      "name": "Scanner",
      "vendored_frameworks": "Frameworks/Scanner/*.framework",
      "dependencies": {
        "LBXScan/LBXZXing": [

        ],
        "LBXScan/UI": [

        ],
        "EMSDK/Core": [

        ]
      },
      "resources": [
        "Frameworks/Scanner/*.bundle"
      ]
    },
    {
      "name": "FileTransfer",
      "vendored_frameworks": "Frameworks/FileTransfer/*.framework",
      "dependencies": {
        "EMSDK/Core": [

        ]
      }
    },
    {
      "name": "DocumentPreviewer",
      "vendored_frameworks": "Frameworks/DocumentPreviewer/*.framework",
      "dependencies": {
        "EMSDK/Core": [

        ]
      }
    },
    {
      "name": "All",
      "dependencies": {
        "EMSDK/AppInfo": [

        ],
        "EMSDK/Scanner": [

        ],
        "EMSDK/FileTransfer": [

        ],
        "EMSDK/DocumentPreviewer": [

        ]
      }
    }
  ]
}
