{
  "name": "IoTLibrarySDK",
  "version": "0.0.3",
  "summary": "IoTLibrary provides core utilities and helpers for iOS apps.",
  "description": "IoTLibrary is an iOS SDK offering utilities and components that can be integrated\nvia CocoaPods, Swift Package Manager, or manual installation.",
  "homepage": "https://github.com/reparag/IoTLibrary",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Parag Patil": "parag.p@refurbedge.com"
  },
  "platforms": {
    "ios": "15.0"
  },
  "source": {
    "git": "https://github.com/reparag/IoTLibrary.git",
    "tag": "0.0.3"
  },
  "pod_target_xcconfig": {
    "CODE_SIGNING_ALLOWED": "NO",
    "EXPANDED_CODE_SIGN_IDENTITY": ""
  },
  "user_target_xcconfig": {
    "CODE_SIGNING_ALLOWED": "NO",
    "EXPANDED_CODE_SIGN_IDENTITY": ""
  },
  "swift_versions": "6.1",
  "source_files": "**/*.{swift}",
  "exclude_files": [
    "Package.swift",
    "*/Package.swift",
    "**/Package.swift"
  ],
  "swift_version": "6.1"
}
