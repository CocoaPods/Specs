{
  "name": "OC_Extend",
  "version": "0.0.9",
  "summary": "在这里你将看到 iOS Objective-C 的组件扩展",
  "description": "不积跬步无以至千里；每天增加一个组建的扩展功能，督促自己每天都要进步。",
  "homepage": "https://github.com/zhj1214/OC_Extend",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "hongjun.zhang": "zhj1214@hotmail.com"
  },
  "source": {
    "git": "https://github.com/zhj1214/OC_Extend.git",
    "tag": "0.0.9"
  },
  "platforms": {
    "ios": "8.0"
  },
  "frameworks": [
    "UIkit",
    "Security",
    "CFNetwork",
    "Foundation",
    "QuartzCore",
    "MapKit"
  ],
  "dependencies": {
    "AFNetworking": [
      "~> 3.2"
    ],
    "SAMKeychain": [
      "~> 1.5.3"
    ]
  },
  "requires_arc": true,
  "source_files": "OC_Extend/Classes/OC_ExtendHeader.h",
  "public_header_files": "OC_Extend/Classes/OC_ExtendHeader.h",
  "subspecs": [
    {
      "name": "APPSeting",
      "source_files": "OC_Extend/Classes/APPSeting/*{h,m}",
      "public_header_files": "OC_Extend/Classes/APPSeting/*.h"
    },
    {
      "name": "UIExtension",
      "source_files": "OC_Extend/Classes/UIExtension/**/*.{h,m}",
      "public_header_files": "OC_Extend/Classes/UIExtension/**/*.h"
    },
    {
      "name": "CommonSecretData",
      "source_files": "OC_Extend/Classes/CommonSecretData/**/*.{h,m}",
      "public_header_files": "OC_Extend/Classes/CommonSecretData/**/*.h"
    },
    {
      "name": "APPLocation",
      "source_files": [
        "OC_Extend/Classes/APPLocation/*.{h,m}",
        "OC_Extend/Classes/UIExtension/UIViewController/UIViewController+Utils.{h,m}"
      ],
      "public_header_files": [
        "OC_Extend/Classes/APPLocation/*.h",
        "OC_Extend/Classes/UIExtension/UIViewController/UIViewController+Utils.h"
      ]
    },
    {
      "name": "CheckText",
      "source_files": [
        "OC_Extend/Classes/CheckText/*.{h,m}",
        "OC_Extend/Classes/APPSeting/AppEnum.h"
      ],
      "public_header_files": [
        "OC_Extend/Classes/CheckText/*.h",
        "OC_Extend/Classes/APPSeting/AppEnum.h"
      ]
    },
    {
      "name": "NSExtension",
      "source_files": "OC_Extend/Classes/NSExtension/**/*.{h,m}",
      "public_header_files": "OC_Extend/Classes/NSExtension/**/*.h"
    },
    {
      "name": "MBProgressHUD",
      "source_files": "OC_Extend/Classes/MBProgressHUD/*.{h,m}",
      "public_header_files": "OC_Extend/Classes/MBProgressHUD/*.h"
    }
  ]
}
