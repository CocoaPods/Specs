{
  "name": "SolanaSwift",
  "version": "1.0.0",
  "summary": "A client for Solana written in Swift.",
  "description": "Solana-blockchain client, written in pure swift, which supports keypair generation, networking, creating and signing transactions",
  "homepage": "https://github.com/p2p-org/solana-swift",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Chung Tran": "chung.t@p2p.org"
  },
  "source": {
    "git": "https://github.com/p2p-org/solana-swift.git",
    "tag": "1.0.0"
  },
  "platforms": {
    "ios": "12.0"
  },
  "source_files": "Sources/SolanaSwift/**/*",
  "swift_versions": "5.0",
  "pod_target_xcconfig": {
    "SWIFT_OPTIMIZATION_LEVEL": "-O"
  },
  "resource_bundles": {
    "SolanaSwift": [
      "Sources/SolanaSwift/Resources/*.json"
    ]
  },
  "dependencies": {
    "CryptoSwift": [
      "~> 1.4.1"
    ],
    "BufferLayoutSwift": [
      "~> 0.9.0"
    ],
    "secp256k1.swift": [
      "~> 0.1.0"
    ],
    "TweetNacl": [
      "~> 1.0.2"
    ],
    "RxCocoa": [
      "~> 6.2.0"
    ],
    "RxAlamofire": [
      "~> 6.1.1"
    ],
    "Starscream": [
      "~> 4.0.0"
    ]
  },
  "swift_version": "5.0"
}
