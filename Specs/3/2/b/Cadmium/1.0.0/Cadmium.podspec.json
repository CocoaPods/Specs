{
  "name": "Cadmium",
  "version": "1.0.0",
  "summary": "Core Data framework for Swift that uses concise syntax to hide context complexity and ensure best practices",
  "description": "Core Data framework for Swift that uses concise syntax to hide context complexity, ensure best practices, and protect you from common Core Data pitfalls.",
  "homepage": "https://github.com/jmfieldman/Cadmium",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Jason Fieldman": "jason@fieldman.org"
  },
  "social_media_url": "http://fieldman.org",
  "platforms": {
    "ios": "9.0",
    "watchos": "2.0"
  },
  "source": {
    "git": "https://github.com/jmfieldman/Cadmium.git",
    "tag": "1.0.0"
  },
  "source_files": "Cadmium/*.swift",
  "requires_arc": true,
  "default_subspecs": "Core",
  "subspecs": [
    {
      "name": "Core",
      "source_files": "Cadmium/*.swift",
      "frameworks": "CoreData"
    },
    {
      "name": "PromiseKit",
      "source_files": "Extensions/PromiseKit/*.swift",
      "weak_frameworks": "PromiseKit",
      "dependencies": {
        "Cadmium/Core": [

        ],
        "PromiseKit": [

        ]
      }
    }
  ]
}
