          "~> 5.4"
        ],
        "SDWebImage": [
          "~> 5.0"
        ],
        "SDWebImageWebPCoder": [

        ],
        "JXPhotoBrowser": [
          "~> 3.0"
        ],
        "PureLayout": [

        ],
        "MJRefresh": [

        ],
        "CryptoSwift": [
          "~> 1.4.1"
        ],
        "Zip": [
          "~> 2.1"
        ],
        "ZLPhotoBrowser": [

        ],
        "KTVHTTPCache": [
          "~> 2.0.0"
        ],
        "SQLite.swift": [
          "~> 0.13.2"
        ],
        "SwiftyRSA": [

        ],
        "SocketRocket": [

        ]
      }
    },
    {
      "name": "Resources",
      "resource_bundles": {
        "Evoker": [
          "iOS/Evoker/Sources/Resources/Image.xcassets",
          "iOS/Evoker/Sources/Resources/Sound/**/*",
          "iOS/Evoker/Sources/Resources/SDK/**/*"
        ]
      }
    },
    {
      "name": "Map",
      "source_files": [
        "iOS/EvokerMap/Sources/**/*.swift"
      ],
      "dependencies": {
        "Evoker/Core": [
          "0.12.0"
        ],
        "PureLayout": [

        ],
        "AMap3DMap": [

        ],
        "AMapSearch": [

        ],
        "AMapLocation": [

        ]
