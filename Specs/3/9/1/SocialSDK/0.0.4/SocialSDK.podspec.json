{
  "name": "SocialSDK",
  "version": "0.0.4",
  "summary": "SocialSDK SDK for iOS",
  "homepage": "https://github.com/GagSquad/SocialSDK",
  "authors": {
    "itlijunjie@gmail.com": "https://github.com/itlijunjie",
    "LVJIALIN": "https://github.com/LVJIALIN"
  },
  "license": {
    "type": "WTFPL",
