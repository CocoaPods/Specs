  "summary": "Swift implementation of Snapify architecture.",
  "homepage": "https://github.com/gsagadyn/Snapify",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Grzegorz Sagadyn": "sagadyn@gmail.com"
  },
