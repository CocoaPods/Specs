  "subspecs": [
    {
      "name": "Extension",
      "source_files": "Sakura/Extension/**/*.{h,swift}",
      "preserve_paths": "Modules/*",
      "pod_target_xcconfig": {
        "SWIFT_INCLUDE_PATHS": "$(PODS_ROOT)/Sakura/Modules"
      },
      "frameworks": "UIKit",
      "libraries": "z"
    },
    {
      "name": "UI",
      "source_files": "Sakura/UI/**/*.{h,swift}"
    },
    {
