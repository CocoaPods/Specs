            },
            {
              "name": "CloudKit",
              "source_files": "YapDatabase/Extensions/CloudKit/**/*.{h,m,mm,c}"
            },
            {
              "name": "RTreeIndex",
              "source_files": "YapDatabase/Extensions/RTreeIndex/**/*.{h,m,mm,c}"
            },
            {
              "name": "ConnectionProxy",
              "source_files": "YapDatabase/Extensions/ConnectionProxy/**/*.{h,m,mm,c}"
            }
          ]
        }
      ]
    }
  ]
}
