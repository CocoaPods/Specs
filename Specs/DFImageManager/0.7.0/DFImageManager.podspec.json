      },
      "source_files": "DFImageManager/Source/NSURLSession/**/*.{h,m}"
    },
    {
      "name": "AFNetworking",
      "dependencies": {
        "DFImageManager/Core": [

        ],
        "AFNetworking/NSURLSession": [
          "~> 2.0"
        ]
      },
      "source_files": "DFImageManager/Source/AFNetworking/**/*.{h,m}"
    },
    {
      "name": "PhotosKit",
      "dependencies": {
        "DFImageManager/Core": [

        ]
      },
      "source_files": "DFImageManager/Source/PhotosKit/**/*.{h,m}"
    },
    {
      "name": "GIF",
      "dependencies": {
        "DFImageManager/Core": [

        ],
        "FLAnimatedImage": [
