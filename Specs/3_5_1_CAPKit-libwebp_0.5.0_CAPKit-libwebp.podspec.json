{
  "name": "CAPKit-libwebp",
  "version": "0.5.0",
  "summary": "Library to encode and decode images in WebP format.",
  "homepage": "https://developers.google.com/speed/webp/",
  "license": {
    "type": "BSD",
    "file": "COPYING"
  },
  "authors": "Google Inc.",
  "platforms": {
    "ios": "7.0.0"
  },
  "source": {
    "git": "https://github.com/successinfo-org/CAPKit-libwebp.git",
    "tag": "v0.5.0"
  },
  "compiler_flags": "-D_THREAD_SAFE",
  "subspecs": [
    {
      "name": "webp",
      "source_files": "src/webp/*.h",
      "header_dir": "webp"
    },
    {
      "name": "core",
      "source_files": [
        "src/webp/*.h",
        "src/utils/*.{h,c}",
        "src/dsp/*.{h,c}",
        "src/enc/*.{h,c}",
        "src/dec/*.{h,c}"
      ],
      "header_dir": "webp"
    }
  ]
}
