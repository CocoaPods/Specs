{
  "name": "YunoSDK",
  "version": "1.1.8",
  "summary": "A short description of YunoSDK.",
  "description": "A fast checkout that integrates your company\nto the payments and fraud ecosystem.\n\nCheckout + Routing + Reconciliations + Alternative Payment Methods",
  "homepage": "https://www.y.uno/",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Jonathan Pacheco": "jonathan.pacheco@y.uno.com"
  },
  "source": {
    "http": "https://github.com/yuno-payments/yuno-sdk-ios/releases/download/1.1.8/YunoSDK.xcframework.zip"
  },
  "platforms": {
    "ios": "13.0"
  },
  "swift_versions": "5.4",
  "requires_arc": true,
  "vendored_frameworks": "YunoSDK.xcframework",
  "resource_bundles": {
    "Resources": [
      "YunoSDK.xcframework/Assets/**/*.xcassets",
      "YunoSDK.xcframework/Assets/Localization/*.lproj",
      "YunoSDK.xcframework/Assets/**/*.{storyboard,xib,json}"
    ]
  },
  "frameworks": "Combine",
  "xcconfig": {
    "SWIFT_INCLUDE_PATHS": [
      "${PODS_XCFRAMEWORKS_BUILD_DIR}/YunoSDK"
    ]
  },
  "pod_target_xcconfig": {
    "BUILD_LIBRARY_FOR_DISTRIBUTION": "YES"
  },
  "swift_version": "5.4"
}
