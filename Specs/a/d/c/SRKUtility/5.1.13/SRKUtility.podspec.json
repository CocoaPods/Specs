{
  "name": "SRKUtility",
  "version": "5.1.13",
  "summary": "A pod which helps you to easily save values to NSUserDefaults, display progressHUD and check Network Reachability. base64 data conversion.",
  "authors": {
    "Sagar Kothari": "sag333ar@gmail.com"
  },
  "homepage": "http://sagarrkothari.com",
  "license": {
    "type": "MIT"
  },
  "platforms": {
    "ios": "9.0"
  },
  "source": {
    "git": "https://github.com/sag333ar/SRKUtility.git",
    "branch": "master",
    "tag": "5.1.13"
  },
  "social_media_url": "https://twitter.com/sag333ar",
  "documentation_url": "https://github.com/sag333ar/SRKUtility/wiki",
  "frameworks": [
    "UIKit",
    "Foundation",
    "AVFoundation"
  ],
  "requires_arc": true,
  "dependencies": {
    "MBProgressHUD": [

    ],
    "KSReachability": [

    ],
    "ADALiOS": [
      "~> 1.2"
    ]
  },
  "subspecs": [
    {
      "name": "Camera",
      "resources": "SRKUtility/Camera/SRKImages/*.png",
      "source_files": [
        "SRKUtility/Camera/SRKClasses/*.{h,m}",
        "SRKUtility/Camera/SRKCamera.swift"
      ]
    },
    {
      "name": "Request",
      "source_files": "SRKUtility/RequestManager/SRKRequestManager.swift"
    },
    {
      "name": "DeviceType",
      "source_files": "SRKUtility/Device/SRKDeviceType.swift"
    },
    {
      "name": "Utilities",
      "source_files": "SRKUtility/Utility/SRKUtility.swift"
    },
    {
      "name": "Strings",
      "source_files": "SRKUtility/Strings/StringHelper.swift"
    },
    {
      "name": "ImageView",
      "source_files": "SRKUtility/UIImageView/UIImageHelper.swift"
    },
    {
      "name": "Controls",
      "source_files": "SRKUtility/Controls/*.{swift}",
      "resource_bundles": {
        "Controls": [
          "SRKUtility/Controls/*.{xib}"
        ]
      }
    },
    {
      "name": "O365",
      "source_files": [
        "SRKUtility/O365/Helpers/*.{h,m}",
        "SRKUtility/O365/Model/*.{h,m}"
      ]
    }
  ]
}
