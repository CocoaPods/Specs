{
  "name": "ios-device-lib",
  "version": "3.1.8",
  "summary": "Secure Tokenized Payments by Global Payments Systems.",
  "description": "Secure Tokenized Payments for iOS by Global Payments Systems.",
  "homepage": "https://github.com/globalpayments/ios-device-lib",
  "license": "EULA",
  "authors": {
    "Heartland Developer Portal": "EntApp_DevPortal@e-hps.com"
  },
  "source": {
