{
  "name": "adquanta-ads-sdk",
  "version": "1.0.1",
  "summary": "AdQuanta Ads SDK for iOS - 专业的移动广告聚合SDK",
  "description": "AdQuanta Ads SDK 是一个功能强大的移动广告聚合SDK，支持多种广告形式，包括：\n- 开屏广告（Splash Ad）\n- Banner 广告\n- 插屏广告（Interstitial Ad）\n- 激励视频广告（Rewarded Ad）\n- 积分墙广告（Offerwall）\n\nSDK 集成了多个主流广告平台，提供统一的API接口，简化广告集成流程。",
  "homepage": "https://github.com/Ad-Quanta/adquanta-sdk-ios",
  "license": {
    "type": "GPL-3.0",
    "file": "LICENSE"
  },
  "authors": {
    "adquanta": "support@adoptrack.com"
  },
  "source": {
    "git": "https://github.com/Ad-Quanta/adquanta-sdk-ios.git",
    "tag": "1.0.1"
  },
  "platforms": {
    "ios": "14.0"
  },
  "requires_arc": true,
  "dependencies": {
    "Google-Mobile-Ads-SDK": [
      "~> 12.0"
    ]
  },
  "vendored_frameworks": [
    "adquanta_ads_sdk.framework",
    "Frameworks/TradPlusAds.xcframework",
    "Frameworks/TPCrossAdapter.xcframework",
    "Frameworks/TPAdMobAdapter.xcframework",
    "Frameworks/TPGoogleIMAAdapter.xcframework",
    "Frameworks/GoogleInteractiveMediaAds.xcframework"
  ],
  "preserve_paths": [
    "adquanta_ads_sdk.framework",
    "Frameworks/*.xcframework"
  ],
  "frameworks": [
    "Foundation",
    "UIKit",
    "CoreGraphics",
    "SystemConfiguration",
    "CoreTelephony",
    "AdSupport",
    "CoreLocation",
    "StoreKit",
    "AVFoundation",
    "WebKit",
    "MediaPlayer",
    "SafariServices",
    "AppTrackingTransparency"
  ],
  "pod_target_xcconfig": {
    "OTHER_LDFLAGS": "-ObjC",
    "VALID_ARCHS": "arm64",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64 x86_64 i386",
    "FRAMEWORK_SEARCH_PATHS": "$(inherited) $(PODS_ROOT)/adquanta-ads-sdk $(PODS_ROOT)/adquanta-ads-sdk/Frameworks"
  },
  "documentation_url": "https://github.com/Ad-Quanta/adquanta-sdk-ios"
}
