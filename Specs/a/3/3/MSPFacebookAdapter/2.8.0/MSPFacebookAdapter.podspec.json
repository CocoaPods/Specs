{
  "name": "MSPFacebookAdapter",
  "version": "2.8.0",
  "summary": "an adapter for Meta SDK",
  "description": "an adapter for Meta SDK for MSP S2S",
  "homepage": "https://github.com/aimsp/msp-ios-sdk/MetaAdapter",
  "license": "Copyright",
  "authors": {
    "huanzhiNB": "huanzhi.zhang@newsbreak.com"
  },
  "source": {
    "http": "https://github.com/ParticleMedia/msp-ios-sdk-public/releases/download/2.8.0/MSPFacebookAdapter-2.8.0.zip",
    "type": "zip"
  },
  "platforms": {
    "ios": "13.0"
  },
  "source_files": "MSPFacebookAdapter/MSPFacebookAdapter/**/*.{h,m,swift}",
  "exclude_files": "Classes/Exclude",
  "dependencies": {
    "FBAudienceNetwork": [
      "~> 6.15"
    ],
    "MSPSharedLibraries": [
      "2.8.0"
    ]
  },
  "pod_target_xcconfig": {
    "VALID_ARCHS": "x86_64 armv7 arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "static_framework": true
}
