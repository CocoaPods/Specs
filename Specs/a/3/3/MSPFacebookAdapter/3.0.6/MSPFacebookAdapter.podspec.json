{
  "name": "MSPFacebookAdapter",
  "version": "3.0.6",
  "summary": "an adapter for Meta SDK",
  "description": "an adapter for Meta SDK for MSP S2S",
  "homepage": "https://github.com/aimsp/msp-ios-sdk/MetaAdapter",
  "license": "Copyright",
  "authors": {
    "huanzhiNB": "huanzhi.zhang@newsbreak.com"
  },
  "source": {
    "http": "https://github.com/ParticleMedia/msp-ios-sdk-public/releases/download/3.0.6/MSPFacebookAdapter-3.0.6.zip",
    "type": "zip"
  },
  "platforms": {
    "ios": "15.0"
  },
  "source_files": "MSPFacebookAdapter/MSPFacebookAdapter/**/*.{h,m,swift}",
  "exclude_files": "Classes/Exclude",
  "dependencies": {
    "FBAudienceNetwork": [
      ">= 6.15",
      "< 6.21"
    ],
    "MSPSharedLibraries": [
      "3.0.6"
    ]
  },
  "pod_target_xcconfig": {
    "VALID_ARCHS": "x86_64 armv7 arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "static_framework": true
}
