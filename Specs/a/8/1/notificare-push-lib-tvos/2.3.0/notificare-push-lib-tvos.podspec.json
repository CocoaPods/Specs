{
  "name": "notificare-push-lib-tvos",
  "version": "2.3.0",
  "summary": "Notificare Library for tvOS apps",
  "description": "Notificare tvOS Library implements the power of smart notifications, location services, contextual marketing and powerful loyalty solutions provided by the Notificare platform in tvOS applications.\n\nFor documentation please refer to: http://docs.notifica.re\n\nFor support please use: http://support.notifica.re",
  "homepage": "https://notificare.com",
  "license": {
    "type": "Commercial",
    "text": "All rights reserved"
  },
  "authors": {
    "Notificare": "info@notifica.re"
  },
  "platforms": {
    "tvos": "10.0"
  },
  "source": {
    "git": "https://github.com/Notificare/notificare-push-lib-tvos.git",
    "tag": "2.3.0"
  },
  "source_files": "NotificarePushLib/*.h",
  "resources": "DefaultTheme.bundle",
  "preserve_paths": "libNotificarePushLib.a",
  "frameworks": [
    "AVFoundation",
    "CoreLocation",
    "UserNotifications",
    "CoreMedia",
    "MapKit",
    "SystemConfiguration",
    "Security",
    "CFNetwork",
    "ImageIO",
    "StoreKit"
  ],
  "libraries": [
    "icucore",
    "NotificarePushLib"
  ],
  "xcconfig": {
    "LIBRARY_SEARCH_PATHS": "\"$(PODS_ROOT)/notificare-push-lib-tvos\"",
    "OTHER_LDFLAGS": "-ObjC"
  },
  "requires_arc": true
}
