{
  "name": "AppleSwiftMDWrapper",
  "version": "1.0.0",
  "summary": "CocoaPods wrapper for Apple's official swift-markdown library.",
  "description": "Provides the Markdown module for parsing, building, editing, and analyzing Markdown documents.\nForked from apple/swift-markdown with CocoaPods support.",
  "homepage": "https://github.com/zjc19891106/swift-markdown-cocoapods",
  "license": {
    "type": "Apache-2.0",
    "file": "LICENSE.txt"
  },
  "authors": {
    "Apple": "apple@apple.com",
    "zjc19891106": "your@email.com"
  },
  "source": {
    "git": "https://github.com/zjc19891106/swift-markdown-cocoapods.git",
    "tag": "1.0.0"
  },
  "platforms": {
    "ios": "15.0"
  },
  "requires_arc": true,
  "module_name": "Markdown",
  "source_files": [
    "Sources/Markdown/*.swift",
    "Sources/CAtomic/**/*.{h,c}",
    "swift-cmark/src/**/*.{h,c}",
    "swift-cmark/extensions/**/*.{h,c}"
  ],
  "preserve_paths": [
    "README.md",
    "swift-cmark/src/*.inc",
    "swift-cmark/COPYING",
    "swift-cmark/README.md"
  ]
}
