{
  "name": "RxQuery",
  "version": "1.8.0",
  "summary": "Extension of RxSwift pod",
  "homepage": "https://github.com/gsagadyn/RxQuery",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Grzegorz Sagadyn": "sagadyn@gmail.com"
  },
  "source": {
    "git": "https://github.com/gsagadyn/RxQuery.git",
    "tag": "1.8.0"
  },
  "swift_versions": "5.1",
  "platforms": {
    "ios": "11.0"
  },
  "frameworks": [
    "UIKit",
    "Foundation"
  ],
  "requires_arc": true,
  "default_subspecs": [
    "Core",
    "Http"
  ],
  "dependencies": {
    "RxSwift": [
      "~> 5.1"
    ]
  },
  "subspecs": [
    {
      "name": "Core",
      "source_files": "RxQuery/Core/**/*"
    },
    {
      "name": "Http",
      "source_files": [
        "RxQuery/Core/**/*",
        "RxQuery/Http/**/*"
      ],
      "dependencies": {
        "Alamofire": [
          "~> 5.0"
        ]
      }
    }
  ],
  "swift_version": "5.1"
}
