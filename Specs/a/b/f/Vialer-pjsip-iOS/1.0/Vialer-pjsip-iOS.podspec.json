{
  "name": "Vialer-pjsip-iOS",
  "version": "1.0",
  "summary": "PJSIP 2.5.5 for iOS",
  "description": "Pod for distributing PJSIP wrapped in a framework.",
  "homepage": "https://www.wearespindle.com",
  "license": {
    "type": "GNU GPLv3",
    "file": "LICENSE"
  },
  "authors": {
    "Devhouse Spindle": "hello@wearespindle.com"
  },
  "source": {
    "git": "https://github.com/VoIPGRID/Vialer-pjsip-iOS.git",
    "tag": "1.0"
  },
  "social_media_url": "https://twitter.com/wearespindle",
  "platforms": {
