{
  "name": "GDTMobSDK",
  "version": "4.14.40",
  "summary": "GDTMobSDK.",
  "description": "GDTMobSDK 广点通广告SDK。",
  "homepage": "http://e.qq.com/dev/index.html",
  "license": {
