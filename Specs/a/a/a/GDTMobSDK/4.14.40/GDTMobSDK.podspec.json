  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "frameworks": [
    "StoreKit",
    "Security",
    "CoreTelephony",
    "AdSupport",
    "CoreLocation",
