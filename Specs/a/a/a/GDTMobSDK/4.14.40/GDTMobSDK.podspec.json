  "xcconfig": {
    "HEADER_SEARCH_PATHS": "$(SDKROOT)/usr/include/libxml2"
  },
  "default_subspecs": "GDTMobSDK",
  "subspecs": [
    {
      "name": "GDTMobSDK",
      "source_files": "lib/*.h",
      "libraries": [
        "z",
        "xml2",
        "sqlite3"
      ],
      "vendored_libraries": "lib/*.a"
    }
