{
  "name": "SudMGPWrapper_Lite",
  "version": "1.3.5.1",
  "summary": "This is a Wrapper for SudMGP SDK",
  "description": "This is a Wrapper for SudMGP SDK, To enable developers to quickly access the SudMGP SDK",
  "homepage": "https://github.com/SudTechnology/SudMGPWrapper",
  "license": {
    "type": "MIT",
    "file": "SudMGPWrapper/LICENSE"
  },
  "authors": {
    "sud.tech": "dev@sud.tech"
  },
  "source": {
    "git": "https://github.com/SudTechnology/SudMGPWrapper.git",
    "tag": "v1.3.5.1"
  },
  "frameworks": [
    "Foundation",
    "UIKit"
  ],
  "requires_arc": true,
  "platforms": {
    "ios": "11.0"
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "public_header_files": "SudMGPWrapper/**/*.h",
  "source_files": [
    "SudMGPWrapper/*.{h,m,mm,cpp,c,hpp,cc,swift}",
    "SudMGPWrapper/Decorator/**/*.{h,m,mm,cpp,c,hpp,cc,swift}",
    "SudMGPWrapper/Model/**/*.{h,m,mm,cpp,c,hpp,cc,swift}",
    "SudMGPWrapper/State/**/*.{h,m,mm,cpp,c,hpp,cc,swift}"
  ],
  "dependencies": {
    "SudMGP_Lite": [
      "~> 1.3.5"
    ],
    "MJExtension": [
      "~> 3.4.1"
    ]
  }
}
