{
  "name": "DITranquillity",
  "version": "4.1.0",
  "summary": "DITranquillity - Dependency injection for iOS/macOS/tvOS (Swift)",
  "description": "DITranquillity - The small library for dependency injection in applications written on pure Swift for iOS/OSX/tvOS. Despite its size, it solves a large enough range of tasks, including Storyboard support. Its main advantage - modularity of support, detailed logs and lots of opportunities.",
  "homepage": "https://github.com/ivlevAstef/DITranquillity",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "documentation_url": "https://htmlpreview.github.io/?https://github.com/ivlevAstef/DITranquillity/blob/master/Documentation/code/index.html",
  "authors": {
    "Alexander.Ivlev": "ivlev.stef@gmail.com"
  },
  "source": {
    "git": "https://github.com/ivlevAstef/DITranquillity.git",
    "tag": "v4.1.0"
  },
  "requires_arc": true,
  "swift_versions": "5.1",
  "dependencies": {
    "SwiftLazy": [
      ">= 1.1.6"
    ]
  },
  "platforms": {
    "ios": "8.0",
    "tvos": "9.0",
    "osx": "10.10",
    "watchos": "2.0"
  },
  "ios": {
    "source_files": [
      "Sources/DITranquillity.h",
      "Sources/Core/**/*.swift",
      "Sources/GraphAPI/*.swift",
      "Sources/UI/Swizzling/*.{h,m,swift}",
      "Sources/UI/Storyboard/*.{h,m,swift}",
      "Sources/UI/InjectIntoSubviews/*.{h,m,swift}",
      "Sources/UI/Storyboard/iOS-tvOS/*.{h,m,swift}",
      "Sources/UI/InjectIntoSubviews/iOS-tvOS/*.{h,m,swift}"
    ]
  },
  "tvos": {
    "source_files": [
      "Sources/DITranquillity.h",
      "Sources/Core/**/*.swift",
      "Sources/GraphAPI/*.swift",
      "Sources/UI/Swizzling/*.{h,m,swift}",
      "Sources/UI/Storyboard/*.{h,m,swift}",
      "Sources/UI/InjectIntoSubviews/*.{h,m,swift}",
      "Sources/UI/Storyboard/iOS-tvOS/*.{h,m,swift}",
      "Sources/UI/InjectIntoSubviews/iOS-tvOS/*.{h,m,swift}"
    ]
  },
  "osx": {
    "source_files": [
      "Sources/DITranquillity.h",
      "Sources/Core/**/*.swift",
      "Sources/GraphAPI/*.swift",
      "Sources/UI/Swizzling/*.{h,m,swift}",
      "Sources/UI/Storyboard/*.{h,m,swift}",
      "Sources/UI/InjectIntoSubviews/*.{h,m,swift}",
      "Sources/UI/Storyboard/OSX/*.{h,m,swift}"
    ]
  },
  "watchos": {
    "source_files": [
      "Sources/DITranquillity.h",
      "Sources/Core/**/*.swift",
      "Sources/GraphAPI/*.swift",
      "Sources/UI/Swizzling/*.{h,m,swift}"
    ]
  },
  "swift_version": "5.1"
}
