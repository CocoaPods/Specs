{
  "name": "Flipper",
  "version": "0.46.0",
  "license": {
    "type": "MIT"
  },
  "homepage": "https://github.com/facebook/sonar",
  "summary": "SonarKit core cpp code with network implementation",
  "authors": "Facebook",
  "source": {
    "git": "https://github.com/facebook/Sonar.git",
    "tag": "v0.46.0"
  },
  "module_name": "Flipper",
  "public_header_files": [
    "xplat/Flipper/*.h",
    "xplat/utils/*.h"
  ],
  "source_files": [
    "xplat/Flipper/*.{h,cpp,m,mm}",
    "xplat/Flipper/utils/*.{h,cpp,m,mm}"
  ],
  "libraries": "stdc++",
  "dependencies": {
    "Flipper-Folly": [
      "~> 2.2"
    ],
    "Flipper-RSocket": [
      "~> 1.1"
    ]
  },
  "compiler_flags": "-DFLIPPER_OSS=1 -DFB_SONARKIT_ENABLED=1 -DFOLLY_NO_CONFIG -DFOLLY_MOBILE=1 -DFOLLY_USE_LIBCPP=1 -DFOLLY_HAVE_LIBGFLAGS=0 -DFOLLY_HAVE_LIBJEMALLOC=0 -DFOLLY_HAVE_PREADV=0 -DFOLLY_HAVE_PWRITEV=0 -DFOLLY_HAVE_TFO=0 -DFOLLY_USE_SYMBOLIZER=0 -Wall\n    -std=c++14\n    -Wno-global-constructors",
  "platforms": {
    "ios": "8.0"
  },
  "pod_target_xcconfig": {
    "USE_HEADERMAP": "NO",
    "CLANG_CXX_LANGUAGE_STANDARD": "c++14",
    "HEADER_SEARCH_PATHS": "\"$(PODS_TARGET_SRCROOT)\" \"$(PODS_ROOT)/boost-for-react-native\" \"$(PODS_ROOT)/Flipper-RSocket\" \"$(PODS_ROOT)/Flipper-DoubleConversion\""
  }
}
