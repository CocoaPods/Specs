        "SFFoundation/Vendor/XML-to-NSDictionary/*.{h,m}",
        "SFFoundation/SFFoundation/SFObjcProperty.{h,m}",
        "SFFoundation/SFFoundation/SFRuntimeUtils.{h,m}"
      ],
      "requires_arc": true
    },
    {
      "name": "iOSKit",
      "dependencies": {
        "SFFoundation/arc": [

        ]
      },
      "frameworks": [
        "CoreText",
        "QuartzCore"
      ],
      "source_files": "SFiOSKit/**/*.{h,m}",
      "osx": {
        "exclude_files": "SFiOSKit/**/*.{h,m}"
      },
      "requires_arc": true
    }
  ]
