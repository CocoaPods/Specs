{
  "name": "KalturaPlayer",
  "version": "4.0.0.beta.2",
  "summary": "KalturaPlayer -- Kaltura Player for iOS",
  "homepage": "https://github.com/kaltura/kaltura-player-ios",
  "license": {
    "type": "AGPLv3",
    "file": "LICENSE"
  },
  "authors": {
    "Kaltura": "community@kaltura.com"
  },
  "source": {
    "git": "https://github.com/kaltura/kaltura-player-ios.git",
    "tag": "v4.0.0.beta.2"
  },
  "swift_versions": "5.0",
  "platforms": {
    "ios": "10.0",
    "tvos": "10.0"
  },
  "default_subspecs": "Core",
  "subspecs": [
    {
      "name": "Core",
      "source_files": [
        "Sources/*",
        "Sources/Basic/*"
      ],
      "dependencies": {
        "PlayKit": [
          "~> 3.19"
        ]
      }
    },
    {
      "name": "OTT",
      "source_files": [
        "Sources/OTT/*",
        "Sources/Common"
      ],
      "resources": "Sources/OTT/*.xcdatamodeld",
      "dependencies": {
        "KalturaPlayer/Core": [

        ],
        "PlayKitProviders": [
          "~> 1.10"
        ],
        "PlayKitKava": [
          "~> 1.6"
        ]
      }
    },
    {
      "name": "OVP",
      "source_files": [
        "Sources/OVP/*",
        "Sources/Common"
      ],
      "resources": "Sources/OVP/*.xcdatamodeld",
      "dependencies": {
        "KalturaPlayer/Core": [

        ],
        "PlayKitProviders": [
          "~> 1.10"
        ],
        "PlayKitKava": [
          "~> 1.6"
        ]
      }
    }
  ],
  "swift_version": "5.0"
}
