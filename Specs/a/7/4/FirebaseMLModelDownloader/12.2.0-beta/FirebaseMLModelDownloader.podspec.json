{
  "name": "FirebaseMLModelDownloader",
  "version": "12.2.0-beta",
  "summary": "Firebase ML Model Downloader",
  "description": "This is the new ML Model Downloader CocoaPod.",
  "homepage": "https://firebase.google.com",
  "license": {
    "type": "Apache-2.0",
    "file": "LICENSE"
  },
  "authors": "Google, Inc.",
  "source": {
    "git": "https://github.com/firebase/firebase-ios-sdk.git",
    "tag": "CocoaPods-12.2.0-beta"
  },
  "social_media_url": "https://twitter.com/Firebase",
  "swift_versions": "5.9",
  "platforms": {
    "ios": "15.0",
    "osx": "10.15",
    "tvos": "15.0",
    "watchos": "7.0"
  },
  "cocoapods_version": ">= 1.12.0",
  "prefix_header_file": false,
  "source_files": [
    "FirebaseMLModelDownloader/Sources/**/*.swift"
  ],
  "frameworks": "Foundation",
  "dependencies": {
    "FirebaseCore": [
      "~> 12.2.0"
    ],
    "FirebaseCoreExtension": [
      "~> 12.2.0"
    ],
    "FirebaseInstallations": [
      "~> 12.2.0"
    ],
    "GoogleDataTransport": [
      "~> 10.1"
    ],
    "GoogleUtilities/UserDefaults": [
      "~> 8.1"
    ],
    "SwiftProtobuf": [
      "~> 1.19"
    ]
  },
  "pod_target_xcconfig": {
    "GCC_PREPROCESSOR_DEFINITIONS": "FIRMLModelDownloader_VERSION=12.2.0-beta",
    "OTHER_CFLAGS": "-fno-autolink"
  },
  "testspecs": [
    {
      "name": "unit",
      "test_type": "unit",
      "scheme": {
        "code_coverage": true
      },
      "platforms": {
        "ios": "15.0",
        "osx": "10.15",
        "tvos": "15.0"
      },
      "source_files": "FirebaseMLModelDownloader/Tests/Unit/**/*.swift",
      "requires_app_host": true
    },
    {
      "name": "integration",
      "test_type": "unit",
      "scheme": {
        "code_coverage": true
      },
      "platforms": {
        "ios": "15.0",
        "osx": "10.15",
        "tvos": "15.0"
      },
      "source_files": "FirebaseMLModelDownloader/Tests/Integration/**/*.swift",
      "resources": "FirebaseMLModelDownloader/Tests/Integration/Resources/GoogleService-Info.plist",
      "requires_app_host": true
    }
  ],
  "swift_version": "5.9"
}
