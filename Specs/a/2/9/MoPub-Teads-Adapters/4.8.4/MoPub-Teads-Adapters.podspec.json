{
  "name": "MoPub-Teads-Adapters",
  "version": "4.8.4",
  "summary": "MoPub Adapter for Teads' iOS SDK",
  "module_name": "TeadsMoPubAdapter",
  "description": "Use this adapter to include MoPub as a demand source in your mediation waterfall",
  "homepage": "https://github.com/teads/TeadsSDK-adapter-mopub-ios",
  "documentation_url": "https://support.teads.tv/support/solutions/articles/36000166728-twitter-mopub-mediation-ios",
  "license": {
    "type": "Copyright",
    "text": "Copyright Teads 2021"
  },
  "authors": {
    "Teads": "support-sdk@teads.tv"
  },
  "source": {
    "git": "https://github.com/teads/TeadsSDK-adapter-mopub-ios.git",
    "tag": "v4.8.4"
  },
  "platforms": {
    "ios": "10.0"
  },
  "static_framework": true,
  "requires_arc": true,
  "source_files": "TeadsMoPubAdapter/**/*{.swift}",
  "swift_versions": [
    "4.3",
    "5.0",
    "5.1"
  ],
  "dependencies": {
    "TeadsSDK": [
      "4.8.4"
    ],
    "mopub-ios-sdk": [
      ">= 5.13"
    ]
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64 arm64e armv7 armv7s",
    "EXCLUDED_ARCHS[sdk=iphoneos*]": "i386 x86_64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64 arm64e armv7 armv7s",
    "EXCLUDED_ARCHS[sdk=iphoneos*]": "i386 x86_64"
  },
  "swift_version": "5.1"
}
