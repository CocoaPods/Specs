{
  "name": "Peak",
  "version": "1.2.3",
  "summary": "A collection of iOS and OS X audio tools, written in Swift.",
  "homepage": "https://github.com/hoseking/Peak",
  "license": "MIT",
  "authors": {
    "hoseking": "steviehosking@gmail.com"
  },
  "platforms": {
    "ios": "8.0",
    "osx": "10.9"
  },
  "source": {
    "git": "https://github.com/hoseking/Peak.git",
    "tag": "1.2.3"
  },
  "frameworks": "AudioToolbox",
  "default_subspecs": "Audio",
  "subspecs": [
    {
      "name": "Audio",
      "source_files": "Source/Audio/**/*"
    },
    {
      "name": "AudioFile",
      "source_files": "Source/AudioFile/**/*"
    },
    {
      "name": "MIDI",
      "source_files": "Source/MIDI/**/*"
    }
  ]
}
