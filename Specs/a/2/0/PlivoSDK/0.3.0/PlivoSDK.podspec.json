{
  "name": "PlivoSDK",
  "version": "0.3.0",
  "summary": "Plivo iOS SDK.",
  "description": "                   The Plivo iOS SDK allows you to create applications\n                   capable of making and receiving calls from iPhone and iPad.\n\n",
  "homepage": "http://plivo.com/docs/sdk/ios/",
  "license": {
    "type": "Commercial",
    "text": ""
  },
  "authors": {
    "Plivo": "hello@plivo.com"
  },
  "platforms": {
    "ios": "6.0"
  },
  "source": {
    "http": "https://s3.amazonaws.com/plivosdk/ios/PlivoSDK-0.3.tar.bz2"
  },
  "source_files": "Headers",
  "preserve_paths": "Libraries/*.a",
  "libraries": "PlivoEndpoint",
  "frameworks": [
    "AudioToolbox",
    "AVFoundation",
    "SystemConfiguration",
    "MobileCoreServices"
  ],
  "xcconfig": {
    "LIBRARY_SEARCH_PATHS": "$(PODS_ROOT)/PlivoSDK/Libraries",
    "OTHER_LD_FLAGS": "$(inherited) -ObjC -all_load"
  },
  "requires_arc": false
}
