  "user_target_xcconfig": {
    "HEADER_SEARCH_PATHS": "$(inherited) $(PODS_ROOT)/GSLCrypto/CommonCrypto"
  },
  "source_files": "GSLCrypto/*.{h,m}",
  "preserve_paths": "CommonCrypto/module.modulemap"
}
