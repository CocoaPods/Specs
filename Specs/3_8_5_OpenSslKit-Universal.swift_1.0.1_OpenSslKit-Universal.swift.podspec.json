{
  "name": "OpenSslKit-Universal.swift",
  "module_name": "OpenSslKit",
  "version": "1.0.1",
  "summary": "OpenSsl crypto library with base58 conversion for Swift. Supports iOS & OSX",
  "description": "OpenSslKit includes crypto functions that can be used in pure Swift. It supports openssl, base58, sha3 keccak256.",
  "homepage": "https://github.com/cuhte3/open-ssl-kit-universal",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Horizontal Systems": "hsdao@protonmail.ch",
    "farid": "farid@getportal.co"
  },
  "source": {
    "git": "https://github.com/cuhte3/open-ssl-kit-universal.git",
    "tag": "1.0.1"
  },
  "social_media_url": "http://horizontalsystems.io/",
  "platforms": {
    "ios": "11.0",
    "osx": "10.14"
  },
  "swift_versions": "5",
  "source_files": "OpenSslKit/Classes/**/*",
  "preserve_paths": [
    "OpenSslKit/Libraries"
  ],
  "ios": {
    "pod_target_xcconfig": {
      "HEADER_SEARCH_PATHS": "\"${PODS_TARGET_SRCROOT}/OpenSslKit/Libraries/ios/include\"",
      "LIBRARY_SEARCH_PATHS": "\"${PODS_TARGET_SRCROOT}/OpenSslKit/Libraries/ios/lib\""
    },
    "vendored_libraries": "OpenSslKit/Libraries/ios/lib/libcrypto.a"
  },
  "osx": {
    "pod_target_xcconfig": {
      "HEADER_SEARCH_PATHS": "\"${PODS_TARGET_SRCROOT}/OpenSslKit/Libraries/osx/include\"",
      "LIBRARY_SEARCH_PATHS": "\"${PODS_TARGET_SRCROOT}/OpenSslKit/Libraries/osx/lib\""
    },
    "vendored_libraries": "OpenSslKit/Libraries/osx/lib/libcrypto.a"
  },
  "swift_version": "5"
}
