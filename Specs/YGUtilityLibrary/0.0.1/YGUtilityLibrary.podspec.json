    {
      "name": "RTDebugSupport",
      "source_files": "YGUtilityLibrary/RTDebugSupport/RTDebugSupport/*.{h,m}",
      "requires_arc": true
    },
    {
      "name": "RTDictionaryObjectMapper",
      "dependencies": {
        "YGUtilityLibrary/RTCategory": [

        ]
