{
  "name": "HMLogger",
  "version": "2.0.4",
  "summary": "simple, pretty and powerful logger for iOS with piker、viewer、console",
  "homepage": "https://github.com/393385724/HMLogger",
  "license": "MIT",
  "authors": {
    "llg": "393385724@qq.com"
  },
  "source": {
    "git": "https://github.com/393385724/HMLogger.git",
    "tag": "2.0.4"
  },
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "source_files": [
    "HMLogger/*.{h,m,mm}",
    "HMLogger/Mars/*.{h,cc}"
  ],
  "public_header_files": [
    "HMLogger/HMLogger.h",
    "HMLogger/HMLoggerDefine.h"
  ],
  "ios": {
    "vendored_frameworks": "HMLogger/Mars/*.framework",
    "resources": [
      "HMLogger/*.png",
      "HMLogger/*.xib"
    ]
  },
  "frameworks": [
    "CoreTelephony",
    "SystemConfiguration",
    "Foundation",
    "CoreGraphics",
    "UIKit"
  ],
  "libraries": [
    "z",
    "stdc++"
  ],
  "user_target_xcconfig": {
    "ENABLE_BITCODE": "NO"
  },
  "dependencies": {
    "ICTextView": [
      "~> 2.0.1"
    ],
    "CocoaLumberjack": [
      "~> 3.2.0"
    ]
  }
}
