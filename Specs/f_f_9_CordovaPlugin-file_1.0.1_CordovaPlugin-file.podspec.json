{
  "name": "CordovaPlugin-file",
  "version": "1.0.1",
  "summary": "Mirror of Apache Cordova Plugin file",
  "description": "                   A longer description of CordovaPlugin-file in Markdown format.\n",
  "homepage": "https://github.com/apache/CordovaPlugin-file",
  "license": {
    "type": "Apache License, Version 2.0",
    "file": "LICENSE"
  },
  "authors": {
    "The Apache Software Foundation": "email@address.com"
  },
  "platforms": {
    "ios": "5.0"
  },
  "source": {
    "git": "https://github.com/apache/cordova-plugin-file.git",
    "tag": "r1.0.1"
  },
  "source_files": "src/ios/*.{h,m}",
  "preserve_paths": [
    "*.md",
    "www/*.js",
    "www/ios/*.js"
  ],
  "frameworks": [
    "AssetsLibrary",
    "MobileCoreServices"
  ],
  "requires_arc": true,
  "dependencies": {
    "Cordova": [
      ">= 3.0.0",
      "<= 3.4.0"
    ]
