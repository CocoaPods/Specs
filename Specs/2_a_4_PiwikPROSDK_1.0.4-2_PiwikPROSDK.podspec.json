{
  "name": "PiwikPROSDK",
  "version": "1.0.4-2",
  "summary": "Piwik PRO SDK for iOS",
  "description": "SDK for iOS to measure your apps with Piwik PRO",
  "homepage": "https://piwik.pro",
  "license": {
    "type": "MIT",
    "file": "LICENSE.md"
  },
  "authors": {
    "Piwik PRO": "contact+cocoapods@piwik.pro"
  },
  "social_media_url": "http://twitter.com/piwikpro/",
  "platforms": {
    "ios": "9.0"
  },
  "source": {
    "http": "https://bitbucket.org/piwikmobile/piwikprosdk-ios/downloads/1.0.4-1.zip"
  },
  "source_files": "PiwikPROSDK.framework/Headers/*.h",
  "public_header_files": "PiwikPROSDK.framework/Headers/*.h",
  "header_dir": "PiwikPROSDK",
  "header_mappings_dir": ".",
  "preserve_paths": [
    "PiwikPROSDK.framework",
    "PiwikPROSDK.framework/**/*"
  ],
  "documentation_url": "http://developers.piwik.pro/en/latest/sdk/Piwik_PRO_SDK_for_iOS.html",
  "module_map": "PiwikPROSDK.framework/Modules/module.modulemap",
  "frameworks": "SystemConfiguration",
  "ios": {
    "vendored_frameworks": "PiwikPROSDK.framework"
  },
  "requires_arc": false,
  "pod_target_xcconfig": {
    "HEADER_SEARCH_PATHS": "$(PODS_ROOT)/PiwikPROSDK/**"
  },
  "xcconfig": {
    "FRAMEWORK_SEARCH_PATHS": "\"$(PODS_ROOT)/PiwikPROSDK/\"",
    "USER_HEADER_SEARCH_PATHS": "$(PODS_ROOT)/PiwikPROSDK/**",
    "OTHER_LDFLAGS": "$(inherited) -ObjC"
  }
}
