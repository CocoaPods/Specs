{
  "name": "mobile-ffmpeg-tvos-https-gpl",
  "version": "4.3",
  "summary": "Mobile FFmpeg tvOS Https GPL Static Framework",
  "description": "Includes FFmpeg v4.3-dev-1181 with gmp v6.1.2, gnutls v3.6.10, libvid.stab v1.1.0, x264 v20191024-2245-stable, x265 v3.2 and xvidcore v1.3.5 libraries enabled.",
  "homepage": "https://github.com/tanersener/mobile-ffmpeg",
  "authors": {
    "Taner Sener": "tanersener@gmail.com"
  },
  "license": {
    "type": "GPL-3.0",
    "file": "mobileffmpeg.framework/LICENSE"
  },
  "platforms": {
    "tvos": "9.2"
  },
  "requires_arc": true,
  "libraries": [
    "z",
    "bz2",
    "c++"
  ],
  "source": {
    "http": "https://github.com/tanersener/mobile-ffmpeg/releases/download/v4.3/mobile-ffmpeg-https-gpl-4.3-tvos-framework.zip"
  },
  "tvos": {
    "frameworks": [
      "AudioToolbox",
      "VideoToolbox",
      "CoreMedia"
    ],
    "vendored_frameworks": [
      "mobileffmpeg.framework",
      "libavcodec.framework",
      "libavdevice.framework",
      "libavfilter.framework",
      "libavformat.framework",
      "libavutil.framework",
      "libswresample.framework",
      "libswscale.framework",
      "gmp.framework",
      "gnutls.framework",
      "libcharset.framework",
      "libhogweed.framework",
      "libiconv.framework",
      "libnettle.framework",
      "libvidstab.framework",
      "x264.framework",
      "x265.framework",
      "xvidcore.framework"
    ]
  }
}
