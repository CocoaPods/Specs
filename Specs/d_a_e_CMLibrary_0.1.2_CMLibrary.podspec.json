{
  "name": "CMLibrary",
  "version": "0.1.2",
  "summary": "A subclass on UILabel that provides a blink.",
  "description": "This CocoaPod provides the ability to use a UILabel that may be started and stopped blinking.",
  "homepage": "https://github.com/adityaaggarwal1/CMLibrary",
  "license": "MIT",
  "authors": {
    "adityaaggarwal1": "aditya.aggarwal@yahoo.co.in"
  },
  "source": {
    "git": "https://github.com/adityaaggarwal1/CMLibrary.git",
    "tag": "0.1.2"
  },
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "source_files": "Pod/Classes/**/*",
  "resource_bundles": {
    "CMLibrary": [
      "Pod/Assets/*.png",
      "Pod/Assets/*.sqlite"
    ]
