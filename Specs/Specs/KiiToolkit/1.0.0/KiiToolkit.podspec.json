{
  "name": "KiiToolkit",
  "version": "1.0.0",
  "summary": "A suite of iOS classes and methods to make app development simpler.",
  "description": "                    Includes not only utilities for developing on \n                    <a href=\"http://developer.kii.com\" title=\"Kii Cloud\">Kii Cloud</a>,\n                    but also general utilities for nice UI elements and other convenient \n                    utilities that we find useful in app development.\n",
  "homepage": "http://github.com/KiiPlatform/KiiToolkit-iOS",
  "license": {
    "type": "Apache License, Version 2.0",
    "file": "LICENSE"
  },
  "authors": {
    "Chris Beauchamp": "chris.beauchamp@kii.com"
  },
  "source": {
    "git": "https://github.com/KiiPlatform/KiiToolkit-iOS.git",
    "tag": "1.0.0"
  },
  "platforms": {
    "ios": "4.3"
  },
  "requires_arc": true,
  "source_files": [
    "KiiToolkit.h",
    "{KTAppRater,KTLoginViewController,KTTableViewController,KTUI,KTUtilities}/**/*.{h,m}"
  ],
  "resources": [
    "KTUI/KTLoader/images/*",
    "KTLoginViewController/images/*"
