{
  "name": "ADiOSUtilities",
  "version": "1.0.0",
  "license": "MIT",
  "summary": "ADiOSUtilities is collection of static libraries with re-useable iOS awesomeness.",
  "homepage": "https://github.com/adamvduke/ADiOSUtilities",
  "authors": {
    "Adam Duke": "adam.v.duke+github@gmail.com"
  },
  "source": {
    "git": "https://github.com/adamvduke/ADiOSUtilities.git",
    "tag": "1.0.0"
  },
  "platforms": {
    "ios": null
  },
  "requires_arc": true,
  "subspecs": [
    {
      "name": "ADFoundationAdditions",
      "source_files": "ADFoundationAdditions/*.{h,m}",
      "frameworks": "Foundation"
    },
    {
      "name": "ADHelpers",
      "source_files": "ADHelpers/*.{h,m}",
      "frameworks": "AudioToolbox"
    },
    {
      "name": "ADUIKitAdditions",
      "source_files": "ADUIKitAdditions/*.{h,m}",
      "frameworks": "UIKit"
    }
  ]
}
