{
  "name": "MERTwitterKit",
  "version": "0.1.0",
  "authors": {
    "William Towe": "willbur1984@gmail.com"
  },
  "license": {
    "type": "MIT",
    "file": "LICENSE.txt"
  },
  "homepage": "https://github.com/MaestroElearning/MERTwitterKit",
  "source": {
    "git": "https://github.com/MaestroElearning/MERTwitterKit.git",
    "tag": "0.1.0"
  },
  "summary": "A Twitter 1.1 API wrapper, built on top of AFNetworking and ReactiveCocoa. Compatible with iOS, 7.0+",
  "platforms": {
    "ios": "7.0"
  },
  "dependencies": {
    "AFNetworking": [
      "~> 2.2.0"
    ],
    "MECoreDataKit": [
      "~> 1.0.0"
    ],
    "MEReactiveFoundation": [
      "~> 1.0.0"
    ],
    "ReactiveCocoa": [
      "~> 2.2.0"
    ],
    "ReactiveViewModel": [
      "~> 0.2.0"
    ],
    "libextobjc/EXTScope": [
      "~> 0.4.0"
    ],
    "libextobjc/EXTKeyPathCoding": [
      "~> 0.4.0"
    ],
    "SDWebImage": [
      "~> 3.6.0"
    ]
  },
  "requires_arc": true,
  "frameworks": [
    "Accounts",
    "CoreLocation",
    "Social"
  ],
  "source_files": [
    "MERTwitterKit",
    "MERTwitterKit/CoreData",
    "MERTwitterKit/CoreData/machine",
    "MERTwitterKit/Private"
  ],
  "private_header_files": [
    "MERTwitterKit/CoreData",
    "MERTwitterKit/CoreData/machine",
    "MERTwitterKit/Private"
  ],
  "resource_bundles": {
    "MERTwitterKitResources": [
      "MERTwitterKitResources/*.plist",
      "MERTwitterKitResources/*.lproj",
      "MERTwitterKitResources/*.xcdatamodeld"
    ]
  }
}
