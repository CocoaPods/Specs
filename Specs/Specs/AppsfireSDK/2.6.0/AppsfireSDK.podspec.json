{
  "name": "AppsfireSDK",
  "version": "2.6.0",
  "license": "Commercial",
  "summary": "Appsfire iOS SDK",
  "description": "The Appsfire SDK for iOS enables you to natively monetize your applications and engage with your users.",
  "homepage": "http://www.appsfire.com/",
  "authors": {
    "Appsfire": "http://www.appsfire.com/"
  },
  "source": {
    "git": "https://github.com/appsfire/Appsfire-iOS-SDK.git",
    "tag": "2.6.0"
  },
  "platforms": {
    "ios": "5.1.1"
  },
  "source_files": "AppsfireSDK/**/*.{h,m}",
  "preserve_paths": "AppsfireSDK/*.a",
  "libraries": "AppsfireSDK",
  "xcconfig": {
    "LIBRARY_SEARCH_PATHS": "\"$(PODS_ROOT)/AppsfireSDK/AppsfireSDK\""
  },
  "weak_frameworks": [
    "AdSupport",
    "StoreKit"
  ],
  "frameworks": [
    "Accelerate",
    "CoreGraphics",
    "CoreText",
    "Foundation",
    "QuartzCore",
    "Security",
    "SystemConfiguration",
    "UIKit"
  ],
  "requires_arc": true
}
