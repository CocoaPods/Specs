{
  "name": "Facelytics",
  "version": "1.0.1",
  "summary": "Facelytics is an SDK allowing mobile apps to detect face criterias of people by analyzing the front video feed in realtime.",
  "description": "Facelytics is able to track multiple faces and then detect gender, some emotions, age range and accessories, for each detected face.",
  "homepage": "http://face-lytics.com",
  "screenshots": [
    "http://a1.mzstatic.com/us/r30/Purple1/v4/f0/91/7b/f0917be0-b832-78a3-e2c3-91e5aa2af982/screen322x572.jpeg",
    "http://a3.mzstatic.com/us/r30/Purple7/v4/c4/5b/bb/c45bbbc4-efca-de49-033d-c29bd574027d/screen322x572.jpeg"
  ],
  "license": "COMMERCIAL",
  "authors": {
    "Wassa": "contact@wassa.fr"
  },
  "source": {
    "git": "https://github.com/wassafr/Facelytics-ios.git",
    "tag": "1.0.1"
  },
  "platforms": {
    "ios": "7.0"
  },
  "requires_arc": true,
  "pod_target_xcconfig": {
    "ENABLE_BITCODE": "NO"
  },
  "resources": [
    "Pod/Assets/*"
  ],
  "source_files": "Pod/Headers/*.h",
  "public_header_files": "Pod/Headers/*.h",
  "vendored_libraries": "Pod/lib/libFacelyticsSDK_debug.a",
  "dependencies": {
    "OpenCV": [
      "~> 2.4.9.1"
    ]
  }
}
