{
  "name": "Evernote-SDK-iOS",
  "version": "1.3.1",
  "platforms": {
    "ios": "5.0"
  },
  "license": "https://github.com/evernote/evernote-sdk-ios/blob/master/LICENSE",
  "summary": "Evernote SDK for iOS.",
  "homepage": "https://github.com/evernote/evernote-sdk-ios",
  "authors": {
    "Evernote": "devsupport@evernote.com"
  },
  "requires_arc": true,
  "source": {
    "git": "https://github.com/evernote/evernote-sdk-ios.git",
    "tag": "1.3.1"
  },
  "source_files": [
    "evernote-sdk-ios/*.{h,m}",
    "evernote-sdk-ios/{EDAM,Utilities,internal}/**/*.{h,m}",
    "evernote-sdk-ios/3rdParty/{AFNetworking,KSHTMLWriter,NSString+URLEncoding,Thrift,cocoa-oauth}/**/*.{h,m}"
  ],
  "frameworks": [
    "Foundation",
    "Security",
    "StoreKit",
    "MobileCoreServices"
  ],
  "libraries": "xml2",
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "\"$(SDKROOT)/usr/include/libxml2\""
  },
  "dependencies": {
    "SSKeychain": [
      "~> 1.2.2"
    ]
  }
}
