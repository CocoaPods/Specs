{
  "name": "PEXML-Utils",
  "version": "1.0.3",
  "license": "MIT",
  "summary": "A collection of XML helper functions.",
  "authors": {
    "Paul Evans": "evansp2@gmail.com"
  },
  "homepage": "https://github.com/evanspa/PEXML-Utils",
