    "Christopher Pickslay": "",
    "Zac Shenker": "",
    "Sinnerschrader Mobile": ""
  },
  "platforms": {
    "ios": "6.0"
  },
  "source": {
    "git": "https://github.com/twobitlabs/AnalyticsKit.git",
    "tag": "1.2.4"
  },
  "requires_arc": false,
