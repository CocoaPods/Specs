{
  "name": "CWFoundation",
  "version": "0.0.5",
  "summary": "CWFoundation is a collection of utility methods and category extensions for Foundation and UIKit classes to accelerate development.",
  "homepage": "https://github.com/guojiubo/CWFoundation",
  "license": "MIT",
  "authors": {
    "guojiubo": "guojiubo@gmail.com"
  },
  "platforms": {
    "ios": "6.0"
  },
  "requires_arc": true,
  "source": {
    "git": "https://github.com/guojiubo/CWFoundation.git",
    "tag": "0.0.5"
  },
  "source_files": "CWFoundation/*.{h,m}",
  "frameworks": [
    "Foundation",
    "UIKit"
  ],
  "subspecs": [
    {
      "name": "Foundation+CWAdditions",
      "source_files": "CWFoundation/Foundation+CWAdditions/*.{h,m}"
    },
    {
      "name": "UIKit+CWAdditions",
      "source_files": "CWFoundation/UIKit+CWAdditions/*.{h,m}"
    },
    {
      "name": "Utilities",
      "source_files": "CWFoundation/Utilities/*.{h,m}",
      "dependencies": {
        "CWFoundation/Foundation+CWAdditions": [

        ]
      }
    }
  ]
}
