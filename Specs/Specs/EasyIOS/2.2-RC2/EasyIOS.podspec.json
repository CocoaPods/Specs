{
  "name": "EasyIOS",
  "version": "2.2-RC2",
  "summary": "EasyIOS is a MVVM frameWork base on AFNetworking and ReactiveCocoa",
  "homepage": "http://easyios.08dream.com",
  "social_media_url": "http://easyios.08dream.com",
  "platforms": {
    "ios": "6.0"
  },
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "zhuchao": "zhuchaowe@163.com"
  },
  "source": {
    "git": "https://github.com/zhuchaowe/EasyIOS.git",
    "tag": "2.2-RC2"
  },
  "requires_arc": true,
  "frameworks": [
    "CoreFoundation",
    "Foundation",
    "CoreGraphics",
    "Security",
    "UIKit"
  ],
  "libraries": [
    "z.1.1.3",
    "stdc++",
    "sqlite3"
  ],
  "subspecs": [
    {
      "name": "Easy",
      "source_files": [
        "*.{h,m,mm}",
        "Easy/**/*.{h,m,mm}",
        "Extend/**/*.{h,m,mm}"
      ],
      "resources": "Extend/**/*.{png}",
      "requires_arc": true,
      "xcconfig": {
        "HEADER_SEARCH_PATHS": "$(SDKROOT)/usr/include/libz, $(SDKROOT)/usr/include/libxml2",
        "CLANG_CXX_LANGUAGE_STANDARD": "gnu++0x",
        "CLANG_CXX_LIBRARY": "libstdc++",
        "CLANG_WARN_DIRECT_OBJC_ISA_USAGE": "YES"
      },
      "dependencies": {
        "ReactiveCocoa": [

        ],
        "UICKeyChainStore": [

        ],
        "MBProgressHUD": [

        ],
        "FontIcon": [

        ],
        "MojoDatabase": [

        ],
        "FLKAutoLayout": [

        ],
        "TTTAttributedLabel": [

        ],
        "GCDObjC": [

        ],
        "RegExCategories": [

        ],
        "TMCache": [

        ],
        "SHGestureRecognizerBlocks": [

        ],
        "UIActivityIndicator-for-SDWebImage": [

        ],
        "AFNetworking": [

        ],
        "IQKeyboardManager": [

        ],
        "UIActionSheet-Blocks": [

        ]
      },
      "prefix_header_contents": "#import \"swift-bridge.h\""
    }
  ]
}
