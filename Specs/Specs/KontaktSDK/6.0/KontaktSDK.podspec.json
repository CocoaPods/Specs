{
  "name": "KontaktSDK",
  "version": "6.0",
  "summary": "Kontakt.io iOS SDK Library",
  "homepage": "http://docs.kontakt.io/ios-sdk/quickstart/",
  "authors": {
    "Kontakt.io": "http://kontakt.io"
  },
  "platforms": {
    "ios": "7.0"
  },
