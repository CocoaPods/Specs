{
  "name": "MERThumbnailKit",
  "version": "2.2.5",
  "authors": {
    "William Towe": "willbur1984@gmail.com"
  },
  "license": {
    "type": "MIT",
    "file": "LICENSE.txt"
  },
  "homepage": "https://github.com/MaestroElearning/MERThumbnailKit",
  "source": {
    "git": "https://github.com/MaestroElearning/MERThumbnailKit.git",
    "tag": "2.2.5"
  },
  "summary": "A framework for generating thumbnails from urls, both local and remote. Compatible with iOS/OSX, 7.0+/10.9+.",
  "platforms": {
    "ios": "7.0",
    "osx": "10.9"
  },
  "dependencies": {
    "MEFoundation": [
      "~> 1.0.0"
    ],
    "ReactiveCocoa": [
      "~> 2.3.0"
    ],
    "libextobjc/EXTScope": [
      "~> 0.4.0"
    ]
  },
  "requires_arc": true,
  "frameworks": [
    "Foundation",
    "Accelerate",
    "AVFoundation"
  ],
  "ios": {
    "frameworks": [
      "UIKit",
      "MobileCoreServices"
    ],
    "exclude_files": "MERThumbnailKitFramework"
  },
  "osx": {
    "frameworks": [
      "AppKit",
      "QuickLook"
    ],
    "exclude_files": [
      "MERThumbnailKit/UI*",
      "MERThumbnailKit/Private/UI*"
    ]
  },
  "source_files": [
    "MERThumbnailKit",
    "MERThumbnailKitFramework",
    "MERThumbnailKit/Private"
  ],
  "private_header_files": "MERThumbnailKit/Private",
  "resource_bundles": {
    "MERThumbnailKitResources": [
      "MERThumbnailKitResources/*.plist",
      "MERThumbnailKitResources/*.js"
    ]
  }
}
