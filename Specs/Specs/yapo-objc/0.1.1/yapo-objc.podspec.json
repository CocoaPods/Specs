    ],
    "yapo-transport-ip-objc": [

    ]
  },
  "preserve_paths": [
    "libzmq_dist/include/*.h",
    "libczmq_dist/include/*.h",
    "libcurve_dist/include/*.h",
    "libsodium/include/*.h"
  ],
  "libraries": [
    "c++",
    "stdc++"
  ],
  "vendored_libraries": [
    "libzmq_dist/lib/libzmq.a",
    "libczmq_dist/lib/libczmq.a",
    "libcurve_dist/lib/libcurve.a",
