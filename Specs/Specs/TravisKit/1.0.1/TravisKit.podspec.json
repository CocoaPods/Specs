{
  "name": "TravisKit",
  "version": "1.0.1",
  "summary": "Travis CI iOS/OS X API Wrapper",
  "homepage": "https://github.com/legoless/TravisKit",
  "license": "MIT",
  "authors": {
    "Dal Rupnik": "legoless@gmail.com"
  },
  "source": {
    "git": "https://github.com/legoless/TravisKit.git",
    "tag": "1.0.1"
  },
  "platforms": {
    "ios": "7.0"
  },
  "frameworks": [
    "Foundation",
    "UIKit",
    "CoreGraphics"
