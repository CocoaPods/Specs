{
  "name": "SinglySDK",
  "version": "1.2.3",
  "summary": "iOS SDK for Singly.",
  "homepage": "https://github.com/Singly/iOS-SDK",
  "license": {
    "type": "BSD",
    "file": "LICENSE"
  },
  "authors": {
    "Singly, Inc": "nerds@singly.com",
    "Justin Mecham": "justin.m@singly.com",
