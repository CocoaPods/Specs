  },
  "source": {
    "git": "https://github.com/Singly/iOS-SDK.git",
    "tag": "v1.2.3"
  },
  "platforms": {
    "ios": "5.0"
  },
  "source_files": "SinglySDK/SinglySDK/**/*.{h,m}",
  "exclude_files": "**/{SinglySharingViewController,DEFacebook,DERuledView,SinglyFriend.,UIApplication+DEFacebookComposeViewController,UIDevice+DEFacebookComposeViewController}*",
