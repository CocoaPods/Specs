{
  "name": "iOS-GTLYouTube",
  "platforms": {
    "ios": "5.1"
  },
  "version": "0.0.4",
  "authors": {
    "bryn austin bellomy": "bryn.bellomy@gmail.com"
  },
  "summary": "Auto-generated Google GTL library for accessing YouTube's new JSON API.",
  "homepage": "http://github.com/brynbellomy/iOS-GTLYouTube",
  "license": {
    "type": "WTFPL",
    "file": "LICENSE.md"
  },
  "source": {
    "git": "https://github.com/brynbellomy/iOS-GTLYouTube.git",
    "tag": "v0.0.4"
  },
  "requires_arc": false,
  "public_header_files": [
    "GTLDefines.h",
    "Services/YouTube/Generated/*.h",
    "Utilities/*.h",
    "Objects/*.h",
    "HTTPFetcher/*.h",
    "OAuth2/*.h",
    "OAuth2/Touch/*.h"
  ],
  "exclude_files": "Services/YouTube/Generated/GTLYouTube_Sources.m",
  "source_files": [
    "GTLDefines.h",
    "Services/YouTube/Generated/*.{h,m}",
    "Utilities/*.{h,m}",
    "Objects/*.{h,m}",
    "HTTPFetcher/*.{h,m}",
    "OAuth2/*.{h,m}",
    "OAuth2/Touch/*.{h,m}"
  ],
  "resources": [
    "OAuth2/Touch/*.xib"
  ]
}
