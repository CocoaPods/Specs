{
  "name": "GHMarkdownParser",
  "version": "0.1.1",
  "license": "MIT",
  "summary": "A GitHub Flavored Markdown parser for iOS and Mac OS, based on discount.",
  "homepage": "https://github.com/OliverLetterer/GHMarkdownParser",
  "authors": {
    "Oliver Letterer": "oliver.letterer@gmail.com"
  },
  "source": {
    "git": "https://github.com/OliverLetterer/GHMarkdownParser.git",
    "tag": "0.1.1",
    "submodules": "true"
  },
  "source_files": [
    "discount/config.h",
    "discount/setup.c",
    "discount/tags.{h,c}",
    "discount/html5.c",
    "discount/emmatch.c",
    "discount/Csio.c",
    "discount/xml.c",
    "discount/resource.c",
    "discount/generate.c",
    "discount/markdown.{h,c}",
    "discount/mkdir.{c,h}",
    "discount/cstring.h",
    "discount/amalloc.{h,c}",
    "discount/mkdio.{h,c}",
    "GHMarkdownParser/**/*.{h,m}"
  ],
  "xcconfig": {
    "OTHER_LDFLAGS": "-ObjC -all_load",
    "HEADER_SEARCH_PATHS": "\"${PODS_ROOT}/GHMarkdownParser/discount\""
  },
  "resources": "discount/blocktags",
  "requires_arc": true
}
