{
  "name": "DocuSign-iOS-SDK",
  "version": "0.1-beta",
  "summary": "DocuSign iOS SDK Beta",
  "description": "An iOS SDK that provides a quick and easy way for registered developers to add DocuSign's world-class document signing experience to their native iOS apps.",
  "homepage": "https://github.com/docusign/docusign-ios-sdk",
  "license": "DOCUSIGN API SDK LICENSE",
  "authors": {
    "Arlo Armstrong": "arlo.armstrong@docusign.com"
  },
  "source": {
    "git": "https://github.com/docusign/docusign-ios-sdk.git",
    "tag": "v0.1-beta"
  },
  "social_media_url": "https://twitter.com/DocuSignDev",
  "platforms": {
    "ios": "7.0"
  },
  "requires_arc": true,
  "source_files": [
    "**/SDK/**/*.{h,m}"
  ],
  "resources": [
    "**/SDK/**/*.{storyboard,xcassets}"
  ],
  "frameworks": [
    "AVFoundation",
    "CoreLocation",
    "Foundation",
    "ImageIO",
    "MobileCoreServices",
    "QuartzCore",
    "UIKit"
  ],
  "libraries": "objc",
  "dependencies": {
    "AKANetworkLogging": [
      "~>0.1"
    ],
    "DSTextEntryValidation": [
      "~>1.0"
    ],
    "Mantle": [
      "~>1.4"
    ],
    "TPKeyboardAvoiding": [
      "~>1.2"
    ]
  }
}
