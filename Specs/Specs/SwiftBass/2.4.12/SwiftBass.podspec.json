{
  "name": "SwiftBass",
  "version": "2.4.12",
  "summary": "Swift module for BASS library.",
  "description": "Straightforward way to use BASS library in your Swift project.",
  "homepage": "https://github.com/plasmLC/SwiftBass",
  "license": "MIT",
  "authors": {
    "Alexander Orlov": "me@orlovalex.com"
  },
  "source": {
    "git": "https://github.com/plasmLC/SwiftBass.git",
    "tag": "2.4.12"
  },
  "social_media_url": "https://twitter.com/plasm_lc",
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "source_files": [
    "Pod/Classes/bass.h",
    "Pod/Classes/**/*.{swift}"
  ],
  "ios": {
    "vendored_libraries": "Pod/Classes/**/*.{a}"
  },
  "frameworks": [
    "AVFoundation",
    "CoreAudio",
    "AudioToolbox"
  ],
  "resource_bundles": {
    "SwiftBass": [
      "Pod/Assets/*.png"
    ]
  }
}
