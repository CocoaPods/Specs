{
  "name": "ScoutMaps-iOS-SDK",
  "version": "2.4-beta1",
  "summary": "Scout Maps & Navigation SDK -  an OpenStreetMap-based MapKit alternative",
  "description": "                       Build immersive location-based products with numerous features like Search, Routing, Offline Maps, Custom Map Styles, Turn-by-Turn Navigation based on the crowd-sourced OpenStreetMap (OSM)\n",
  "homepage": "http://www.scout.me/developer/",
  "license": {
    "type": "Commercial",
    "text": "see http://www.skobbler.com/legal#termsSDK"
  },
  "authors": {
    "Telenav Inc.": "http://www.telenav.com/about/"
  },
  "source": {
    "http": "http://www1.skobbler.com/iphonebuilds/20150113/ScoutMaps-iOS-SDK_2.4.0.beta.zip"
  },
  "platforms": {
    "ios": "6.0"
  },
  "requires_arc": true,
  "preserve_paths": "SKMaps.framework",
  "source_files": "SKMaps.framework/Versions/A/Headers/*.h",
  "resources": [
    "SKMaps.bundle",
    "SKAdvisorResources.bundle"
  ],
  "header_dir": "ScoutMaps-iOS",
  "frameworks": [
    "SKMaps",
    "CoreLocation",
    "OpenGLES",
    "QuartzCore",
    "SystemConfiguration",
    "CoreMotion",
    "AVFoundation"
  ],
  "libraries": [
    "z",
    "stdc++.6",
    "c++"
  ],
  "xcconfig": {
    "FRAMEWORK_SEARCH_PATHS": "\"$(PODS_ROOT)/ScoutMaps-iOS-SDK/\""
  }
}
