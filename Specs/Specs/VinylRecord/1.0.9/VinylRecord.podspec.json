{
  "name": "VinylRecord",
  "version": "1.0.9",
  "license": "MIT",
  "summary": "Pure SQLite ORM for iOS without CoreData.",
  "homepage": "https://github.com/valerius/VinylRecord.git",
  "description": "Pure SQLite ORM for iOS without CoreData.\nFor more details check Wiki on Github.",
  "authors": {
    "James Whitfield": "jwhitfield@neilab.com"
  },
  "source": {
    "git": "https://github.com/valerius/VinylRecord.git",
    "tag": "1.0.9"
  },
  "platforms": {
    "ios": "7.0"
  },
  "source_files": "iActiveRecord/**/*.{c,h,m,mm}",
  "module_map": "iActiveRecord/VinylRecord.modulemap",
  "private_header_files": "iActiveRecord/**/*Private.h",
  "libraries": "sqlite3",
  "requires_arc": true,
  "xcconfig": {
    "OTHER_LDFLAGS": "-lc++",
    "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited) SQLITE_CORE SQLITE_ENABLE_UNICODE"
  }
}
