        ],
        "Reachability": [
          "~> 3.1.1"
        ],
        "MBProgressHUD": [
          "~> 0.8"
        ],
        "Objective-LevelDB": [
          "~> 2.0.6"
        ],
        "RNCryptor": [
          "~> 2.2"
        ],
        "SSKeychain": [

        ],
        "TMOTableView": [

        ]
      }
    },
    {
      "name": "TMOFMDB",
      "platforms": {
        "ios": "5.0"
      },
      "source_files": "TeemoSubmodule/TMOFMDB/*.{h,m}",
      "dependencies": {
        "FMDB": [
          "~> 2.2"
        ]
      }
    },
    {
      "name": "TMOSmarty",
      "platforms": {
        "ios": "5.0"
      },
