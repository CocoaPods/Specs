{
  "name": "CocoaUtils",
  "version": "1.0.10",
  "license": "Apache License 2.0",
  "summary": "Cocoa Utils",
  "homepage": "https://github.com/zendobk/CocoaUtils",
  "authors": {
    "Dao Nguyen": "zendobk"
  },
  "source": {
    "git": "https://github.com/zendobk/CocoaUtils.git",
    "tag": "1.0.10"
  },
  "requires_arc": true,
  "platforms": {
    "ios": "7.0"
  },
  "public_header_files": [
    "CocoaUtils/*.h",
    "CocoaUtils/Classes/Common/*.h",
    "CocoaUtils/Classes/Foundation/*.h",
    "CocoaUtils/Classes/UIKit/*.h",
    "CocoaUtils/Classes/MapKit/*.h"
  ],
  "subspecs": [
    {
      "name": "Common",
      "source_files": "CocoaUtils/Classes/Common/*.{h,m}",
      "ios": {
        "frameworks": [
          "Foundation",
          "UIKit"
        ]
      }
    },
    {
      "name": "CoreData",
      "source_files": "CocoaUtils/Classes/CoreData/*.{h,m}",
      "ios": {
        "frameworks": [
          "Foundation",
          "UIKit",
          "CoreData"
        ]
      }
    },
    {
      "name": "Foundation",
      "source_files": "CocoaUtils/Classes/Foundation/*.{h,m}",
      "ios": {
        "frameworks": "Foundation",
        "dependencies": {
          "JRSwizzle": [
            "~> 1.0"
          ],
          "CocoaUtils/Common": [

          ]
        }
      }
    },
    {
      "name": "MapKit",
      "source_files": "CocoaUtils/Classes/MapKit/*.{h,m}",
      "ios": {
        "frameworks": [
          "Foundation",
          "UIKit",
          "MapKit"
        ]
      }
    },
    {
      "name": "UIKit",
      "source_files": "CocoaUtils/Classes/UIKit/*.{h,m}",
      "ios": {
        "frameworks": [
          "Foundation",
          "UIKit"
        ],
        "dependencies": {
          "JRSwizzle": [
            "~> 1.0"
          ],
          "CocoaUtils/Foundation": [

          ],
          "CocoaUtils/Common": [

          ]
        }
      }
    }
  ]
}
