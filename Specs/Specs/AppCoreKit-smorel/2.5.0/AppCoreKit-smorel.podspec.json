    "UIKit",
    "Foundation",
    "CoreImage",
    "CoreGraphics",
    "AddressBook",
    "CoreData",
    "QuartzCore",
    "CoreLocation",
    "MapKit",
    "MediaPlayer",
    "CoreFoundation",
    "CFNetwork",
    "SystemConfiguration",
    "MobileCoreServices",
    "Security",
    "AdSupport",
    "Accelerate"
  ],
