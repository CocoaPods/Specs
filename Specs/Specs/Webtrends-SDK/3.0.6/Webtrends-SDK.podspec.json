{
  "name": "Webtrends-SDK",
  "version": "3.0.6",
  "summary": "The data collection SDK for Webtrends",
  "license": "COMMERCIAL",
  "homepage": "http://webtrends.com",
  "authors": {
    "Sam Ingle": "Sam.Ingle@webtrends.com",
    "Blake Clough": "Blake.Clough@webtrends.com",
    "John Park": "John.Park@webtrends.com",
    "Claudine Morales": "Claudine.Morales@webtrends.com"
  },
  "platforms": {
    "ios": "7.0"
  },
  "ios": {
    "frameworks": [
      "Foundation",
      "UIKit",
      "CoreData",
      "SystemConfiguration",
      "Security",
      "CoreTelephony"
    ]
  },
  "source": {
    "git": "https://github.com/webtrends/ios-sdk.git",
    "tag": "3.0.6"
  },
  "subspecs": [
    {
      "name": "Watch",
      "source_files": [
        "WatchHeaders",
        "WatchHeaders/**/*.h"
      ],
      "vendored_libraries": "libWebtrendsWatchKitSDK.a"
    },
    {
      "name": "Core",
      "source_files": [
        "Headers",
        "Headers/**/*.h"
      ],
      "exclude_files": "Headers/Exclude",
      "resources": "SharedAssets/*",
      "libraries": [
        "z",
        "sqlite3"
      ],
      "vendored_libraries": "libWebtrendsSDK.a"
    }
  ]
}
