{
  "name": "FPPicker",
  "version": "3.0.1",
  "summary": "SDK to access Filepicker.io API",
  "description": "    Filepicker helps developers connect with all the data sources they might have.\n    This is an SDK that lets developers easily add a bunch of cloud file handling features without coding.\n",
  "homepage": "https://github.com/Ink/ios-picker/",
  "screenshots": "https://github.com/Ink/ios-picker/raw/develop/Docs/filepicker_ios.png",
  "license": {
    "type": "MIT",
    "file": "LICENSE.md"
  },
  "authors": {
    "Filepicker.io": "contact@filepicker.io"
  },
  "source": {
    "git": "https://github.com/Ink/ios-picker.git",
    "tag": "v3.0.1"
  },
  "platforms": {
    "ios": "6.0"
  },
  "prefix_header_file": "FPPicker/FPPicker-Prefix.pch",
  "public_header_files": [
    "FPPicker/FPConstants.h",
    "FPPicker/FPPicker.h",
    "FPPicker/FPPickerController.h",
    "FPPicker/FPSaveController.h",
    "FPPicker/FPExternalHeaders.h"
  ],
  "source_files": "FPPicker/*.{h,m}",
  "frameworks": [
    "AssetsLibrary",
    "CoreFoundation",
    "CoreGraphics",
    "Foundation",
    "MobileCoreServices",
    "QuartzCore",
    "SystemConfiguration"
  ],
  "dependencies": {
    "AFNetworking": [
      "~> 2.3.1"
    ]
  },
  "requires_arc": true,
  "resource_bundles": {
    "FPPicker": "FPPicker Resources/Assets/*.*"
  }
}
