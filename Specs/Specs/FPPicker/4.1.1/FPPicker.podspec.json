{
  "name": "FPPicker",
  "version": "4.1.1",
  "summary": "SDK to access Filepicker.io API",
  "description": "    Filepicker helps developers connect with all the data sources they might have.\n    This is an SDK that lets developers easily add a bunch of cloud file handling features without coding.\n",
  "homepage": "https://github.com/Ink/ios-picker/",
  "screenshots": "https://github.com/Ink/ios-picker/raw/develop/Docs/filepicker.png",
  "license": {
    "type": "MIT",
    "file": "LICENSE.md"
  },
  "authors": {
    "Filepicker.io": "contact@filepicker.io"
  },
  "source": {
    "git": "https://github.com/Ink/ios-picker.git",
    "tag": "v4.1.1"
  },
  "platforms": {
    "ios": "8.0",
    "osx": "10.9"
  },
  "ios": {
    "prefix_header_file": "Resources-iOS/FPPicker-Prefix.pch",
    "public_header_files": [
      "FPPicker/Platforms/iOS/FPPicker.h",
      "FPPicker/Platforms/iOS/FPPickerController.h",
      "FPPicker/Platforms/iOS/FPSaveController.h",
      "FPPicker/Shared/FPExternalHeaders.h",
      "FPPicker/Shared/FPConfig.h",
      "FPPicker/Shared/FPConstants.h",
      "FPPicker/Shared/FPMediaInfo.h"
    ],
    "source_files": [
      "FPPicker/Shared/*.{h,m}",
      "FPPicker/Platforms/iOS/*.{h,m}"
    ],
    "frameworks": [
      "AssetsLibrary",
      "CoreFoundation",
      "CoreGraphics",
      "MobileCoreServices",
      "QuartzCore",
      "SystemConfiguration"
    ],
    "dependencies": {
      "MBProgressHUD": [
        "~> 0.9"
      ]
    },
    "resource_bundles": {
      "FPPicker": "Resources-Shared/*.*"
    }
  },
  "osx": {
    "prefix_header_file": "Resources-Mac/FPPicker-Mac-Prefix.pch",
    "public_header_files": [
      "FPPicker/Platforms/Mac/FPPickerMac.h",
      "FPPicker/Platforms/Mac/FPPickerController.h",
      "FPPicker/Platforms/Mac/FPSaveController.h",
      "FPPicker/Shared/FPExternalHeaders.h",
      "FPPicker/Shared/FPConfig.h",
      "FPPicker/Shared/FPConstants.h",
      "FPPicker/Shared/FPMediaInfo.h"
    ],
    "source_files": [
      "FPPicker/Shared/*.{h,m}",
      "FPPicker/Platforms/Mac/*.{h,m}"
    ],
    "frameworks": [
      "WebKit",
      "Quartz"
    ],
    "resource_bundles": {
      "FPPicker": "Resources-Shared/*.*",
      "FPPickerMac": "Resources-Mac/*.*"
    }
  },
  "dependencies": {
    "AFNetworking": [
      "~> 2.4.1"
    ]
  },
  "requires_arc": true
}
