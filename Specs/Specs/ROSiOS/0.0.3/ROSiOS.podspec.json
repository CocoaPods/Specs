{
  "name": "ROSiOS",
  "version": "0.0.3",
  "summary": "template package of ROSiOS.",
  "description": "This is a bootstrap package for creating iOS App using ROS(Robot Operating System).\nThis includes some examples (Topic Viewer, Talker and Listener) on Example directory.\n",
  "homepage": "https://github.com/furushchev/ROSiOS",
  "license": {
    "type": "MIT"
  },
  "authors": {
    "Yuki Furuta": "furushchev@jsk.imi.i.u-tokyo.ac.jp"
  },
  "source": {
    "git": "https://github.com/furushchev/ROSiOS.git",
    "tag": "0.0.3"
  },
  "social_media_url": "https://twitter.com/furushchev",
  "platforms": {
    "ios": "6.0"
  },
  "requires_arc": true,
  "source_files": "Pod/Classes/*.{h,m,mm}",
  "resource_bundles": {
    "ROSiOS": [
      "Pod/Assets/Configuration.storyboard"
    ]
  },
  "public_header_files": "Pod/Classes/*.h",
  "frameworks": "UIKit",
  "dependencies": {
    "ROSiOS-core": [

    ]
  },
  "ios": {
    "xcconfig": {
      "FRAMEWORK_SEARCH_PATHS": "${PODS_ROOT}/**"
    }
  }
}
