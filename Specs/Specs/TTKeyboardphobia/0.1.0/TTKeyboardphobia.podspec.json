{
  "name": "TTKeyboardphobia",
  "version": "0.1.0",
  "summary": "Simple keyboard avoiding for iOS.",
  "description": "TTKeyboardphobia is a simple and straightforward to use library for keyboard avoidance on iOS.\n\n* No subclasses\n* Simple API.\n* Supports accessory views.\n* Supports alignment with a UIView container.",
  "homepage": "https://github.com/thumbtack/TTKeyboardphobia",
  "license": "BSD",
  "authors": {
    "Ian Leitch": "ileitch@thumbtack.com"
  },
  "source": {
    "git": "https://github.com/thumbtack/TTKeyboardphobia.git",
    "tag": "0.1.0"
  },
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "source_files": "Pod/Classes/**/*",
  "resource_bundles": {
    "TTKeyboardphobia": [
      "Pod/Assets/*.png"
    ]
  },
  "frameworks": "UIKit"
}
