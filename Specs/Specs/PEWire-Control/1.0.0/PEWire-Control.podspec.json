{
  "name": "PEWire-Control",
  "version": "1.0.0",
  "license": "MIT",
  "summary": "An iOS library for the easy stubbing of HTTP responses using simple XML files.",
  "authors": {
    "Paul Evans": "evansp2@gmail.com"
  },
  "homepage": "https://github.com/evanspa/PEWire-Control",
