{
  "name": "OpenTokSDK",
  "version": "1.2.4",
  "authors": "TokBox",
  "summary": "The OpenTok iOS SDK lets you use OpenTok video sessions in apps you build for iPad, iPhone, and iPod touch devices.",
  "homepage": "https://github.com/opentok/opentok-ios-sdk/",
  "license": {
    "type": "Apache",
    "file": "LICENSE.txt"
  },
  "source": {
    "git": "https://github.com/opentok/opentok-ios-sdk.git",
    "tag": "v1.2.4"
  },
  "platforms": {
    "ios": "5.0"
  },
  "source_files": "Opentok.framework/Versions/A/Headers/*.h",
  "resources": "Opentok.framework/Versions/A/Resources/opentok.bundle",
  "frameworks": [
    "Opentok",
    "UIKit",
    "CoreMedia",
    "CoreVideo",
    "CoreAudio",
    "CoreTelephony",
    "CoreGraphics",
    "CFNetwork",
    "AVFoundation",
    "SystemConfiguration",
    "Security",
    "QuartzCore",
    "AudioToolbox",
    "OpenGLES"
  ],
  "libraries": [
    "stdc++",
    "z"
  ],
  "xcconfig": {
    "FRAMEWORK_SEARCH_PATHS": "\"$(PODS_ROOT)/OpenTokSDK/\""
  },
  "preserve_paths": "Opentok.framework",
  "requires_arc": false
}
