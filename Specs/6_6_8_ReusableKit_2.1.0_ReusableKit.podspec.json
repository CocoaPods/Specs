{
  "name": "ReusableKit",
  "version": "2.1.0",
  "summary": "Generic reusables for Cocoa.",
  "homepage": "https://github.com/devxoul/ReusableKit",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Suyeol Jeon": "devxoul@gmail.com"
  },
  "source": {
    "git": "https://github.com/devxoul/ReusableKit.git",
    "tag": "2.1.0"
  },
  "frameworks": [
    "UIKit",
    "Foundation"
  ],
  "requires_arc": true,
