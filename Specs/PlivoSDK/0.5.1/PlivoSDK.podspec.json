{
  "name": "PlivoSDK",
  "version": "0.5.1",
  "summary": "The Plivo iOS SDK allows you to create applications capable of making and receiving calls from iPhone and iPad.",
  "homepage": "http://www.plivo.com/docs/sdk/ios/",
  "authors": {
    "Plivo, Inc": "hello@plivo.com"
  },
  "platforms": {
    "ios": "7.0"
  },
  "source": {
    "http": "http://s3.amazonaws.com/plivosdk/ios/plivo-ios-package-0.5.zip"
  },
  "source_files": "plivo-ios-package-0.5/Headers/*.h",
  "vendored_libraries": "plivo-ios-package-0.5/Libraries/*.a",
  "frameworks": [
    "AudioToolbox",
    "AVFoundation",
    "SystemConfiguration",
    "MobileCoreServices"
  ],
  "requires_arc": false,
  "xcconfig": {
    "LIBRARY_SEARCH_PATHS": "\"$(PODS_ROOT)/PlivoSDK/plivo-ios-package-0.5/Libraries\"",
    "HEADER_SEARCH_PATHS": "\"${PODS_ROOT}/Headers/PlivoSDK\""
  },
  "license": {
