{
  "name": "SAWebBridge",
  "version": "1.0.0",
  "license": "MIT",
  "summary": "A web bridge for swift H5 project",
  "authors": {
    "sarkizz": "sarkizz@yahoo.com.sg"
  },
  "homepage": "https://github.com/Sarkizz",
  "source": {
    "git": "https://github.com/Sarkizz/SAWebBridge.git",
    "tag": "1.0.0"
  },
  "platforms": {
    "ios": "11.0"
  },
  "requires_arc": true,
  "swift_versions": "5.0",
  "resources": "SAWebBridge/JS/*.js",
  "subspecs": [
    {
      "name": "WebBridge",
      "source_files": "SAWebBridge/WebBridge/*.swift",
      "ios": {
        "frameworks": [
          "UIKit",
          "Foundation",
          "WebKit"
        ]
      },
      "subspecs": [
        {
          "name": "Utils",
          "source_files": "SAWebBridge/WebBridge/Utils/*.swift",
          "ios": {
            "frameworks": [
              "UIKit",
              "Foundation"
            ]
          }
        }
      ]
    }
  ],
  "swift_version": "5.0"
}
