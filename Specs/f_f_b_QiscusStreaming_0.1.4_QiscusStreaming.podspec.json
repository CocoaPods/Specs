{
  "name": "QiscusStreaming",
  "version": "0.1.4",
  "summary": "Qiscus Video Streaming.",
  "description": "Qiscus SDK for iOS Video Streaming.",
  "homepage": "https://qisc.us",
  "license": "MIT",
  "authors": {
    "ashari juang": "asharijuang@me.com"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source": {
    "git": "https://github.com/qiscus/qiscus-streaming-sdk-ios.git",
    "tag": "0.1.4"
  },
  "source_files": [
    "QiscusLive",
    "QiscusLive/**/*.{h,m,swift,xib}"
  ],
  "resources": "QiscusLive/**/*.xcassets",
  "resource_bundles": {
    "QiscusLive": [
