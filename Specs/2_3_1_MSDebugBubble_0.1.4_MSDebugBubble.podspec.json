  },
  "source": {
    "git": "https://github.com/MaherKSantina/MSDebugBubble.git",
    "tag": "0.1.4"
  },
  "platforms": {
    "ios": "9.0"
  },
  "source_files": "MSDebugBubble/Classes/**/*",
