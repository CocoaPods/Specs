{
  "name": "WordPressKit",
  "version": "1.0.1",
  "summary": "WordPressKit offers a clean and simple WordPress.com and WordPress.org API.",
  "description": "This framework encapsulates all of the networking calls and entity parsers required\nto interact with WordPress.com and WordPress.org websites.",
  "homepage": "http://apps.wordpress.com",
  "license": "GPLv2",
  "authors": {
    "Jorge Leandro Perez": "jorge.perez@automattic.com"
  },
  "platforms": {
    "ios": "10.0"
  },
  "swift_version": "4.0",
  "source": {
    "git": "https://github.com/wordpress-mobile/WordPressKit-iOS.git",
    "tag": "1.0.1"
  },
  "source_files": "WordPressKit/**/*.{h,m,swift}",
  "private_header_files": "WordPressKit/Private/*.h",
  "requires_arc": true,
  "header_dir": "WordPressKit",
  "dependencies": {
    "AFNetworking": [
      "3.2.1"
    ],
    "Alamofire": [
      "4.7.2"
    ],
    "CocoaLumberjack": [
      "3.4.2"
    ],
    "WordPressShared": [
      "1.0.1"
    ],
    "wpxmlrpc": [
      "0.8.3"
    ],
    "UIDeviceIdentifier": [
      "~> 0.4"
    ]
  }
}
