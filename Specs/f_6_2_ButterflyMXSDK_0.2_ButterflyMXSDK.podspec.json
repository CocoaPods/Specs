{
  "name": "ButterflyMXSDK",
  "version": "0.2",
  "license": {
    "type": "Apache-2.0",
    "file": "LICENSE"
  },
  "summary": "A Swift framework to implement ButterflyMX SDK",
  "homepage": "https://github.com/zuchie/bmxsdk.git",
  "authors": {
    "zuchie": "zuchie@gmail.com"
  },
  "platforms": {
    "ios": "10.0"
  },
  "source": {
    "http": "https://github.com/zuchie/bmxsdk/raw/master/ButterflyMXSDK.zip"
  },
  "ios": {
    "vendored_frameworks": "ButterflyMXSDK.framework",
    "source_files": "pjsip/include/**/*.{h,hpp}",
    "public_header_files": "pjsip/include/**/*.{h,hpp}",
    "header_dir": "include",
    "preserve_paths": [
      "pjsip/include/**/*.{h,hpp}",
      "pjsip/lib/pjsip.a"
    ],
    "vendored_libraries": "pjsip/lib/pjsip.a",
    "libraries": "pjsip",
    "requires_arc": false
  },
  "dependencies": {
    "Alamofire": [
      "~> 4.7.3"
    ]
  }
}
