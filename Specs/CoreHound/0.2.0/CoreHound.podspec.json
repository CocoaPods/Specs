{
  "name": "CoreHound",
  "version": "0.2.0",
  "summary": "The MediaHound iOS SDK",
  "homepage": "https://github.com/MediaHound/CoreHound",
  "license": "Apache",
  "authors": {
    "Dustin Bachrach": "dustin@mediahound.com"
  },
  "source": {
    "git": "https://github.com/MediaHound/CoreHound.git",
    "tag": "0.2.0"
  },
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "source_files": "Pod/**/*.{h,m}",
  "private_header_files": [
    "Pod/Model/Internal/**/*.h",
    "Pod/**/*+Internal.h",
    "Pod/Network/Internal/**/*.h"
  ],
  "resources": [
    "Pod/Resources/*.cer"
  ],
  "dependencies": {
    "AFNetworking": [
      "~> 2.6"
    ],
    "AtSugar": [
      "~> 0.1"
    ],
    "Avenue": [
      "~> 0.4"
    ],
    "AvenueFetcher": [
      "~> 0.4"
    ],
    "JSONModel": [
      "~> 1.1.2"
    ],
    "PromiseKit/CorePromise": [
      "~> 3.0"
    ],
    "UICKeyChainStore": [
      "~> 2.0"
    ]
  }
}
