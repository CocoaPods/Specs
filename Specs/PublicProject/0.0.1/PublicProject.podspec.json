        },
        {
          "name": "QBase64",
          "source_files": "Classes/Vendors/QBase64/**/*.{h,m}",
          "public_header_files": "Classes/Vendors/QBase64/*.h",
          "requires_arc": false
        },
        {
          "name": "OpenUDID",
          "source_files": "Classes/Vendors/OpenUDID/**/*.{h,m}",
          "public_header_files": "Classes/Vendors/OpenUDID/*.h",
          "requires_arc": false
        }
      ]
    }
  ]
