          "source_files": "Classes/Vendors/Timer/**/*.{h,m}",
          "public_header_files": "Classes/Vendors/Timer/*.h",
          "dependencies": {
            "PublicProject/Macro": [

            ]
          }
        },
        {
          "name": "GTMBase64",
          "source_files": "Classes/Vendors/GTMBase64/**/*.{h,m}",
          "public_header_files": "Classes/Vendors/GTMBase64/*.h",
