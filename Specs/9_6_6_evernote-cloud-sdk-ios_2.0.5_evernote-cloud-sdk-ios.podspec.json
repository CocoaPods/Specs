{
  "name": "evernote-cloud-sdk-ios",
  "version": "2.0.5",
  "summary": "Evernote Cloud SDK for iOS.",
  "homepage": "https://github.com/evernote/evernote-cloud-sdk-ios",
  "license": "https://github.com/evernote/evernote-cloud-sdk-ios/blob/master/LICENSE",
  "authors": {
    "Evernote": "devsupport@evernote.com"
  },
  "source": {
    "git": "https://github.com/evernote/evernote-cloud-sdk-ios.git",
    "tag": "2.0.5"
  },
  "platforms": {
    "ios": "8.0"
  },
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "$(SDKROOT)/usr/include/libxml2"
  },
  "exclude_files": "evernote-sdk-ios/evernote-sdk-ios-Prefix.pch",
  "resources": "ENSDKResources.bundle",
  "source_files": [
    "evernote-sdk-ios/**/*.{h,m}",
    "evernote-sdk-ios/evernote-sdk-ios-Prefix.pch",
    "ENSDKResources.bundle"
  ],
  "requires_arc": true,
  "frameworks": [
    "MobileCoreServices",
    "CoreGraphics",
    "Foundation",
    "UIKit"
  ],
  "libraries": "xml2"
}
