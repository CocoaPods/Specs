  "summary": "Some swift extension for myself.",
  "homepage": "https://github.com/GPX-NSlog/Flare",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Gpx": "gpxnslog@gmail.com"
  },
  "source": {
