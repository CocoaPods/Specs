      "name": "All",
      "source_files": "Flare/**/*.{swift}",
      "frameworks": "UIKit",
      "dependencies": {
        "Hue": [
          "~> 5.0.0"
        ],
        "HandyJSON": [
          "~> 5.0.0"
        ],
        "Moya": [
          "~> 13.0.1"
        ]
      }
    },
    {
      "name": "Net",
      "source_files": "Flare/Net/**/*.{swift}",
      "frameworks": "UIKit",
      "dependencies": {
        "HandyJSON": [
          "~> 5.0.0"
        ],
        "Moya": [
          "~> 13.0.1"
        ]
      }
    },
    {
      "name": "UI",
      "source_files": "Flare/UI/**/*.{swift}",
      "frameworks": "UIKit",
      "dependencies": {
        "Hue": [
          "~> 5.0.0"
        ]
      }
    }
  ]
