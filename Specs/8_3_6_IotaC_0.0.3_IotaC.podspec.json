{
  "name": "IotaC",
  "swift_version": "4.0",
  "version": "0.0.3",
  "summary": "A Swift wrapper for Iota C implementation",
  "description": "A Swift wrapper for Iota C implementation.",
  "homepage": "https://github.com/pascalbros/IotaC",
  "license": "MIT (Copyright (c) 2018 Pasquale Ambrosini)",
  "source": {
    "git": "https://github.com/pascalbros/IotaC.git",
    "tag": "v0.0.3"
  },
  "authors": {
    "Pasquale Ambrosini": "pasquale.ambrosini@gmail.com"
  },
  "platforms": {
    "ios": "8.0",
    "osx": "10.10"
  },
  "requires_arc": true,
  "module_map": "IotaC.modulemap",
  "source_files": "Sources/**/*.{swift,c,h}",
  "pod_target_xcconfig": {
    "SWIFT_INCLUDE_PATHS": "$(PODS_TARGET_SRCROOT)/Sources/fastiota/**",
    "LIBRARY_SEARCH_PATHS": "$(PODS_TARGET_SRCROOT)/Sources/",
    "SWIFT_VERSION": "4.0"
  },
  "preserve_paths": "Sources/fastiota/include/module.modulemap"
}
