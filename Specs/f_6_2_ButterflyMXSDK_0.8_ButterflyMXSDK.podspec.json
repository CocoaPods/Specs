{
  "name": "ButterflyMXSDK",
  "version": "0.8",
  "license": {
    "type": "Apache-2.0",
    "file": "LICENSE"
  },
  "summary": "A Swift framework to implement ButterflyMX SDK",
  "homepage": "https://github.com/zuchie/bmxsdk.git",
  "authors": {
    "zuchie": "zuchie@gmail.com"
  },
  "platforms": {
    "ios": "10.0"
  },
  "source": {
    "http": "https://github.com/zuchie/bmxsdk/raw/master/ButterflyMXSDK.zip"
  },
  "ios": {
    "vendored_frameworks": "ButterflyMXSDK.framework",
    "preserve_paths": "pjsip/**/*",
    "xcconfig": {
      "HEADER_SEARCH_PATHS": "$(inherited) \"${PODS_ROOT}/ButterflyMXSDK/pjsip/include\"",
      "LIBRARY_SEARCH_PATHS": "$(inherited) \"${PODS_ROOT}/ButterflyMXSDK/pjsip/lib\""
    }
  },
  "dependencies": {
    "Alamofire": [
      "~> 4.7.3"
    ]
  }
}
