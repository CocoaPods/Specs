  "default_subspecs": "Core",
  "subspecs": [
    {
      "name": "Core",
      "source_files": [
        "ALSPayMent/Classes/**/*",
        "ALSPayMent/lib/AlipaySDK.framework/Headers/*.h"
      ],
      "public_header_files": [
        "ALSPayMent/lib/AlipaySDK.framework/Headers/*.h",
        "ALSPayMent/Classes/**/*.h"
      ],
      "resources": [
