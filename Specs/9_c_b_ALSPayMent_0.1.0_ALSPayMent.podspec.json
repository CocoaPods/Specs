          "z",
          "sqlite3.0",
          "c++",
          "stdc++",
          "ssl",
          "crypto"
        ]
      },
      "xcconfig": {
        "OTHER_LDFLAGS": "-ObjC",
        "ENABLE_BITCODE": "NO",
        "HEADER_SEARCH_PATHS": "/Users/yangziminyangzimin/Desktop/test/ALSPayMent/ALSPayMent/ALSPayMent/Classes/openssl/include/**"
      },
      "vendored_frameworks": "ALSPayMent/lib/AlipaySDK.framework",
