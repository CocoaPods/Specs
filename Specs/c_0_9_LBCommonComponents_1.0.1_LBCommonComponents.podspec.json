        },
        {
          "name": "NSString+ToDate",
          "source_files": "LBCommonComponents/Category/NSString+ToDate/**/*.{h,m}"
        },
        {
          "name": "UIColor+ToImage",
          "source_files": "LBCommonComponents/Category/UIColor+ToImage/**/*.{h,m}"
        },
        {
