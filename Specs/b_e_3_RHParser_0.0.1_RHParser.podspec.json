{
  "name": "RHParser",
  "version": "0.0.1",
  "summary": "html、xml等标记语言解析。可以方便的转换为NSAttributedString。",
  "description": "html、xml等标记语言解析。可以方便的把html转换为NSAttributedString。支持自定义样式。",
  "homepage": "https://github.com/zhu410289616/RHParser",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "zhu410289616": "zhu410289616@163.com"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source": {
    "git": "https://github.com/zhu410289616/RHParser.git",
    "tag": "0.0.1"
  },
  "source_files": [
    "RHParser/*.{h,m}",
    "RHParser/**/*.{h,m}"
  ],
  "frameworks": [
    "UIKit",
    "Foundation"
  ],
  "libraries": "xml2",
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "$(SDKROOT)/usr/include/libxml2"
  }
}
