{
  "name": "Textile",
  "version": "2.0.4",
  "summary": "Encrypted, recoverable, schema-based, cross-application data storage built on IPFS and LibP2P",
  "description": "The Textile pod provides iOS native access and helpers for the Textile platform.\nLearn more about Textile at https://github.com/textileio/go-textile.",
  "homepage": "https://github.com/textileio/ios-textile",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Textile": "contact@textile.io"
  },
  "source": {
    "git": "https://github.com/textileio/ios-textile.git",
    "tag": "2.0.4"
  },
  "platforms": {
    "ios": "10.0"
  },
  "source_files": "Textile/Classes/**/*",
  "requires_arc": true,
  "pod_target_xcconfig": {
    "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited) GPB_USE_PROTOBUF_FRAMEWORK_IMPORTS=1",
    "OTHER_LDFLAGS[arch=i386]": "-Wl,-read_only_relocs,suppress"
  },
  "dependencies": {
    "Protobuf": [
      "~> 3.7"
    ],
    "TextileCore": [
      "~> 0.6.6"
    ]
  }
}
