{
  "name": "Lela",
  "version": "0.2.1",
  "summary": "Provides perceptial difference screen testing within KIF.",
  "homepage": "https://github.com/kif-framework/Lela/",
  "license": "GPL 3.0",
  "authors": {
    "Brian Nickel": "brian.nickel@gmail.com"
  },
  "source": {
    "git": "https://github.com/kif-framework/Lela.git",
    "tag": "v0.2.1"
  },
  "platforms": {
    "ios": "5.1"
  },
  "public_header_files": [
    "Lela/Lela.h",
    "Lela/KIFUITestActor+Lela.h"
  ],
  "frameworks": [
    "UIKit",
    "QuartzCore",
    "CoreGraphics"
  ],
  "libraries": [
    "c++",
    "stdc++"
  ],
  "requires_arc": true,
  "default_subspecs": "XCTest",
  "subspecs": [
    {
      "name": "OCUnit",
      "source_files": "Lela/**/*.{h,m,mm}",
      "dependencies": {
        "KIF/OCUnit": [
          "~> 3.0"
        ]
      },
      "frameworks": "SenTestingKit",
      "xcconfig": {
        "OTHER_CFLAGS": "-DKIF_SENTEST"
      }
    },
    {
      "name": "XCTest",
      "source_files": "Lela/**/*.{h,m,mm}",
      "dependencies": {
        "KIF/XCTest": [
          "~> 3.0"
        ]
      },
      "frameworks": "XCTest",
      "xcconfig": {
        "OTHER_CFLAGS": "-DKIF_XCTEST"
      }
    }
  ]
}
