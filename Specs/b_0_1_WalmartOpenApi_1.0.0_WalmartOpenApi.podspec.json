    "ios": "8.0"
  },
  "requires_arc": true,
  "homepage": "https://developer.walmartlabs.com/",
  "license": "Private",
  "source": {
    "http": "https://affiliates.walmart.com/linksharepromo/assets/WalmartOpenApi-1.0.0.zip"
  },
  "authors": {
    "@WalmartLabs": "affilops@wal-mart.com"
  },
  "preserve_paths": [
    "WalmartOpenApi.framework",
    "docs"
