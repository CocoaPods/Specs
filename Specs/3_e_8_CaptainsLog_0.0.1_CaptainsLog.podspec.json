{
  "name": "CaptainsLog",
  "version": "0.0.1",
  "summary": "Captain's Log logging library",
  "description": "Swift library to integrate in your project to enable logging using Captain's Log app.",
  "homepage": "https://www.reactant.tech",
  "license": "MIT",
  "authors": {
    "Tadeas Kriz": "tadeas@brightify.org",
    "Robin Krenecky": "rkrenecky@gmail.com"
  },
  "source": {
    "git": "https://github.com/Brightify/CaptainsLog.git",
    "tag": "0.0.1"
  },
  "swift_version": "4.2",
  "platforms": {
    "ios": "10.3",
    "osx": "10.13"
  },
  "default_subspecs": [
    "Models",
    "Discovery",
    "Core",
    "NSURLSession"
  ],
  "subspecs": [
    {
      "name": "Models",
      "source_files": [
        "Sources/Models/**/*.swift"
      ]
    },
    {
      "name": "Discovery",
      "dependencies": {
        "CaptainsLog/Models": [

        ]
      },
      "source_files": [
        "Sources/Discovery/**/*.swift"
      ]
    },
    {
      "name": "Core",
      "dependencies": {
        "CaptainsLog/Discovery": [

        ]
      },
      "source_files": [
        "Sources/Core/**/*.swift"
      ]
    },
    {
      "name": "Fetcher",
      "dependencies": {
        "CaptainsLog/Core": [

        ],
        "Fetcher/Core": [

        ]
      },
      "source_files": [
        "Sources/Fetcher/**/*.swift"
      ]
    },
    {
      "name": "SwiftyBeaver",
      "dependencies": {
        "CaptainsLog/Core": [

        ],
        "SwiftyBeaver": [

        ]
      },
      "source_files": [
        "Sources/SwiftyBeaver/**/*.swift"
      ]
    },
    {
      "name": "NSURLSession",
      "dependencies": {
        "CaptainsLog/Core": [

        ]
      },
      "source_files": [
        "Sources/NSURLSession/**/*.swift"
      ]
    },
    {
      "name": "CocoaLumberjack",
      "dependencies": {
        "CaptainsLog/Core": [

        ],
        "CocoaLumberjack/Swift": [

        ]
      },
      "source_files": [
        "Sources/CocoaLumberjack/**/*.swift"
      ]
    }
  ]
}
