      "libaom.framework",
      "libass.framework",
      "libcharset.framework",
      "libiconv.framework",
      "libogg.framework",
      "libpng.framework",
      "libtheora.framework",
      "libtheoradec.framework",
      "libtheoraenc.framework",
      "libuuid.framework",
      "libvorbis.framework",
      "libvorbisenc.framework",
      "libvorbisfile.framework",
      "libvpx.framework",
      "libwebp.framework",
