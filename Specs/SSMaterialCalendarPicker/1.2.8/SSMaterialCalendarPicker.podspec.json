{
  "name": "SSMaterialCalendarPicker",
  "version": "1.2.8",
  "summary": "A lightly-customizable calendar pop-up view for range selection inspired by Google's Material Design Guidelines and apps.",
  "homepage": "https://github.com/iurichiba/SSMaterialCalendarPicker",
  "screenshots": [
    "http://i.imgur.com/h2kNho0.png",
    "http://i.imgur.com/30Eivbn.gif"
  ],
  "license": {
    "type": "MIT",
    "file": "LICENSE.md"
  },
  "authors": {
    "Iuri Chiba": "iurichiba@gmail.com"
  },
  "platforms": {
    "ios": "7.1"
  },
  "source": {
    "git": "https://github.com/iurichiba/SSMaterialCalendarPicker.git",
    "tag": "v1.2.8"
  },
  "source_files": [
    "SSMaterialCalendarPicker/Classes",
    "SSMaterialCalendarPicker/Classes/**/*.{h,m}"
  ],
  "exclude_files": "SSMaterialCalendarPicker/Classes/Exclude",
  "resource_bundles": {
    "CalendarViews": [
      "SSMaterialCalendarPicker/Classes/**/*.xib"
    ]
  },
  "requires_arc": true,
  "xcconfig": {
    "FRAMEWORK_SEARCH_PATHS": "$(PODS_ROOT)/../../Framework $(PODS_ROOT)/../External"
  }
}
