    },
    {
      "name": "PictureBrowse",
      "source_files": "CMKit/CMKit-Tool(工具类)/Tool-PictureBrowse(图片浏览器)/*.{h,m}",
      "dependencies": {
        "SDWebImage": [
          "~> 4.0.0-beta2"
        ]
      },
      "resources": "CMKit/CMKit-Tool(工具类)/Tool-PictureBrowse(图片浏览器)/MSSBrowse.bundle"
    },
    {
      "name": "UnLock",
      "source_files": "CMKit/CMKit-Tool(工具类)/Tool-UnLock(手势、指纹解锁)/*.{h,m}",
      "resources": "CMKit/CMKit-Tool(工具类)/Tool-UnLock(手势、指纹解锁)/DWUNlock.bundle"
    }
  ]
}
