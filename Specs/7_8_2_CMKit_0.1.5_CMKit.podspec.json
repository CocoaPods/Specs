      "dependencies": {
        "CMKit/Category": [

        ]
      }
    },
    {
      "name": "ProgressHUD",
      "source_files": "CMKit/CMKit-Tool(工具类)/Tool-HUD(提示框)/*.{h,m}",
      "resources": "CMKit/CMKit-Tool(工具类)/Tool-HUD(提示框)/MBProgressHUD+CMKit.bundle",
      "frameworks": [
        "CoreGraphics",
        "QuartzCore"
      ]
    },
    {
      "name": "QRCode",
      "source_files": "CMKit/CMKit-Tool(工具类)/Tool-QRCode(二维码)/*.{h,m}",
