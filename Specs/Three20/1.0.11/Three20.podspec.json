      "header_dir": "Three20UICommon",
      "header_mappings_dir": "src/Three20UICommon/Headers",
      "dependencies": {
        "Three20/Core": [

        ]
      },
      "frameworks": [
        "UIKit",
        "CoreGraphics"
      ]
    },
    {
      "name": "UINavigator",
      "source_files": "src/Three20UINavigator/{Sources,Headers}/**/*.{h,m}",
