          "dependencies": {
            "Three20/ext/JSON": [

            ]
          }
        },
        {
          "name": "YAJL",
          "source_files": [
            "src/extThree20JSON/Vendors/YAJL/*.{h,m}",
            "src/extThree20JSON/Vendors/YAJL/{GHKit,GTM}/*.{h,m}",
