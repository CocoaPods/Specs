    "SWIFT_OBJC_BRIDGING_HEADER": "$(PODS_TARGET_SRCROOT)/"
  },
  "cocoapods_version": ">= 1.7.1",
  "source_files": "DJIUXSDKBeta/**/*.{h,m}",
  "dependencies": {
    "DJI-UXSDK-iOS-Beta-Core": [
      "0.1.1"
    ],
    "DJI-UXSDK-iOS-Beta-Communication": [
      "0.1.1"
    ],
    "DJI-UXSDK-iOS-Beta-Widgets": [
      "0.1.1"
