          "name": "UIKit",
          "source_files": "BitautoTech/BitautoTech/Category/UIKit/*.{h,m}",
          "dependencies": {
            "BitautoTech/Category/Foundation": [

            ]
          }
        },
        {
          "name": "Foundation",
          "source_files": "BitautoTech/BitautoTech/Category/Foundation/*.{h,m}"
        }
      ]
    },
    {
      "name": "Indicator",
      "platforms": {
        "ios": "8.0"
