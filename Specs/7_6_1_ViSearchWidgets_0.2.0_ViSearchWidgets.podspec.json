{
  "name": "ViSearchWidgets",
  "version": "0.2.0",
  "summary": "ViSenze mobile widgets",
  "description": "ViSenze mobile widgets SDK (Swift)",
  "homepage": "https://github.com/visenze/visearch-widget-swift",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Ngo Hung": "hung@visenze.com"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source": {
    "git": "https://github.com/visenze/visearch-widget-swift.git",
    "tag": "0.2.0"
  },
  "source_files": "ViSearchWidgets/ViSearchWidgets/**/*.{h,swift}",
  "resource_bundles": {
    "com.visenze.icons": [
      "ViSearchWidgets/ViSearchWidgets/**/*.xcassets"
    ],
    "com.visenze.fonts": [
      "ViSearchWidgets/ViSearchWidgets/**/*.ttf"
    ],
    "com.visenze.string": [
      "ViSearchWidgets/ViSearchWidgets/String/*.strings"
    ],
    "com.visenze.ui": [
      "ViSearchWidgets/ViSearchWidgets/**/*.xib"
    ]
  },
  "frameworks": [
    "Photos",
    "AVFoundation",
    "MediaPlayer"
  ],
  "requires_arc": true,
  "dependencies": {
    "ViSearchSDK": [
      "~> 1.1.0"
    ],
    "Kingfisher": [
      "~> 3.2.1"
    ],
    "LayoutKit": [
      "~> 4.0.0"
    ]
  },
  "xcconfig": {
    "SWIFT_VERSION": "3.0"
  },
  "pushed_with_swift_version": "3.0"
}
