{
  "name": "BinanceApiClient",
  "version": "0.1.0",
  "summary": "A lightweight client lib for Binance.com",
  "description": "BinanceApiClient is a lightweight client lib for Binance.com,\n which fullfill all the APIs provided by [Binance](https://www.binance.com/restapipub.html).",
  "homepage": "https://github.com/xilin/binance-objc-api",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "XiLin": "linxi60@gmail.com"
  },
  "source": {
    "git": "https://github.com/xilin/binance-objc-api.git",
    "tag": "0.1.0"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source_files": "BinanceApiClient/Classes/**/*",
  "exclude_files": "Example",
  "public_header_files": "BinanceApiClient/Classes/**/*.h",
  "frameworks": "CoreFoundation",
  "dependencies": {
    "JSONModel": [
      "~> 1.7.0"
    ],
    "YTKNetwork": [
      "~> 2.0.4"
    ],
    "CocoaLumberjack": [
      "~> 2.3.0"
    ],
    "YYCategories": [
      "~> 1.0.4"
    ],
    "BlocksKit": [
      "~> 2.2.0"
    ],
    "SocketRocket": [
      "~> 0.5.1"
    ]
  },
  "requires_arc": true
}
