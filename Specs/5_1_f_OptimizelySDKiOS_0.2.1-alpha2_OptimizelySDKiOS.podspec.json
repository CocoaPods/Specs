  "subspecs": [
    {
      "name": "JSONModel",
      "dependencies": {
        "JSONModel": [
          "=1.3.0"
        ]
      },
      "xcconfig": {
        "FRAMEWORK_SEARCH_PATHS": "$(PODS_ROOT)/JSONModel"
      }
    }
  ]
