    "AFNetworking": [
      "~> 2.5.4"
    ],
    "SVProgressHUD": [
      "~> 1.1.3"
    ],
    "SDWebImage": [
      "~> 3.7.2"
    ],
    "Masonry": [
      "~> 0.6.2"
    ],
    "FMDB": [
      "~> 2.5"
    ],
    "EGOTableViewPullRefresh": [
      "~> 0.1.0"
    ],
    "IQKeyboardManager": [
      "~> 3.2.4"
    ]
  }
