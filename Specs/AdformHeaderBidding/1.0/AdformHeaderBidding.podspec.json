{
  "name": "AdformHeaderBidding",
  "version": "1.0",
  "homepage": "https://github.com/adform/adform-header-bidding-ios",
  "summary": "Adform Header Bidding SDK for iOS.",
  "description": "By using Header Bidding solution, you can have an access to external demand that otherwise may not be available through your primary ad server. Therefore Adform introduces Header Bidding SDK, which allows to check demand from Adform side.",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": "Adform",
  "platforms": {
    "ios": "7.1"
  },
  "source": {
    "git": "https://github.com/adform/adform-header-bidding-ios.git",
    "tag": "v.1.0"
  },
  "preserve_paths": "AdformHeaderBidding.framework",
  "public_header_files": "AdformHeaderBidding.framework/Versions/A/Headers/*.h",
  "vendored_frameworks": "AdformHeaderBidding.framework",
  "xcconfig": {
    "FRAMEWORK_SEARCH_PATHS": "$(PODS_ROOT)/AdformHeaderBidding/**",
    "HEADER_SEARCH_PATHS": "$(SDKROOT)/usr/include/libxml2"
  },
  "frameworks": [
    "UIKit",
    "SystemConfiguration",
    "AdSupport",
    "CoreTelephony",
    "CoreLocation"
  ],
  "requires_arc": true
}
