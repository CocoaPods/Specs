{
  "name": "LoLAPI",
  "version": "0.1.2",
  "summary": "RESTful Objective-C client for the popular online game \"League of Legends\" by Riot Games.",
  "description": "\t\t\tLoLAPI is the most powerful Objective-C RESTful client for the popular online game \"League of Legends\" by Riot Games. All client responses are fully object mapped, allowing developers to focus on writing their application without the hassle of consuming web services.\n",
  "license": "Apache License, Version 2.0",
  "authors": {
    "troystump": "troystump@gmail.com"
  },
  "homepage": "https://github.com/troystump/LoLAPI",
  "social_media_url": "http://twitter.com/troystump",
  "source": {
    "git": "https://github.com/troystump/LoLAPI.git",
    "tag": "v0.1.2"
  },
  "source_files": "LoLAPI/Code/**/*.{h,m}",
  "header_mappings_dir": "LoLAPI/Code",
  "platforms": {
    "ios": "5.0",
    "osx": "10.7"
  },
  "requires_arc": true,
  "prefix_header_contents": "      #import <Availability.h>\n\n      #define _AFNETWORKING_PIN_SSL_CERTIFICATES_\n\n      #if __IPHONE_OS_VERSION_MIN_REQUIRED\n          #import <SystemConfiguration/SystemConfiguration.h>\n          #import <MobileCoreServices/MobileCoreServices.h>\n          #import <Security/Security.h>\n      #else\n          #import <SystemConfiguration/SystemConfiguration.h>\n          #import <CoreServices/CoreServices.h>\n          #import <Security/Security.h>\n      #endif\n",
  "dependencies": {
    "RestKit": [
      "~> 0.23.2"
    ]
  }
}
