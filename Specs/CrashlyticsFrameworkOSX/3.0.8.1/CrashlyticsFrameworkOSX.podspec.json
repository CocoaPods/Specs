  "homepage": "http://crashlytics.com",
  "license": {
    "type": "Copyright",
    "file": "LICENSE"
  },
  "authors": "Crashlytics",
  "source": {
    "git": "https://github.com/abbeycode/CrashlyticsFrameworkOSX.git",
    "tag": "3.0.8.1"
  },
  "platforms": {
    "osx": "10.6"
