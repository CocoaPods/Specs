{
  "name": "AIQJSBridge",
  "version": "1.0.3",
  "summary": "Provides JavaScript access to AppearIQ cloud services.",
  "homepage": "https://github.com/appear/AIQJSBridge",
  "license": "MIT",
  "authors": {
    "Appear Networks AB": "ios@appearnetworks.com"
  },
  "source": {
    "git": "https://github.com/appear/AIQJSBridge.git",
    "tag": "1.0.3"
  },
  "social_media_url": "https://twitter.com/appear",
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "source_files": "Pod/Classes/**/*",
  "resource_bundles": {
    "AIQJSBridge": [
      "Pod/Assets/*"
    ]
  },
  "public_header_files": "Pod/Classes/**/AIQ*.h",
  "dependencies": {
    "AIQCoreLib": [
      "~> 1.5.2"
    ],
    "Google/Analytics": [

    ]
  },
  "subspecs": [
    {
      "name": "Cordova",
      "source_files": "Cordova/Classes/**/*",
      "public_header_files": [
        "Cordova/Classes/**/CDV*.h",
        "Cordova/Classes/**/NS*.h",
        "Cordova/Classes/**/UI*.h"
      ],
      "header_dir": "Cordova",
      "header_mappings_dir": "Cordova/Classes"
    }
  ]
}
