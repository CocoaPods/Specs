  "pod_target_xcconfig": {
    "SWIFT_INCLUDE_PATHS": "$(SRCROOT)/TokenRealmCore/Modules",
    "OTHER_LDFLAGS": "-lObjC",
    "SWIFT_OPTIMIZATION_LEVEL": "-Owholemodule"
