{
  "name": "HS-Font-Icons",
  "version": "1.3.0",
  "swift_versions": "5.0",
  "license": "MIT",
  "summary": "Font icons. Easy to use icons that look perfect at any scale",
  "description": "Use font icons instead of images\nGoogle Material Design Icons 3.0.1\nFont Awesome 5.0\nEasy to add new fonts if you like\nEasy use for buttons, labels, etc\nGenerate images if you must!",
  "homepage": "https://github.com/ConfusedVorlon/HS-Font-Icons",
  "screenshots": "https://raw.githubusercontent.com/ConfusedVorlon/HS-Font-Icons/master/resources/image1.png",
  "authors": [
    "Rob Jonson",
    "Patrik Vaberer"
  ],
  "source": {
    "git": "https://github.com/ConfusedVorlon/HS-Font-Icons.git",
    "tag": "1.3.0"
  },
  "platforms": {
    "ios": "9.0"
  },
  "frameworks": [
    "UIKit",
    "CoreText"
  ],
  "requires_arc": true,
  "default_subspecs": [
    "Core",
    "GMD",
    "FARegular",
    "FASolid",
    "FABrand"
  ],
  "swift_version": "5.0",
  "subspecs": [
    {
      "name": "Core",
      "source_files": "Source/Core/*.swift"
    },
    {
      "name": "GMD",
      "source_files": "Source/GMD/*.swift",
      "resource_bundles": {
        "MaterialIcons-Regular": "Source/GMD/GMDIcons.ttf"
      },
      "dependencies": {
        "HS-Font-Icons/Core": [

        ]
      }
    },
    {
      "name": "FARegular",
      "source_files": "Source/FARegular/*.swift",
      "resource_bundles": {
        "FontAwesome5FreeRegular": "Source/FARegular/*.ttf"
      },
      "dependencies": {
        "HS-Font-Icons/Core": [

        ]
      }
    },
    {
      "name": "FASolid",
      "source_files": "Source/FASolid/*.swift",
      "resource_bundles": {
        "FontAwesome5FreeSolid": "Source/FASolid/*.ttf"
      },
      "dependencies": {
        "HS-Font-Icons/Core": [

        ]
      }
    },
    {
      "name": "FABrand",
      "source_files": "Source/FABrands/*.swift",
      "resource_bundles": {
        "FontAwesome5BrandsRegular": "Source/FABrands/*.ttf"
      },
      "dependencies": {
        "HS-Font-Icons/Core": [

        ]
      }
    }
  ]
}
