{
  "name": "RxFirebase",
  "version": "0.2",
  "summary": "RxSwift extensions for Firebase.",
  "description": "RxSwift extensions for Firebase.\nIncluding for now Database, Firestore, RemoteConfig",
  "homepage": "https://github.com/RxSwiftCommunity/RxFirebase",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Arnaud Dorgans": "arnaud.dorgans@gmail.com"
  },
  "source": {
    "git": "https://github.com/RxSwiftCommunity/RxFirebase.git",
    "tag": "0.2"
  },
  "cocoapods_version": ">= 1.4.0",
  "swift_version": "4.0",
  "platforms": {
    "ios": "8.0"
  },
  "static_framework": true,
  "dependencies": {
    "RxSwift": [
      "~> 4"
    ],
    "RxCocoa": [
      "~> 4"
    ]
  },
  "subspecs": [
    {
      "name": "Firestore",
      "source_files": "Sources/Firestore/**/*",
      "dependencies": {
        "FirebaseFirestore": [
          "~> 0.12"
        ]
      }
    },
    {
      "name": "RemoteConfig",
      "source_files": "Sources/RemoteConfig/**/*",
      "dependencies": {
        "FirebaseRemoteConfig": [
          "~> 3"
        ]
      }
    },
    {
      "name": "Database",
      "source_files": "Sources/Database/**/*",
      "dependencies": {
        "FirebaseDatabase": [
          "~> 5"
        ]
      }
    }
  ]
}
