{
  "name": "mobile-ffmpeg-min-gpl",
  "version": "4.2.LTS",
  "summary": "Mobile FFmpeg Min GPL Static Framework",
  "description": "Includes FFmpeg v4.2-dev-480 with libvid.stab v1.1.0, x264 20181224-2245-stable, x265 v2.9 and xvidcore v1.3.5 libraries enabled.",
  "homepage": "https://github.com/tanersener/mobile-ffmpeg",
  "authors": {
    "Taner Sener": "tanersener@gmail.com"
  },
  "license": {
    "type": "GPL-3.0",
    "file": "mobileffmpeg.framework/LICENSE"
  },
  "platforms": {
    "ios": "9.3"
  },
  "requires_arc": true,
  "ios": {
    "frameworks": [
      "AudioToolbox",
      "AVFoundation",
      "CoreMedia",
      "VideoToolbox"
    ],
    "vendored_frameworks": [
      "mobileffmpeg.framework",
      "libavcodec.framework",
      "libavdevice.framework",
      "libavfilter.framework",
      "libavformat.framework",
      "libavutil.framework",
      "libswresample.framework",
      "libswscale.framework",
      "libvidstab.framework",
      "x264.framework",
      "x265.framework",
      "xvidcore.framework"
    ]
  },
  "libraries": [
    "z",
    "bz2",
    "c++"
  ],
  "source": {
    "http": "https://github.com/tanersener/mobile-ffmpeg/releases/download/v4.2.LTS/mobile-ffmpeg-min-gpl-4.2.LTS-ios-framework.zip"
  }
}
