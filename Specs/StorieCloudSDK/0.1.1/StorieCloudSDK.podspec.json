    ],
    "SwiftyJSON": [
      "~> 2.3"
    ],
    "RxSwift": [
      "~> 2.0"
    ],
    "RxCocoa": [
      "~> 2.0"
    ],
    "Moya/RxSwift": [
      "~> 6.4"
    ],
    "SwiftyBeaver": [
      "~> 0.5"
    ],
    "FileMD5Hash": [
      "~> 2.0"
    ]
  }
