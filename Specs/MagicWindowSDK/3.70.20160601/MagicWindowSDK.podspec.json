{
  "name": "MagicWindowSDK",
  "version": "3.70.20160601",
  "summary": "MagicWindowSDK for Cocoapods convenience.",
  "homepage": "http://magicwindow.cn/",
  "license": "MIT",
  "authors": {
    "MagicWindow": "support@magicwindow.cn"
  },
  "source": {
    "git": "https://github.com/magicwindow/mw-iossdk.git",
    "tag": "3.70.20160601"
  },
  "platforms": {
    "ios": "7.0"
  },
  "source_files": "MagicWindowSDK/*.{h,m}",
  "public_header_files": "MagicWindowSDK/*.h",
  "resources": "MagicWindowSDK/MagicWindow.bundle",
  "preserve_paths": "MagicWindowSDK/libMagicWindowSDK.a",
  "vendored_libraries": "MagicWindowSDK/libMagicWindowSDK.a",
  "requires_arc": true,
  "xcconfig": {
    "LIBRARY_SEARCH_PATHS": "\"$(PODS_ROOT)/MagicWindowSDK/MagicWindowSDK\"",
    "HEADER_SEARCH_PATHS": "\"$(PODS_ROOT)/MagicWindowSDK/MagicWindowSDK\""
  },
  "frameworks": [
    "AdSupport",
    "CoreTelephony",
    "CoreGraphics",
    "CoreFoundation",
    "SystemConfiguration",
    "CoreLocation",
    "SafariServices"
  ],
  "libraries": [
    "z",
    "sqlite3.0"
  ],
  "subspecs": [
    {
      "name": "WeChat",
      "vendored_libraries": "MagicWindowSDK/WeChat/*.a",
      "source_files": "MagicWindowSDK/WeChat/*.{h,m}",
      "public_header_files": "MagicWindowSDK/WeChat/*.h",
      "libraries": "sqlite3"
    }
  ]
}
