{
  "name": "PLVCloudClassSDK",
  "version": "0.8.11",
  "summary": "PLVCloudClassSDK. PLV直播观看。",
  "homepage": "https://www.polyv.net/",
  "authors": {
    "LLH": "luolinhao@polyv.net"
  },
  "source": {
    "http": "https://repo.polyv.net/ios/download/plvcloudclasssdk/PLVCloudClassSDK_0.8.11+20200108.zip"
  },
  "vendored_frameworks": [
    "*.framework",
    "PLVCloudClassSDK.framework/Frameworks/*.framework"
  ],
  "default_subspecs": [
    "Basic",
    "Extra",
    "Socket"
  ],
  "xcconfig": {
    "OTHER_LDFLAGS": "$(inherited) -ObjC -lz"
  },
  "requires_arc": true,
  "platforms": {
    "ios": "8.0"
  },
  "subspecs": [
    {
      "name": "Basic",
      "vendored_frameworks": [
        "*.framework",
        "PLVCloudClassSDK.framework/Frameworks/*.framework"
      ]
    },
    {
      "name": "Extra",
      "dependencies": {
        "PLVCloudClassSDKExtra": [
          "0.1.12"
        ]
      }
    },
    {
      "name": "Socket",
      "dependencies": {
        "PLVSocket": [
          "0.1.2"
        ]
      }
    }
  ]
}
