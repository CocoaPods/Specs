{
  "name": "BDGhostover",
  "version": "8.0.0",
  "summary": "An SDK that allows to load and display Ghostover ads",
  "homepage": "https://www.ghostover.com",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": "Bitdrome Srl",
  "platforms": {
    "ios": "9.0"
  },
  "source": {
    "http": "https://repo.ghostover.com/ios/cocoapods/BDGhostover800.zip"
  },
  "source_files": "BDGhostover.framework/Headers/*.{h}",
  "ios": {
    "vendored_frameworks": "BDGhostover.framework"
  },
  "frameworks": [
    "SpriteKit",
    "SafariServices",
    "UIKit",
    "AVFoundation",
    "AdSupport",
    "QuartzCore"
  ],
  "libraries": "xml2",
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "$(SDKROOT)/usr/include/libxml2",
    "OTHER_LDFLAGS": "-ObjC"
  }
}
