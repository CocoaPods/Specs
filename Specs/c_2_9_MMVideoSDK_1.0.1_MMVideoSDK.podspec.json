{
  "name": "MMVideoSDK",
  "version": "1.0.1",
  "summary": "short videos record and edit",
  "description": "this framework use to record,edit short videos",
  "homepage": "https://www.baidu.com/",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "bi.s_fish": "sunfeifish@gmail.com"
  },
  "source": {
