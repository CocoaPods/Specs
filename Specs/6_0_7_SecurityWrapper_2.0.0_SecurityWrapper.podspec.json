{
  "name": "SecurityWrapper",
  "version": "2.0.0",
  "summary": "A very simple wrapper for common security implementations in iOS - Sign, Verify, Encrypt, Decrypt, and Deffie Helman.",
  "source": {
    "git": "https://github.com/Hassaniiii/Security.git",
    "branch": "master"
  },
  "description": "A very simple wrapper for common security implementations in iOS",
  "homepage": "http://vancosys.com",
  "license": {
    "type": "GNU",
    "file": "LICENSE.txt"
  },
  "authors": "Hassan Shahbazi",
  "social_media_url": "https://www.linkedin.com/in/hassanshahbazi/",
  "platforms": {
    "ios": "10.0"
  },
  "source_files": [
    "Classes",
    "SecurityWrapper/Classes/**/*.{swift}"
  ],
  "exclude_files": "Classes/Exclude",
  "preserve_paths": "CocoaPods/**/*",
  "pod_target_xcconfig": {
    "SWIFT_INCLUDE_PATHS[sdk=iphoneos*]": "$(PODS_ROOT)/SecurityWrapper/CocoaPods/iphoneos",
    "SWIFT_INCLUDE_PATHS[sdk=iphonesimulator*]": "$(PODS_ROOT)/SecurityWrapper/CocoaPods/iphonesimulator"
  }
}
