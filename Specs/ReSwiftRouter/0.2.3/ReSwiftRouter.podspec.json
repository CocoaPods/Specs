    "tag": "0.2.3"
  },
  "platforms": {
    "ios": "8.0",
    "osx": "10.10",
    "tvos": "9.0",
    "watchos": "2.0"
  },
  "requires_arc": true,
  "source_files": "ReSwiftRouter/**/*.swift",
