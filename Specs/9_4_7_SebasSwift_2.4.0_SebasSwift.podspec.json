{
  "name": "SebasSwift",
  "version": "2.4.0",
  "summary": "Utility extensions written in Swift.",
  "description": "SebasSwift is utility extensions for iOS and macOS X written in Swift.",
  "homepage": "https://github.com/HituziANDO/SebasSwift",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": "Hituzi Ando",
  "source": {
    "git": "https://github.com/HituziANDO/SebasSwift.git",
    "tag": "2.4.0"
  },
  "requires_arc": true,
  "swift_version": "4.2",
  "platforms": {
    "ios": "9.3",
    "osx": "10.10"
  },
  "source_files": "SebasSwift/Common/*.{swift,h}",
  "ios": {
    "source_files": "SebasSwift/UIKit/*.swift",
    "frameworks": "UIKit"
  },
  "osx": {
    "source_files": "SebasSwift/AppKit/*.swift",
    "frameworks": "AppKit"
  },
  "frameworks": [
    "Foundation",
    "CoreGraphics"
  ]
}
