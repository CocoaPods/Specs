{
  "name": "ReactNativeSDK",
  "version": "0.0.1",
  "summary": "This is ReactNativeSDK.",
  "description": "ReactNativeSDK for ios project,you can use it by pod.",
  "homepage": "https://github.com/es-yincheng/ReactNative",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "yc": "cheng.yin@ymm56.com"
  },
  "source": {
    "git": "https://github.com/es-yincheng/ReactNative.git",
    "tag": "0.0.1"
  },
  "platforms": {
    "ios": "9.0"
  },
  "default_subspecs": [
    "React",
    "DoubleConversion",
    "folly",
    "glog",
    "yoga"
  ],
  "subspecs": [
    {
      "name": "React",
      "header_dir": "React",
      "source_files": "frameworks/React.framework/Headers/*.h",
      "vendored_frameworks": "frameworks/React.framework"
    },
    {
      "name": "DoubleConversion",
      "header_dir": "DoubleConversion",
      "source_files": "frameworks/DoubleConversion.framework/Headers/*.h",
      "vendored_frameworks": "frameworks/DoubleConversion.framework"
    },
    {
      "name": "folly",
      "header_dir": "React",
      "source_files": "frameworks/folly.framework/Headers/*.h",
      "vendored_frameworks": "frameworks/folly.framework"
    },
    {
      "name": "glog",
      "header_dir": "React",
      "source_files": "frameworks/glog.framework/Headers/*.h",
      "vendored_frameworks": "frameworks/glog.framework"
    },
    {
      "name": "yoga",
      "header_dir": "React",
      "source_files": "frameworks/yoga.framework/Headers/*.h",
      "vendored_frameworks": "frameworks/yoga.framework"
    }
  ]
}
