{
  "name": "ProRTC_iOS",
  "version": "1.0.1",
  "summary": "An easy-to-use framework provides native WebRTC support on iOS platform.",
  "homepage": "http://prortc.com",
  "license": {
    "type": "Commercial",
    "text": "http://prortc.com/terms-conditions.php"
  },
  "authors": {
    "ProRTC project authors": "support@prortc.com"
  },
  "source": {
    "git": "https://github.com/ProRTC/ProRTC_iOS.git",
    "tag": "1.0.1"
  },
  "social_media_url": "https://twitter.com/ProRTCApp",
  "platforms": {
    "ios": "9.0"
  },
  "requires_arc": true,
  "ios": {
    "vendored_frameworks": "ProRTC_iOS/ProRTC.framework"
  },
  "frameworks": [
    "SystemConfiguration",
    "UIKit",
    "GLKit",
    "CoreMedia",
    "CoreGraphics",
    "AVFoundation",
    "Foundation",
    "Security",
    "CFNetwork",
    "AudioToolbox",
    "VideoToolbox",
    "CoreAudio",
    "CoreVideo",
    "OpenGLES",
    "QuartzCore"
  ],
  "libraries": [
    "c++",
    "icucore",
    "stdc++",
    "System",
    "sqlite3"
  ],
  "pod_target_xcconfig": {
    "OTHER_LDFLAGS": "-ObjC",
    "FRAMEWORK_SEARCH_PATHS": "${PODS_ROOT}/ProRTC_iOS"
  }
}
