{
  "name": "mParticle-Optimizely",
  "version": "7.15.1",
  "summary": "Optimizely integration for mParticle",
  "description": "This is the Optimizely integration for mParticle.",
  "homepage": "https://www.mparticle.com",
  "license": {
    "type": "Apache 2.0",
    "file": "LICENSE"
  },
  "authors": {
    "mParticle": "support@mparticle.com"
  },
  "source": {
    "git": "https://github.com/mparticle-integrations/mparticle-apple-integration-optimizely.git",
    "tag": "7.15.1"
  },
  "social_media_url": "https://twitter.com/mparticle",
  "platforms": {
    "ios": "8.0",
    "tvos": "9.0"
  },
  "ios": {
    "source_files": "mParticle_Optimizely/*.{h,m,mm}",
    "dependencies": {
      "mParticle-Apple-SDK/mParticle": [
        "~> 7.0"
      ],
      "OptimizelySDKiOS": [
        "~> 3.0"
      ]
    },
    "frameworks": [
      "CoreTelephony",
      "SystemConfiguration"
    ]
  },
  "libraries": "z",
  "tvos": {
    "source_files": "mParticle_Optimizely/*.{h,m,mm}",
    "dependencies": {
      "mParticle-Apple-SDK/mParticle": [
        "~> 7.0"
      ],
      "OptimizelySDKTVOS": [
        "~> 3.0"
      ]
    },
    "frameworks": "SystemConfiguration",
    "pod_target_xcconfig": {
      "LIBRARY_SEARCH_PATHS": "$(inherited) $(PODS_ROOT)/OptimizelySDKTVOS/**"
    }
  }
}
