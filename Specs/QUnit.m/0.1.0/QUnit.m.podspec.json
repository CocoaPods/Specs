  "frameworks": "SenTestingKit",
  "xcconfig": {
    "FRAMEWORK_SEARCH_PATHS": "\"$(SDKROOT)/Developer/Library/Frameworks\" \"$(DEVELOPER_LIBRARY_DIR)/Frameworks\""
  },
  "requires_arc": false
}