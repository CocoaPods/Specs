{
  "name": "AutoLayoutCells",
  "platforms": {
    "ios": "7.0"
  },
  "version": "0.10.2",
  "summary": "AutoLayoutCells makes working with dynamic table view cells easy.",
  "homepage": "https://github.com/JRG-Developer/AutoLayoutCells",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Joshua Greene": "jrg.developer@gmail.com"
  },
  "source": {
    "git": "https://github.com/JRG-Developer/AutoLayoutCells.git",
    "tag": "0.10.2"
  },
  "requires_arc": true,
  "frameworks": "UIKit",
  "subspecs": [
    {
      "name": "SharedCategories",
      "dependencies": {
        "UIImageView+ALActivityIndicatorView": [
          "~> 1.0"
        ]
      },
      "xcconfig": {
        "FRAMEWORK_SEARCH_PATHS": "$(PODS_ROOT)/UIImageView+ALActivityIndicatorView"
      },
      "source_files": "AutoLayoutCells/SharedCategories/*{h,m}"
    },
    {
      "name": "TableViewCells",
      "dependencies": {
        "AutoLayoutCells/SharedCategories": [

        ],
        "ALLabel": [
          "~> 2.0"
        ],
        "AutoLayoutTextViews": [
          "~> 1.1"
        ]
      },
      "resource_bundles": {
        "ALTableViewCellsBundle": [
          "AutoLayoutCells/TableViewCells/ResourcesBundle/*"
        ]
      },
      "source_files": "AutoLayoutCells/TableViewCells/*.{h,m}"
    }
  ]
}
