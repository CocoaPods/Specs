{
  "name": "ReactiveMapKit",
  "version": "8.0.0-rc.1",
  "summary": "MapKit bindings for ReactiveCocoa.",
  "description": "Provide MapKit bindings for ReactiveCocoa. ReactiveCocoa (RAC) is a Cocoa framework built on top of ReactiveSwift. It provides APIs for using ReactiveSwift with Apple's Cocoa frameworks.",
  "homepage": "https://github.com/ReactiveCocoa/ReactiveCocoa",
  "license": {
    "type": "MIT",
    "file": "LICENSE.md"
  },
  "authors": "ReactiveCocoa",
  "platforms": {
    "osx": "10.9",
    "ios": "8.0",
    "tvos": "9.0"
  },
  "source": {
    "git": "https://github.com/ReactiveCocoa/ReactiveCocoa.git",
    "tag": "8.0.0-rc.1"
  },
  "source_files": "ReactiveMapKit/*.{swift,h,m}",
  "dependencies": {
    "ReactiveCocoa": [
      "8.0.0-rc.1"
    ]
  },
  "pod_target_xcconfig": {
    "OTHER_SWIFT_FLAGS[config=Release]": "$(inherited) -suppress-warnings"
  }
}
