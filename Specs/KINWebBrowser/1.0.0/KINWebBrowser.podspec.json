    "David F. Muir V": "dfmuir@gmail.com"
  },
  "platforms": {
    "ios": "7.0"
  },
  "source": {
    "git": "https://github.com/dfmuir/KINWebBrowser.git",
    "tag": "1.0.0"
  },
  "source_files": [
    "KINWebBrowser",
    "KINWebBrowser/**/*.{h,m}"
