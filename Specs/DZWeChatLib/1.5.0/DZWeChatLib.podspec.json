{
  "name": "DZWeChatLib",
  "version": "1.5.0",
  "summary": "DZWeChatLib微信的静态库管理",
  "description": "                    DZWeChatLib微信的静态库管理， 会实时跟进\n",
  "homepage": "https://github.com/yishuiliunian/DZWeChatLib",
  "license": "MIT",
  "authors": {
    "stonedong": "yishuiliunian@gmail.com"
  },
  "source": {
    "git": "https://github.com/yishuiliunian/DZWeChatLib.git",
    "tag": "1.5.0"
  },
  "platforms": {
    "ios": "5.0"
  },
  "requires_arc": true,
  "source_files": [
    "Pod/Classes/**/*",
    "Pod/Wechat/**/*.{h,m}"
  ],
  "resource_bundles": {
    "DZWeChatLib": [
      "Pod/Assets/*.png"
    ]
  },
  "libraries": [
    "z",
    "sqlite3"
  ],
  "frameworks": [
    "CoreTelephony",
    "CoreGraphics",
    "SystemConfiguration"
  ],
  "vendored_libraries": "Pod/Wechat/**/*.{a}"
}
