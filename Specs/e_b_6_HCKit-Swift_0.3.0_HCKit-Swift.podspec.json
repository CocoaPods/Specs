{
  "name": "HCKit-Swift",
  "version": "0.3.0",
  "summary": "一个简单的Swift库",
  "description": "一个森罗万象的Swift库",
  "homepage": "https://github.com/chyo/HCKit-Swift",
  "license": "MIT",
  "authors": {
    "ChenHongchao": "xmchc@hotmail.com"
  },
  "platforms": {
    "ios": "8.2"
  },
  "swift_version": "4.0",
  "source": {
    "git": "https://github.com/chyo/HCKit-Swift.git",
    "tag": "0.3.0"
  },
  "source_files": [
    "Classes/**/*.swift",
    "Module/*.modulemap"
  ],
  "resources": "Resources/*",
  "frameworks": [
    "Photos",
    "CoreLocation",
    "AVFoundation"
  ],
  "requires_arc": true,
  "preserve_paths": "Module/module.modulemap",
  "xcconfig": {
    "SWIFT_INCLUDE_PATHS": "$(PODS_ROOT)/HCKit-Swift/Module"
  },
  "dependencies": {
    "SnapKit": [
      "~> 4.0.0"
    ],
    "Kingfisher": [
      "~> 4.8.0"
    ]
  }
}
