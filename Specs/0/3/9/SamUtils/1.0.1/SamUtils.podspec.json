        {
          "name": "Extensions",
          "source_files": "SamUtils/Extensions/*.swift"
        }
      ]
    },
    {
      "name": "Bluetooth",
      "source_files": "SamUtils/Bluetooth/*.swift"
    },
    {
      "name": "SwiftUITools",
      "source_files": "SamUtils/SwiftUITools/**/*.swift"
    },
    {
      "name": "DrawImage",
      "source_files": "SamUtils/DrawImage/**/*.swift"
    }
  ],
  "swift_version": "5.7"
