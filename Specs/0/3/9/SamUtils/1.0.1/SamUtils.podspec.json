  "dependencies": {
    "Alamofire": [

    ],
    "SwifterSwift": [

    ],
    "IQKeyboardManagerSwift": [

    ]
  },
  "subspecs": [
    {
      "name": "CommonTools",
      "source_files": "SamUtils/*.swift",
      "exclude_files": "SamUtils/Bluetooth/*.swift",
      "subspecs": [
        {
          "name": "API",
          "source_files": "SamUtils/API/*.swift"
