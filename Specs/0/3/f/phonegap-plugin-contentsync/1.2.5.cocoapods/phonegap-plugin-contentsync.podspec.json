{
  "name": "phonegap-plugin-contentsync",
  "version": "1.2.5.cocoapods",
  "summary": "Download and cache remotely hosted content.",
  "description": "Download and cache remotely hosted zipped content bundles, unzipping automatically.",
  "homepage": "https://github.com/phonegap/phonegap-plugin-contentsync",
  "authors": "Adobe PhoneGap Team",
  "license": "Apache License, Version 2.0",
  "platforms": {
    "ios": null
  },
  "source": {
    "git": "https://github.com/ttahmouch/phonegap-plugin-contentsync.git",
    "tag": "1.2.5.cocoapods"
  },
  "source_files": "src/ios/ContentSync.{h,m}",
  "preserve_paths": [
    "*.md",
    "www/*.js"
  ],
  "frameworks": [
    "Foundation",
    "MobileCoreServices"
  ],
  "dependencies": {
    "Cordova": [
      ">= 4.0.1"
    ],
    "SSZipArchive": [
      "1.6.2"
    ]
