{
  "name": "PCloudSDKSwift",
  "version": "2.0.2",
  "summary": "Swift SDK for the pCloud API",
  "homepage": "https://github.com/pcloud/pcloud-sdk-swift",
  "license": "MIT",
  "authors": "pCloud",
  "source": {
    "git": "https://github.com/pcloud/pcloud-sdk-swift.git",
    "tag": "v2.0.2"
  },
  "platforms": {
    "osx": "10.11",
    "ios": "9.0"
  },
  "osx": {
    "frameworks": [
      "AppKit",
      "Webkit",
      "SystemConfiguration",
      "Foundation"
    ],
    "public_header_files": "PCloudSDKSwift/Source/**/*.h",
    "source_files": [
      "PCloudSDKSwift/Source/**/*.{swift,h}",
      "PCloudSDKSwift/SDK macOS/**/*.swift"
    ]
  },
  "ios": {
    "frameworks": [
      "UIKit",
      "Webkit",
      "SystemConfiguration",
      "Foundation"
    ],
    "public_header_files": "PCloudSDKSwift/Source/**/*.h",
    "source_files": [
      "PCloudSDKSwift/Source/**/*.{swift,h}",
      "PCloudSDKSwift/SDK iOS/**/*.swift"
    ]
  },
  "requires_arc": true
}
