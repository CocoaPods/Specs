{
  "name": "AirplayMuxer",
  "version": "0.1.4",
  "summary": "Airplay muxer for feed iOS SDK.",
  "description": "Airplay muxer for feed iOS SDK. Refer to documentaion for more info",
  "homepage": "https://github.com/feedfm/FeedAirplayMuxer",
  "license": {
    "type": "MIT",
    "file": "LICENSE.md"
  },
  "authors": {
    "Arveen Kumar": "arveen@feed.fm",
    "Feed Media": "support@feed.fm"
  },
  "source": {
    "git": "https://github.com/feedfm/FeedAirplayMuxer.git",
    "tag": "v0.1.4"
  },
  "platforms": {
    "ios": "12.0"
  },
  "xcconfig": {
    "FRAMEWORK_SEARCH_PATHS": "\"$(PODS_ROOT)/AirplayMuxer/**\""
  },
  "requires_arc": true,
  "dependencies": {
    "FeedMedia": [
      "~> 5.1"
    ]
  },
  "frameworks": [
    "AVFoundation",
    "MediaPlayer",
    "CoreMedia"
  ],
  "default_subspecs": "Core",
  "subspecs": [
    {
      "name": "Core",
      "preserve_paths": "AirplayMuxer.xcframework",
      "vendored_frameworks": "AirplayMuxer.xcframework"
    }
  ]
}
