{
  "name": "JuphoonCloudSDK_iOS",
  "version": "2.1",
  "summary": "Juphoon SDK.",
  "description": "JCSDK for video talk, broadcasting and message service",
  "homepage": "http://developer.juphoon.com/cn/download/",
  "license": "Copyright 2020 juphoon. All rights reserved",
  "authors": {
    "app-admin": "app-admin@juphoon.com"
  },
  "platforms": {
    "ios": "9.0"
  },
  "requires_arc": true,
  "source": {
    "http": "https://justalkcloud.com/wo_upload/JC_SDK_iOS_2.1.tar.gz"
  },
  "vendored_frameworks": [
    "framework/JCSDKOC.framework",
    "framework/JCCloudWrapper.framework"
  ],
  "vendored_libraries": [
    "lib/ios/libmtc.a",
    "lib/ios/libzmf.a"
  ],
  "frameworks": [
    "AVFoundation",
    "Security",
    "AudioToolbox",
    "CoreMedia",
    "AssetsLibrary",
    "CoreVideo",
    "CFNetwork",
    "CoreMotion",
    "GLKit",
    "ReplayKit",
    "PushKit",
    "SystemConfiguration",
    "VideoToolbox"
  ],
  "libraries": [
    "c++",
    "resolv",
    "z"
  ],
  "dependencies": {
    "FMDB": [

    ]
  },
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "$(PODS_ROOT)/JC_SDK_iOS_2.1/include"
  },
  "pod_target_xcconfig": {
    "VALID_ARCHS": "arm64 armv7 armv7s arm64e x86_64"
  },
  "subspecs": [
    {
      "name": "include",
      "subspecs": [
        {
          "name": "avatar",
          "subspecs": [
            {
              "name": "zos",
              "source_files": "include/avatar/zos/*.h",
              "subspecs": [
                {
                  "name": "os",
                  "subspecs": [
                    {
                      "name": "ios",
                      "source_files": "include/avatar/zos/os/ios/*.h"
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "name": "lemon",
          "source_files": "include/lemon/*.h",
          "subspecs": [
            {
              "name": "mtc",
              "source_files": "include/lemon/mtc/*.h"
            }
          ]
        },
        {
          "name": "grape",
          "source_files": "include/grape/*.h"
        }
      ]
    }
  ]
}
