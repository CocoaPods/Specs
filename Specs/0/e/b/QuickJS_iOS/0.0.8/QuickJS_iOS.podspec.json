{
  "name": "QuickJS_iOS",
  "version": "0.0.8",
  "summary": "QuickJS iOS Bridge",
  "description": "QuickJS iOS Bridge, you can invoke objc api in javascript, and invoke javascript api in objc.",
  "homepage": "https://github.com/quickjs-ios/quickjs-ios",
  "license": "MIT",
  "authors": "Sam Chang",
  "platforms": {
    "ios": "11.0"
  },
  "source": {
    "git": "https://github.com/quickjs-ios/quickjs-ios.git",
    "tag": "0.0.8",
    "submodules": true
  },
  "source_files": [
    "QuickJS-iOS/Classes",
    "QuickJS-iOS/QuickJS_iOS.h",
    "quickjs/quickjs.c",
    "quickjs/bjson.c",
    "quickjs/cutils.c",
    "quickjs/libbf.c",
    "quickjs/libregexp.c",
    "quickjs/libunicode.c",
    "quickjs/quickjs-libc.c",
    "quickjs/*.h"
  ],
  "public_header_files": [
    "QuickJS-iOS/Classes/**/*.h",
    "QuickJS-iOS/QuickJS_iOS.h",
    "quickjs/quickjs.h",
    "quickjs/quickjs-libc.h",
    "quickjs/quickjs-atom.h"
  ],
  "xcconfig": {
    "OTHER_CFLAGS": "    $(inherited) -DCONFIG_VERSION=\\\"2021-03-27\\\"\n"
  }
}
