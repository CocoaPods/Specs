{
  "name": "hailuoLib",
  "version": "1.0.4",
  "summary": "A short description of hailuoLib.",
  "description": "hailuoLib test",
  "homepage": "https://github.com/yojjoyy1/hailuoLib",
  "license": "MIT",
  "authors": {
    "sinyilin": "yojjoyy1@gmail.com"
  },
  "platforms": {
    "ios": "10.0"
  },
  "xcconfig": {
    "OTHER_LDFLAGS": "-ObjC",
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES",
    "FRAMEWORK_SEARCH_PATHS": "$(inherited) $(SRCROOT)",
    "HEADER_SEARCH_PATHS": "$(inherited)     $(SDKROOT)/usr/include/libxml2 $(SRCROOT)",
    "ARCHS": "$(inherited)",
    "VALID_ARCHS": "$(inherited)"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "source": {
    "git": "https://github.com/yojjoyy1/hailuoLib.git",
    "tag": "1.0.4"
  },
  "vendored_frameworks": "hailuoTest.framework",
  "frameworks": [
    "UIKit",
    "AVFoundation",
    "Foundation"
  ]
}
