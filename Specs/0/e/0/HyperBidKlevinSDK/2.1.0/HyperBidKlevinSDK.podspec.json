{
  "name": "HyperBidKlevinSDK",
  "version": "2.1.0",
  "summary": "HyperBidKlevinSDK",
  "description": "HyperBidKlevinSDK,HyperBidKlevinSDK,Klevin",
  "homepage": "https://github.com/hyperbid/hyperbid-klevin-sdk",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "HyperBid": "ios"
  },
  "source": {
    "git": "https://github.com/hyperbid/hyperbid-klevin-sdk.git",
    "tag": "2.1.0"
  },
  "platforms": {
    "ios": "9.0"
  },
  "requires_arc": true,
  "vendored_frameworks": "Klevin/KlevinAdSDK.framework",
  "weak_frameworks": [
    "AdSupport",
    "AppTrackingTransparency",
    "WebKit"
  ],
  "frameworks": [
    "StoreKit",
    "SystemConfiguration",
    "CoreTelephony",
    "AVKit",
    "AVFoundation",
    "CoreMedia"
  ],
  "xcconfig": {
    "OTHER_LDFLAGS": "-ObjC"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "pod_target_xcconfig": {
    "VALID_ARCHS": "$(ARCHS_STANDARD_64_BIT) armv7",
    "ENABLE_BITCODE": "NO"
  }
}
