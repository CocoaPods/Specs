{
  "name": "ButterflyMX",
  "version": "0.13.1",
  "swift_version": "4.2",
  "source": {
    "http": "https://github.com/runslikebutter/butterflymx-ios-sdk/raw/master/ButterflyMX.zip",
    "flatten": true
  },
  "summary": "A Swift framework to implement ButterflyMX SDK",
  "homepage": "https://github.com/runslikebutter/butterflymx-ios-sdk",
  "authors": {
    "butterflymx": "admin@butterflymx.com"
  },
  "license": {
    "type": "Apache-2.0",
    "file": "LICENSE"
  },
  "platforms": {
    "ios": "10.0"
  },
  "ios": {
    "preserve_paths": "pjsip/**/*",
    "xcconfig": {
      "HEADER_SEARCH_PATHS": "$(inherited) \"${PODS_ROOT}/ButterflyMX/pjsip/include\"",
      "LIBRARY_SEARCH_PATHS": "$(inherited) \"${PODS_ROOT}/ButterflyMX/pjsip/lib\"",
      "FRAMEWORK_SEARCH_PATHS": "$(inherited) \"${PODS_ROOT}/ButterflyMX\"",
      "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited) PJ_AUTOCONF=1"
    },
    "vendored_frameworks": "ButterflyMXSDK.framework"
  },
  "dependencies": {
    "Alamofire": [

    ]
  }
}
