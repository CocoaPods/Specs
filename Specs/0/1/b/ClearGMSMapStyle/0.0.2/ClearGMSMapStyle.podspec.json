  "homepage": "https://github.com/davidseca/clear-gmsmap-style.git",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "David Seca": "davidseca@gmail.com"
  },
  "platforms": {
