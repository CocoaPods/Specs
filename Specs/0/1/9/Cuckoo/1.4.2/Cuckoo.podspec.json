  },
  "subspecs": [
    {
      "name": "Swift",
      "source_files": "Source/**/*.swift"
    },
    {
      "name": "OCMock",
      "source_files": "OCMock/**/*.{h,m,swift}",
      "dependencies": {
        "Cuckoo/Swift": [

        ],
        "OCMock": [
          "3.4.3"
        ]
      }
    }
  ],
  "swift_version": "5.0"
}
