{
  "name": "YunoAntifraudOpenpay",
  "version": "0.0.3",
  "summary": "OpenPay antifraud by Yuno",
  "description": "Openpay antifraud solution provided by Yuno.",
  "homepage": "https://www.y.uno/",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Lautaro Pinto": "lautaro.pinto@y.uno"
  },
  "source": {
    "http": "https://github.com/yuno-payments/yuno-antifraud-openpay-ios/releases/download/0.0.3/YunoAntifraudOpenpay.xcframework.zip"
  },
  "platforms": {
    "ios": "13.0"
  },
  "swift_versions": "5.4",
  "requires_arc": true,
  "prepare_command": "curl -L -o OpenpayKit.xcframework.zip https://github.com/open-pay/openpay-swift-ios/releases/download/3.1.0/OpenpayKit.xcframework.zip &&     unzip OpenpayKit.xcframework.zip &&     rm OpenpayKit.xcframework.zip",
  "vendored_frameworks": [
    "OpenpayKit.xcframework",
    "YunoAntifraudOpenpay.xcframework"
  ],
  "frameworks": "Combine",
  "xcconfig": {
    "SWIFT_INCLUDE_PATHS": [
      "${PODS_XCFRAMEWORKS_BUILD_DIR}/YunoAntifraudOpenpay"
    ]
  },
  "pod_target_xcconfig": {
    "BUILD_LIBRARY_FOR_DISTRIBUTION": "YES"
  },
  "swift_version": "5.4"
}
