{
  "name": "JumioMobileSDK",
  "version": "3.2.0",
  "platforms": {
    "ios": "9.0"
  },
  "summary": "This Jumio SDK includes frameworks for Netverify, Fastfill, Authentication, Document Verification and BAM Checkout.",
  "documentation_url": "https://support.jumio.com/",
  "homepage": "http://www.jumio.com",
  "authors": {
    "Jumio Corporation": "support@jumio.com"
  },
  "source": {
    "http": "https://mobile-sdk.jumio.com/com/jumio/ios/jumio-mobile-sdk/3.2.0/ios-jumio-mobile-sdk-3.2.0.zip"
  },
  "license": {
    "type": "Commercial",
    "text": "See http://www.jumio.com/"
  },
  "requires_arc": true,
  "xcconfig": {
    "OTHER_LDFLAGS": "-ObjC -lc++ -lz",
    "CLANG_MODULES_AUTOLINK": "YES"
  },
  "default_subspecs": "JumioMobileSDK",
  "subspecs": [
    {
      "name": "JumioMobileSDK",
      "vendored_frameworks": [
        "JumioMobileSDK-3.2.0/JumioCore.framework",
        "JumioMobileSDK-3.2.0/BAMCheckout.framework",
        "JumioMobileSDK-3.2.0/Netverify.framework",
        "JumioMobileSDK-3.2.0/NetverifyBarcode.framework",
        "JumioMobileSDK-3.2.0/MicroBlink.framework",
        "JumioMobileSDK-3.2.0/NetverifyFace.framework",
        "JumioMobileSDK-3.2.0/ZoomAuthenticationHybrid.framework"
      ],
      "preserve_paths": "JumioMobileSDK-3.2.0/*.lproj/Localizable-*.strings"
    },
    {
      "name": "BAMCheckout",
      "vendored_frameworks": [
        "JumioMobileSDK-3.2.0/JumioCore.framework",
        "JumioMobileSDK-3.2.0/BAMCheckout.framework"
      ],
      "preserve_paths": "JumioMobileSDK-3.2.0/*.lproj/Localizable-BAMCheckout.strings"
    },
    {
      "name": "Netverify",
      "vendored_frameworks": [
        "JumioMobileSDK-3.2.0/JumioCore.framework",
        "JumioMobileSDK-3.2.0/Netverify.framework",
        "JumioMobileSDK-3.2.0/NetverifyBarcode.framework",
        "JumioMobileSDK-3.2.0/MicroBlink.framework",
        "JumioMobileSDK-3.2.0/NetverifyFace.framework",
        "JumioMobileSDK-3.2.0/ZoomAuthenticationHybrid.framework"
      ],
      "preserve_paths": [
        "JumioMobileSDK-3.2.0/*.lproj/Localizable-Netverify.strings",
        "JumioMobileSDK-3.2.0/*.lproj/Localizable-DocumentVerification.strings",
        "JumioMobileSDK-3.2.0/*.lproj/Localizable-Authentication.strings"
      ]
    },
    {
      "name": "Netverify-Light",
      "vendored_frameworks": [
        "JumioMobileSDK-3.2.0/JumioCore.framework",
        "JumioMobileSDK-3.2.0/Netverify.framework",
        "JumioMobileSDK-3.2.0/NetverifyBarcode.framework",
        "JumioMobileSDK-3.2.0/MicroBlink.framework"
      ],
      "preserve_paths": [
        "JumioMobileSDK-3.2.0/*.lproj/Localizable-Netverify.strings",
        "JumioMobileSDK-3.2.0/*.lproj/Localizable-DocumentVerification.strings"
      ]
    }
  ]
}
