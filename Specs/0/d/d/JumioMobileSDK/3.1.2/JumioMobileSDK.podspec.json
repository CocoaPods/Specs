{
  "name": "JumioMobileSDK",
  "version": "3.1.2",
  "platforms": {
    "ios": "9.0"
  },
  "summary": "This Jumio SDK includes frameworks for Netverify, Fastfill, Authentication, Document Verification and BAM Checkout.",
  "documentation_url": "https://support.jumio.com/",
  "homepage": "http://www.jumio.com",
  "authors": {
    "Jumio Corporation": "support@jumio.com"
  },
  "source": {
    "http": "https://mobile-sdk.jumio.com/com/jumio/ios/jumio-mobile-sdk/3.1.2/ios-jumio-mobile-sdk-3.1.2.zip"
  },
  "license": {
    "type": "Commercial",
    "text": "See http://www.jumio.com/"
  },
  "requires_arc": true,
  "xcconfig": {
    "OTHER_LDFLAGS": "-ObjC -lc++ -lz",
    "CLANG_MODULES_AUTOLINK": "YES"
  },
  "default_subspecs": "JumioMobileSDK",
  "subspecs": [
    {
      "name": "JumioMobileSDK",
      "vendored_frameworks": [
        "JumioMobileSDK-3.1.2/JumioCore.framework",
        "JumioMobileSDK-3.1.2/BAMCheckout.framework",
        "JumioMobileSDK-3.1.2/Netverify.framework",
        "JumioMobileSDK-3.1.2/NetverifyBarcode.framework",
        "JumioMobileSDK-3.1.2/MicroBlink.framework",
        "JumioMobileSDK-3.1.2/NetverifyFace.framework",
        "JumioMobileSDK-3.1.2/ZoomAuthenticationHybrid.framework"
      ],
      "preserve_paths": "JumioMobileSDK-3.1.2/*.lproj/Localizable-*.strings"
    },
    {
      "name": "BAMCheckout",
      "vendored_frameworks": [
        "JumioMobileSDK-3.1.2/JumioCore.framework",
        "JumioMobileSDK-3.1.2/BAMCheckout.framework"
      ],
      "preserve_paths": "JumioMobileSDK-3.1.2/*.lproj/Localizable-BAMCheckout.strings"
    },
    {
      "name": "Netverify",
      "vendored_frameworks": [
        "JumioMobileSDK-3.1.2/JumioCore.framework",
        "JumioMobileSDK-3.1.2/Netverify.framework",
        "JumioMobileSDK-3.1.2/NetverifyBarcode.framework",
        "JumioMobileSDK-3.1.2/MicroBlink.framework",
        "JumioMobileSDK-3.1.2/NetverifyFace.framework",
        "JumioMobileSDK-3.1.2/ZoomAuthenticationHybrid.framework"
      ],
      "preserve_paths": [
        "JumioMobileSDK-3.1.2/*.lproj/Localizable-Netverify.strings",
        "JumioMobileSDK-3.1.2/*.lproj/Localizable-DocumentVerification.strings",
        "JumioMobileSDK-3.1.2/*.lproj/Localizable-Authentication.strings"
      ]
    },
    {
      "name": "Netverify-Light",
      "vendored_frameworks": [
        "JumioMobileSDK-3.1.2/JumioCore.framework",
        "JumioMobileSDK-3.1.2/Netverify.framework",
        "JumioMobileSDK-3.1.2/NetverifyBarcode.framework",
        "JumioMobileSDK-3.1.2/MicroBlink.framework"
      ],
      "preserve_paths": [
        "JumioMobileSDK-3.1.2/*.lproj/Localizable-Netverify.strings",
        "JumioMobileSDK-3.1.2/*.lproj/Localizable-DocumentVerification.strings"
      ]
    }
  ]
}
