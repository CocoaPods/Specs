{
  "name": "PubyYanSDK",
  "version": "0.0.3",
  "summary": "Pubeasy 广告聚合 SDK（Binary 分发）",
  "description": "以 .xcframework 二进制形式分发的 Pubeasy 广告聚合 SDK，\n内含 PubyYanSDK.xcframework 与 TradPlus 依赖及资源。",
  "homepage": "https://github.com/guangyao/pbsdk",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "yanguangyao": "976660988@qq.com"
  },
  "platforms": {
    "ios": "12.0"
  },
  "source": {
    "http": "https://github.com/guangyao/pbsdk/releases/download/v0.0.3/PubYanSDK_0.0.3.zip"
  },
  "static_framework": true,
  "requires_arc": true,
  "vendored_frameworks": [
    "PubeasySDK.xcframework",
    "TradPlusFrameworks/TradPlusAds/TradPlusAds.framework",
    "TradPlusFrameworks/TradPlusAds/TPExchange.framework",
    "TradPlusFrameworks/TPCross/TPCrossAdapter.framework"
  ],
  "resources": [
    "TradPlusFrameworks/TradPlusAds/TradPlusAds.bundle",
    "TradPlusFrameworks/TradPlusAds/TradPlusADX.bundle",
    "Assets/PrivacyInfo.xcprivacy"
  ],
  "frameworks": [
    "UIKit",
    "Foundation",
    "AdSupport",
    "CoreTelephony"
  ],
  "libraries": [
    "z",
    "sqlite3",
    "c++"
  ],
  "pod_target_xcconfig": {
    "DEFINES_MODULE": "YES",
    "OTHER_LDFLAGS": "-ObjC -all_load"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "source_files": [

  ]
}
