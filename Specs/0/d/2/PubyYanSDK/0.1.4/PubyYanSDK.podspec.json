{
  "name": "PubyYanSDK",
  "version": "0.1.4",
  "summary": "Pubeasy Ad Aggregation SDK - Binary Distribution",
  "description": "Pubeasy Ad Aggregation SDK distributed in xcframework binary format.\nIncludes PubyYanSDK xcframework and TradPlus dependencies and resources.\nSupports banner, interstitial, native, rewarded, and splash ads.",
  "homepage": "https://github.com/guangyao/pbsdk",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "PubyYanSDK Team": "976660988@qq.com",
    "yanguangyao": "976660988@qq.com"
  },
  "platforms": {
    "ios": "12.0"
  },
  "source": {
    "http": "https://github.com/guangyao/pbsdk/releases/download/v0.1.4/PubYanSDK_0.1.4.zip"
  },
  "static_framework": true,
  "requires_arc": true,
  "vendored_frameworks": [
    "PubeasySDK.xcframework",
    "TradPlusFrameworks/TradPlusAds/TradPlusAds.framework",
    "TradPlusFrameworks/TradPlusAds/TPExchange.framework",
    "TradPlusFrameworks/TPCross/TPCrossAdapter.framework"
  ],
  "resources": [
    "TradPlusFrameworks/TradPlusAds/TradPlusAds.bundle",
    "TradPlusFrameworks/TradPlusAds/TradPlusADX.bundle",
    "Assets/PrivacyInfo.xcprivacy"
  ],
  "frameworks": [
    "UIKit",
    "Foundation",
    "AdSupport",
    "CoreTelephony"
  ],
  "libraries": [
    "z",
    "sqlite3",
    "c++"
  ],
  "pod_target_xcconfig": {
    "DEFINES_MODULE": "YES",
    "OTHER_LDFLAGS": "-ObjC -all_load"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  }
}
