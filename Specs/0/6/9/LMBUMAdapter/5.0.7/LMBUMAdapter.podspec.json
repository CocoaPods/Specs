{
  "name": "LMBUMAdapter",
  "version": "5.0.7",
  "summary": "LMBUMAdapter - LitemobSDK 的穿山甲（BUM）适配器",
  "description": "LMBUMAdapter 是 LitemobSDK 的第三方广告平台适配器，用于将 LitemobSDK 接入到穿山甲（BUM）SDK。",
  "homepage": "https://github.com/aimagicat/Litemize",
  "license": {
    "type": "MIT"
  },
  "authors": {
    "Litemob": "shibao@litemob.com"
  },
  "source": {
    "git": "https://github.com/aimagicat/Litemize.git",
    "tag": "v5.0.7"
  },
  "platforms": {
    "ios": "11.0"
  },
  "requires_arc": true,
  "source_files": "LMBUMAdapter/**/*.{h,m}",
  "public_header_files": "LMBUMAdapter/**/*.h",
  "dependencies": {
    "LitemobSDK": [
      "~> 5.0.7"
    ],
    "Ads-CN-Beta": [
      "~> 7.3.0.4"
    ]
  },
  "frameworks": [
    "Foundation",
    "UIKit"
  ],
  "pod_target_xcconfig": {
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES"
  }
}
