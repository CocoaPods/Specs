{
  "name": "Nubrick",
  "version": "0.13.0",
  "summary": "Nubrick SDK for iOS",
  "description": "Nubrick SDK for iOS.",
  "homepage": "https://docs.nativebrik.com",
  "license": {
    "type": "Apache-2.0",
    "file": "LICENSE"
  },
  "authors": {
    "Nubrick": "dev.share+nubrick@plaid.co.jp"
  },
  "platforms": {
    "ios": "13.4"
  },
  "source": {
    "git": "https://github.com/plaidev/nubrick-ios.git",
    "tag": "v0.13.0"
  },
  "source_files": [
    "Sources/Nubrick/**/*.{swift,h,m}",
    "Sources/YogaKit/**/*.{h,m,mm,cpp}"
  ],
  "preserve_paths": "Sources/YogaKit/module.modulemap",
  "private_header_files": "Sources/YogaKit/include/YogaKit/**/*.h",
  "resource_bundles": {
    "Nubrick": [
      "Sources/Nubrick/PrivacyInfo.xcprivacy"
    ]
  },
  "requires_arc": true,
  "swift_versions": "5.9",
  "frameworks": [
    "UIKit",
    "Foundation",
    "SwiftUI",
    "Combine",
    "ImageIO",
    "SafariServices"
  ],
  "weak_frameworks": "TipKit",
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "\"$(PODS_TARGET_SRCROOT)/Sources/YogaKit/include\"",
    "SWIFT_INCLUDE_PATHS": "\"$(PODS_TARGET_SRCROOT)/Sources/YogaKit\""
  },
  "dependencies": {
    "Yoga": [
      "~> 3.2.1"
    ]
  },
  "swift_version": "5.9"
}
