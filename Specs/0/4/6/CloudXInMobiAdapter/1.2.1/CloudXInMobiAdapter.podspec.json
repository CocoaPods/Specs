{
  "name": "CloudXInMobiAdapter",
  "version": "1.2.1",
  "summary": "CloudX InMobi Adapter - Static Framework",
  "description": "InMobi adapter for CloudX iOS SDK - binary distribution",
  "homepage": "https://github.com/cloudx-io/cloudx-ios",
  "license": {
    "type": "Business Source License 1.1"
  },
  "authors": {
    "CloudX": "support@cloudx.io"
  },
  "source": {
    "git": "https://github.com/cloudx-io/cloudx-ios.git",
    "tag": "v1.2.1-inmobi"
  },
  "platforms": {
    "ios": "15.0"
  },
  "vendored_frameworks": "adapter-inmobi/CloudXInMobiAdapter.xcframework",
  "dependencies": {
    "CloudXCore": [
      "1.2.1"
    ],
    "InMobiSDK": [
      "~> 10.8"
    ]
  },
  "frameworks": [
    "AVFoundation",
    "AVKit",
    "AdSupport",
    "CoreGraphics",
    "CoreLocation",
    "CoreTelephony",
    "Foundation",
    "StoreKit",
    "SystemConfiguration",
    "UIKit"
  ],
  "weak_frameworks": [
    "Combine",
    "CryptoKit",
    "SafariServices",
    "SwiftUI",
    "WebKit"
  ],
  "requires_arc": true,
  "static_framework": true,
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "i386"
  },
  "user_target_xcconfig": {
    "OTHER_LDFLAGS": "-ObjC"
  },
  "swift_versions": [
    "5.0",
    "5.1",
    "5.2",
    "5.3",
    "5.4",
    "5.5",
    "5.6",
    "5.7",
    "5.8",
    "5.9",
    "6.0",
    "6.1",
    "6.2"
  ],
  "swift_version": "6.2"
}
