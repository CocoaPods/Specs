{
  "name": "YFPushStreamKit",
  "version": "0.0.1",
  "summary": "云帆推流器",
  "description": "支持推流，faceu美颜，滤镜等等",
  "homepage": "https://github.com/YfCloudKit/YFPushStreamKit-iOS",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "liuzhibing": "373301593@qq.com"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source": {
    "git": "https://github.com/YfCloudKit/YFPushStreamKit-iOS.git",
    "tag": "0.0.1"
  },
  "vendored_frameworks": "YFPushStreamKit/*.{framework}",
  "vendored_libraries": "YFPushStreamKit/Classes/ffmpeg/lib/*.a",
  "preserve_paths": [
    "YFPushStreamKit/Classes/ffmpeg/lib/*.a",
    "YFPushStreamKit/Classes/ffmpeg/include/**/*.{h,c,cpp}"
  ],
  "frameworks": [
    "VideoToolbox",
    "Security",
    "SystemConfiguration",
    "ImageIO",
    "MobileCoreServices",
    "CoreMotion",
    "Accelerate",
    "GLKit",
    "OpenGLES",
    "CoreVideo",
    "CoreMedia",
    "CFNetwork",
    "AudioToolbox",
    "AVFoundation",
    "CoreGraphics",
    "Foundation",
    "UIKit"
  ],
  "libraries": [
    "z",
    "c++",
    "bz2",
    "stdc++.6",
    "avcodec",
    "avfilter",
    "avformat",
    "avutil",
    "swresample",
    "swscale"
  ],
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "\"$(PODS_ROOT)/YFPushStreamKit/Classes/ffmpeg/include\"",
    "LIBRARY_SEARCH_PATHS": "\"$(PODS_ROOT)/YFPushStreamKit/Classes/ffmpeg/lib\""
  },
  "requires_arc": true
}
