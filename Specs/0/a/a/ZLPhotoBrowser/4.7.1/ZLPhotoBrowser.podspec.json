{
  "name": "ZLPhotoBrowser",
  "version": "4.7.1",
  "summary": "A lightweight and pure Swift implemented library for select photos from album",
  "description": "Wechat-like image picker. Support select photos, videos, gif and livePhoto. Support edit image and crop video.",
  "homepage": "https://github.com/longitachi/ZLPhotoBrowser",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "longitachi": "longitachi@163.com"
  },
  "social_media_url": "https://github.com/longitachi",
  "source": {
    "git": "https://github.com/longitachi/ZLPhotoBrowser.git",
    "tag": "4.7.1"
  },
  "platforms": {
    "ios": "10.0"
  },
  "swift_versions": [
    "5.0",
    "5.1",
    "5.2"
  ],
  "requires_arc": true,
  "frameworks": [
    "UIKit",
    "Photos",
    "PhotosUI",
    "AVFoundation",
    "CoreMotion",
    "Accelerate"
  ],
  "resources": "Sources/*.{png,bundle}",
  "resource_bundles": {
    "ZLPhotoBrowser_Privacy": [
      "Sources/PrivacyInfo.xcprivacy"
    ]
  },
  "subspecs": [
    {
      "name": "Core",
      "source_files": [
        "Sources/**/*.{swift,h,m}",
        "Sources/ZLPhotoBrowser.h"
      ],
      "exclude_files": [
        "Sources/General/ZLWeakProxy.swift"
      ]
    }
  ],
  "swift_version": "5.2"
}
