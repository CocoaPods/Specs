{
  "name": "InstantSearch",
  "version": "7.24.0",
  "platforms": {
    "ios": "9.0",
    "osx": "10.11",
    "watchos": "2.0",
    "tvos": "9.0"
  },
  "license": {
    "type": "Apache 2.0",
    "file": "LICENSE.md"
  },
  "summary": "A library of widgets and helpers to build instant-search applications on iOS.",
  "homepage": "https://github.com/algolia/instantsearch-ios",
  "authors": {
    "Algolia": "contact@algolia.com"
  },
  "source": {
    "git": "https://github.com/algolia/instantsearch-ios.git",
    "tag": "7.24.0"
  },
  "swift_versions": "5.2",
  "default_subspecs": "UI",
  "subspecs": [
    {
      "name": "Insights",
      "source_files": "Sources/InstantSearchInsights/**/*.{swift}",
      "dependencies": {
        "AlgoliaSearchClient": [
          "~> 8.17"
        ]
      },
      "platforms": {
        "ios": "9.0",
        "osx": "10.10",
        "watchos": "2.0",
        "tvos": "9.0"
      }
    },
    {
      "name": "Core",
      "source_files": "Sources/InstantSearchCore/**/*.{swift}",
      "dependencies": {
        "AlgoliaSearchClient": [
          "~> 8.17"
        ],
        "InstantSearch/Insights": [

        ],
        "InstantSearchTelemetry": [
          "~> 0.1.3"
        ]
      },
      "platforms": {
        "ios": "9.0",
        "osx": "10.11",
        "watchos": "2.0",
        "tvos": "9.0"
      },
      "pod_target_xcconfig": {
        "OTHER_SWIFT_FLAGS": "-DInstantSearchCocoaPods"
      }
    },
    {
      "name": "UI",
      "source_files": "Sources/InstantSearch/**/*.{swift}",
      "dependencies": {
        "InstantSearch/Core": [

        ]
      },
      "platforms": {
        "ios": "9.0",
        "osx": "10.11",
        "watchos": "2.0",
        "tvos": "9.0"
      },
      "pod_target_xcconfig": {
        "OTHER_SWIFT_FLAGS": "-DInstantSearchCocoaPods"
      }
    },
    {
      "name": "SwiftUI",
      "source_files": "Sources/InstantSearchSwiftUI/**/*.{swift}",
      "dependencies": {
        "InstantSearch/Core": [

        ],
        "InstantSearchTelemetry": [
          "~> 0.1.3"
        ]
      },
      "platforms": {
        "ios": "13.0",
        "osx": "10.15",
        "watchos": "6.0",
        "tvos": "13.0"
      },
      "pod_target_xcconfig": {
        "OTHER_SWIFT_FLAGS": "-DInstantSearchCocoaPods"
      },
      "weak_frameworks": [
        "SwiftUI",
        "Combine"
      ]
    }
  ],
  "swift_version": "5.2"
}
