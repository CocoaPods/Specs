{
  "name": "IronSourceSnapAdapter",
  "version": "4.3.2.1",
  "summary": "Snap Adapter",
  "description": "Use this adapter to show Snap ads",
  "homepage": "http://www.is.com/",
  "license": {
    "type": "Commercial",
    "text": "https://platform.ironsrc.com/partners/terms-and-conditions-new-user"
  },
  "authors": {
    "IronSource": "http://www.is.com/contact/"
  },
  "source": {
    "http": "https://raw.githubusercontent.com/ironsource-mobile/iOS-adapters/master/snap-adapter/4.3.2/ISSnapAdapter4.3.2.zip"
  },
  "source_files": "ISSnapAdapter/ISSnapAdapter.framework/**/*.{h,m}",
  "public_header_files": "ISSnapAdapter/ISSnapAdapter.framework/**/*.h",
  "preserve_paths": "ISSnapAdapter/ISSnapAdapter.framework",
  "platforms": {
    "ios": "10.0"
  },
  "requires_arc": true,
  "pod_target_xcconfig": {
    "LD_RUNPATH_SEARCH_PATHS": "@executable_path/Frameworks",
    "XCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64",
    "VALID_ARCHS": "arm64 arm64e armv7 armv7s x86_64"
  },
  "user_target_xcconfig": {
    "XCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "vendored_frameworks": "ISSnapAdapter/ISSnapAdapter.framework",
  "dependencies": {
    "IronSourceSDK": [
      "= 7.1.11.0"
    ],
    "SAKSDK": [
      "= 1.1.0"
    ]
  }
}
