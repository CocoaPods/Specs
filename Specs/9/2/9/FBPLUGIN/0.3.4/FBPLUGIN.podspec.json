    ],
    "AVHexColor": [
      "~> 2.0"
    ],
    "SVProgressHUD": [

    ],
    "Toast": [
      "~> 2.4"
    ],
    "ASIHTTPRequest": [
      "~> 1.8"
    ],
    "JSONKit-NoWarning": [
      "~> 1.2"
    ],
    "SDWebImage": [
      "~> 3.7"
    ],
    "hpple": [
      "~> 0.2"
    ]
  }
}
