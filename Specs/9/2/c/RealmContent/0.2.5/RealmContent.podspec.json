{
  "name": "RealmContent",
  "version": "0.2.5",
  "summary": "Realm powered content management system",
  "description": "Realm powered content management system providing built-in view controllers and views to rapidly add dynamic content to iOS apps.",
  "homepage": "https://github.com/realm-demos/RealmContent",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "source": {
    "git": "https://github.com/realm-demos/RealmContent.git",
    "tag": "0.2.5"
  },
  "authors": {
    "Realm": "help@realm.io"
  },
  "libraries": "c++",
  "requires_arc": true,
  "social_media_url": "https://twitter.com/realm",
  "platforms": {
    "ios": "9.0"
  },
  "frameworks": "UIKit",
  "dependencies": {
    "RealmSwift": [
      "~> 3"
    ],
    "Kingfisher": [
      "~> 4"
    ],
    "NSString+Color": [

    ]
  },
  "default_subspecs": "Core",
  "pod_target_xcconfig": {
    "SWIFT_VERSION": "4.0"
  },
  "pushed_with_swift_version": "4.0",
  "subspecs": [
    {
      "name": "Core",
      "source_files": [
        "RealmContent/Entities/*",
        "RealmContent/Core/Classes/*",
        "RealmContent/Core/View/*"
      ]
    },
    {
      "name": "Markdown",
      "dependencies": {
        "MMMarkdown": [

        ]
      },
      "source_files": [
        "RealmContent/Entities/*",
        "RealmContent/Markdown/Classes/*",
        "RealmContent/Markdown/View/*"
      ]
    }
  ]
}
