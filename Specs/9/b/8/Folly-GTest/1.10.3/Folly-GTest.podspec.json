{
  "name": "Folly-GTest",
  "version": "1.10.3",
  "license": {
    "type": "BSD",
    "file": "LICENSE"
  },
  "homepage": "https://github.com/google/googletest.git",
  "authors": {
    "Pritesh Nandgaonkar": "prit91@fb.com"
  },
  "summary": "GTest is googles c++ testing framework",
  "module_name": "gtest",
  "source": {
    "git": "https://github.com/google/googletest.git",
    "tag": "release-1.10.0"
  },
  "source_files": "googletest/include/gtest/**/*.h",
  "preserve_paths": "googletest/include/**",
  "compiler_flags": "-DGTEST_INCLUDE_GTEST_GTEST_SPI_H_=0",
  "requires_arc": true,
  "platforms": {
    "ios": "8.0",
    "osx": "10.10"
  },
  "header_dir": "gtest",
  "description": "Just the headers of GTest, a c++ testing framework",
  "pod_target_xcconfig": {
    "USE_HEADERMAP": "NO",
    "CLANG_CXX_LANGUAGE_STANDARD": "c++11",
    "HEADER_SEARCH_PATHS": "\"$(PODS_TARGET_SRCROOT)/googletest/include/**\" \"$(PODS_TARGET_SRCROOT)/googletest/src/**\""
  }
}
