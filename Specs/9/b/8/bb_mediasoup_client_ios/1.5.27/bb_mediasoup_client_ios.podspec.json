{
  "name": "bb_mediasoup_client_ios",
  "version": "1.5.27",
  "summary": "Mediasoup 3 iOS Client",
  "description": "iOS implementation of libmediasoupclient",
  "homepage": "https://github.com/Bojan7B/mediasoup-ios-client",
  "license": "MIT",
  "authors": {
    "Bojan7B": "bojan7b@gmail.com"
  },
  "platforms": {
    "ios": "13.0"
  },
  "source": {
    "git": "https://github.com/Bojan7B/mediasoup-ios-client.git",
    "tag": "1.5.27"
  },
  "module_name": "vl_mediasoup_client_ios",
  "requires_arc": false,
  "pod_target_xcconfig": {
    "USE_HEADERMAP": "NO",
    "ALWAYS_SEARCH_USER_PATHS": "NO",
    "CLANG_CXX_LANGUAGE_STANDARD": "c++14",
    "CLANG_CXX_LIBRARY": "libc++",
    "OTHER_CPLUSPLUSFLAGS": "\"-stdlib=libc++\" \"-Wall\" \"-Wextra\" \"-Wpedantic\"",
    "VALID_ARCHS": "$(ARCHS_STANDARD_64_BIT)",
    "OTHER_LD_FLAGS": "-all_load"
  },
  "frameworks": [
    "AVFoundation",
    "AudioToolbox",
    "CoreAudio",
    "CoreMedia",
    "CoreVideo"
  ],
  "vendored_frameworks": [
    "bin/vl_mediasoup_client_ios.xcframework",
    "bin/WebRTC.xcframework"
  ],
  "module_map": "vl-mediasoup-client-ios/vl_mediasoup_client_ios.modulemap"
}
