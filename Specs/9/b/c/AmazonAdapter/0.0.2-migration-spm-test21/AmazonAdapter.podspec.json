{
  "name": "AmazonAdapter",
  "version": "0.0.2-migration-spm-test21",
  "summary": "an adapter for Google SDK",
  "description": "an adapter for Google SDK for MSP S2S",
  "homepage": "https://github.com/aimsp/msp-ios-sdk/AmazonAdapter",
  "license": "Copyright",
  "authors": {
    "huanzhiNB": "huanzhi.zhang@newsbreak.com"
  },
  "source": {
    "http": "https://github.com/ParticleMedia/msp-ios-sdk-public/releases/download/0.0.2-migration-spm-test21/AmazonAdapter-0.0.2-migration-spm-test21.zip",
    "type": "zip"
  },
  "platforms": {
    "ios": "15.0"
  },
  "source_files": "AmazonAdapter/AmazonAdapter/**/*.{h,m,swift}",
  "exclude_files": "Classes/Exclude",
  "dependencies": {
    "Google-Mobile-Ads-SDK": [
      "~> 12.0"
    ],
    "AmazonPublisherServicesSDK": [
      "4.5.5"
    ],
    "AmazonPublisherServicesAdMobAdapter": [
      "2.2.0"
    ],
    "MSPSharedLibraries": [
      "0.0.2-migration-spm-test21"
    ]
  },
  "pod_target_xcconfig": {
    "VALID_ARCHS": "x86_64 armv7 arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "static_framework": true
}
