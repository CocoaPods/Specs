{
  "name": "VIDVAuth",
  "version": "2.0.0",
  "summary": "Valify Identity Verification Authentication SDK",
  "description": "VIDVAuth provides comprehensive identity verification with liveness detection,\ndocument OCR, and OTP capabilities for iOS applications. Includes VIDVIdentityKit\nwith all sub-SDK dependencies bundled.",
  "homepage": "http://www.valify.me/",
  "authors": {
    "Valify": "sayed@valify.me"
  },
  "license": {
    "type": "Valify",
    "file": "LICENSE"
  },
  "platforms": {
    "ios": "13.0"
  },
  "swift_versions": "5.0",
  "source": {
    "http": "https://valify-public-sdks.s3.eu-central-1.amazonaws.com/VIDVAuth.2.0.0.zip"
  },
  "ios": {
    "vendored_frameworks": [
      "VIDVAuth_package/VIDVAuth.xcframework",
      "VIDVAuth_package/VIDVIdentityKit.xcframework"
    ]
  },
  "dependencies": {
    "VIDVLiveness": [
      "~> 4.1.0"
    ],
    "VIDVDocKit": [
      "~> 1.4.2"
    ],
    "VIDVOTP": [
      "~> 1.0.3"
    ]
  },
  "pod_target_xcconfig": {
    "BUILD_LIBRARY_FOR_DISTRIBUTION": "YES",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "",
    "DEFINES_MODULE": "YES"
  },
  "user_target_xcconfig": {
    "BUILD_LIBRARY_FOR_DISTRIBUTION": "YES"
  },
  "swift_version": "5.0"
}
