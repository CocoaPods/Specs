{
  "name": "InterfaceBuilderKit",
  "version": "1.0.0",
  "summary": "在interface builder中快速设置一些Xcode没有提供的custom runtime attributes的UI分类",
  "homepage": "http://findertiwk.github.io",
  "license": "MIT",
  "authors": {
    "_Finder丶Tiwk": "136652711@qq.com"
  },
  "source": {
    "git": "https://github.com/FinderTiwk/InterfaceBuilderKit.git",
    "tag": "v1.0.0"
  },
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "subspecs": [
    {
      "name": "UIView",
      "source_files": "InterfaceBuilderKit/Src/UIView/*.{h,m}",
      "public_header_files": "InterfaceBuilderKit/Src/UIView/*.h"
    },
    {
      "name": "UIButton",
      "source_files": "InterfaceBuilderKit/Src/UIButton/*.{h,m}",
      "public_header_files": "InterfaceBuilderKit/Src/UIButton/*.h"
    },
    {
      "name": "UITextField",
      "source_files": "InterfaceBuilderKit/Src/UITextField/*.{h,m}",
      "public_header_files": "InterfaceBuilderKit/Src/UITextField/*.h"
    },
    {
      "name": "UITableView",
      "source_files": "InterfaceBuilderKit/Src/UITableView/*.{h,m}",
      "public_header_files": "InterfaceBuilderKit/Src/UITableView/*.h"
    }
  ]
}
