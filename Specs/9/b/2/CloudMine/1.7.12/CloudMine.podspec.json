{
  "name": "CloudMine",
  "version": "1.7.12",
  "summary": "The iOS Framework for interacting with CloudMine.",
  "homepage": "https://cloudmine.io/docs/#/ios",
  "license": "MIT",
  "authors": {
    "CloudMine": "support@cloudmineinc.com"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source": {
    "git": "https://github.com/cloudmine/CloudMineSDK-iOS.git",
    "tag": "1.7.12"
  },
  "source_files": "ios/ios/src/**/*.{h,m}",
  "exclude_files": [
    "NSString+UUID.h",
    "NSURL+QueryParameterAdditions.h",
    "CMObject+Private.h",
    "CMObjectClassNameRegistry.h",
    "MARTNSObject.{h,m}",
    "RT*.{h,m}"
  ],
  "frameworks": [
    "UIKit",
    "CoreGraphics",
    "MobileCoreServices",
    "SystemConfiguration",
    "CFNetwork",
    "Foundation",
    "CoreFoundation",
    "CoreLocation",
    "Social",
    "Accounts"
  ],
  "libraries": "z",
  "requires_arc": true,
  "xcconfig": {
    "OTHER_LDFLAGS": "-ObjC"
  },
  "dependencies": {
    "AFNetworking": [
      "~> 2.6.3"
    ]
  },
  "prefix_header_contents": [
    "#import <SystemConfiguration/SystemConfiguration.h>",
    "#import <MobileCoreServices/MobileCoreServices.h>"
  ],
  "subspecs": [
    {
      "name": "no-arc",
      "requires_arc": false,
      "source_files": [
        "ios/ios/src/**/MARTNSObject*.{h,m}",
        "ios/ios/src/**/RT*.{h,m}"
      ]
    }
  ]
}
