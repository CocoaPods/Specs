{
  "name": "LibMobileCoin",
  "version": "1.0.0",
  "summary": "A library for communicating with MobileCoin network",
  "authors": "MobileCoin",
  "homepage": "https://www.mobilecoin.com/",
  "license": {
    "type": "GPLv3"
  },
  "source": {
    "git": "https://github.com/mobilecoinofficial/libmobilecoin-ios-artifacts.git",
    "tag": "1.0.0"
  },
  "platforms": {
    "ios": "10.0"
  },
  "source_files": [
    "Artifacts/include/*.h",
    "Sources/Generated/Proto/*.{grpc,pb}.swift"
  ],
  "vendored_libraries": "Artifacts/libmobilecoin.a",
  "dependencies": {
    "gRPC-Swift": [
      "~> 1.0.0"
    ],
    "SwiftProtobuf": [
      "~> 1.5"
    ]
  },
  "swift_versions": "5.2",
  "pod_target_xcconfig": {
    "ENABLE_BITCODE": "NO",
    "OTHER_LDFLAGS": "-u _mc_string_free",
    "SUPPORTS_MACCATALYST": "NO",
    "VALID_ARCHS[sdk=iphoneos*]": "arm64",
    "VALID_ARCHS[sdk=iphonesimulator*]": "x86_64"
  },
  "user_target_xcconfig": {
    "ENABLE_BITCODE": "NO",
    "SUPPORTS_MACCATALYST": "NO",
    "VALID_ARCHS[sdk=iphoneos*]": "arm64",
    "VALID_ARCHS[sdk=iphonesimulator*]": "x86_64"
  },
  "swift_version": "5.2"
}
