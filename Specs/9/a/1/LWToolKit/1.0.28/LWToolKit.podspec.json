        "Sources/LWUI/**/*.{h,m,mm,swift}"
      ],
      "frameworks": [
        "UIKit"
      ]
    },
    {
      "name": "LWMedia",
      "dependencies": {
        "LWToolKit/LWCore": [

        ]
      },
      "source_files": [
        "LWToolKit/Sources/LWMedia/**/*.{h,m,mm,swift}",
        "Sources/LWMedia/**/*.{h,m,mm,swift}"
      ],
      "frameworks": [
        "PhotosUI",
        "UniformTypeIdentifiers"
      ]
    },
    {
      "name": "LWAnalytics",
      "dependencies": {
        "LWToolKit/LWCore": [

        ]
      },
      "source_files": [
        "LWToolKit/Sources/LWAnalytics/**/*.{h,m,mm,swift}",
        "Sources/LWAnalytics/**/*.{h,m,mm,swift}"
      ]
    },
    {
      "name": "LWH5Bridge",
      "dependencies": {
        "LWToolKit/LWCore": [

        ]
      },
      "source_files": [
        "LWToolKit/Sources/LWH5Bridge/**/*.{h,m,mm,swift}",
        "Sources/LWH5Bridge/**/*.{h,m,mm,swift}"
      ]
    },
    {
      "name": "LWNetwork",
      "dependencies": {
        "LWToolKit/LWCore": [

        ],
        "Alamofire": [
          "~> 5.8"
        ]
      },
      "source_files": [
        "LWToolKit/Sources/LWNetwork/**/*.{h,m,mm,swift}",
        "Sources/LWNetwork/**/*.{h,m,mm,swift}"
      ],
      "frameworks": [
        "Network",
        "Security"
      ]
    },
    {
      "name": "All",
      "dependencies": {
