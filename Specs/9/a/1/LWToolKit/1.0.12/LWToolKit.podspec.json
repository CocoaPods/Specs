{
  "name": "LWToolKit",
  "version": "1.0.12",
  "summary": "iOS 工具包：Core、UI、Media、Analytics、Network 等基础能力。",
  "description": "长期可用的轻量 iOS 基础工具包，包含节流/防抖、任务队列、Keychain、缓存、本地化、AB 实验、通知、深链路由、UI 组件（Toast/Alert）、媒体选择与加载、事件上报，以及基于 Alamofire 的网络层等模块。",
  "homepage": "https://github.com/linlishu8/LWToolKit",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "linlishu8": "linlishu8@163.com"
  },
  "source": {
    "git": "https://github.com/linlishu8/LWToolKit.git",
    "tag": "1.0.12"
  },
  "platforms": {
    "ios": "14.0"
  },
  "swift_versions": [
    "5.9",
    "5.10"
  ],
  "requires_arc": true,
  "default_subspecs": "LWCore",
  "subspecs": [
    {
      "name": "LWCore",
      "ios": {
        "frameworks": [
          "UIKit",
          "UserNotifications",
          "Network"
        ]
      },
      "source_files": "Sources/LWCore/**/*.{h,m,swift}"
    },
    {
      "name": "LWUI",
      "dependencies": {
        "LWToolKit/LWCore": [

        ]
      },
      "source_files": "Sources/LWUI/**/*.{h,m,swift}"
    },
    {
      "name": "LWMedia",
      "dependencies": {
        "LWToolKit/LWCore": [

        ]
      },
      "source_files": "Sources/LWMedia/**/*.{h,m,swift}"
    },
    {
      "name": "LWAnalytics",
      "dependencies": {
        "LWToolKit/LWCore": [

        ]
      },
      "source_files": "Sources/LWAnalytics/**/*.{h,m,swift}"
    },
    {
      "name": "LWNetwork",
      "dependencies": {
        "LWToolKit/LWCore": [

        ],
        "Alamofire": [
          "~> 5.8"
        ]
      },
      "frameworks": [
        "Security",
        "Network"
      ],
      "source_files": "Sources/LWNetwork/**/*.{h,m,swift}"
    }
  ],
  "swift_version": "5.10"
}
