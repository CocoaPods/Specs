{
  "name": "KeccakC",
  "version": "1.0.0",
  "summary": "Keccak C wrapper for CocoaPods",
  "homepage": "https://github.com/sublabdev/keccak-swift",
  "license": {
    "type": "Apache 2.0",
    "file": "LICENSE"
  },
  "authors": {
    "Substrate Laboratory LLC": "info@sublab.dev"
  },
  "source": {
    "git": "https://github.com/sublabdev/keccak-swift.git",
    "tag": "c-1.0.0"
  },
  "platforms": {
    "ios": "13.0",
    "osx": "10.15"
  },
  "source_files": "Sources/KeccakC/**/*.{c,h}",
  "public_header_files": "Sources/KeccakC/include/*.h",
  "pod_target_xcconfig": {
    "OTHER_CFLAGS": "-pedantic -Wall -Wextra -Wcast-align -Wnested-externs -Wshadow -Wstrict-prototypes -Wno-shorten-64-to-32 -Wno-conditional-uninitialized -Wno-unused-function -Wno-long-long -Wno-overlength-strings -O3",
    "HEADER_SEARCH_PATHS": "\"${PODS_ROOT}/KeccakC\""
  },
  "module_name": "KeccakC"
}
