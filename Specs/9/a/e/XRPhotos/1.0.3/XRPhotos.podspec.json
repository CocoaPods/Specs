      "UIKit",
      "Foundation"
    ]
  },
  "weak_frameworks": "Photos",
  "requires_arc": true,
  "dependencies": {
    "MWPhotoBrowser": [
      "~> 2.1.2"
    ],
    "RSKImageCropper": [
      "~> 2.2.1"
    ]
  }
