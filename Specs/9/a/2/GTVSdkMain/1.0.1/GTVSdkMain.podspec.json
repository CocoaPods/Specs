{
  "name": "GTVSdkMain",
  "version": "1.0.1",
  "summary": "GTV SDK cho iOS",
  "description": "SDK hỗ trợ login, notification, tracking và quảng cáo cho ứng dụng iOS.",
  "homepage": "https://github.com/duynk96/gtv-private-specs",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "duynk96": "duynk@gtv.vn"
  },
  "source": {
    "git": "https://github.com/duynk96/gtv-private-specs.git",
    "branch": "release"
  },
  "platforms": {
    "ios": "15.0"
  },
  "swift_versions": [
    "5.0",
    "5.5",
    "5.9"
  ],
  "static_framework": false,
  "default_subspecs": "Core",
  "subspecs": [
    {
      "name": "Core",
      "vendored_frameworks": "GTVSdkIos.xcframework",
      "dependencies": {
        "Firebase/Messaging": [
          "~> 12.3.0"
        ],
        "airbridge-ios-sdk": [
          "~> 4.8.1"
        ]
      }
    },
    {
      "name": "Full",
      "vendored_frameworks": "FrameworkFull/GTVSdkIos.xcframework",
      "dependencies": {
        "Google-Mobile-Ads-SDK": [
          "~> 12.11.0"
        ],
        "Firebase/Messaging": [
          "~> 12.3.0"
        ],
        "airbridge-ios-sdk": [
          "~> 4.8.1"
        ]
      }
    }
  ],
  "swift_version": "5.9"
}
