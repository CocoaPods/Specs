{
  "name": "protobuf-c",
  "version": "1.2.1",
  "summary": "C bindings for Google's Protocol Buffers",
  "authors": {
    "Dave Benson": ""
  },
  "source": {
    "git": "https://github.com/protobuf-c/protobuf-c.git",
    "tag": "v1.2.1"
  },
  "homepage": "https://github.com/protobuf-c/protobuf-c",
  "license": {
    "type": "BSD",
    "file": "LICENSE"
  },
  "source_files": "protobuf-c/*.{h,c}",
  "platforms": {
    "ios": "8.0"
  },
  "module_map": "protobuf-c.modulemap",
  "prepare_command": "printf \"framework module protobuf_c {\\n  umbrella header \\\"protobuf-c.h\\\"\\n\\n  export *\\n  module * { export * }\\n}\" > protobuf-c.modulemap",
  "prefix_header_contents": [
    "#define HAVE_ALLOCA_H 1",
    "#define HAVE_MALLOC_H 1",
    "#define HAVE_SYS_POLL_H 1",
    "#define HAVE_SYS_SELECT_H 1",
    "#define HAVE_INTTYPES_H 1",
    "#define HAVE_SYS_UIO_H 1",
    "#define HAVE_UNISTD_H 1",
    "#define HAVE_IO_H 1"
  ],
  "requires_arc": false,
  "xcconfig": {
    "GCC_WARN_64_TO_32_BIT_CONVERSION": "NO"
  }
}
