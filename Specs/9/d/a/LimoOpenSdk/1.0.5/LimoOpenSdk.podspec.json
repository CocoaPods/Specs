{
  "name": "LimoOpenSdk",
  "version": "1.0.5",
  "summary": "Lightweight Objective-C SDK wrapping AnyThink for splash and interstitial ads with event reporting.",
  "description": "LimoOpenSdk 提供基于 AnyThink 的开屏与插屏广告封装，并内置事件上报能力，简化集成流程。",
  "homepage": "https://gitee.com/youzhadoubao_admin/limo-open-sdk-demo",
  "license": {
    "type": "MIT"
  },
  "authors": {
    "Limo": "support@limo.example"
  },
  "source": {
    "git": "https://gitee.com/youzhadoubao_admin/limo-open-sdk-pods.git",
    "tag": "1.0.5"
  },
  "platforms": {
    "ios": "13.0"
  },
  "requires_arc": true,
  "static_framework": true,
  "module_name": "LimoOpenSdk",
  "vendored_frameworks": "LimoOpenSdk.xcframework",
  "preserve_paths": "LimoOpenSdk.xcframework",
  "dependencies": {
    "AnyThinkiOS": [
      "6.5.33"
    ],
    "AnyThinkMediationGromoreAdapter": [
      "7.2.0.0.3"
    ],
    "AnyThinkMediationKuaiShouAdapter": [
      "4.9.20.3.1"
    ],
    "AnyThinkMediationTTAdapter_Mix": [
      "7.2.0.0.3"
    ],
    "AnyThinkMediationGDTAdapter": [
      "4.15.60.5"
    ]
  },
  "frameworks": [
    "UIKit",
    "AdSupport"
  ],
  "swift_versions": [
    "5.0"
  ],
  "pod_target_xcconfig": {
    "OTHER_LDFLAGS": "-ObjC",
    "IPHONEOS_DEPLOYMENT_TARGET": "13.0",
    "ARCHS[sdk=iphonesimulator*]": "x86_64",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64",
    "FRAMEWORK_SEARCH_PATHS": "$(inherited)"
  },
  "user_target_xcconfig": {
    "IPHONEOS_DEPLOYMENT_TARGET": "13.0",
    "ARCHS[sdk=iphonesimulator*]": "x86_64",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64",
    "FRAMEWORK_SEARCH_PATHS": "$(inherited)"
  },
  "swift_version": "5.0"
}
