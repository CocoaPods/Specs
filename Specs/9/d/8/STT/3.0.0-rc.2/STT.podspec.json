      "source_files": [
        "STT/Core/*.swift",
        "STT/UIElements/*.swift"
      ]
    },
    {
      "name": "Support2x",
      "source_files": [
        "STT/Core/*.swift",
        "STT/Bindings/*.swift",
        "STT/Extensions/*.swift",
        "STT/RxExtensions/*.swift",
        "STT/UIElements/*.swift",
        "STT/Support2x/Bindings/*.swift",
        "STT/Support2x/Components/*.swift",
        "STT/Support2x/Components/Core/*.swift",
        "STT/Support2x/Components/View/*.swift"
      ],
      "dependencies": {
        "TinyConstraints": [

        ],
