        "SDWebImage": [
          "~> 5.0.0"
        ]
      }
    },
    {
      "name": "Bindings",
      "source_files": [
        "STT/Core/*.swift",
        "STT/Bindings/*.swift"
      ],
      "dependencies": {
        "RxCocoa": [
          "~> 5"
        ]
      }
    },
    {
      "name": "NotificationBanner",
      "source_files": "STT/NotificationBanner/*.swift"
    },
    {
