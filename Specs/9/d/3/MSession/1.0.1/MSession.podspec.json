{
  "name": "MSession",
  "version": "1.0.1",
  "summary": "A simple way to manager session in your application",
  "homepage": "https://github.com/vitormesquita/MSession",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Vitor mesquita": "vitor.mesquita09@gmail.com"
  },
  "source": {
    "git": "https://github.com/vitormesquita/MSession.git",
    "tag": "1.0.1"
  },
  "platforms": {
    "ios": "10.0"
  },
  "default_subspecs": "Core",
  "subspecs": [
    {
      "name": "Core",
      "source_files": "Source/**/*"
    },
    {
      "name": "Session",
      "source_files": [
        "Source/Keychain/*",
        "Source/Session/*"
      ]
    },
    {
      "name": "Auth",
      "source_files": [
        "Source/Keychain/*",
        "Source/Auth/*"
      ]
    }
  ]
}
