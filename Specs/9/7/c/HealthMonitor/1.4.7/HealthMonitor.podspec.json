        ],
        "JSONModel": [

        ],
        "Toast": [

        ],
        "MBProgressHUD": [

        ],
        "IQKeyboardManager": [
          "4.0.13"
        ],
        "MJRefresh": [
          "~> 3.1.12"
        ],
        "NJKWebViewProgress": [

        ]
      }
    },
    {
      "name": "AJKCommon",
      "source_files": "MVVM/AJKCommon/AJKCommon_Class/**/*.{h,m}",
      "resource_bundles": {
        "AJKCommon": [
          "MVVM/AJKCommon/AJKCommon_Resources/RefreshImg/*.png"
        ]
      },
      "dependencies": {
        "HealthMonitor/AJKFrame": [

        ]
      }
    },
    {
      "name": "Monitor",
      "source_files": [
        "Monitor/Classes/**/*.{h,m}",
        "Monitor/LollypopSDK.framework/Headers/**/*.h",
        "Monitor/Classes/LMTP/lame.h"
      ],
      "resource_bundles": {
        "Monitor": [
          "Monitor/Assets/**/*.png",
          "Monitor/Assets/**/*.jpg",
          "Monitor/Assets/**/*.json",
          "Monitor/Classes/**/*.xib",
          "Monitor/Classes/**/*.storyboard",
          "Monitor/Classes/**/*.bundle"
