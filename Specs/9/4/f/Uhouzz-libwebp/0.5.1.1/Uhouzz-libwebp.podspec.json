{
  "name": "Uhouzz-libwebp",
  "version": "0.5.1.1",
  "summary": "Library to encode and decode images in WebP format.",
  "homepage": "https://github.com/Uhouzz/libwebp",
  "license": {
    "type": "BSD",
    "file": "COPYING"
  },
  "authors": "Google Inc.",
  "platforms": {
    "ios": "7.0.0"
  },
  "source": {
    "git": "https://github.com/Uhouzz/libwebp.git",
    "tag": "v0.5.1.1"
  },
  "compiler_flags": "-D_THREAD_SAFE",
  "requires_arc": false,
  "xcconfig": {
    "USER_HEADER_SEARCH_PATHS": "$(inherited) $(SRCROOT)/Uhouzz-libwebp/src"
  },
  "subspecs": [
    {
      "name": "webp",
      "source_files": [
        "src/utils/*.{h,c}",
        "src/dsp/*.{h,c}",
        "src/enc/*.{h,c}",
        "src/dec/*.{h,c}",
        "src/webp/*.h"
      ],
      "private_header_files": [
        "src/utils/*.h",
        "src/dsp/*.h",
        "src/enc/*.h",
        "src/dec/*.h",
        "src/webp/*.h"
      ]
    }
  ]
}
