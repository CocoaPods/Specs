{
  "name": "RxCache",
  "version": "1.0.1",
  "summary": "Reactive caching library for Swift",
  "homepage": "https://github.com/FuckBoilerplate/RxCache",
  "license": "MIT",
  "authors": {
    "Victor Albertos": "me@victoralbertos.com"
  },
  "source": {
    "git": "https://github.com/FuckBoilerplate/RxCache.git",
    "tag": "1.0.1"
  },
  "social_media_url": "https://github.com/FuckBoilerplate",
  "platforms": {
    "watchos": "2.0",
    "ios": "8.0",
    "osx": "10.10"
  },
  "requires_arc": true,
  "default_subspecs": "Core",
  "subspecs": [
    {
      "name": "Core",
      "source_files": "Sources/Core/**/*",
      "dependencies": {
        "RxSwift": [
          ">= 3.0.0-beta.1"
        ]
      }
    },
    {
      "name": "ObjectMapper",
      "source_files": "Sources/Cacheables/ObjectMapper/*.swift",
      "dependencies": {
        "RxCache/Core": [

        ],
        "ObjectMapper": [
          "~> 2.0"
        ]
      }
    },
    {
      "name": "Gloss",
      "source_files": "Sources/Cacheables/Gloss/*.swift",
      "dependencies": {
        "RxCache/Core": [

        ],
        "Gloss": [
          "~> 1.0"
        ]
      }
    }
  ]
}
