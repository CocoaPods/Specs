      "name": "Core",
      "source_files": "ISHPermissionKit/**/*.{h,m}",
      "private_header_files": "ISHPermissionKit/Private/**/*.h",
      "pod_target_xcconfig": {
        "OTHER_LDFLAGS": "-ObjC"
      },
      "requires_arc": true
    },
