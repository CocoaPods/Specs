{
  "name": "NonaPics",
  "version": "0.2.1",
  "summary": "A simple library to generate images from octicons.ttf",
  "homepage": "https://github.com/dtweston/NonaPics",
  "license": "MIT",
  "authors": {
    "Dave Weston": "dave.weston@binocracy.com"
  },
  "social_media_url": "https://twitter.com/dt_weston",
  "source": {
    "git": "https://github.com/dtweston/NonaPics.git",
    "tag": "0.2.1"
  },
  "requires_arc": true,
  "source_files": "Pod/Classes",
  "platforms": {
    "ios": "7.0",
    "osx": "10.9"
  },
  "prepare_command": "./Pod/download_font.sh",
  "osx": {
    "resource_bundles": {
      "NonaPics-OSX": [
        "Pod/Assets/*"
      ]
    }
  },
  "ios": {
    "resource_bundles": {
      "NonaPics-iOS": [
        "Pod/Assets/*"
      ]
    }
  }
}
