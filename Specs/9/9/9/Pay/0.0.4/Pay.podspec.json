  "requires_arc": true,
  "platforms": {
    "ios": "9.0"
  },
  "source_files": [
    "Pay/Classes/**/*.swift",
    "Alipay/**/*.h",
    "Wechat/**/*.h",
    "Pay/Classes/*.h"
  ],
  "vendored_frameworks": "Alipay/AlipaySDK.framework",
  "vendored_libraries": "Wechat/libWeChatSDK.a",
  "frameworks": [
    "SystemConfiguration",
    "Security",
    "CoreTelephony",
    "CFNetwork",
    "UIKit",
    "QuartzCore",
    "CoreText",
    "CoreGraphics",
    "Foundation",
    "CoreMotion"
  ],
  "libraries": [
    "z",
    "c++",
    "sqlite3.0"
  ],
  "xcconfig": {
    "OTHER_LDFLAGS": "-ObjC -all_load"
  },
  "resource_bundles": {
    "Pay": [
      "Pay/Assets/**/*.png",
      "Alipay/AlipaySDK.bundle"
    ]
  },
  "public_header_files": [
    "Pay/Classes/*.h",
    "Alipay/**/*.h",
    "Wechat/**/*.h"
  ],
