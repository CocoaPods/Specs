{
  "name": "FirebaseOAuthUI",
  "version": "12.1.1",
  "summary": "A collection of OAuth providers for FirebaseAuthUI.",
  "homepage": "https://github.com/firebase/FirebaseUI-iOS",
  "license": {
    "type": "Apache 2.0",
    "file": "LICENSE"
  },
  "source": {
    "git": "https://github.com/firebase/FirebaseUI-iOS.git",
    "tag": "v12.1.1"
  },
  "authors": "Firebase",
  "platforms": {
    "ios": "10.0"
  },
  "ios": {
    "frameworks": "UIKit"
  },
  "requires_arc": true,
  "cocoapods_version": ">= 1.8.0",
  "pod_target_xcconfig": {
    "HEADER_SEARCH_PATHS": "\"${PODS_TARGET_SRCROOT}\""
  },
  "public_header_files": "FirebaseOAuthUI/Sources/Public/FirebaseOAuthUI/*.h",
  "source_files": "FirebaseOAuthUI/Sources/**/*.{h,m}",
  "dependencies": {
    "FirebaseAuthUI": [
      ">= 12.0.2",
      "< 13.0"
    ],
    "FirebaseAuth": [
      "~> 8.0"
    ]
  },
  "resource_bundles": {
    "FirebaseOAuthUI": [
      "FirebaseOAuthUI/Sources/{Resources,Strings}/*.{png,lproj}"
    ]
  }
}
