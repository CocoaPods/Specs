{
  "name": "GliaWidgets",
  "version": "2.0.0",
  "summary": "The Glia iOS Widgets library",
  "description": "The Glia Widgets library allows to integrate easily a UI/UX for Glia's Digital Customer Service platform",
  "homepage": "https://github.com/salemove/ios-sdk-widgets",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Glia": "support@glia.com"
  },
  "source": {
    "git": "https://github.com/salemove/ios-sdk-widgets.git",
    "tag": "2.0.0"
  },
  "module_name": "GliaWidgets",
  "platforms": {
    "ios": "13.0"
  },
  "source_files": "GliaWidgets/**/*.swift",
  "swift_versions": "5.3",
  "resources": [
    "GliaWidgets/Resources/*.{xcassets}",
    "GliaWidgets/Resources/**/*.{strings}"
  ],
  "resource_bundles": {
    "GliaWidgets": [
      "GliaWidgets/Resources/*.{xcassets}",
      "GliaWidgets/Resources/**/*.{strings}"
    ]
  },
  "exclude_files": [
    "GliaWidgets/Window/**"
  ],
  "dependencies": {
    "GliaCoreSDK": [
      "1.0.2"
    ],
    "PureLayout": [
      "~>3.1"
    ],
    "lottie-ios": [
      "3.2.3"
    ]
  },
  "swift_version": "5.3"
}
