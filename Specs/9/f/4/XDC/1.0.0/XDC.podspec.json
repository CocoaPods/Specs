{
  "name": "XDC",
  "version": "1.0.0",
  "summary": "XDC cocoapod",
  "description": "XDC framework is used for getting XDC data.",
  "homepage": "https://github.com/angshuman2611/XDC.git",
  "license": "MIT",
  "authors": {
    "Angshuman": "angshuman@leewayhertz.com"
  },
  "source": {
    "git": "https://github.com/angshuman2611/XDC.git",
    "tag": "1.0.0"
  },
  "source_files": [
    "XDC/**/*.{Swift}",
    "XDC/lib/**/*.{c,h}"
  ],
  "platforms": {
    "ios": "11.0"
  },
  "swift_versions": "5.0",
  "static_framework": true,
  "pod_target_xcconfig": {
    "SWIFT_INCLUDE_PATHS[sdk=iphonesimulator*]": "$(PODS_TARGET_SRCROOT)/XDC/lib/**",
    "SWIFT_INCLUDE_PATHS[sdk=iphoneos*]": "$(PODS_TARGET_SRCROOT)/XDC/lib/**"
  },
  "dependencies": {
    "GenericJSON": [
      "~> 2.0.0"
    ],
    "BigInt": [
      "~> 5.2"
    ],
    "secp256k1.swift": [
      "~> 0.1.0"
    ]
  },
  "swift_version": "5.0"
}
