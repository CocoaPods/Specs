{
  "name": "MMSmartStreamingSDK",
  "version": "0.2.3",
  "summary": "A short description of MMSmartStreamingSDK.",
  "description": "The MediaMelon Player SDK adds SmartSight Analytics and QBR SmartStreaming capability to any media player and is available for all ABR media players.",
  "homepage": "https://bitbucket.org/mediameloninc/mmsmartstreamingios-genericpods.git",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "mediamelon": "samit@mediamelon.com"
  },
  "platforms": {
    "ios": "11.0",
    "tvos": "11.0"
  },
  "source": {
    "git": "https://bitbucket.org/mediameloninc/mmsmartstreamingios-genericpods",
    "tag": "0.2.3"
  },
  "source_files": [
    "MMSmartStreamingSDK/Classes/Common/*.{h,m,swift}",
    "MMSmartStreamingSDK/Classes/Wrapper/ssai/*.swift",
    "MMSmartStreamingSDK/Classes/Utilities/M3U8Parser/*.{h,m}",
    "MMSmartStreamingSDK/Classes/Utilities/VastClient/*.{h,swift}"
  ],
  "ios": {
    "frameworks": "CoreTelephony",
    "vendored_libraries": "MMSmartStreamingSDK/Classes/StaticLibrary/iOS/libmmsmartstreamer.a"
  },
  "tvos": {
    "vendored_libraries": "MMSmartStreamingSDK/Classes/StaticLibrary/tvOS/libmmsmartstreaming-tvos.a"
  },
  "libraries": "stdc++",
  "public_header_files": [
    "MMSmartStreamingSDK/Classes/Common/*.h",
    "MMSmartStreamingSDK/Classes/Utilities/M3U8Parser/*.h"
  ],
  "swift_versions": "5.0",
  "pod_target_xcconfig": {
    "VALID_ARCHS[sdk=iphonesimulator*]": "armv7 arm64 x86_64",
    "VALID_ARCHS[sdk=appletvsimulator*]": "armv7 arm64 x86_64"
  },
  "swift_version": "5.0"
}
