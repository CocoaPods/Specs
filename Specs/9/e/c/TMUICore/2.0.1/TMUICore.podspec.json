    "file": "LICENSE"
  },
  "authors": {
    "chengzongxin": "joe.cheng@corp.to8to.com"
  },
  "source": {
    "git": "https://github.com/chengzongxin/TMUIKit.git",
    "tag": "v2.0.1"
  },
  "platforms": {
    "ios": "9.0"
  },
  "requires_arc": true,
  "public_header_files": "TMUICore/*.h",
  "source_files": "TMUICore/*",
