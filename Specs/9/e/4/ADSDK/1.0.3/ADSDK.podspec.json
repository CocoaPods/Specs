{
  "name": "ADSDK",
  "version": "1.0.3",
  "summary": "广告聚合平台，用于请求广告的iOS SDK",
  "description": "目前第一个版本仅支持信息流模版广告.",
  "homepage": "https://gitee.com/idcool/adsdk.git",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "yueshuai": "yueshuai321@gmail.com"
  },
  "source": {
    "git": "https://gitee.com/idcool/adsdk.git",
    "tag": "1.0.3"
  },
  "platforms": {
    "ios": "12.0"
  },
  "requires_arc": true,
  "libraries": [
    "z",
    "c++",
    "sqlite3",
    "xml2",
    "bz2",
    "c++abi",
    "resolv.9",
    "iconv",
    "c"
  ],
  "frameworks": [
    "AdSupport",
    "AVFoundation",
    "AVKit",
    "Accelerate",
    "AudioToolbox",
    "CoreData",
    "CoreLocation",
    "CoreMedia",
    "CoreMotion",
    "CoreGraphics",
    "CoreTelephony",
    "CoreImage",
    "CoreText",
    "Foundation",
    "MediaPlayer",
    "MessageUI",
    "MobileCoreServices",
    "MapKit",
    "QuartzCore",
    "QuickLook",
    "SystemConfiguration",
    "Security",
    "StoreKit",
    "WebKit",
    "UIKit",
    "ImageIO",
    "SafariServices",
    "JavaScriptCore"
  ],
  "weak_frameworks": "AppTrackingTransparency",
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "armv7"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "armv7"
  },
  "xcconfig": {
    "OTHER_LDFLAGS": "-ObjC"
  },
  "subspecs": [
    {
      "name": "ADSDK",
      "vendored_frameworks": "ADSDK.xcframework"
    },
    {
      "name": "ZBSDK-iOS",
      "dependencies": {
        "BeiZiSDK": [
          "4.90.5.14"
        ]
      }
    },
    {
      "name": "SKSDK-iOS",
      "dependencies": {
        "KSAdSDK": [
          "3.3.76"
        ]
      }
    }
  ]
}
