  "homepage": "https://github.com/ilia3546/Graphus",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Ilya Kharlamov": "ilia3546@me.com"
  },
  "source": {
    "git": "https://github.com/ilia3546/Graphus.git",
