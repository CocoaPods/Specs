{
  "name": "MoEngage-iOS-SDK",
  "version": "9.16.0",
  "summary": "MoEngage for iOS",
  "description": "MoEngage is a mobile marketing automation company. This framework acts like a umbrella framework to include MoEngageCore, MoEngageAnalytics and MoEngageMessaging frameworks.\nMoEngageCore contains the central configuration of your app and common Utils and modules to be used accross all the MoEngage frameworks. MoEngageAnalytics framework enables you to track required data regarding user and user's activities inside the app to use the same for effective engagement.\nMoEngageMessaging module will help you support all the features related to push notifications in your App.",
  "homepage": "https://www.moengage.com",
  "documentation_url": "https://developers.moengage.com",
  "license": {
    "type": "Commercial",
    "file": "LICENSE"
  },
  "authors": {
    "MobileDev": "mobiledevs@moengage.com"
  },
  "social_media_url": "https://twitter.com/moengage",
  "platforms": {
    "ios": "11.0",
    "tvos": "11.0"
  },
  "source": {
    "git": "https://github.com/moengage/MoEngage-iOS-SDK.git",
    "tag": "moengage-9.16.0"
  },
  "ios": {
    "vendored_frameworks": [
      "Frameworks/MoEngageSDK.xcframework",
      "Frameworks/MoEngageCore.xcframework",
      "Frameworks/MoEngageAnalytics.xcframework",
      "Frameworks/MoEngageMessaging.xcframework",
      "Frameworks/MoEngageObjCUtils.xcframework",
      "Frameworks/MoEngageSecurity.xcframework"
    ]
  },
  "tvos": {
    "vendored_frameworks": [
      "Frameworks/MoEngageSDK.xcframework",
      "Frameworks/MoEngageCore.xcframework",
      "Frameworks/MoEngageAnalytics.xcframework",
      "Frameworks/MoEngageMessaging.xcframework",
      "Frameworks/MoEngageObjCUtils.xcframework",
      "Frameworks/MoEngageSecurity.xcframework"
    ]
  },
  "requires_arc": true,
  "frameworks": [
    "Foundation",
    "UIKit",
    "SystemConfiguration",
    "Security"
  ],
  "weak_frameworks": [
    "AdSupport",
    "UserNotifications",
    "AppTrackingTransparency"
  ]
}
