{
  "name": "FZKTools",
  "version": "1.0.2",
  "platforms": {
    "ios": "8.0"
  },
  "summary": "some utilities",
  "homepage": "https://github.com/qqq5201260/FZKTools",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
