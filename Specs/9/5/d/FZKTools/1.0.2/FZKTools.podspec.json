  },
  "source": {
    "git": "https://github.com/qqq5201260/FZKTools.git",
    "tag": "1.0.2"
  },
  "requires_arc": true,
  "source_files": "FZKTools/*",
  "subspecs": [
    {
      "name": "Category",
