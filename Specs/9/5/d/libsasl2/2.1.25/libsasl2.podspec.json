  "summary": "Cyrus SASL Library.",
  "license": {
    "type": "BSD",
    "file": "COPYING"
  },
  "authors": "Carnegie Mellon University",
  "homepage": "http://asg.web.cmu.edu/sasl/sasl-implementations.html",
