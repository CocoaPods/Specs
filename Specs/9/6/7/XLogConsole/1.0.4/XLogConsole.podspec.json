{
  "name": "XLogConsole",
  "version": "1.0.4",
  "summary": "Add a console print log to the app to facilitate troubleshooting",
  "description": "The log can be displayed according to level, name, support keyword search, and export txt, etc.",
  "homepage": "https://github.com/xing3523/XLogConsole",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "xing": "xinxof@foxmail.com"
  },
  "source": {
    "git": "https://github.com/xing3523/XLogConsole.git",
    "tag": "1.0.4"
  },
  "platforms": {
    "ios": "10.0"
  },
  "swift_versions": [
    "4.2",
    "5.0"
  ],
  "default_subspecs": "Core",
  "subspecs": [
    {
      "name": "Core",
      "source_files": "Sources/Classes/*",
      "resource_bundles": {
        "XLogConsole": [
          "Sources/Assets/*"
        ]
      }
    },
    {
      "name": "Helper",
      "source_files": "Sources/Helper/*/*",
      "dependencies": {
        "XLogConsole/Core": [

        ]
      }
    }
  ],
  "swift_version": "5.0"
}
