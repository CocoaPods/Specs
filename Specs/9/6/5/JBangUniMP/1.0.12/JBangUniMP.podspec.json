{
  "name": "JBangUniMP",
  "version": "1.0.12",
  "summary": "JBUniMP",
  "homepage": "https://git.jbangit.com/hongmao/uni-ios",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
