{
  "name": "SSKit",
  "version": "0.3.0",
  "summary": "响应式iOS开发框架",
  "description": "以‘ReactiveCocoa’为基础搭建的简单易懂的响应式iOS开发框架",
  "homepage": "https://github.com/bassamyan/SSKit",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Quincy Yan": "yanyibin.nz@gmail.com"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source": {
    "git": "https://github.com/bassamyan/SSKit.git",
    "tag": "0.3.0"
  },
  "resources": "SSKit/SSResource/**/*",
  "requires_arc": true,
  "dependencies": {
    "ReactiveCocoa": [
      "~> 2.5"
    ],
    "Masonry": [
      "~> 1.0.1"
    ],
    "SDWebImage": [
      "~> 3.8.2"
    ],
    "AFNetworking": [
      "~> 3.1.0"
    ],
    "YYModel": [
      "~> 1.0.4"
    ],
    "FDFullscreenPopGesture": [
      "~> 1.1"
    ],
    "KMNavigationBarTransition": [
      "~> 0.0.10"
    ],
    "TPKeyboardAvoiding": [
      "~> 1.3"
    ]
  },
  "subspecs": [
    {
      "name": "SSConfigure",
      "source_files": "SSKit/SSConfigure/**/*.{h,m}"
    },
    {
      "name": "SSCategory",
      "source_files": "SSKit/SSCategory/**/*.{h,m}"
    },
    {
      "name": "SSCustomUI",
      "source_files": "SSKit/SSCustomUI/**/*.{h,m}",
      "dependencies": {
        "SSKit/SSCategory": [

        ]
      }
    },
    {
      "name": "SSHandler",
      "source_files": "SSKit/SSHandler/**/*.{h,m}",
      "dependencies": {
        "SSKit/SSCustomUI": [

        ]
      }
    },
    {
      "name": "SSInherit",
      "source_files": "SSKit/SSInherit/**/*.{h,m}"
    },
    {
      "name": "SSService",
      "source_files": "SSKit/SSService/**/*.{h,m}",
      "dependencies": {
        "SSKit/SSHandler": [

        ]
      }
    },
    {
      "name": "SSMVVM",
      "source_files": "SSKit/SSMVVM/**/*.{h,m}",
      "dependencies": {
        "SSKit/SSCustomUI": [

        ],
        "SSKit/SSCategory": [

        ],
        "SSKit/SSConfigure": [

        ]
      }
    }
  ]
}
