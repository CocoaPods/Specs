  "subspecs": [
    {
      "name": "All",
      "ios": {
        "dependencies": {
          "nakilib/MRC": [

          ],
          "nakilib/ARC": [

          ]
        }
      }
    },
    {
      "name": "MRC",
      "requires_arc": false,
