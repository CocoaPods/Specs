{
  "name": "YoukeSDK",
  "version": "1.0.8",
  "summary": "YoukeSDK帮助你的APP快速构建APP客服系统。",
  "homepage": "https://github.com/jxd001/YoukeSDK",
  "license": "MIT",
  "authors": {
    "jxd001": "http://weibo.com/jxd001"
  },
  "source": {
    "git": "https://github.com/jxd001/YoukeSDK.git",
    "tag": "1.0.8"
  },
  "platforms": {
    "ios": "7.0"
  },
  "requires_arc": true,
  "source_files": "YoukeSDK/*",
  "preserve_paths": "YoukeSDK/*",
  "libraries": [
    "resolv",
    "sqlite3",
    "xml2"
  ],
  "frameworks": "CoreData",
  "vendored_libraries": [
    "YoukeSDK/libidn.a",
    "YoukeSDK/libPingSDK.a"
  ],
  "resources": [
    "YoukeSDK/YoukeBundle.bundle",
    "YoukeSDK/skin.plist",
    "YoukeSDK/strings.plist",
    "YoukeSDK/*.{mom,momd}"
  ],
  "xcconfig": {
    "OTHER_LDFLAGS": "-ObjC"
  }
}
