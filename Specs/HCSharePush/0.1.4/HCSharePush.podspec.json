{
  "name": "HCSharePush",
  "version": "0.1.4",
  "summary": "这是一个与分享、推送的核心库。",
  "description": "这是一个特定的核心库。包含了常用的分享、命令、推送。简化了外部引用的一些问题。",
  "homepage": "https://github.com/halfking/HCSharePush",
  "license": "MIT",
  "authors": {
    "halfking": "kimmy.huang@gmail.com"
  },
  "platforms": {
    "ios": "7.0"
  },
  "source": {
    "git": "https://github.com/halfking/hcsharepush",
    "tag": "0.1.4",
    "submodules": true
  },
  "frameworks": [
    "UIKit",
    "SystemConfiguration",
    "Foundation",
    "TencentOpenAPI"
  ],
  "xcconfig": {
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES",
    "ENABLE_BITCODE": "YES",
    "DEFINES_MODULE": "YES"
  },
  "dependencies": {
    "HCMinizip": [

    ],
    "hccoren": [

    ],
    "HCBaseSystem": [

    ],
    "UMengAnalytics-NO-IDFA": [

    ],
    "MOBFoundation_IDFA": [

    ],
    "SMSSDK": [

    ],
    "UMengSocial": [

    ],
    "GTSDK": [
      "~> 1.4.2-noidfa"
    ],
    "QQOpenSDK": [

    ]
  },
  "subspecs": [
    {
      "name": "ShareTM",
      "requires_arc": true,
      "source_files": [
        "HCSharePush/TM/*.{h,m,mm,cpp,c}",
        "HCSharePush/Share/*.{h,m,mm,cpp,c}",
        "HCSharePush/HCShareConfig.{h,m}",
        "HCSHarePush/HCShareConfig.h",
        "HCSharePush/shareConfig.h"
      ],
      "public_header_files": [
        "HCSharePush/TM/*.h",
        "HCSharePush/Share/*.h",
        "HCSHarePush/HCShareConfig.h",
        "HCSharePush/shareConfig.h"
      ]
    }
  ]
}
