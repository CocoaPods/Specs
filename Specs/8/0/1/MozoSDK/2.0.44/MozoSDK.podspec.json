      "~> 2.3.0"
    ],
    "ReachabilitySwift": [

    ],
    "MBProgressHUD": [

    ],
    "AlamofireImage": [

    ],
    "Kingfisher": [
      "6.1.1"
    ],
    "SwiftyJSON": [
      "5.0"
    ],
    "libPhoneNumber-iOS": [

    ],
    "JWTDecode": [
      "2.6.0"
    ]
  },
  "swift_version": "4.1"
}
