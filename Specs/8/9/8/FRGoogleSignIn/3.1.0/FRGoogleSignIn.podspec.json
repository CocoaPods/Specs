{
  "name": "FRGoogleSignIn",
  "version": "3.1.0",
  "summary": "ForgeRock Auth Google Sign-in SDK for iOS",
  "description": "FRGoogleSignIn is a SDK that allows a user to sign-in through Google. FRGoogleSignIn depends on GoogleSignIn, and uses Google's SDK to perform authorization following Google's protocol.",
  "homepage": "https://www.forgerock.com",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": "ForgeRock",
  "source": {
    "git": "https://github.com/ForgeRock/forgerock-ios-sdk.git",
    "tag": "3.1.0"
  },
  "static_framework": true,
  "module_name": "FRGoogleSignIn",
  "swift_versions": [
    "5.0",
    "5.1"
  ],
  "platforms": {
    "ios": "10.0"
  },
  "source_files": [
    "FRGoogleSignIn/FRGoogleSignIn/**/*.swift",
    "FRGoogleSignIn/FRGoogleSignIn/**/*.c",
    "FRGoogleSignIn/FRGoogleSignIn/**/*.h"
  ],
  "ios": {
    "dependencies": {
      "FRAuth": [
        "~> 3.1.0"
      ],
      "GoogleSignIn": [
        "~> 5.0.2"
      ]
    }
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "swift_version": "5.1"
}
