{
  "name": "MapxusMapSDK",
  "version": "3.21.0",
  "summary": "Indoor map SDK.",
  "description": "To be the largest global indoor map.",
  "homepage": "http://www.mapxus.com",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Mapxus": "developer@maphive.io"
  },
  "platforms": {
    "ios": "9.0"
  },
  "source": {
    "http": "https://ios-sdk.mapxus.com/3.21.0/mapxus-map-sdk-ios.zip",
    "flatten": true
  },
  "requires_arc": true,
  "module_name": "MapxusMapSDK",
  "vendored_frameworks": [
    "dynamic/MapxusMapSDK.framework",
    "dependency/Mapbox.framework"
  ],
  "preserve_paths": "**/*.bcsymbolmap",
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "$(EXCLUDED_ARCHS__EFFECTIVE_PLATFORM_SUFFIX_$(EFFECTIVE_PLATFORM_SUFFIX)__NATIVE_ARCH_64_BIT_$(NATIVE_ARCH_64_BIT)__XCODE_$(XCODE_VERSION_MAJOR))",
    "EXCLUDED_ARCHS__EFFECTIVE_PLATFORM_SUFFIX_simulator__NATIVE_ARCH_64_BIT_x86_64__XCODE_1200": "arm64 arm64e armv7 armv7s armv6 armv8"
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "$(EXCLUDED_ARCHS__EFFECTIVE_PLATFORM_SUFFIX_$(EFFECTIVE_PLATFORM_SUFFIX)__NATIVE_ARCH_64_BIT_$(NATIVE_ARCH_64_BIT)__XCODE_$(XCODE_VERSION_MAJOR))",
    "EXCLUDED_ARCHS__EFFECTIVE_PLATFORM_SUFFIX_simulator__NATIVE_ARCH_64_BIT_x86_64__XCODE_1200": "arm64 arm64e armv7 armv7s armv6 armv8"
  },
  "dependencies": {
    "MapxusBaseSDK": [
      "3.21.0"
    ],
    "MapboxMobileEvents": [
      "~> 0.10.4"
    ]
  }
}
