{
  "name": "ZTSportKit",
  "version": "1.1.8",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "summary": "Official Zhortech ZTSportKit SDK for iOS to work with Sport products.",
  "homepage": "https://zhortech.com/",
  "social_media_url": "https://twitter.com/zhortech",
  "authors": {
    "Pavel Shpak": "p.shpak@zhortech.com"
  },
  "source": {
    "git": "https://github.com/zhortech/ztsportkit-ios-sdk.git",
    "tag": "1.1.8"
  },
  "platforms": {
    "ios": "13.0"
