{
  "name": "WordPressShared",
  "version": "1.8.15",
  "summary": "Shared components used in building the WordPress iOS apps and other library components.",
  "description": "Shared components used in building the WordPress iOS apps and other library components.\n\nThis is the first step required to build WordPress-iOS with UI components.",
  "homepage": "https://github.com/wordpress-mobile/WordPress-iOS-Shared",
  "license": "GPLv2",
  "authors": {
    "WordPress": "mobile@automattic.com"
  },
  "platforms": {
    "ios": "11.0"
  },
  "swift_versions": "5.0",
  "source": {
    "git": "https://github.com/wordpress-mobile/WordPress-iOS-Shared.git",
    "tag": "1.8.15"
  },
  "source_files": "WordPressShared/**/*.{h,m,swift}",
  "private_header_files": "WordPressShared/Private/*.h",
  "resource_bundles": {
    "WordPressShared": [
      "WordPressShared/Resources/*.{ttf,otf,json}"
    ]
  },
  "exclude_files": "WordPressShared/Exclude",
  "requires_arc": true,
  "header_dir": "WordPressShared",
  "dependencies": {
    "CocoaLumberjack": [
      "~> 3.4"
    ],
    "FormatterKit/TimeIntervalFormatter": [
      "1.8.2"
    ]
  },
  "swift_version": "5.0"
}
