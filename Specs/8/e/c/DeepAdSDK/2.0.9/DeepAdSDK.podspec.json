    "tag": "2.0.9"
  },
  "platforms": {
    "ios": "12.0"
  },
  "requires_arc": true,
  "static_framework": true,
  "user_target_xcconfig": {
    "OTHER_LDFLAGS": [
      "-ObjC"
    ]
  },
  "pod_target_xcconfig": {
    "VALID_ARCHS": "x86_64 armv7 arm64",
    "ENABLE_BITCODE": "NO"
  },
  "default_subspecs": "Core",
  "subspecs": [
    {
      "name": "Core",
      "vendored_frameworks": "Frameworks/DeepAdCore.xcframework",
      "resource_bundles": {
        "DeepAdSDK": [
          "Resources/**/*"
        ]
      },
      "frameworks": [
        "UIKit",
        "Foundation",
        "AdSupport"
      ]
    },
    {
