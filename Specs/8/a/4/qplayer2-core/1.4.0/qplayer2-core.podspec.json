{
  "name": "qplayer2-core",
  "version": "1.4.0",
  "summary": "Pili iOS video player SDK, RTMP, HLS video streaming supported.",
  "homepage": "https://github.com/pili-engineering/QPlayer2-IOS.git",
  "platforms": {
    "ios": "10.0"
  },
  "requires_arc": true,
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "license": "Apache License, Version 2.0",
  "authors": {
    "pili": "pili-coresdk@qiniu.com"
  },
  "source": {
    "http": "https://sdk-release.qnsdk.com/qplayer2-core-v1.4.0.zip"
  },
  "frameworks": [
    "UIKit",
    "Foundation",
    "AVFoundation",
    "AudioToolbox",
    "QuartzCore",
    "OpenGLES",
    "CoreVideo",
    "CoreMedia",
    "VideoToolbox"
  ],
  "libraries": [
    "c++",
    "bz2",
    "iconv",
    "z"
  ],
  "subspecs": [
    {
      "name": "iphoneos",
      "vendored_frameworks": [
        "Pod/Library/qplayer2_core.framework"
      ]
    }
  ]
}
