{
  "name": "Extra",
  "version": "1.2.1",
  "summary": "Swift library with usefull and lightfull extensions for your Cocoa Touch projects.",
  "homepage": "https://github.com/smartnsoft/Extra",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Smart&Soft": "contact@smartnsoft.com"
  },
  "source": {
    "git": "https://github.com/smartnsoft/Extra.git",
    "tag": "1.2.1"
  },
  "platforms": {
    "ios": "9.0",
    "tvos": "9.0"
  },
  "ios": {
    "frameworks": [
      "UIKit",
      "Foundation",
      "QuartzCore"
    ]
  },
  "default_subspecs": [
    "UIKit",
    "Foundation",
    "CoreAnimation"
  ],
  "subspecs": [
    {
      "name": "UIKit",
      "source_files": [
        "Extra/Classes/Extra/**/*.{swift}",
        "Extra/Classes/UIKit/**/*.{swift}"
      ]
    },
    {
      "name": "Foundation",
      "source_files": [
        "Extra/Classes/Extra/**/*.{swift}",
        "Extra/Classes/Foundation/**/*.{swift}"
      ]
    },
    {
      "name": "CoreAnimation",
      "source_files": [
        "Extra/Classes/Extra/**/*.{swift}",
        "Extra/Classes/CoreAnimation/**/*.{swift}"
      ]
    },
    {
      "name": "Realm",
      "source_files": [
        "Extra/Classes/Extra/**/*.{swift}",
        "Extra/Classes/Realm/**/*.{swift}"
      ],
      "dependencies": {
        "RealmSwift": [
          "~> 3.10"
        ]
      }
    }
  ]
}
