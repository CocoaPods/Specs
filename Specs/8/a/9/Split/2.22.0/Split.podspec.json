{
  "name": "Split",
  "module_name": "Split",
  "version": "2.22.0",
  "summary": "iOS SDK for Split",
  "description": "This SDK is designed to work with Split, the platform for controlled rollouts, serving features to your users via the Split feature flag to manage your complete customer experience.",
  "homepage": "http://www.split.io",
  "license": {
    "type": "Apache 2.0",
    "file": "LICENSE"
  },
  "authors": {
    "Patricio Echague": "pato@split.io",
    "Sebastian Arrubia": "sebastian@split.io",
    "Fernando Martin": "fernando@split.io"
  },
  "source": {
    "git": "https://github.com/splitio/ios-client.git",
    "tag": "2.22.0"
  },
  "platforms": {
    "ios": "9.0",
    "osx": "10.11",
    "watchos": "7.0",
    "tvos": "9.0"
  },
  "frameworks": "Foundation",
  "swift_versions": [
    "4.0",
    "4.2",
    "5.0",
    "5.1",
    "5.2",
    "5.3"
  ],
  "resources": "Split/Storage/split_cache.xcdatamodeld",
  "pod_target_xcconfig": {
    "HEADER_SEARCH_PATHS": "$(PROJECT_DIR)/Split/Common/Utils/JFBCrypt",
    "SWIFT_INCLUDE_PATHS": "${PROJECT_DIR}/Split/Common/Utils/JFBCrypt"
  },
  "source_files": [
    "Split/**/*.{swift}",
    "Split/Common/Utils/JFBCrypt/*.{h,m}"
  ],
  "exclude_files": "Split/Common/Utils/Lib/*",
  "swift_version": "5.3"
}
