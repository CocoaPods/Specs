{
  "name": "ZohoSignSDK",
  "version": "1.0-beta",
  "summary": "iOS SDK for securely signing documents using Zoho Sign.",
  "description": "ZohoSignSDK provides APIs for document signing, authentication, and secure workflows \nusing Zoho Sign from iOS applications.",
  "homepage": "https://www.zoho.com/sign/",
  "license": {
    "type": "Custom",
    "file": "LICENSE"
  },
  "authors": {
    "ME Mobile": "memobile@zohocorp.com"
  },
  "source": {
    "git": "https://github.com/zoho/ZohoSignSDK-iOS.git",
    "tag": "1.0-beta"
  },
  "platforms": {
    "ios": "15.5"
  },
  "ios": {
    "vendored_frameworks": "ZohoSignSDK/ZohoSignSDK.xcframework"
  },
  "static_framework": true,
  "requires_arc": true,
  "swift_versions": [
    "5.0",
    "5.1",
    "5.2",
    "5.3",
    "5.4",
    "5.5"
  ],
  "dependencies": {
    "AMPopTip": [
      "~> 4.12.0"
    ],
    "SSZipArchive": [
      "~> 2.5.0"
    ],
    "Alamofire": [
      "~> 5.10.0"
    ],
    "SVProgressHUD": [
      "~> 2.3.1"
    ],
    "SnapKit": [
      "~> 5.7.0"
    ]
  },
  "pod_target_xcconfig": {
    "BUILD_LIBRARY_FOR_DISTRIBUTION": "YES",
    "SWIFT_COMPILATION_MODE": "wholemodule",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": ""
  },
  "user_target_xcconfig": {
    "IPHONEOS_DEPLOYMENT_TARGET": "15.5"
  },
  "swift_version": "5.5"
}
