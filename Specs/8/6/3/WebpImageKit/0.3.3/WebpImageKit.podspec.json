    "Oleg Komaristov": "oleg.komaristov@gmail.com"
  },
  "source": {
    "git": "https://github.com/oleg-komaristov/WebpImageKit.git",
    "tag": "0.3.3"
  },
  "platforms": {
    "ios": "12.0"
  },
  "source_files": "WebpImageKit/**/*",
