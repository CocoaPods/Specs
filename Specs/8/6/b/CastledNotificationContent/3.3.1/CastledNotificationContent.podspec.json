{
  "name": "CastledNotificationContent",
  "version": "3.3.1",
  "summary": "A Notification Content Extension for displaying custom content in iOS push notifications.",
  "description": "The CastledNotificationContent framework provides a Notification Content Extension that allows you to create custom interfaces for displaying rich content in push notifications on iOS.",
  "homepage": "https://github.com/castledio/castled-ios-sdk",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Castled Data": "https://castled.io"
  },
  "platforms": {
    "ios": "13.0"
  },
  "swift_versions": "5.7",
  "weak_frameworks": [
    "UserNotifications",
    "UIKit"
  ],
  "ios": {
    "dependencies": {
      "SDWebImage": [
        "~> 5.11"
      ]
    }
  },
  "source": {
    "git": "https://github.com/castledio/castled-ios-sdk.git",
    "tag": "3.3.1"
  },
  "source_files": "Sources/CastledNotificationContent/**/*.{h,m,swift}",
  "resource_bundles": {
    "Castled": [
      "Sources/CastledNotificationContent/**/*.{xcassets,storyboard,xib}"
    ]
  },
  "readme": "README.md",
  "pod_target_xcconfig": {
    "DEFINES_MODULE": "YES"
  },
  "swift_version": "5.7"
}
