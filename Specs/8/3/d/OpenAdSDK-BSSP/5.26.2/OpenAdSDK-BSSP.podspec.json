{
  "name": "OpenAdSDK-BSSP",
  "version": "5.26.2",
  "summary": "OpenAd广告聚合 SDK",
  "description": "OpenAd广告SDK是一个专为移动应用开发者设计的广告聚合和收益优化平台。它能够帮助开发者通过多种广告形式提高应用内广告的填充率和eCPM（每千次展示成本），从而增加广告收入",
  "homepage": "https://github.com/konnitiha/OpenAdSDK.git",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "openad": "1053591151@qq.com"
  },
  "source": {
    "git": "https://github.com/konnitiha/OpenAdSDK.git",
    "tag": "5.26.2"
  },
  "platforms": {
    "ios": "12.0"
  },
  "requires_arc": true,
  "frameworks": [
    "Foundation",
    "UIKit",
    "MobileCoreServices",
    "Security",
    "SystemConfiguration",
    "CoreTelephony",
    "AdSupport",
    "WebKit",
    "Accelerate",
    "CoreMotion",
    "StoreKit",
    "AVFoundation",
    "MediaPlayer",
    "CoreMedia",
    "CoreLocation",
    "AVKit",
    "CoreGraphics",
    "CoreImage",
    "AudioToolbox"
  ],
  "user_target_xcconfig": {
    "OTHER_LDFLAGS": [
      "-lObjC"
    ],
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "libraries": [
    "z",
    "resolv.9",
    "sqlite3",
    "c++",
    "c++abi",
    "resolv"
  ],
  "pod_target_xcconfig": {
    "VALID_ARCHS": "x86_64 arm64"
  },
  "default_subspecs": "OpenAdSDK",
  "subspecs": [
    {
      "name": "OpenAdSDK",
      "platforms": {
        "ios": "12.0"
      },
      "vendored_frameworks": [
        "OpenAdSDK/OpenAdSDK.framework",
        "OpenAdSDK/OpenAdZJAdapter.framework"
      ],
      "resources": [
        "OpenAdSDK/OpenAdSDK.bundle"
      ],
      "dependencies": {
        "ZJSDK/ZJSDKModuleDSP": [
          "2.5.9.17"
        ]
      }
    }
  ]
}
