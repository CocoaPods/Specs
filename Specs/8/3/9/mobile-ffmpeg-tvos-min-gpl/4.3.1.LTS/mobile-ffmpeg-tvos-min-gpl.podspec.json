{
  "name": "mobile-ffmpeg-tvos-min-gpl",
  "version": "4.3.1.LTS",
  "summary": "Mobile FFmpeg tvOS Min GPL Static Framework",
  "description": "Includes FFmpeg v4.3-dev-1944 with libvid.stab v1.1.0, x264 v20191125-stable, x265 v3.2.1 and xvidcore v1.3.7 libraries enabled.",
  "homepage": "https://github.com/tanersener/mobile-ffmpeg",
  "authors": {
    "Taner Sener": "tanersener@gmail.com"
  },
  "license": {
    "type": "GPL-3.0",
    "file": "mobileffmpeg.framework/LICENSE"
  },
  "platforms": {
    "tvos": "9.2"
  },
  "requires_arc": true,
  "libraries": [
    "z",
    "bz2",
    "c++",
    "iconv"
  ],
  "source": {
    "http": "https://github.com/tanersener/mobile-ffmpeg/releases/download/v4.3.1.LTS/mobile-ffmpeg-min-gpl-4.3.1.LTS-tvos-framework.zip"
