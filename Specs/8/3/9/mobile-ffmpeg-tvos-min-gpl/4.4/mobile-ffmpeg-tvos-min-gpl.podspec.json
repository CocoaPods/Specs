{
  "name": "mobile-ffmpeg-tvos-min-gpl",
  "version": "4.4",
  "summary": "Mobile FFmpeg tvOS Min GPL Static Framework",
  "description": "Includes FFmpeg v4.4-dev-416 with libvid.stab v1.1.0, x264 v20200630-stable, x265 v3.4 and xvidcore v1.3.7 libraries enabled.",
  "homepage": "https://github.com/tanersener/mobile-ffmpeg",
  "authors": {
    "Taner Sener": "tanersener@gmail.com"
  },
  "license": {
    "type": "GPL-3.0",
    "file": "mobileffmpeg.framework/LICENSE"
  },
  "platforms": {
    "tvos": "9.2"
  },
  "requires_arc": true,
  "libraries": [
    "z",
    "bz2",
    "c++",
    "iconv"
  ],
  "source": {
    "http": "https://github.com/tanersener/mobile-ffmpeg/releases/download/v4.4/mobile-ffmpeg-min-gpl-4.4-tvos-framework.zip"
  },
  "tvos": {
    "frameworks": [
      "AudioToolbox",
      "VideoToolbox",
      "CoreMedia"
    ],
    "vendored_frameworks": [
      "mobileffmpeg.framework",
      "libavcodec.framework",
      "libavdevice.framework",
      "libavfilter.framework",
      "libavformat.framework",
      "libavutil.framework",
      "libswresample.framework",
      "libswscale.framework",
      "libvidstab.framework",
      "x264.framework",
      "x265.framework",
      "xvidcore.framework"
    ]
  }
}
