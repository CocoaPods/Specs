{
  "name": "linkus-vivo",
  "version": "0.0.16",
  "summary": "为V消息提供Sip通话能力",
  "homepage": "https://github.com/Yeastar-PBX/linkus-ios-sdk-vivo.git",
  "license": {
    "type": "MIT"
  },
  "authors": {
    "杨桂福": "ygf@yeastar.com"
  },
  "source": {
    "git": "https://github.com/Yeastar-PBX/linkus-ios-sdk-vivo.git",
    "tag": "0.0.16"
  },
  "requires_arc": true,
  "platforms": {
    "ios": "11.0",
    "osx": "10.13"
  },
  "ios": {
    "pod_target_xcconfig": {
      "VALID_ARCHS[sdk=iphonesimulator*]": ""
    },
    "vendored_frameworks": "Framework/iOS/linkus_vivo.framework"
  },
  "osx": {
    "vendored_frameworks": "Framework/MacOS/linkus_vivo.framework"
  },
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "$(SDKROOT)/usr/include/libc++"
  },
  "dependencies": {
    "OpenSSL-Universal": [
      "~> 1.0.2.20"
    ]
  }
}
