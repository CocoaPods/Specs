  "watchos": {
    "source_files": "Sources/Amplitude/**/*.{h,swift}",
    "resource_bundles": {
      "Amplitude": [
        "Sources/Amplitude/PrivacyInfo.xcprivacy"
      ]
    }
  },
  "visionos": {
    "source_files": "Sources/Amplitude/**/*.{h,swift}",
