{
  "name": "NIMKitExtension",
  "version": "0.2.0",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "summary": "NIMKitExtension is a collection of extensions for the NIMKit (Netease IM UI Kit).",
  "homepage": "https://github.com/ElfSundae/NIMKitExtension",
  "authors": {
    "Elf Sundae": "https://0x123.com"
  },
  "source": {
    "git": "https://github.com/ElfSundae/NIMKitExtension.git",
    "tag": "0.2.0"
  },
  "platforms": {
    "ios": "9.0"
  },
  "default_subspecs": "Lite",
  "source_files": "NIMKitExtension/**/*.{h,m}",
  "subspecs": [
    {
      "name": "Lite",
      "dependencies": {
        "NIMKit/Lite": [
          "~> 2.11"
        ]
      }
    },
    {
      "name": "Full",
      "dependencies": {
        "NIMKit/Full": [
          "~> 2.11"
        ]
      }
    }
  ]
}
