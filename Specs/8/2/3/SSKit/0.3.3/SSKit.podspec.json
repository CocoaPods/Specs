{
  "name": "SSKit",
  "version": "0.3.3",
  "summary": "iOS响应式开发框架",
  "description": "以‘ReactiveCocoa’为基础搭建的简单易懂的iOS响应式开发框架",
  "homepage": "https://github.com/bassamyan/SSKit",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Quincy Yan": "yanyibin.nz@gmail.com"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source": {
    "git": "https://github.com/bassamyan/SSKit.git",
    "tag": "0.3.3"
  },
  "resources": "SSKit/SSResource/**/*",
  "requires_arc": true,
  "dependencies": {
    "ReactiveCocoa": [
      "~> 2.5"
    ],
    "Masonry": [
      "~> 1.0.1"
    ],
    "SDWebImage": [
      "~> 3.8.2"
    ],
    "AFNetworking": [
      "~> 3.1.0"
    ],
    "KMNavigationBarTransition": [
      "~> 0.0.10"
    ],
    "TPKeyboardAvoiding": [
      "~> 1.3"
    ]
  },
  "subspecs": [
    {
      "name": "Configure",
      "source_files": "SSKit/SSConfigure/**/*.{h,m}"
    },
    {
      "name": "Category",
      "subspecs": [
        {
          "name": "Foundation",
          "source_files": "SSKit/SSCategory/Foundation/*.{h,m}"
        },
        {
          "name": "UIKit",
          "source_files": "SSKit/SSCategory/UIKit/*.{h,m}",
          "dependencies": {
            "SSKit/Category/Foundation": [

            ]
          }
        }
      ]
    },
    {
      "name": "CustomUI",
      "dependencies": {
        "SSKit/Category": [

        ]
      },
      "subspecs": [
        {
          "name": "SSAlertView",
          "source_files": "SSKit/SSCustomUI/SSAlertView/*.{h,m}"
        },
        {
          "name": "SSDialog",
          "source_files": "SSKit/SSCustomUI/SSDialog/*.{h,m}"
        },
        {
          "name": "SSPageControl",
          "source_files": "SSKit/SSCustomUI/SSPageControl/*.{h,m}"
        },
        {
          "name": "SSProgressIndicator",
          "source_files": "SSKit/SSCustomUI/SSProgressIndicator/*.{h,m}"
        },
        {
          "name": "SSPullRefresh",
          "source_files": "SSKit/SSCustomUI/SSPullRefresh/*.{h,m}"
        },
        {
          "name": "SSStatus",
          "source_files": "SSKit/SSCustomUI/SSStatus/*.{h,m}"
        },
        {
          "name": "SSTabbar",
          "source_files": "SSKit/SSCustomUI/SSTabbar/*.{h,m}"
        }
      ]
    },
    {
      "name": "Handler",
      "source_files": "SSKit/SSHandler/**/*.{h,m}",
      "dependencies": {
        "SSKit/CustomUI": [

        ]
      }
    },
    {
      "name": "Inherit",
      "source_files": "SSKit/SSInherit/**/*.{h,m}"
    },
    {
      "name": "Service",
      "source_files": "SSKit/SSService/**/*.{h,m}",
      "dependencies": {
        "SSKit/Handler": [

        ]
      }
    },
    {
      "name": "MVVMBase",
      "source_files": "SSKit/SSMVVM/**/*.{h,m}",
      "dependencies": {
        "SSKit/CustomUI": [

        ],
        "SSKit/Category": [

        ],
        "SSKit/Configure": [

        ]
      }
    },
    {
      "name": "Headers",
      "source_files": "SSKit/SSHeaders/*.h",
      "dependencies": {
        "SSKit/CustomUI": [

        ],
        "SSKit/Category": [

        ],
        "SSKit/Inherit": [

        ],
        "SSKit/MVVMBase": [

        ]
      }
    }
  ]
}
