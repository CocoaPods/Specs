{
  "name": "TruoraValidationsSDK",
  "version": "0.0.4-alpha.3",
  "summary": "SDK of biometric validations",
  "description": "TruoraValidationsSDK provides a complete biometric validation flow -> enrollment,\nbase image upload, status check, passive capture in face, get input from ID document.",
  "homepage": "https://bitbucket.org/truora/truora-sdks",
  "license": {
    "type": "Proprietary",
    "text": "Copyright Truora. All rights reserved."
  },
  "authors": {
    "Truora": "truora-apps@truora.com"
  },
  "platforms": {
    "ios": "13.0"
  },
  "swift_versions": "5.9",
  "static_framework": true,
  "source": {
    "git": "https://bitbucket.org/truora/truora-sdks.git",
    "tag": "0.0.4-alpha.3"
  },
  "source_files": [
    "ios/validations/TruoraValidationsSDK/Sources/**/*.{swift}",
    "ios/validations/Derived/Sources/TuistAssets+TruoraValidationsSDK.swift",
    "ios/validations/Derived/Sources/TuistBundle+TruoraValidationsSDK.swift",
    "ios/validations/Derived/Sources/TuistStrings+TruoraValidationsSDK.swift"
