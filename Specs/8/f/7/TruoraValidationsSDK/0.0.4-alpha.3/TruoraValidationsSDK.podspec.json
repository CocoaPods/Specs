  "resource_bundles": {
    "validations_TruoraValidationsSDK": [
      "ios/validations/TruoraValidationsSDK/Resources/**/*.strings",
      "ios/validations/TruoraValidationsSDK/Resources/Assets.xcassets"
    ]
  },
  "frameworks": [
    "UIKit",
    "Foundation",
    "SwiftUI"
  ],
  "dependencies": {
    "TruoraCamera": [
      "0.0.4-alpha.3"
    ]
  },
  "pod_target_xcconfig": {
    "DEFINES_MODULE": "YES",
    "SWIFT_VERSION": "5.9",
    "SWIFT_ACTIVE_COMPILATION_CONDITIONS": "COCOAPODS"
  },
  "swift_version": "5.9"
}
