{
  "name": "advista",
  "version": "1.1.1",
  "summary": "Advista iOS SDK - 广告SDK",
  "description": "Advista iOS SDK 是一个功能完整的广告SDK，支持开屏广告、插屏广告、信息流广告等多种广告形式。",
  "homepage": "https://github.com/AdVista-iOS/advista-ios-sdk",
  "license": {
    "type": "MIT"
  },
  "authors": {
    "Eric": "819473904@qq.com"
  },
  "source": {
    "http": "https://github.com/AdVista-iOS/advista-ios-sdk/releases/download/1.1.1/advista.framework.zip",
    "type": "zip"
  },
  "platforms": {
    "ios": "12.0"
  },
  "requires_arc": true,
  "vendored_frameworks": "advista.framework",
  "dependencies": {
    "OpenSSL-Universal": [
      "~> 1.1"
    ]
  },
  "frameworks": [
    "UIKit",
    "Foundation",
    "CoreMotion",
    "AVFoundation",
    "WebKit",
    "SafariServices",
    "CoreTelephony",
    "SystemConfiguration",
    "CoreLocation",
    "AdSupport",
    "AppTrackingTransparency"
  ],
  "pod_target_xcconfig": {
    "ONLY_ACTIVE_ARCH": "NO",
    "VALID_ARCHS": "arm64",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64 x86_64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64 x86_64"
  }
}
