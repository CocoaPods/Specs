{
  "name": "DotstudioPRO",
  "version": "0.0.1",
  "summary": "DotstudioPRO ios & tvos library to make apps created on DotstudioPRO platform.",
  "description": "DotstudioPRO ios & tvos library to make apps created on DotstudioPRO platform. This repository makes you to use Dotstudio APIs.",
  "homepage": "https://github.com/dotstudiopro/DotstudioPRO.swift",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Ketan Sakariya": "ketan@dotstudiopro.com"
  },
  "platforms": {
    "ios": "9.2",
    "tvos": "9.2"
  },
  "source": {
    "git": "https://github.com/dotstudiopro/DotstudioPRO.swift.git",
    "tag": "0.0.1"
  },
  "source_files": [
    "source/common/**/*",
    "source/common/**/**/*",
    "source/ui/**/**/**/*"
  ],
  "ios": {
    "source_files": [
      "source/ios/**/*",
      "source/ios/**/**/*",
      "source/ios/**/**/**/*",
      "source/ios/**/**/**/**/*"
    ],
    "dependencies": {
      "Lock": [
        "~> 2.10"
      ],
      "GoogleAds-IMA-iOS-SDK": [
        ">=3.3"
      ],
      "google-cast-sdk": [
        "< 5.0",
        ">=4.3.4"
      ]
    }
  },
  "tvos": {
    "source_files": [
      "source/tvos/**/*",
      "source/tvos/**/**/*",
      "source/tvos/**/**/**/*",
      "source/tvos/**/**/**/**/*"
    ],
    "dependencies": {
      "Auth0": [
        "~> 1.15"
      ]
    }
  },
  "exclude_files": "Classes/Exclude",
  "static_framework": true,
  "swift_version": "5.0",
  "dependencies": {
    "Alamofire": [

    ],
    "AlamofireImage": [
      "~> 3.5"
    ]
  }
}
