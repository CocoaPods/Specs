{
  "name": "ZDLibffi",
  "version": "0.343.0",
  "summary": "Libffi source code integrate",
  "description": "libffi v3.4.3 integrate to iOS && macOS && watchos && tvos.",
  "homepage": "https://github.com/libffi/libffi",
  "license": "MIT",
  "authors": {
    "atgreen": "green@moxielogic.com",
    "faimin": "fuxianchao@gmail.com"
  },
  "source": {
    "git": "https://github.com/faimin/ZDLibffi.git",
    "tag": "0.343.0"
  },
  "prefix_header_file": false,
  "platforms": {
    "ios": "10.0",
    "osx": "10.12",
    "watchos": "3.0",
    "tvos": "10.0"
  },
  "module_name": "ZDLibffi",
  "pod_target_xcconfig": {
    "DEFINES_MODULE": "YES",
    "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited) USE_DL_PREFIX=1 HAVE_MORECORE=0"
