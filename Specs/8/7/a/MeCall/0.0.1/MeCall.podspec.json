{
  "name": "MeCall",
  "version": "0.0.1",
  "homepage": "https://bitbucket.org/u2systems-ios/mecall",
  "summary": "A VoIP Framework for iOS App",
  "description": "MeCall is a VoIP framework that can be integrate into any iOS app, extend with voice calling feature",
  "license": "GPL",
  "authors": {
    "Yep Mun Chun": "munchun.yep@u2systems.com"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source": {
    "git": "https://bitbucket.org/u2systems-ios/mecall.git",
    "tag": "0.0.1"
  },
  "source_files": "MeCall/**/*.{h,m}",
  "public_header_files": "MeCall/*.h",
  "private_header_files": "MeCall/liblinphone-sdk/apple-darwin/include/**/*.h",
  "vendored_libraries": "MeCall/liblinphone-sdk/apple-darwin/lib/**/*.a",
  "pod_target_xcconfig": {
    "HEADER_SEARCH_PATHS": "${PODS_ROOT}/MeCall/MeCall/liblinphone-sdk/apple-darwin/include"
