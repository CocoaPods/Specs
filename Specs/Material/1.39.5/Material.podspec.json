{
  "name": "Material",
  "version": "1.39.5",
  "license": "BSD",
  "summary": "Express your creativity with Material, an animation and graphics framework for Google's Material Design and Apple's Flat UI in Swift.",
  "homepage": "http://cosmicmind.io",
  "social_media_url": "https://www.facebook.com/graphkit",
  "authors": {
    "CosmicMind, Inc.": "support@cosmicmind.io"
  },
  "source": {
    "git": "https://github.com/CosmicMind/Material.git",
    "tag": "1.39.5"
  },
  "platforms": {
    "ios": "8.0",
    "osx": "10.9"
  },
  "ios": {
    "source_files": "Sources/iOS/**/*.swift"
  },
  "osx": {
    "source_files": "Sources/OSX/**/*.swift"
  },
  "requires_arc": true,
  "resource_bundles": {
    "io.cosmicmind.material.fonts": [
      "Sources/**/*.ttf"
    ],
    "io.cosmicmind.material.icons": [
      "Sources/Assets.xcassets/**/*.png"
    ]
  }
