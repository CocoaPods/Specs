  "homepage": "https://github.com/AlbertMontserrat/UIImageViewSoftFrameAnimations",
  "license": "MIT",
  "authors": {
    "Albert Montserrat": "albert.montserrat.gambus@gmail.com"
  },
  "source": {
    "git": "https://github.com/AlbertMontserrat/UIImageViewSoftFrameAnimations.git",
    "tag": "1.0"
  },
  "platforms": {
    "ios": "6.0"
  },
  "requires_arc": true,
