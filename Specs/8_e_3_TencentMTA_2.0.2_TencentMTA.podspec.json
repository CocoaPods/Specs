{
  "name": "TencentMTA",
  "version": "2.0.2",
  "summary": "Tencent MTA SDK for iOS to access Tencent MTA platform.",
  "description": "Tencent does not have an official pod for their MTA SDK. Therefore, we create this pod to track the official MTA iOS SDK.",
  "homepage": "http://mta.qq.com/mta/",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "iCell": "xiaoyu.li@shanbay.com"
  },
  "source": {
    "git": "https://github.com/iCell/BayMTASDK.git",
    "tag": "2.0.2"
  },
  "platforms": {
    "ios": "7.0"
  },
  "source_files": "TencentMTA/*.{h,m}",
  "vendored_libraries": "TencentMTA/*.a",
  "frameworks": [
    "CoreTelephony",
    "SystemConfiguration",
    "Security",
    "CFNetwork"
  ],
  "libraries": [
    "z",
    "sqlite3"
  ],
  "pod_target_xcconfig": {
    "LIBRARY_SEARCH_PATHS": "${PODS_ROOT}/TencentMTA/TencentMTA",
    "OTHER_LDFLAGS": "-lTencentMTA"
  }
}
