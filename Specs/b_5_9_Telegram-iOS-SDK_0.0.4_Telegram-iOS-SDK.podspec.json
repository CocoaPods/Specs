{
  "name": "Telegram-iOS-SDK",
  "version": "0.0.4",
  "summary": "Telegram-iOS-SDK. Source code of Telegram 2.8 ported to pods",
  "description": "                       arm64 compatible. Work in progress.\n                       Feel free to contribute.\n",
  "homepage": "https://github.com/batkov/Telegram-iOS-SDK",
  "license": "GPLv2",
  "authors": {
    "Hariton Batkov": "batkov@i.ua"
  },
  "source": {
    "git": "https://github.com/batkov/Telegram-iOS-SDK.git",
    "tag": "0.0.4"
  },
  "social_media_url": "https://twitter.com/batkov111",
  "platforms": {
    "ios": "6.0"
  },
  "requires_arc": "Pod/Classes/ARC/**/*",
  "xcconfig": {
    "OTHER_LDFLAGS": "-lstdc++ -ObjC"
  },
  "source_files": "Pod/Classes/**/*.{h,m,c,cpp,mm}",
  "compiler_flags": "-Wall",
  "resource_bundles": {
    "Telegram-iOS-SDK": [
      "Pod/Assets/*"
    ]
  },
  "frameworks": [
    "AddressBook",
    "AddressBookUI",
    "CoreLocation"
  ],
  "dependencies": {
    "BLPhoneFormat": [

    ],
    "BLContactsViewController": [

    ],
    "pop": [

    ]
  },
  "ios": {
    "vendored_libraries": "Pod/Libs/**"
  },
  "libraries": [
    "z",
    "crypto"
  ]
}
