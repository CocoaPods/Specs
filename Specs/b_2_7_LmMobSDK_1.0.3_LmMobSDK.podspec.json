{
  "name": "LmMobSDK",
  "version": "1.0.3",
  "summary": "蓝莓互娱 提供的iOS端的SDK.",
  "description": "帮助开发者在开发项目时可以通过有效的第三方来更好更快的完善自己的激励视频，实现游戏变现，增加开发者收益.",
  "homepage": "http://www.lmjoy.cn/",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "lanmhy": "lmios@lmjoy.cn"
  },
  "platforms": {
    "ios": "8.0"
  },
  "xcconfig": {
    "OTHER_LDFLAGS": "-ObjC"
  },
  "vendored_frameworks": [
    "LmMobSDK/frameworks/VungleSDK.framework",
    "LmMobSDK/frameworks/AdColony.framework",
    "LmMobSDK/frameworks/Centrixlink.framework",
    "LmMobSDK/frameworks/Chartboost.framework",
    "LmMobSDK/frameworks/MVSDK.framework",
    "LmMobSDK/frameworks/MVSDKReward.framework",
    "LmMobSDK/frameworks/UnityAds.framework"
  ],
  "vendored_libraries": "LmMobSDK/libLmMobSDK.a",
  "frameworks": [
    "Accelerate",
    "AdSupport",
    "AudioToolbox",
    "AVFoundation",
    "CFNetwork",
    "CoreGraphics",
    "CoreLocation",
    "CoreTelephony",
    "CoreMedia",
    "CoreMotion",
    "EventKit",
    "GLKit",
    "iAd",
    "ImageIO",
    "MediaPlayer",
    "MessageUI",
    "MobileCoreServices",
    "QuartzCore",
    "Security",
    "Social",
    "StoreKit",
    "SystemConfiguration",
    "WatchConnectivity",
    "JavaScriptCore"
  ],
  "weak_frameworks": [
    "WebKit",
    "UIKit",
    "Foundation"
  ],
  "libraries": [
    "c++",
    "sqlite3",
    "xml2",
    "z"
  ],
  "source": {
    "git": "https://github.com/lanmhy/LmMobSDK-iOS.git",
    "tag": "1.0.3"
  },
  "source_files": [
    "LmMobSDK",
    "LmMobSDK/**/*.{h,m}"
  ],
  "resources": [
    "LmMobSDK/Assets/*.png",
    "LmMobSDK/Assets/*.bundle"
  ],
  "public_header_files": "LmMobSDK/LmMobSDK.h",
  "requires_arc": true
}
