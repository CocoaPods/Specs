{
  "name": "ObjectiveDropboxOfficial",
  "version": "1.0.0",
  "summary": "Dropbox Objective C SDK for APIv2",
  "homepage": "https://dropbox.com/developers/",
  "license": "MIT",
  "authors": {
    "Stephen Cobbe": "scobbe@dropbox.com"
  },
  "source": {
    "git": "https://github.com/dropbox/dropbox-sdk-obj-c.git",
    "tag": "1.0.0"
  },
  "osx": {
    "source_files": [
      "Source/PlatformNeutral/**/*.{h,m}",
      "Source/PlatformDependent/OSX/*.{h,m}"
    ],
    "frameworks": [
      "AppKit",
      "Foundation"
    ]
  },
  "ios": {
    "source_files": [
      "Source/PlatformNeutral/**/*.{h,m}",
      "Source/PlatformDependent/iOS/*.{h,m}"
    ],
    "frameworks": [
      "UIKit",
      "Webkit",
      "Foundation"
    ]
  },
  "requires_arc": true,
  "platforms": {
    "osx": "10.10",
    "ios": "8.0"
  },
  "public_header_files": "Source/**/*.h"
}
