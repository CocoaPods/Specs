{
  "name": "FavIcon",
  "version": "3.0.4",
  "summary": "A library for downloading website icons",
  "homepage": "https://github.com/leonbreedt/FavIcon",
  "license": {
    "type": "Apache",
    "file": "LICENSE"
  },
  "authors": {
    "Leon Breedt": "leon@sector42.io"
  },
  "source": {
    "git": "https://github.com/leonbreedt/FavIcon.git",
    "tag": "3.0.4"
  },
  "source_files": "Sources/**/*.swift",
  "preserve_paths": [
    "Sources/Modules/*",
    "Support/*.sh"
  ],
  "platforms": {
    "ios": "9.0",
    "osx": "10.10",
    "tvos": "9.0",
    "watchos": "2.0"
  },
  "requires_arc": true,
  "pod_target_xcconfig": {
    "SWIFT_VERSION": "4.0",
    "SWIFT_WHOLE_MODULE_OPTIMIZATION": "YES"
  },
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "/usr/include/libxml2",
    "SWIFT_INCLUDE_PATHS": "$(PODS_TARGET_SRCROOT)/Sources/Modules"
  }
}
