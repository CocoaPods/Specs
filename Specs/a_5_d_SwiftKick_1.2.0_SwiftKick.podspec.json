{
  "name": "SwiftKick",
  "version": "1.2.0",
  "license": {
    "type": "MIT"
  },
  "summary": "Some classes and functions for kickstarting your Swift development project",
  "homepage": "https://github.com/dudash/swiftkick",
  "social_media_url": "http://about.me/jasondudash",
  "authors": {
    "Jason Dudash": "jason.dudash@gmail.com"
  },
  "platforms": {
    "ios": "9.3",
    "tvos": "10.2"
  },
  "requires_arc": true,
  "source": {
    "git": "https://github.com/dudash/swiftkick.git",
    "tag": "1.2.0"
  },
  "source_files": "Sources/*.{h,swift}",
  "default_subspecs": "All",
  "pod_target_xcconfig": {
    "SWIFT_VERSION": "3.1"
  },
  "pushed_with_swift_version": "3.0",
  "subspecs": [
    {
      "name": "All",
      "source_files": "Sources/**/*.swift"
    },
    {
      "name": "Collections",
      "source_files": "Sources/Collections/*.swift"
    },
    {
      "name": "Extensions",
      "source_files": [
        "Sources/Extensions/*.swift",
        "Sources/Extensions/**/*.swift"
      ]
    },
    {
      "name": "NumberExtensions",
      "source_files": "Sources/Extensions/NumberTypes/*.swift"
    },
    {
      "name": "StringExtensions",
      "source_files": "Sources/Extensions/String/*.swift"
    },
    {
      "name": "UIExtensions",
      "source_files": "Sources/Extensions/UITypes/*.swift"
    },
    {
      "name": "PreferencesExtensions",
      "source_files": "Sources/Extensions/FoundationPreferences/*.swift"
    },
    {
      "name": "Logger",
      "source_files": "Sources/Logger/*.swift"
    }
  ]
}
