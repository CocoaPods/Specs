{
  "name": "OpenXMSDK",
  "platforms": {
    "ios": "5.0"
  },
  "version": "2.0",
  "summary": "OpenX SDK for iOS v2.0",
  "description": "The OpenX SDK for iOS allows a user of the OpenX Ad Server to incorporate ads into their iOS applications.",
  "homepage": "http://www.openx.com",
  "authors": "OpenX Technologies, Inc. | maintained by @lawrenceleach",
  "license": {
    "type": "commercial",
    "text": "Copyright 2013 OpenX Technologies, Inc. All rights reserved."
  },
  "source": {
    "http": "http://i.cdn.openx.com/sdks/OpenX_Mobile_SDK_iOS.zip"
  },
  "source_files": "SDK/OpenXMSDK.framework/Versions/A/Headers/*.{h}",
  "resources": "SDK/OpenXMSDK.framework/Versions/A/Resources/*.{png,html,js}",
  "preserve_paths": "SDK/OpenXMSDK.framework",
  "header_dir": "OpenXMSDK",
  "requires_arc": false,
  "ios": {
    "vendored_frameworks": "OpenXMSDK.framework"
  },
  "frameworks": [
    "SystemConfiguration",
    "AdSupport",
    "CoreGraphics",
    "CoreLocation",
    "CoreTelephony",
    "EventKit",
    "EventKitUI",
    "MapKit",
    "MediaPlayer"
  ],
  "xcconfig": {
    "FRAMEWORK_SEARCH_PATHS": "\"$(PODS_ROOT)/OpenXMSDK\""
  }
}
