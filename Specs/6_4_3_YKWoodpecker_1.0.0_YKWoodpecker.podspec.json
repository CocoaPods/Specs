{
  "name": "YKWoodpecker",
  "license": "MIT",
  "version": "1.0.0",
  "summary": "An in-app-debug tool for iOS.",
  "description": "This library provides a powerful collection of iOS develop tools, such as view picker, method listen-in, po command and so on.",
  "homepage": "https://github.com/alibaba/youku-sdk-tool-woodpecker.git",
  "authors": {
    "Zim": "dingzm77@163.com"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source": {
    "git": "https://github.com/alibaba/youku-sdk-tool-woodpecker.git",
    "tag": "1.0.0"
  },
  "source_files": "YKWoodpecker/**/*.{h,m}",
  "prefix_header_file": "YKWoodpecker/PrefixHeader.pch",
  "public_header_files": [
    "YKWoodpecker/YKWoodpecker.h",
    "YKWoodpecker/Base/**/*.h",
    "YKWoodpecker/Common/**/*.h",
    "YKWoodpecker/Plugins/ProbeRulerPlugin/**/*.h",
    "YKWoodpecker/Plugins/CommandPlugin/**/*.h"
  ],
  "resources": "YKWoodpecker/**/*.{png,plist,xcassets,json}",
  "frameworks": [
    "UIKit",
    "Foundation"
  ],
  "requires_arc": true,
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "$(PODS_ROOT)",
    "ENABLE_STRICT_OBJC_MSGSEND": "NO"
  }
}
