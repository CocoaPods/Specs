{
  "name": "SQLCipher",
  "version": "2.1.1",
  "license": "BSD",
  "summary": "Full Database Encryption for SQLite.",
  "description": "SQLCipher is an open source extension to SQLite that provides transparent 256-bit AES encryption of database files.",
  "homepage": "http://sqlcipher.net",
  "authors": "Zetetic LLC",
  "source": {
    "git": "https://github.com/guilhermeandrade/sqlcipher.git",
    "tag": "2.1.1"
  },
  "preserve_paths": "*.a",
  "libraries": [
    "crypto",
    "sqlcipher"
  ],
  "xcconfig": {
    "OTHER_CFLAGS": "$(inherited) -DSQLITE_HAS_CODEC",
    "LIBRARY_SEARCH_PATHS": "$(inherited) \"$(PODS_ROOT)/SQLCipher\""
  },
  "platforms": {
    "ios": null
  },
  "requires_arc": false
}
