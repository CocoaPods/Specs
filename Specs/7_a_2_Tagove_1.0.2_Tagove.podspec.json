  "xcconfig": {
    "FRAMEWORK_SEARCH_PATHS": "\"$(PODS_ROOT)/Tagove/tagove-sdk-ios-v1.0\"",
    "LD_RUNPATH_SEARCH_PATHS": "@loader_path/../Frameworks",
    "ENABLE_BITCODE": "NO",
    "EMBEDDED_CONTENT_CONTAINS_SWIFT": "YES",
    "ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES": "YES"
