{
  "name": "RamblerSegues",
  "version": "1.1.0",
  "summary": "Helpful Segues",
  "description": "Helpful Segues\n\n- Cross storyboard segues with placeholder view controller\n- Non-automatic embed view controller segue<<-DESC",
  "homepage": "https://github.com/rambler-ios/RamblerSegues.git",
  "license": "MIT",
  "authors": {
    "Andrey Zarembo-Godzyatskiy": "andrey.zarembo@gmail.com"
  },
  "source": {
    "git": "https://github.com/rambler-ios/RamblerSegues.git",
    "tag": "1.1.0"
  },
  "header_mappings_dir": "Pod/Classes",
  "platforms": {
    "ios": "7.0"
  },
  "requires_arc": true,
  "source_files": "Pod/Classes/**/*",
  "subspecs": [
    {
      "name": "Core",
      "source_files": "Pod/Classes/RamblerSegues.h"
    },
    {
      "name": "CrossStoryboardSegue",
      "source_files": [
        "Pod/Classes/CrossStoryboardSegue/CrossStoryboardSegue.h",
        "Pod/Classes/CrossStoryboardSegue/**/*.{h,m}"
      ]
    },
    {
      "name": "EmbedSegue",
      "source_files": [
        "Pod/Classes/EmbedSegue/EmbedSegue.h",
        "Pod/Classes/EmbedSegue/**/*.{h,m}"
      ]
    }
  ]
}
