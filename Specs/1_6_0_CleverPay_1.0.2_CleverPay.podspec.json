{
  "name": "CleverPay",
  "version": "1.0.2",
  "summary": "CleverPay",
  "homepage": "https://github.com/cleverpumpkin/cleverpay-ios-sdk",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": "Cleverpumpkin, Ltd.",
  "source": {
    "git": "https://github.com/cleverpumpkin/cleverpay-ios-sdk.git",
    "tag": "1.0.2"
  },
  "source_files": "CleverPay/**/*.{h,m,swift}",
  "resource_bundles": {
    "CleverPay": [
      "CleverPay/Resources/*.cer",
      "CleverPay/Resources/*.xcdatamodeld"
    ]
  },
  "swift_version": "4.1",
  "platforms": {
    "ios": "10.3"
  },
  "pod_target_xcconfig": {
    "_XC_SLASH": "/",
    "_XC_DBL_SLASH": "${_XC_SLASH}${_XC_SLASH}",
    "VERSION_INFO_EXPORT_DECL": "${_XC_DBL_SLASH}",
    "DEFINES_MODULE": "YES",
    "GCC_PREPROCESSOR_DEFINITIONS": "PRODUCT_VERSION_NUMBER=1.0 PRODUCT_VERSION_STRING=\\\"1.0.2\\\""
  },
  "dependencies": {
    "Moya": [
      "~> 11.0"
    ],
    "Valet": [
      "~> 3.1.3"
    ]
  }
}
