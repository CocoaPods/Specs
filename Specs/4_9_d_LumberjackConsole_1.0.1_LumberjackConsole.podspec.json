{
  "name": "LumberjackConsole",
  "version": "1.0.1",
  "summary": "On-device CocoaLumberjack console with support for search, filters and more.",
  "homepage": "http://ptez.github.io/LumberjackConsole",
  "license": {
    "type": "Apache License, Version 2.0",
    "file": "LICENSE"
  },
  "authors": {
    "Ernesto Rivera": "rivera.ernesto@gmail.com"
  },
  "screenshots": [
    "http://ptez.github.io/LumberjackConsole/images/screenshot1.png",
    "http://ptez.github.io/LumberjackConsole/images/screenshot2.png",
    "http://ptez.github.io/LumberjackConsole/images/screenshot3.png"
  ],
  "source": {
    "git": "https://github.com/PTEz/LumberjackConsole.git",
    "tag": "1.0.1"
  },
  "platforms": {
    "ios": "5.0"
  },
  "requires_arc": true,
  "preserve_paths": [
    "README.md",
    "NOTICE"
  ],
  "dependencies": {
    "NBUCore": [
      ">= 2.0.0"
    ],
    "CocoaLumberjack": [
      ">= 1.6.4"
    ]
  },
  "subspecs": [
    {
      "name": "Base",
      "source_files": "Source/*.{h,m}",
      "resources": "Source/*.{xib}"
    },
    {
      "name": "AdjustLevels",
      "source_files": "Source/Adjust Levels/*.{h,m}",
      "resources": "Source/Adjust Levels/*.{xib}"
    }
  ]
}
