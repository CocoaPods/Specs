{
  "name": "mobile-ffmpeg-min",
  "version": "4.2",
  "summary": "Mobile FFmpeg Min Static Framework",
  "description": "Includes FFmpeg v4.2-dev-480 without any external libraries enabled.",
  "homepage": "https://github.com/tanersener/mobile-ffmpeg",
  "authors": {
    "Taner Sener": "tanersener@gmail.com"
  },
  "license": {
    "type": "LGPL-3.0",
    "file": "mobileffmpeg.framework/LICENSE"
  },
  "platforms": {
    "ios": "9.3"
  },
  "requires_arc": true,
  "ios": {
    "frameworks": [
      "AudioToolbox",
      "AVFoundation",
      "CoreMedia",
      "VideoToolbox"
    ],
    "vendored_frameworks": [
      "mobileffmpeg.framework",
      "libavcodec.framework",
      "libavdevice.framework",
      "libavfilter.framework",
      "libavformat.framework",
      "libavutil.framework",
      "libswresample.framework",
      "libswscale.framework"
    ]
  },
  "libraries": [
    "z",
    "bz2",
    "c++"
  ],
  "source": {
    "http": "https://github.com/tanersener/mobile-ffmpeg/releases/download/v4.2/mobile-ffmpeg-min-4.2-ios-framework.zip"
  }
}
