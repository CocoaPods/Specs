  },
  "platforms": {
    "ios": "8.1"
  },
  "authors": {
    "Saravanan": "saru2020@gmail.com"
  },
  "libraries": "z",
  "source": {
    "git": "https://github.com/saru2020/SARUnArchiveANY.git",
    "tag": "0.0.2"
  },
  "source_files": "SARUnArchiveANY/SARUnArchiveANY.{h,m}",
