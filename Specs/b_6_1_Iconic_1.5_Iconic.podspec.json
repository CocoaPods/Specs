{
  "name": "Iconic",
  "version": "1.5",
  "summary": "Auto-generated icon font library for iOS",
  "description": "Iconic will help you make icon fonts integration on iOS easy and effortless. Its main component is in charge of auto-generating strongly typed code in Swift, fully compatible with Objective-C, allowing the integration of vector icons as image or text in your apps.",
  "homepage": "https://github.com/home-assistant/Iconic",
  "authors": {
    "Ignacio Romero Zurbuchen": "iromero@dzen.cl",
    "Robbie Trencheny": "me@robbiet.us",
    "The Home Assistant Authors": "hello@home-assistant.io"
  },
  "license": {
    "type": "Apache 2.0",
    "file": "LICENSE.md"
  },
  "social_media_url": "https://twitter.com/hass_devs",
  "source": {
    "git": "https://github.com/home-assistant/Iconic.git",
    "tag": "1.5"
  },
  "source_files": "Source/*.{swift}",
  "resources": "Source/**/*.{ttf,otf}",
  "preserve_paths": "Source/*",
  "frameworks": [
    "UIKit",
    "CoreText"
  ],
  "platforms": {
    "ios": "8.0",
    "tvos": "9.0",
    "watchos": "2.0"
  },
  "swift_versions": "4.2",
  "prepare_command": "sh Source/Iconizer.sh '' ''",
  "swift_version": "4.2"
}
