{
  "name": "MongoMobile",
  "version": "0.1.0",
  "summary": "An embedded version of MongoDB for mobile",
  "homepage": "https://github.com/mongodb/swift-mongo-mobile",
  "license": "AGPL 3.0",
  "authors": {
    "Matt Broadstone": "mbroadst@mongodb.com",
    "Kaitlin Mahar": "kaitlin.mahar@mongodb.com",
    "Patrick Freed": "patrick.freed@mongodb.com"
  },
  "source": {
    "git": "https://github.com/mongodb/swift-mongo-mobile.git",
    "tag": "v0.1.0"
  },
  "platforms": {
    "ios": "11.0",
    "tvos": "10.2",
    "watchos": "4.3"
  },
  "requires_arc": true,
  "source_files": [
    "Sources/MongoMobile/**/*.swift"
  ],
  "dependencies": {
    "MongoSwift": [
      "~> 0.1.0"
    ],
    "mongoc_embedded": [
      "~> 4.0.4"
    ],
    "mongo_embedded": [
      "~> 4.0.4"
    ]
  },
  "testspecs": [
    {
      "name": "Tests",
      "test_type": "unit",
      "platforms": {
        "ios": "11.0"
      },
      "source_files": "Tests/MongoMobileTests/*.swift"
    }
  ]
}
