{
  "name": "Snappy",
  "version": "1.0.0",
  "license": "MIT",
  "summary": "A port of snappy-c to Objective-C.",
  "description": "Google's Snappy compression power as NSData/NSString categories.",
  "homepage": "https://github.com/matehat/Snappy-ObjC",
  "authors": "Mathieu D'Amours",
  "platforms": {
    "ios": "5.0",
    "osx": "10.7"
  },
  "source": {
    "git": "https://github.com/matehat/Snappy-ObjC.git",
    "tag": "v1.0.0",
    "submodules": true
  },
  "source_files": [
    "snappy-c/snappy.{h,c}",
    "snappy-c/compat.h",
    "snappy-c/snappy-int.h",
    "Classes/*.{h,m}"
  ],
  "xcconfig": {
    "CC": "clang",
    "CXX": "clang++"
  },
  "requires_arc": false
}