{
  "name": "KurentoToolbox",
  "version": "0.2.2",
  "summary": "Kurento Toolbox for iOS",
  "description": "Kurento Toolbox for iOS provides a set of basic components that have been found useful during the native development of the WebRTC applications with Kurento.",
  "homepage": "https://github.com/nubomediaTI/Kurento-iOS",
  "license": {
    "type": "GNU LGPL 2.1",
    "file": "LICENSE"
  },
  "authors": {
    "Marco Rossi": "marco5.rossi@guest.telecomitalia.it"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source": {
    "git": "https://github.com/nubomediaTI/Kurento-iOS.git",
    "tag": "v0.2.2"
  },
  "source_files": [
    "Classes",
    "Classes/**/*.{h,m}"
  ],
  "public_header_files": [
    "Classes/*.h",
    "Classes/Connection/*.h",
    "Classes/JSON-RPC/*.h",
    "Classes/Room/*.h"
  ],
  "private_header_files": [
    "Classes/Connection/Internals/*.h",
    "Classes/JSON-RPC/Internals/*.h",
    "Classes/Room/Internals/*.h",
    "Classes/Utils/*.h"
  ],
  "prefix_header_contents": "#import \"NBMLog.h\"",
  "requires_arc": true,
  "dependencies": {
    "libjingle_peerconnection": [
      "~> 10763.2.0"
    ],
    "SBJson": [
      "~> 4.0.2"
    ],
    "SocketRocket": [
      "~> 0.4.1"
    ],
    "CocoaLumberjack": [

    ]
  }
}
