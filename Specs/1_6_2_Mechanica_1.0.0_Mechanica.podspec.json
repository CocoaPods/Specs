{
  "name": "Mechanica",
  "version": "1.0.0",
  "license": "MIT",
  "documentation_url": "http://www.tinrobots.org/Mechanica",
  "summary": "A library of Swift utils to ease your iOS/macOS/watchOS/tvOS development.",
  "homepage": "https://github.com/tinrobots/Mechanica",
  "authors": {
    "Alessandro Marzoli": "me@alessandromarzoli.com"
  },
  "source": {
    "git": "https://github.com/tinrobots/Mechanica.git",
    "tag": "1.0.0"
  },
  "requires_arc": true,
  "platforms": {
    "ios": "10.0",
    "osx": "10.12",
    "tvos": "10.0",
    "watchos": "3.0"
  },
  "source_files": [
    "Sources/*.swift",
    "Sources/*.{h,m}",
    "Sources/Color/*.swift",
    "Sources/CoreData/*.swift",
    "Sources/CoreData/NSManagedObject/*.swift",
    "Sources/CoreData/NSManagedObjectContext/*.swift",
    "Sources/Dictionary/*.swift",
    "Sources/Integer/*.swift",
    "Sources/Interface Builder/*.swift",
    "Sources/NSAttributedString/*.swift",
    "Sources/NSObject/*.swift",
    "Sources/Protocols/*.swift",
    "Sources/String/*.swift",
    "Sources/UIKit/*.swift"
  ],
  "osx": {
    "exclude_files": "Sources/UIKit/*.swift"
  },
  "watchos": {
    "exclude_files": [
      "Sources/Interface Builder/Storyboard.swift",
      "Sources/Interface Builder/Nib.swift",
      "Sources/UIKit/UIButton+Utils.swift",
      "Sources/UIKit/UICollectionView+Reusable.swift",
      "Sources/UIKit/UITableView+Reusable.swift"
    ]
  },
  "pushed_with_swift_version": "4.0"
}
