{
  "name": "HCPlayer",
  "version": "0.1.8",
  "summary": "这是一个视频与音频播放器的库。",
  "description": "这是一个视频的播放特定的核心库。包含了视频、音频、歌词及缓冲处理的功能。\nsetPlayerData:...\nplay",
  "homepage": "https://github.com/halfking/HCPlayer",
  "license": "MIT",
  "authors": {
    "halfking": "kimmy.huang@gmail.com"
  },
  "platforms": {
    "ios": "7.0"
  },
  "source": {
    "git": "https://github.com/halfking/HCPlayer",
    "tag": "0.1.8"
  },
  "source_files": [
    "HCPlayer/**/*.{h,m,mm,c,cpp}",
    "HCPlayer/**/*.bundle"
  ],
  "exclude_files": "HCPlayer/HCPlayerTest/**/*",
  "libraries": [
    "icucore",
    "stdc++"
  ],
  "xcconfig": {
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES",
    "ENABLE_BITCODE": "YES",
    "DEFINES_MODULE": "YES",
    "HEADER_SEARCH_PATHS": "$(inherited)",
    "LIBRARY_SEARCH_PATHS": "$(inherited)"
  },
  "pod_target_xcconfig": {
    "LIBRARY_SEARCH_PATHS": "$(inherited) "
  },
  "dependencies": {
    "HCMinizip": [

    ],
    "hccoren": [

    ],
    "HCBaseSystem": [

    ],
    "HCMVManager": [

    ],
    "HCAudioUnit": [

    ]
  }
}
