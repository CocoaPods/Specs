{
  "name": "aerserv-ios-sdk",
  "version": "3.1.2",
  "summary": "AerServ-iOS-SDK Pod",
  "description": "AerServ-iOS-SDK's CocoaPods Integration",
  "homepage": "https://bitbucket.org/aerservllc/ios-sdk-pod",
  "license": "Copyright 2013-2018 AerServ-InMobi, all rights reserved",
  "authors": {
    "Albert Zhu": "albert.zhu@inmobi.com"
  },
  "source": {
    "git": "https://bitbucket.org/aerservllc/ios-sdk-pod.git",
    "tag": "3.1.2"
  },
  "platforms": {
    "ios": "8.0"
  },
  "ios": {
    "frameworks": "WebKit",
    "libraries": "xml2.2",
    "public_header_files": "Frameworks/AerServSDK.framework/Headers/*.h"
  },
  "xcconfig": {
    "OTHER_LDFLAGS": "-ObjC",
    "HEADER_SEARCH_PATHS": "$(SDKROOT)/usr/include/libxml2"
  },
  "source_files": "Frameworks/AerServSDK.framework/Headers/*.h",
  "vendored_frameworks": "Frameworks/AerServSDK.framework"
}
