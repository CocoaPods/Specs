{
  "name": "Wolf",
  "version": "0.2.0",
  "summary": "Handy solutions to common iOS app development problems.",
  "description": "Wolf brings handy solutions to common iOS app development problems. It includes storyboard management, error handling shortcuts, an opinionated networking layer and more.",
  "homepage": "https://github.com/fellipecaetano/Wolf",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Fellipe Caetano": "fellipe.caetano4@gmail.com"
  },
  "source": {
    "git": "https://github.com/fellipecaetano/Wolf.git",
    "tag": "0.2.0"
  },
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "default_subspecs": "All",
  "subspecs": [
    {
      "name": "All",
      "source_files": [
        "Wolf/Classes/**/*"
      ],
      "dependencies": {
        "Alamofire": [
          "~> 3.4"
        ],
        "Argo": [
          "~> 3.1"
        ]
      }
    },
    {
      "name": "Storyboards",
      "source_files": [
        "Wolf/Classes/Storyboards/**/*"
      ]
    },
    {
      "name": "Reuse",
      "source_files": [
        "Wolf/Classes/Reuse/**/*"
      ]
    },
    {
      "name": "Networking",
      "source_files": [
        "Wolf/Classes/Networking/**/*"
      ],
      "dependencies": {
        "Alamofire": [
          "~> 3.4"
        ]
      }
    },
    {
      "name": "Argo",
      "source_files": [
        "Wolf/Classes/Networking/**/*",
        "Wolf/Classes/Argo/**/*"
      ],
      "dependencies": {
        "Alamofire": [
          "~> 3.4"
        ],
        "Argo": [
          "~> 3.1"
        ]
      }
    },
    {
      "name": "Cache",
      "source_files": [
        "Wolf/Classes/Networking/**/*",
        "Wolf/Classes/Cache/**/*"
      ],
      "dependencies": {
        "Alamofire": [
          "~> 3.4"
        ]
      }
    }
  ]
}
