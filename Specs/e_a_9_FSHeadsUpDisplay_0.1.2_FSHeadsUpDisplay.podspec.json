{
  "name": "FSHeadsUpDisplay",
  "version": "0.1.2",
  "summary": "Presents a heads-up display on iOS.",
  "description": "      Provides a simple heads-up display for iOS apps. Can be used\n      to display messages on either iPhone or iPad.  Display automatically\n      hides after a predetermined time if no additional messages are\n      presented.\n",
  "homepage": "https://github.com/FocalShift/FSHeadsUpDisplay",
  "screenshots": [
    "https://raw.github.com/FocalShift/FSHeadsUpDisplay/develop/Screenshots/iphone-hud-landscape.png",
    "https://raw.github.com/FocalShift/FSHeadsUpDisplay/develop/Screenshots/iphone-hud-portrait.png",
    "https://raw.github.com/FocalShift/FSHeadsUpDisplay/develop/Screenshots/ipad-hud-landscape.png",
    "https://raw.github.com/FocalShift/FSHeadsUpDisplay/develop/Screenshots/ipad-hud-portrait.png"
  ],
  "license": "MIT",
  "authors": {
    "Bennett Smith": "bennett@focalshift.com"
  },
  "source": {
    "git": "https://github.com/FocalShift/FSHeadsUpDisplay.git",
    "tag": "0.1.2"
  },
  "social_media_url": "https://twitter.com/NAME",
  "platforms": {
    "ios": "7.0"
  },
  "requires_arc": true,
  "source_files": "Classes/ios/*.{h,m}",
  "ios": {
    "exclude_files": "Classes/osx"
  },
  "osx": {
    "exclude_files": "Classes/ios"
  },
  "dependencies": {
    "FSClassExtensions": [
      "~> 1.6"
    ]
  }
}
