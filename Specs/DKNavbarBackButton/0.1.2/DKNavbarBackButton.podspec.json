  "homepage": "https://github.com/wade0n/DKNavbarBackButton",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Dmitrii Kalashnikov": "mr.dmitriikalashnikov@gmail.com"
  },
  "source": {
    "git": "https://github.com/wade0n/DKNavbarBackButton.git",
    "tag": "0.1.2"
  },
  "platforms": {
    "ios": "7.0"
  },
  "default_subspec": "core",
  "requires_arc": false,
  "subspecs": [
    {
      "name": "core",
      "requires_arc": true,
      "source_files": "core/source/*",
      "resources": "core/resources/*"
