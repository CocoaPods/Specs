  "homepage": "http://github.com/wepay/wepay-ios",
  "documentation_url": "http://wepay.github.io/wepay-ios/",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "source": {
    "http": "https://github.com/wepay/wepay-ios/releases/download/v7.0.2/wepay-ios-7.0.2.zip"
  },
  "authors": {
    "Chaitanya Bagaria": "mobile@wepay.com"
  },
  "platforms": {
    "ios": "8.0"
  },
  "ios": {
    "requires_arc": true,
    "preserve_paths": "**",
    "frameworks": [
      "AudioToolbox",
      "AVFoundation",
      "ExternalAccessory",
      "MediaPlayer",
      "WebKit"
