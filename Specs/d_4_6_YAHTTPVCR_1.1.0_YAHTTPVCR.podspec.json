    "YAHTTPVCR/Matchers/*.h",
    "YAHTTPVCR/Misc/{Categories,Helpers}/*.h",
    "YAHTTPVCR/Misc/Protocols/{YHVNSURLProtocol,YHVSerializableDataProtocol}.h",
    "YAHTTPVCR/Misc/YHVPrivateStructures.h",
    "YAHTTPVCR/Data/YHVScene.h",
    "YAHTTPVCR/**/*Private.h"
  ],
  "frameworks": "XCTest",
  "license": "MIT"
}
