{
  "name": "GzoneLib",
  "version": "0.1.1",
  "summary": "GzoneLib Gzone Collection liblary",
  "homepage": "https://github.com/dungnt/GzoneLib.git",
  "license": "MIT",
  "authors": {
    "dungnt": "dung.nt@gzone.com.vn"
  },
  "source": {
    "git": "https://github.com/dungnt/GzoneLib.git",
    "tag": "0.1.1"
  },
  "platforms": {
    "ios": "7.0"
  },
  "requires_arc": true,
  "source_files": "Pod/Classes/*.{h,m}",
  "resource_bundles": {
    "GzoneLib": [
      "Pod/Assets/*.png"
    ]
  },
  "subspecs": [
    {
      "name": "GzFileAmazonUpload",
      "source_files": "Pod/Classes/GzFileAmazonUpload/*.{h,m}",
      "dependencies": {
        "AFNetworking": [
          "~> 2.4.1"
        ]
      }
    },
    {
      "name": "GzCrashLogMessage",
      "source_files": "Pod/Classes/GzCrashLogMessage/*.{h,m}",
      "dependencies": {
        "Parse": [
          "~> 1.5.0"
        ]
      }
    },
    {
      "name": "GzInternetConnection",
      "source_files": "Pod/Classes/GzInternetConnection/*.{h,m}",
      "dependencies": {
        "Reachability": [
          "~> 3.1.1"
        ]
      }
    }
  ]
}
