{
  "name": "MMTopOnAdapter",
  "version": "1.5.0",
  "summary": "Adapter",
  "description": "AD Adapter.",
  "homepage": "https://github.com/yangdimuyi/MMTopOnAdapter.git",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "WangMai": "yangdi.yang@adwangmai.com"
  },
  "platforms": {
    "ios": "11.0"
  },
  "xcconfig": {
    "VALID_ARCHS": "arm64 x86_64",
    "VALID_ARCHS[sdk=iphoneos*]": "arm64",
    "VALID_ARCHS[sdk=iphonesimulator*]": "x86_64",
    "OTHER_LDFLAGS": [
      "-ObjC"
    ],
    "ENABLE_BITCODE": "NO",
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES"
  },
  "user_target_xcconfig": {
    "OTHER_LDFLAGS": [
      "-ObjC"
    ],
    "ENABLE_BITCODE": "NO",
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "pod_target_xcconfig": {
    "OTHER_LDFLAGS": [
      "-ObjC"
    ],
    "ENABLE_BITCODE": "NO",
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "source": {
    "git": "https://github.com/yangdimuyi/MMTopOnAdapter.git",
    "tag": "1.5.0"
  },
  "requires_arc": true,
  "default_subspecs": "AdSDk",
  "subspecs": [
    {
      "name": "AdSDk",
      "ios": {
        "vendored_frameworks": "MMTopOnAdapter/WMAdapter.framework"
      },
      "dependencies": {
        "AnyThinkiOS": [
          "6.4.92"
        ],
        "AdWangMaiSDK": [
          "7.5.2.20"
        ]
      }
    }
  ]
}
