{
  "name": "Coralogix",
  "version": "1.2.5",
  "summary": "Coralogix OpenTelemetry pod for iOS.",
  "description": "The Coralogix RUM agent for iOS provides a Swift package that captures:\n- HTTP requests, using URLSession instrumentation\n- Unhandled exceptions (NSException, NSError, Error)\n- Custom Log ()\n- Crashes - using PLCrashReporter\n- Page navigation (Swift use swizzeling / SwiftUI use modifier)",
  "swift_versions": "5.9",
  "cocoapods_version": ">= 1.10",
  "platforms": {
    "ios": "13.0"
  },
  "homepage": "https://github.com/coralogix/cx-ios-sdk.git",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "source": {
    "git": "https://github.com/coralogix/cx-ios-sdk.git",
    "tag": "1.2.5"
  },
  "authors": {
    "Coralogix": "www.coralogix.com"
  },
  "source_files": "Coralogix/Sources/**/*.swift",
  "exclude_files": "Coralogix/Sources/Exclude",
  "static_framework": true,
  "dependencies": {
    "PLCrashReporter": [
      "~> 1.12"
    ],
    "CoralogixInternal": [
      "1.2.5"
    ]
  },
  "testspecs": [
    {
      "name": "Tests",
      "test_type": "unit",
      "source_files": "Tests/CoralogixRumTests/**/*.swift"
    }
  ],
  "swift_version": "5.9"
}
