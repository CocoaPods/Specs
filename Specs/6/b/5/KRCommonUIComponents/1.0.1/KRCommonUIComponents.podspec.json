      "frameworks": "UIKit",
      "dependencies": {
        "KRCommonComponents": [
          "~> 1.0.4"
        ],
        "Masonry": [
          "1.1.0"
        ]
      }
    },
    {
      "name": "Hud",
      "source_files": "KRCommonUIComponents/Hud/**/*",
