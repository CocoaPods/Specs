{
  "name": "SendsaySDK",
  "version": "0.1.1",
  "summary": "Sendsay SDK used to track and fetch data from Sendsay Experience Cloud.",
  "description": "This library allows you to interact from your application or game with the Sendsay App.\nSendsay empowers B2C marketers to raise conversion rates, improve acquisition ROI, and maximize customer lifetime value.",
  "homepage": "https://github.com/sendsay-ru/sendsay-mobile-sdk-ios",
  "readme": "https://github.com/sendsay-ru/sendsay-mobile-sdk-ios/blob/main/README.md",
  "license": "MIT",
  "authors": {
    "Sendsay": "ask@sendsay.ru"
  },
  "platforms": {
    "ios": "13.0"
  },
  "swift_versions": [
    "5.9",
    "5.10"
  ],
  "source": {
    "git": "https://github.com/sendsay-ru/sendsay-mobile-sdk-ios.git",
    "tag": "0.1.1"
  },
  "source_files": [
    "SendsaySDK/SendsaySDK/**/*.swift",
    "SendsaySDK/SendsaySDK/**/*.storyboard",
    "SendsaySDK/SendsaySDK/**/*.xcassets",
    "SendsaySDK/SendsaySDKShared/**/*.swift",
    "SendsaySDK/SendsaySDKObjC/objc_tryCatch.h",
    "SendsaySDK/SendsaySDKObjC/objc_tryCatch.m"
  ],
  "resource_bundles": {
    "SendsaySDK": [
      "SendsaySDK/SendsaySDK/Supporting Files/PrivacyInfo.xcprivacy"
    ]
  },
  "exclude_files": "SendsaySDK/SendsaySDK-Notifications/**/*",
  "resources": [
    "SendsaySDK/SendsaySDK/Classes/Database/*.xcdatamodeld"
  ],
  "dependencies": {
    "SwiftSoup": [
      ">= 2.7.6",
      "< 3.0"
    ]
  },
  "swift_version": "5.10"
}
