{
  "name": "ffmpeg-kit-https",
  "version": "1.1",
  "summary": "FFmpeg Kit iOS Https Shared Framework",
  "description": "Includes FFmpeg with gmp and gnutls libraries enabled.",
  "homepage": "https://github.com/voyageh/ffmpeg-kit",
  "authors": {
    "ARTHENICA": "open-source@arthenica.com"
  },
  "license": {
    "type": "LGPL-3.0",
    "file": "ffmpeg-kit-https/ffmpegkit.xcframework/ios-arm64/ffmpegkit.framework/LICENSE"
  },
  "platforms": {
    "ios": "12.1"
  },
  "requires_arc": true,
  "libraries": [
    "z",
    "bz2",
    "c++",
    "iconv"
  ],
  "source": {
    "http": "https://github.com/voyageh/ffmpeg-kit/releases/download/v6.0/ffmpeg-kit-https.zip"
  },
  "ios": {
    "frameworks": [
      "AudioToolbox",
      "AVFoundation",
      "CoreMedia",
      "VideoToolbox"
    ]
  },
  "vendored_frameworks": [
    "ffmpeg-kit-https/ffmpegkit.xcframework",
    "ffmpeg-kit-https/libavcodec.xcframework",
    "ffmpeg-kit-https/libavdevice.xcframework",
    "ffmpeg-kit-https/libavfilter.xcframework",
    "ffmpeg-kit-https/libavformat.xcframework",
    "ffmpeg-kit-https/libavutil.xcframework",
    "ffmpeg-kit-https/libswresample.xcframework",
    "ffmpeg-kit-https/libswscale.xcframework"
  ]
}
