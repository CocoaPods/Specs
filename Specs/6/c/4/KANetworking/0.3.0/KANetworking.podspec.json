{
  "name": "KANetworking",
  "version": "0.3.0",
  "summary": "A network extension for Alamofire for faster DTO parsing.",
  "description": "This is an extension to the Alamofire Pod with easyer DTO and error parsing.",
  "homepage": "https://github.com/kadarandras/KANetworking",
  "license": "MIT",
  "authors": {
    "Andras Kadar": "kadarandrass@gmail.com"
  },
  "source": {
    "git": "https://github.com/kadarandras/KANetworking.git",
    "tag": "0.3.0"
  },
  "platforms": {
