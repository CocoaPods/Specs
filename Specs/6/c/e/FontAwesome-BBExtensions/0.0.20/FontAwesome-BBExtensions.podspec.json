{
  "name": "FontAwesome-BBExtensions",
  "version": "0.0.20",
  "summary": "Categories on NSString, UIFont, and UIImage to make creation of FontAwesome icons easier.",
  "description": "Categories to Foundation and UIKit classes to make it easier to create UIImage instances from FontAwesome Unicode characters. The naming scheme mirrors the class names used on the FontAwesome cheat sheet page at http://fontawesome.io/cheatsheet/.",
  "homepage": "https://github.com/BionBilateral/FontAwesome-BBExtensions",
  "license": {
    "type": "BSD",
    "file": "license.txt"
  },
  "authors": {
    "William Towe": "willbur1984@gmail.com"
  },
  "platforms": {
    "ios": "10.0",
    "osx": "10.12",
    "tvos": "10.0",
    "watchos": "3.0"
  },
  "source": {
    "git": "https://github.com/BionBilateral/FontAwesome-BBExtensions.git",
    "tag": "0.0.20"
  },
  "source_files": "FontAwesome-BBExtensions/**/*.{h,m}",
  "exclude_files": "FontAwesome-BBExtensions/FontAwesome-BBExtensions-Info.h",
  "ios": {
    "exclude_files": "FontAwesome-BBExtensions/macOS",
    "frameworks": "UIKit"
  },
  "osx": {
    "exclude_files": "FontAwesome-BBExtensions/iOS",
    "frameworks": "AppKit"
  },
  "tvos": {
    "exclude_files": "FontAwesome-BBExtensions/macOS",
    "frameworks": "UIKit"
  },
  "watchos": {
    "exclude_files": [
      "FontAwesome-BBExtensions/iOS/UIButton*.{h,m}",
      "FontAwesome-BBExtensions/macOS"
    ],
    "frameworks": "UIKit"
  },
  "frameworks": "Foundation",
  "requires_arc": true,
  "header_dir": "FontAwesome_BBExtensions"
}
