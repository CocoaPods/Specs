          "~>0.2.4"
        ],
        "ServiceRegistry/API": [
          "~>0.1.3"
        ]
      }
    },
    {
      "name": "Implementation",
      "source_files": "Pod/Implementation/*.swift",
      "dependencies": {
        "ModelService/API": [

        ]
