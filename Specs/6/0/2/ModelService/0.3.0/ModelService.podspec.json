  "homepage": "https://github.com/AttilaTheFun/ModelService",
  "license": "MIT",
  "authors": {
    "Logan Shire": "logan.shire@gmail.com"
  },
  "source": {
    "git": "https://github.com/AttilaTheFun/ModelService.git",
    "tag": "0.3.0"
  },
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "subspecs": [
    {
      "name": "API",
      "source_files": "Pod/API/*.swift",
