{
  "name": "BxObjC",
  "version": "1.0.29",
  "summary": "Objective-C library for all",
  "description": "This framework will help iOS developers simplify development",
  "homepage": "https://github.com/ByteriX/BxObjC",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Sergey Balalaev": "sof.bix@mail.ru"
  },
  "platforms": {
    "ios": "5.0"
  },
  "source": {
    "git": "https://github.com/ByteriX/BxObjC.git",
    "tag": "1.0.29"
  },
  "source_files": [
    "**/Frameworks/**/*.{h,m}",
    "**/Sources/**/*.{h,m}"
  ],
  "exclude_files": [
    "**/**Tests/**/*.*",
    "**/**Test/**/*.*",
    "**/iBXDB/**/*.*"
  ],
  "frameworks": [
    "Foundation",
    "UIKit",
    "MapKit",
    "CoreLocation"
  ],
  "public_header_files": [
    "**/Frameworks/**/*.h",
    "**/Sources/**/*.h"
  ],
  "requires_arc": [
    "**/BxIconWorkspace**.m",
    "**/BxNavigation**.m",
    "**/BxPushNotificationMessageQueue.m",
    "**/XMLDictionary.m"
  ],
  "resources": [
    "**/iBXCommon/**/Resources/**/*.strings",
    "**/iBXData/**/*.{png,xib}",
    "**/iBXVcl/**/*.{png,xib}",
    "**/iBXMap/**/*.{png,xib}"
  ]
}
