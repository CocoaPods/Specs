    "tag": "1.4.3"
  },
  "subspecs": [
    {
      "name": "Common",
      "frameworks": [
        "Foundation",
        "UIKit"
      ],
      "resources": "**/iBXCommon/Resources/**/*.strings",
      "public_header_files": [
        "**/iBXCommon/Frameworks/**/*.h",
        "**/iBXCommon/Sources/**/*.h"
      ],
      "source_files": [
        "**/iBXCommon/Frameworks/**/*.{h,m,c}",
        "**/iBXCommon/Sources/**/*.{h,m,c}"
      ],
      "exclude_files": [
        "**/**Tests/**/*.*",
        "**/**Test/**/*.*"
      ],
      "requires_arc": [
        "**/BxPushNotificationMessageQueue.m"
      ]
    },
    {
      "name": "Data",
      "dependencies": {
        "BxObjC/Common": [

        ],
        "XMLDictionary": [
          "1.4"
        ]
      },
      "frameworks": [
        "Foundation",
        "UIKit"
      ],
      "public_header_files": [
        "**/iBXData/Frameworks/**/*.h",
