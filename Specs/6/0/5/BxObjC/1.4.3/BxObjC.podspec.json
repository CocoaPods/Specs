        "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited) SQLITE_CORE=1 SQLITE_UNICODE_ENABLE=1 SQLITE_ENABLE_FTS4=1 SQLITE_ENABLE_FTS4_PARENTHESIS=1"
      },
      "frameworks": [
        "Foundation",
        "UIKit"
      ],
      "public_header_files": [
        "**/iBXDB/Frameworks/**/*.h",
        "**/iBXDB/Sources/**/*.h"
      ],
      "source_files": [
        "**/iBXDB/Frameworks/**/*.{h,m,c}",
        "**/iBXDB/Sources/**/*.{h,m,c}"
      ],
      "exclude_files": [
        "**/**Tests/**/*.*",
        "**/**Test/**/*.*"
      ],
      "requires_arc": [

      ]
    },
