{
  "name": "RFKeyboard",
  "version": "0.3.0",
  "summary": "iOS Keyboard helper.",
  "homepage": "https://github.com/RFUI/RFKeyboard",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "BB9z": "bb9z@me.com"
  },
  "source": {
    "git": "https://github.com/RFUI/RFKeyboard.git",
    "tag": "0.3.0"
  },
  "requires_arc": true,
  "platforms": {
    "ios": "9.0"
  },
  "source_files": [
    "RFKeyboard.{h,m}"
  ],
