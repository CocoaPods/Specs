{
  "name": "Log4s",
  "version": "1.4.0",
  "summary": "A logging library written in swift.",
  "description": "Log4s is a logging library similar in philosophy to log4j.\nIt is meant to be :\n\n* very simple to use for simple cases\n* extensively configurable for less simple cases",
  "homepage": "http://github.com/igor-makarov/Log4s",
  "license": {
    "type": "Apache v2.0",
    "file": "LICENSE"
  },
  "authors": {
    "Igor Makarov": "igormaka@gmail.com",
    "Jerome Duquennoy": "jerome@duquennoy.fr"
  },
  "platforms": {
    "ios": "8.0",
    "watchos": "2.0",
    "osx": "10.10"
  },
  "swift_versions": [
    "4.0",
    "4.1",
    "4.2",
    "5.0"
  ],
  "module_name": "Log4swift",
  "source": {
    "git": "https://github.com/igor-makarov/Log4s",
    "tag": "1.4.0"
  },
  "source_files": [
    "Log4swift",
    "Log4swift/**/*.{swift,h,m}",
    "Third parties/**/*.{h,m}"
  ],
  "public_header_files": [
    "Log4swift/log4swift.h",
    "Third Parties/NSLogger/*.h",
    "Log4swift/Objective-c wrappers/*.h"
  ],
  "watchos": {
    "exclude_files": [
      "Third Parties/NSLogger/*",
      "Log4swift/Appenders/NSLoggerAppender.swift"
    ]
  },
  "swift_version": "5.0"
}
