{
  "name": "ZSViewUtil",
  "version": "0.5.0",
  "summary": "自定义 View",
  "description": "包含 Web、Toast、Animation、Player等等",
  "homepage": "https://github.com/zhangsen093725/ZSViewUtil",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "zhangsen093725": "376019018@qq.com"
  },
  "source": {
    "git": "https://github.com/zhangsen093725/ZSViewUtil.git",
    "tag": "0.5.0"
  },
  "swift_versions": "5.0",
  "platforms": {
    "ios": "8.0"
  },
  "default_subspecs": "All",
  "resource_bundles": {
    "ZSViewUtil": [
      "ZSViewUtil/Assets/**/*"
    ]
  },
  "subspecs": [
    {
      "name": "All",
      "source_files": "ZSViewUtil/Classes/**/*"
    },
    {
      "name": "Button",
      "source_files": "ZSViewUtil/Classes/Button/**/*"
    },
    {
      "name": "CollectionView",
      "source_files": "ZSViewUtil/Classes/CollectionView/**/*"
    },
    {
      "name": "DragView",
      "source_files": "ZSViewUtil/Classes/DragView/**/*"
    },
    {
      "name": "Field",
      "source_files": "ZSViewUtil/Classes/Field/**/*"
    },
    {
      "name": "FlowLayout",
      "source_files": "ZSViewUtil/Classes/FlowLayout/**/*",
      "subspecs": [
        {
          "name": "Plain",
          "source_files": "ZSViewUtil/Classes/FlowLayout/Plain/**/*"
        }
      ]
    },
    {
      "name": "Image",
      "source_files": "ZSViewUtil/Classes/Image/**/*"
    },
    {
      "name": "LoopView",
      "source_files": "ZSViewUtil/Classes/LoopView/**/*"
    },
    {
      "name": "Layer",
      "source_files": "ZSViewUtil/Classes/Layer/**/*"
    },
    {
      "name": "Player",
      "source_files": "ZSViewUtil/Classes/Preview/Player/**/*"
    },
    {
      "name": "Preview",
      "source_files": "ZSViewUtil/Classes/Preview/**/*",
      "resource_bundles": {
        "ZSViewUtil": [
          "ZSViewUtil/Assets/ZSPreview.xcassets"
        ]
      }
    },
    {
      "name": "Profiles",
      "source_files": "ZSViewUtil/Classes/Profiles/**/*"
    },
    {
      "name": "SphereView",
      "source_files": "ZSViewUtil/Classes/SphereView/**/*"
    },
    {
      "name": "Toast",
      "source_files": "ZSViewUtil/Classes/Toast/**/*",
      "subspecs": [
        {
          "name": "Load",
          "source_files": "ZSViewUtil/Classes/Toast/Load/**/*"
        },
        {
          "name": "Toast",
          "source_files": "ZSViewUtil/Classes/Toast/Toast/**/*"
        }
      ]
    },
    {
      "name": "TabPage",
      "source_files": "ZSViewUtil/Classes/TabPage/**/*"
    },
    {
      "name": "UIExt",
      "source_files": "ZSViewUtil/Classes/UIExt/**/*"
    },
    {
      "name": "ViewAnimation",
      "source_files": "ZSViewUtil/Classes/ViewAnimation/**/*"
    },
    {
      "name": "WebView",
      "source_files": "ZSViewUtil/Classes/WebView/**/*"
    }
  ],
  "swift_version": "5.0"
}
