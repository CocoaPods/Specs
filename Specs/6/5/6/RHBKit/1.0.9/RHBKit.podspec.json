    "tag": "1.0.9"
  },
  "social_media_url": "https://twitter.com/redhotbits",
  "swift_version": "4.0",
  "platforms": {
    "ios": "10.0",
    "watchos": "3.0",
    "tvos": "10.0",
    "osx": "10.12"
  },
  "default_subspecs": "All/Core",
