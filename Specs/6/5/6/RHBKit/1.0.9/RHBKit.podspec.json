            {
              "name": "Core",
              "platforms": {
                "ios": "10.0",
                "watchos": "3.0",
                "tvos": "10.0"
              },
              "source_files": "Sources/UIKit/Core/**/*"
            },
            {
              "name": "Extras",
              "platforms": {
                "ios": "10.0",
                "tvos": "10.0"
              },
              "source_files": "Sources/UIKit/Extras/**/*",
              "dependencies": {
                "RHBKit/All/Core": [

                ]
              }
