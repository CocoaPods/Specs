{
  "name": "ApesterKit",
  "version": "3.3.8",
  "swift_versions": "5.0",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "summary": "ApesterKit provides a light-weight framework that loads Apester Unit in a webView",
  "homepage": "https://github.com/Qmerce/ios-sdk.git",
  "authors": {
    "Hasan Sa": "hasansa007@gmail.com"
  },
  "source": {
    "git": "https://github.com/apester-dev/ios-sdk.git",
    "tag": "v3.3.8"
  },
  "platforms": {
    "ios": "11.0"
  },
  "requires_arc": true,
  "source_files": [
    "ApesterKit",
    "Sources/*.{h,m,swift}",
    "Sources/*/*.{h,m,swift}",
    "Sources/*/*/*.{h,m,swift}",
    "Sources/*/*/*/*.{h,m,swift}"
  ],
  "static_framework": true,
  "dependencies": {
    "Google-Mobile-Ads-SDK": [
      "~> 8.7"
    ],
    "OpenWrapSDK": [
      "~> 2.3.1"
    ]
  },
  "pod_target_xcconfig": {
    "SWIFT_INCLUDE_PATHS": "$(PODS_TARGET_SRCROOT)/Sodium/libsodium",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "swift_version": "5.0"
}
