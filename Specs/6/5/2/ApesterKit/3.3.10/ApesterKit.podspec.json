  "testspecs": [
    {
      "name": "UnitTests",
      "test_type": "unit",
      "platforms": {
        "ios": "11.0"
      },
      "scheme": {
        "code_coverage": true,
        "launch_arguments": [

        ]
      },
      "source_files": [
        "ApesterKit/Tests/Classes/*.{h,m,swift}"
      ],
      "preserve_paths": [
        "ApesterKit/Tests/Classes/*.{h,m,swift}"
      ],
      "requires_app_host": true,
