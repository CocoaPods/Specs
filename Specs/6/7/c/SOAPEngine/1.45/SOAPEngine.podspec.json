{
  "name": "SOAPEngine",
  "version": "1.45",
  "summary": "This generic SOAP client allows you to access web services using your iOS, macOS, and tvOS app.",
  "description": "A comprehensive and generic SOAP client for Apple platforms, allowing easy access and communication with web services.",
  "license": {
    "type": "Apache 2.0",
    "file": "LICENSE.txt"
  },
  "authors": {
    "Danilo Priore": "support@prioregroup.com"
  },
  "homepage": "https://github.com/priore/SOAPEngine",
  "social_media_url": "https://twitter.com/danilopriore",
  "source": {
    "git": "https://github.com/priore/SOAPEngine.git",
    "tag": "1.45"
  },
  "requires_arc": true,
  "pod_target_xcconfig": {
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES",
    "OTHER_LDFLAGS": "-lxml2"
  },
  "libraries": "xml2",
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "$(SDKROOT)/usr/include/libxml2"
  },
  "source_files": "SOAPEngine/**/*.{h,m,pch}",
  "public_header_files": "SOAPEngine/Headers/*.h",
  "platforms": {
    "ios": "12.0",
    "osx": "10.13",
    "tvos": "12.0"
  },
  "ios": {
    "frameworks": "Security"
  },
  "osx": {
    "frameworks": "Accounts"
  },
  "tvos": {
    "frameworks": "Security"
  }
}
