{
  "name": "BrightIndoorSDK",
  "version": "2.3.4",
  "summary": "Indoor location library for iBeacon devices",
  "homepage": "http://www.brtbeacon.com",
  "authors": {
    "BrightBeacon": "o2owlkj@163.com"
  },
  "social_media_url": "http://bbs.brtbeacon.com",
  "documentation_url": "http://help.brtbeacon.com",
  "requires_arc": true,
  "platforms": {
    "ios": "7.0"
  },
  "source": {
    "git": "https://github.com/BrightBeacon/BrightIndoorSDK_IOS.git",
    "tag": "2.3.4"
  },
  "preserve_paths": [
    "libs/TYLocationEngine.framework",
    "libs/TYMapData.framework",
    "libs/TYMapSDK.framework"
  ],
  "ios": {
    "vendored_frameworks": [
      "libs/TYLocationEngine.framework",
      "libs/TYMapData.framework",
      "libs/TYMapSDK.framework"
    ]
  },
  "resources": "resource/*.png",
  "libraries": [
    "sqlite3",
    "z",
    "stdc++.6.0.9",
    "c++"
  ],
  "xcconfig": {
    "CLANG_CXX_LIBRARY": "libc++",
    "FRAMEWORK_SEARCH_PATHS": "\"$HOME/Library/SDKs/ArcGIS/iOS\"",
    "LIBRARY_SEARCH_PATHS": "\"${SRCROOT}\"",
    "OTHER_LDFLAGS": "\"-framework ArcGIS -lgeos\""
  },
  "license": {
    "type": "Copyright",
    "text": "      Copyright 2017 BrightBeacon All rights reserved.\n"
  }
}
