{
  "name": "BraintreeDropIn",
  "version": "9.9.0",
  "summary": "Braintree Drop-in SDK: A modern foundation for accepting payments",
  "description": "Braintree is a full-stack payments platform for developers\n\nThis CocoaPod will help you accept payments in your iOS app.\n\nCheck out our development portal at https://developer.paypal.com/braintree/docs.",
  "homepage": "https://developer.paypal.com/braintree/docs",
  "documentation_url": "https://developer.paypal.com/braintree/docs/start/hello-client/ios/v5",
  "screenshots": [
    "https://github.com/braintree/braintree-ios-drop-in/raw/main/Images/client-sdk-ios-series-light.png",
    "https://github.com/braintree/braintree-ios-drop-in/raw/main/Images/client-sdk-ios-series-dark.png"
  ],
  "license": "MIT",
  "authors": {
    "Braintree": "team-bt-sdk@paypal.com"
  },
  "source": {
    "git": "https://github.com/braintree/braintree-ios-drop-in.git",
    "tag": "9.9.0"
  },
  "platforms": {
    "ios": "12.0"
  },
  "requires_arc": true,
  "compiler_flags": "-Wall -Werror -Wextra",
  "swift_versions": "5.7",
  "source_files": "Sources/BraintreeDropIn/**/*.{h,m}",
  "public_header_files": "Sources/BraintreeDropIn/Public/BraintreeDropIn/*.h",
  "frameworks": "UIKit",
  "dependencies": {
    "Braintree/ApplePay": [
      "~> 5.19"
    ],
    "Braintree/Card": [
      "~> 5.19"
    ],
    "Braintree/Core": [
      "~> 5.19"
    ],
    "Braintree/UnionPay": [
      "~> 5.19"
    ],
    "Braintree/PayPal": [
      "~> 5.19"
    ],
    "Braintree/ThreeDSecure": [
      "~> 5.19"
    ],
    "Braintree/Venmo": [
      "~> 5.19"
    ]
  },
  "resource_bundles": {
    "BraintreeDropIn-Localization": [
      "Sources/BraintreeDropIn/Resources/*.lproj"
    ]
  },
  "swift_version": "5.7"
}
