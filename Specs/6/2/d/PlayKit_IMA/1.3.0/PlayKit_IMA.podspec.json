  },
  "summary": "PlayKit IMA Plugin",
  "homepage": "https://github.com/kaltura/playkit-ios-ima",
  "source": {
    "git": "https://github.com/kaltura/playkit-ios-ima.git",
    "tag": "v1.3.0"
  },
  "swift_version": "4.0",
  "source_files": "**/*.swift",
  "platforms": {
