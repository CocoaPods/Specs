{
  "name": "VesperSDK",
  "version": "2.1.0",
  "summary": "Umbrella framework that provides simplified access to Vesper platform features",
  "description": "Binary version of VesperSDK",
  "homepage": "https://dicetechnology.github.io/vesper-sdk-apple/",
  "license": "DiceTechnology",
  "authors": {
    "author": "Endeavor Streaming"
  },
  "source": {
    "http": "https://d1st2jzonb6gjl.cloudfront.net/vesper/2.1.0/VesperSDK.zip"
  },
  "swift_versions": "5.2",
  "platforms": {
    "ios": "14.0"
  },
  "ios": {
    "vendored_frameworks": "iphoneFrameworks/*",
    "resources": "Resources/*",
    "dependencies": {
      "GoogleAds-IMA-iOS-SDK": [
        "~> 3.22"
      ]
    }
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "i386 x86_64"
  },
  "tvos": {
    "dependencies": {
      "GoogleAds-IMA-tvOS-SDK": [
        "~> 4.12"
      ]
    }
  },
  "dependencies": {
    "ConvivaAVFoundation": [
      ">= 4.0.51",
      "< 4.1"
    ],
    "Mux-Stats-AVPlayer": [
      "~> 3.6.0"
    ]
  },
  "swift_version": "5.2"
}
