      "vendored_frameworks": "mediation/mediatedviews/GoogleAdMob/GoogleAdMobSDK/GoogleMobileAds.framework",
      "frameworks": [
        "SafariServices",
        "CoreBluetooth",
        "GLKit",
        "CoreMotion",
        "MobileCoreServices"
      ]
    },
    {
      "name": "MoPubAdapter",
      "dependencies": {
        "AppNexusSDK/AppNexusSDK": [
          "3.1"
        ],
        "mopub-ios-sdk": [
          "~> 4.6.0"
        ]
