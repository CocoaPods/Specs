{
  "name": "CLKits",
  "version": "1.0.4",
  "summary": "CLKits framework just save your time. deployment target ios 8.0",
  "homepage": "https://github.com/coooliang/CLKits",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": "coooliang",
  "platforms": {
    "ios": "8.0"
  },
  "source": {
    "git": "https://github.com/coooliang/CLKits.git",
    "tag": "1.0.4"
  },
  "requires_arc": true,
  "default_subspecs": "All",
  "subspecs": [
    {
      "name": "All",
      "source_files": [
        "Sources/CLFaster/**/*.{h,m}",
        "Sources/CLNetworking/**/*.{h,m}",
        "Sources/CLPopAnimation/**/*.{h,m}",
        "Sources/JKDBModel/**/*.{h,m}",
        "Sources/CLUI/CLHUD/**/*.{h,m}"
      ],
      "dependencies": {
        "AFNetworking": [
          "~> 3.2.1"
        ],
        "pop": [
          "~> 1.0.12"
        ],
        "FMDB": [
          "~> 2.7.5"
        ],
        "SVProgressHUD": [
          "~> 2.1.2"
        ],
        "FTIndicator": [
          "~> 1.2.9"
        ]
      },
      "ios": {
        "frameworks": [
          "AVFoundation",
          "UIKit"
        ]
      }
    },
    {
      "name": "CLFaster",
      "source_files": "Sources/CLFaster/**/*.{h,m}",
      "ios": {
        "frameworks": "UIKit"
      }
    },
    {
      "name": "CLNetworking",
      "source_files": "Sources/CLNetworking/**/*.{h,m}",
      "dependencies": {
        "AFNetworking": [
          "~> 3.2.1"
        ]
      },
      "ios": {
        "frameworks": "AVFoundation"
      }
    },
    {
      "name": "CLPopAnimation",
      "source_files": "Sources/CLPopAnimation/**/*.{h,m}",
      "dependencies": {
        "pop": [
          "~> 1.0.12"
        ]
      },
      "ios": {
        "frameworks": "UIKit"
      }
    },
    {
      "name": "JKDBModel",
      "source_files": "Sources/JKDBModel/**/*.{h,m}",
      "dependencies": {
        "FMDB": [
          "~> 2.7.5"
        ]
      },
      "ios": {
        "frameworks": "AVFoundation"
      }
    },
    {
      "name": "CLHUD",
      "source_files": "Sources/CLUI/CLHUD/**/*.{h,m}",
      "dependencies": {
        "SVProgressHUD": [
          "~> 2.1.2"
        ],
        "FTIndicator": [
          "~> 1.2.9"
        ]
      },
      "ios": {
        "frameworks": [
          "AVFoundation",
          "UIKit"
        ]
      }
    }
  ]
}
