      "source_files": "XCBaseModule/Classes/Network/**/*"
    },
    {
      "name": "Tools",
      "source_files": "XCBaseModule/Classes/Tools/**/*",
      "dependencies": {
        "XCMacros": [
          "~> 1.0.2"
        ]
      }
