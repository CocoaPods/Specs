{
  "name": "pjsip-opus-ssl",
  "version": "2.6.0",
  "summary": "PJSIP with OPUS and OPENSSL Support for iOS",
  "description": "PJSIP 2.6 for iOS built with OpenSSL and Opus Codec support",
  "homepage": "https://github.com/georgepoenaru/pjsip-opus-ssl.git",
  "license": "MIT",
  "authors": {
    "George Poenaru": "george.poenaru@me.com"
  },
  "source": {
    "git": "https://github.com/georgepoenaru/pjsip-opus-ssl.git",
    "tag": "2.6.0"
  },
  "social_media_url": "https://keybase.io/georgepoenaru",
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": false,
  "preserve_paths": "include/**/**/*{h,hpp}",
  "vendored_libraries": "lib/*a",
  "frameworks": [
    "CFNetwork",
    "AudioToolbox",
    "AVFoundation"
  ],
  "header_mappings_dir": "include/",
  "module_name": "Pjsip",
  "xcconfig": {
    "GCC_PREPROCESSOR_DEFINITIONS": "PJ_AUTOCONF=1",
    "HEADER_SEARCH_PATHS": "$(inherited) $(PODS_ROOT)/pjsip-opus-ssl/include"
  }
}
