    "EventKit",
    "GLKit",
    "iAd",
    "ImageIO",
    "MediaPlayer",
    "MessageUI",
    "MobileCoreServices",
    "QuartzCore",
    "Security",
    "Social",
    "StoreKit",
    "SystemConfiguration",
    "WatchConnectivity",
    "JavaScriptCore"
