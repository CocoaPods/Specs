{
  "name": "YTLiveScenesVodSDK",
  "version": "1.0.7",
  "license": "MIT",
  "summary": "研途iOS封装保利威直播录播SDK",
  "homepage": "https://github.com/Boxzhi",
  "authors": {
    "HeZhizhi": "coderhzz@163.com"
  },
  "source": {
    "git": "https://gitee.com/kaoyanvip/livevodsdk.git",
    "tag": "1.0.7"
  },
  "platforms": {
    "ios": "11.0"
  },
  "frameworks": "UIKit",
  "requires_arc": true,
  "static_framework": true,
  "default_subspecs": "Phone",
  "dependencies": {
    "PLVLiveScenesSDK/Core": [
      "1.10.6"
    ],
    "PLVLiveScenesSDK/BaseSDK": [
      "1.10.6"
    ],
    "PLVFoundationSDK": [
      "1.10.6"
    ],
    "PLVImagePickerController": [
      "0.1.2"
    ],
    "SDWebImage": [
      "5.12.1"
    ],
    "MJRefresh": [
      "3.7.5"
    ],
    "PolyvVodSDK/Core": [
      "2.18.3"
    ],
    "PolyvVodSDK/Player": [
      "2.18.3"
    ],
    "SSZipArchive": [
      "2.1.5"
    ],
    "SVGAPlayer": [
      "2.5.7"
    ]
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "subspecs": [
    {
      "name": "Phone",
      "source_files": "YTLiveScenesVodSDK/**/*.{h,m}",
      "resources": "YTLiveScenesVodSDK/LiveSource/**/*.bundle",
      "dependencies": {
        "AgoraRtcEngine_iOS": [
          "3.6.0.1"
        ],
        "PLVSocketIOClientSwift": [
          "~> 0.1.0"
        ],
        "UCloudRtcSdk": [
          "1.9.2.9"
        ],
        "TXLiteAVSDK_Professional": [
          "8.3.9884"
        ],
        "PLVAliHttpDNS/AlicloudHttpDNS": [
          "1.10.0"
        ],
        "PLVModel": [
          "1.0.4"
        ],
        "WCDB": [
          "1.0.7.5"
        ]
      }
    },
    {
      "name": "Pad",
      "source_files": "YTLiveScenesVodSDK/**/*.{h,m}",
      "resources": "YTLiveScenesVodSDK/LiveSource/**/*.bundle",
      "dependencies": {
        "PLVLiveScenesSDK/OtherPart": [
          "1.10.6"
        ]
      }
    }
  ]
}
