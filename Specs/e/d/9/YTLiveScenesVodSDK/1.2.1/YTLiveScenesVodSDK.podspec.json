    "PLVImagePickerController": [
      "0.1.2"
    ],
    "SDWebImage": [
      "5.12.1"
    ],
    "MJRefresh": [
      "3.7.5"
    ],
    "PolyvVodSDK/Core": [
      "2.23.0"
    ],
    "PolyvVodSDK/Player": [
      "2.23.0"
    ],
    "SVGAPlayer": [
      "2.5.7"
    ],
    "Protobuf": [
      "3.22.4"
    ]
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
