{
  "name": "SimctlCLI",
  "version": "0.2.0",
  "summary": "Swift client-server tool to call xcrun simctl from your simulator. Automate push notification testing!",
  "description": "This is a small tool (SimctlCLI) and library (Simctl), written in Swift, to automate xcrun simctl commands for Simulator in unit and UI tests.\nIt enables, among other things, reliable fully automated testing of Push Notifications with dynamic content, and driven by a UI Test you control.",
  "homepage": "https://github.com/ctreffs/SwiftSimctl",
  "screenshots": "https://raw.githubusercontent.com/ctreffs/SwiftSimctl/master/docs/SimctlExample.gif",
  "authors": {
    "Christian Treffs": "ctreffs@gmail.com"
  },
  "social_media_url": "https://twitter.com/chrisdailygrind",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "source": {
    "git": "https://github.com/ctreffs/SwiftSimctl.git",
    "tag": "0.2.0"
  },
  "swift_versions": "5.2",
  "requires_arc": true,
  "exclude_files": ".swift-version",
  "preserve_paths": "bin/*",
  "resource_bundles": {
    "SimctlCLI": [
      "bin/SimctlCLI"
    ]
  },
  "source_files": "Sources/SimctlCLI/**/*.swift",
  "osx": {
    "dependencies": {
      "SimctlShared": [
        "~> 0.2.0"
      ],
      "ShellOut": [
        "~> 2.0.0"
      ],
      "Swifter": [
        "~> 1.4.7"
      ]
    },
    "frameworks": "AppKit"
  },
  "platforms": {
    "osx": "10.12"
  },
  "swift_version": "5.2"
}
