{
  "name": "mobile-ffmpeg-tvos-min",
  "version": "4.4.LTS",
  "summary": "Mobile FFmpeg tvOS Min Static Framework",
  "description": "Includes FFmpeg v4.4-dev-416 without any external libraries enabled.",
  "homepage": "https://github.com/tanersener/mobile-ffmpeg",
  "authors": {
    "Taner Sener": "tanersener@gmail.com"
  },
  "license": {
    "type": "LGPL-3.0",
    "file": "mobileffmpeg.framework/LICENSE"
  },
  "platforms": {
    "tvos": "9.2"
  },
  "requires_arc": true,
  "libraries": [
    "z",
    "bz2",
    "c++",
    "iconv"
  ],
  "source": {
    "http": "https://github.com/tanersener/mobile-ffmpeg/releases/download/v4.4.LTS/mobile-ffmpeg-min-4.4.LTS-tvos-framework.zip"
  },
  "tvos": {
    "frameworks": [
      "AudioToolbox",
      "CoreMedia"
    ],
    "vendored_frameworks": [
      "mobileffmpeg.framework",
      "libavcodec.framework",
      "libavdevice.framework",
      "libavfilter.framework",
      "libavformat.framework",
      "libavutil.framework",
      "libswresample.framework",
      "libswscale.framework"
    ]
  }
}
