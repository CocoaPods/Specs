{
  "name": "Bricks",
  "version": "0.1.0",
  "summary": "Instead of boilerplate, use bricks!",
  "description": "Pod with utility classes which provide the building blocks for create iOS apps.\nDont waste your time creating again or copying boilerplate from project to project.\nUse our building blocks for that, go bricks!",
  "homepage": "https://github.com/jonastomazalves/Bricks",
  "license": "MIT",
  "authors": {
    "Thiago Lioy": "thiago.lioy@gmail.com",
    "Jonas Tomaz": "jonastomazalves@gmail.com"
  },
  "source": {
    "git": "https://github.com/jonastomazalves/Bricks.git",
    "tag": "0.1.0"
  },
  "social_media_url": "https://twitter.com/@tplioy",
  "platforms": {
    "ios": "7.0"
  },
  "requires_arc": true,
  "source_files": "Pod/Classes/**/*",
  "resource_bundles": {
    "Bricks": [
      "Pod/Assets/*.png"
    ]
  },
  "public_header_files": "Pod/Classes/**/*.h",
  "dependencies": {
    "Mantle": [
      "~> 2.0.5"
    ]
  }
}
