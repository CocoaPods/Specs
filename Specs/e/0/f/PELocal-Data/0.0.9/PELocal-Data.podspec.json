    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES"
  },
  "dependencies": {
    "PEObjc-Commons": [
      "~> 1.0.101"
    ],
    "FMDB": [
      "~> 2.5"
    ],
    "PEHateoas-Client": [
      "~> 1.0.14"
    ],
    "CocoaLumberjack": [
      "~> 1.9"
    ]
