  ],
  "vendored_frameworks": [
    "MATMoatMobileAppKit.framework",
    "MobFoxSDKCore.embeddedframework/MobFoxSDKCore.framework"
  ],
  "preserve_paths": [
    "MATMoatMobileAppKit.framework",
    "MobFoxSDKCore.embeddedframework/MobFoxSDKCore.framework"
