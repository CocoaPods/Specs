{
  "name": "Map4dMapUtils",
  "version": "0.4.0",
  "summary": "A utilities library for use with Map4dMap SDK for iOS.",
  "description": "This library contains classes that are useful for a wide range of applications using the Map4dMap SDK for iOS.",
  "authors": "IOTLink",
  "homepage": "https://map4d.vn",
  "license": {
    "type": "Apache 2.0",
    "file": "LICENSE"
  },
  "source": {
    "git": "https://github.com/map4d/map4d-map-ios-utils.git",
    "tag": "0.4.0"
  },
  "platforms": {
    "ios": "9.3"
  },
  "requires_arc": true,
  "static_framework": true,
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "i386"
  },
  "private_header_files": "src/*/Private/*.h",
  "public_header_files": "src/**/*.h",
  "source_files": "src/**/*.{h,m,swift}",
  "dependencies": {
    "Map4dMap": [
      "~> 2.6"
    ]
  }
}
