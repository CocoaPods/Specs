{
  "name": "SVGAPlayerLite",
  "version": "1.0.2",
  "summary": "SVGAPlayerLite 是一个轻量级高性能的动画播放器",
  "description": "SVGAPlayerLite 是基于 SVGAPlayer 的轻量级版本；\nSVGA 是一种全新的动画格式，由 YY UED 团队主导开发；\nSVGA 让动画开发分工明确，大大减少动画交互的沟通成本，提升开发效率；\nSVGA 可以在 iOS / Android / Web / Flutter 实现高性能的动画播放。\nSVGAPlayerLite 提供了更轻量级的实现，优化了性能和内存占用。",
  "homepage": "https://github.com/jfyGiveMeFive/SVGAPlayer-Lite",
  "license": "Apache 2.0",
  "authors": {
    "jfyGiveMeFive": "aygtech@qq.com"
  },
  "platforms": {
    "ios": "12.0"
  },
  "source": {
    "git": "https://github.com/jfyGiveMeFive/SVGAPlayer-Lite.git",
    "tag": "1.0.2"
  },
  "default_subspecs": "Core",
  "subspecs": [
    {
      "name": "Core",
      "source_files": "Source/*.{h,m}",
      "requires_arc": true,
      "dependencies": {
        "SSZipArchive": [
          ">= 1.8.1"
        ],
        "SVGAPlayerLite/ProtoFiles": [

        ]
      },
      "libraries": "z",
      "frameworks": "AVFoundation"
    },
    {
      "name": "ProtoFiles",
      "source_files": "Source/pbobjc/*.{h,m}",
      "requires_arc": false,
      "dependencies": {
        "Protobuf": [
          "3.27.2"
        ]
      },
      "pod_target_xcconfig": {
        "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited) GPB_USE_PROTOBUF_FRAMEWORK_IMPORTS=1",
        "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES",
        "WARNING_CFLAGS": "-Wno-deprecated-declarations -Wno-implicit-function-declaration"
      },
      "compiler_flags": "-Wno-deprecated-declarations -Wno-implicit-function-declaration"
    }
  ]
}
