{
  "name": "ffmpeg-kit-macos-audio",
  "version": "5.1",
  "summary": "FFmpeg Kit macOS Audio Shared Framework",
  "description": "Includes FFmpeg with lame, libilbc, libvorbis, opencore-amr, opus, shine, soxr, speex, twolame and vo-amrwbenc libraries enabled.",
  "homepage": "https://github.com/arthenica/ffmpeg-kit",
  "authors": {
    "ARTHENICA": "open-source@arthenica.com"
  },
  "license": {
    "type": "LGPL-3.0",
    "file": "ffmpegkit.xcframework/macos-arm64_x86_64/ffmpegkit.framework/Resources/LICENSE"
  },
  "platforms": {
    "osx": "10.15"
  },
  "requires_arc": true,
  "libraries": [
    "z",
    "bz2",
    "c++",
    "iconv"
  ],
  "source": {
    "http": "https://github.com/arthenica/ffmpeg-kit/releases/download/v5.1/ffmpeg-kit-audio-5.1-macos-xcframework.zip"
  },
  "osx": {
    "frameworks": [
      "AudioToolbox",
      "AVFoundation",
      "CoreAudio",
      "CoreImage",
      "CoreMedia",
      "OpenCL",
      "OpenGL",
      "VideoToolbox"
    ],
    "vendored_frameworks": [
      "ffmpegkit.xcframework",
      "libavcodec.xcframework",
      "libavdevice.xcframework",
      "libavfilter.xcframework",
      "libavformat.xcframework",
      "libavutil.xcframework",
      "libswresample.xcframework",
      "libswscale.xcframework"
    ]
  }
}
