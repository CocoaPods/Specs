{
  "name": "JumpingUITextField",
  "version": "0.2.0",
  "summary": "Extension for UITextField written in swift 4.2",
  "description": "Adding the ability to jump between UITextFields withing groups by using the return-key on the soft-keyboard",
  "homepage": "https://github.com/AWeleczka/JumpingUITextField",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "AWeleczka": "github@accounts.aweleczka.de"
  },
  "source": {
    "git": "https://github.com/AWeleczka/JumpingUITextField.git",
    "tag": "0.2.0"
  },
  "social_media_url": "https://twitter.com/AWeleczka",
  "platforms": {
    "ios": "8.0"
  },
  "source_files": "JumpingUITextField/JumpingUITextField/**/*",
  "exclude_files": "JumpingUITextField/JumpingUITextField/*.plist",
  "subspecs": [
    {
      "name": "Localized",
      "source_files": "JumpingUITextField/LocalizedJumpingUITextField/**/*",
      "exclude_files": "JumpingUITextField/LocalizedJumpingUITextField/*.plist",
      "dependencies": {
        "LocalizableUIElements": [
          "~> 1.0"
        ]
      }
    }
  ]
}
