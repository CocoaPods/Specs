{
  "name": "DeepLinkNow",
  "version": "1.0.4",
  "summary": "DeepLinkNow iOS SDK for deep linking and attribution",
  "description": "DeepLinkNow is a powerful SDK for iOS that provides deep linking, \ndeferred deep linking, and attribution tracking capabilities. \nIt supports custom parameters, universal links, and clipboard detection.",
  "homepage": "https://github.com/deeplinknow/dln-ios",
  "license": {
    "type": "MIT",
    "file": "LICENSE.txt"
  },
  "authors": {
    "DeepLinkNow": "support@deeplink.now"
  },
  "platforms": {
    "ios": "13.0"
  },
  "swift_versions": [
    "5.0"
  ],
  "preserve_paths": "Example/*",
  "source": {
    "git": "https://github.com/deeplinknow/dln-ios.git",
    "tag": "1.0.4"
  },
  "source_files": "Sources/DeepLinkNow/**/*.swift",
  "exclude_files": "Classes/Exclude",
  "frameworks": [
    "UIKit",
    "Foundation",
    "CoreTelephony",
    "AdSupport"
  ],
  "static_framework": true,
  "pod_target_xcconfig": {
    "SWIFT_INCLUDE_PATHS": "$(PODS_TARGET_SRCROOT)/Sources",
    "OTHER_LDFLAGS": "-framework CoreTelephony -framework AdSupport"
  },
  "swift_version": "5.0"
}
