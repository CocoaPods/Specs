{
  "name": "libscryptpod",
  "version": "1.21.0",
  "summary": "A shared library that implements scrypt() functionality - a replacement for bcrypt()",
  "description": "A shared library that implements scrypt() functionality - a replacement for bcrypt()\nThis one sets :DEFINES_MODULE => \"YES\"",
  "homepage": "https://github.com/normanzb/libscrypt-podspec",
  "license": {
    "type": "BSD",
    "file": "libscrypt/LICENSE"
  },
  "authors": {
    "technion": "https://lolware.net"
  },
  "source": {
    "git": "https://github.com/normanzb/libscrypt-podspec.git",
    "tag": "v1.21.0"
  },
  "platforms": {
    "ios": "10.0"
  },
  "source_files": "libscrypt/*.{h,c}",
  "exclude_files": "libscrypt/main.c",
  "public_header_files": "libscrypt/libscrypt.h",
  "pod_target_xcconfig": {
    "GCC_OPTIMIZATION_LEVEL": "2",
    "DEFINES_MODULE": "YES"
  }
}
