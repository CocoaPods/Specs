{
  "name": "Notifiable",
  "version": "0.1.5",
  "platforms": {
    "ios": "8.0"
  },
  "summary": "Utility classes to integrate with Notifiable-Rails gem",
  "dependencies": {
    "AFNetworking": [
      "~> 3.0.4"
    ]
  },
  "ios": {
    "frameworks": "MobileCoreServices"
  },
