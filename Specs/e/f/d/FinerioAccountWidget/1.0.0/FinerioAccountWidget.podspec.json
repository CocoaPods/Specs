{
  "name": "FinerioAccountWidget",
  "version": "1.0.0",
  "swift_versions": "5.0",
  "summary": "Finerio Connect Widget",
  "homepage": "https://finerioconnect.com",
  "license": {
    "type": "GLP",
    "file": "LICENSE"
  },
  "authors": "Finerio Connect",
  "source": {
    "git": "https://github.com/Finerio-Connect/widget-ios.git",
    "tag": "1.0.0"
  },
  "default_subspecs": "Sources",
  "platforms": {
    "ios": "11.0"
  },
  "dependencies": {
    "Firebase/Analytics": [
      "~> 7.7.0"
    ],
    "Firebase/Database": [
      "~> 7.7.0"
    ],
    "SwiftyRSA": [
      "1.6.0"
    ],
    "lottie-ios": [
      "3.2.1"
    ]
  },
  "requires_arc": true,
  "pod_target_xcconfig": {
    "FRAMEWORK_SEARCH_PATHS": "$(inherited) $(PODS_ROOT)/**",
    "HEADER_SEARCH_PATHS": "$(inherited) $(PODS_ROOT)/**",
    "VALID_ARCHS": "armv7 arm64 x86_64",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "subspecs": [
    {
      "name": "Sources",
      "platforms": {
        "ios": "11.0"
      },
      "source_files": [
        "FinerioConnectWidget/**/**/**.{h,m,swift}"
      ],
      "resource_bundles": {
        "FinerioConnectWidget": [
          "FinerioConnectWidget/Resources/**/*.{lproj,json,png,xcassets,plist,strings}"
        ]
      }
    }
  ],
  "swift_version": "5.0"
}
