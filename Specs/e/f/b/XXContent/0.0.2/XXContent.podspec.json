{
  "name": "XXContent",
  "version": "0.0.2",
  "summary": "ContentInter的一个简单示范工程.",
  "description": "ContentInter的一个简单示范工程示范工程的长描述.",
  "homepage": "https://github.com/HfKates/XXContent",
  "license": "MIT",
  "authors": {
    "颜志炜": "yanzhiwei147@gmail.com"
  },
  "platforms": {
    "ios": "12.0"
  },
  "source": {
    "git": "https://github.com/HfKates/XXContent.git",
    "tag": "0.0.2"
  },
  "swift_versions": [
    "5.0"
  ],
  "user_target_xcconfig": {
    "LIBRARY_SEARCH_PATHS": "$(TOOLCHAIN_DIR)/usr/lib/swift/$(PLATFORM_NAME) $(TOOLCHAIN_DIR)/usr/lib/swift-5.0/$(PLATFORM_NAME)"
  },
  "pod_target_xcconfig": {
    "SWIFT_INCLUDE_PATHS": [
      "$(PODS_ROOT)/XXContent/src/oc",
      "$(PODS_TARGET_SRCROOT)/XXContent/src/oc"
    ]
  },
  "default_subspecs": "Core",
  "subspecs": [
    {
      "name": "Core",
      "dependencies": {
        "Masonry": [
          "~>1.1.0"
        ],
        "AFNetworking": [
          "~>4.0.1"
        ],
        "FMDB": [
          "~>2.7.5"
        ],
        "MBProgressHUD": [
          "~>1.2.0"
        ],
        "Toast": [
          "~>4.0.0"
        ]
      },
      "vendored_frameworks": "XXContent.framework",
      "resource_bundles": {
        "XXContent": [
          "XXContentBundle/*.{jpg,png,xib,strings,plist,bundle}"
        ]
      }
    }
  ],
  "swift_version": "5.0"
}
