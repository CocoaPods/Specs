{
  "name": "BoyaaDevice",
  "version": "1.1.0",
  "summary": "Device For IOS",
  "description": "BoyaaDevice ForIOS is an SDK developed by Boyaa Corporation to serve as the underlying data support.",
  "homepage": "https://266.com",
  "license": "MIT",
  "authors": {
    "Aaron": "aaronhu@boyaa.com"
  },
  "platforms": {
    "ios": "9.0"
  },
  "source": {
    "git": "https://code.266.com/aaronhu/boyaadeviceframework.git",
    "tag": "1.1.0"
  },
  "source_files": "BoyaaDevice/BoyaaDevice.framework/Headers/*.{h,m}",
  "vendored_frameworks": "BoyaaDevice/BoyaaDevice.framework",
  "public_header_files": "BoyaaDevice/BoyaaDevice.framework/Headers/device.h",
  "frameworks": [
    "WebKit",
    "CoreTelephony",
    "SystemConfiguration",
    "AVFoundation",
    "Foundation",
    "CoreLocation",
    "CoreGraphics",
    "CoreImage",
    "MediaPlayer",
    "CoreMotion",
    "CoreMedia",
    "Photos",
    "AudioToolbox",
    "ImageIO",
    "QuartzCore",
    "Security",
    "Contacts",
    "AddressBook",
    "UIKit",
    "EventKit",
    "MobileCoreServices",
    "UserNotifications"
  ],
  "resources": "BoyaaDevice/**/*.{bundle}",
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  }
}
