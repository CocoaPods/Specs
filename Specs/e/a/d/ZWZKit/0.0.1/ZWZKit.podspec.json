{
  "name": "ZWZKit",
  "version": "0.0.1",
  "summary": "ZWZKit.",
  "description": "此框架集合了开发中自定义的部分功能代码",
  "homepage": "https://github.com/zbwbb/ZWZKit.git",
  "license": "MIT",
  "authors": {
    "zbwbb": "zhoubinxinna@sina.com"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source": {
    "git": "https://github.com/zbwbb/ZWZKit.git",
    "tag": "0.0.1"
  },
  "source_files": [
    "Classes",
    "Classes/*.{h,m}"
  ],
  "exclude_files": "Classes/Exclude",
  "public_header_files": "Classes/ZWZKit.h",
  "subspecs": [
    {
      "name": "ZBFactory",
      "source_files": "Classes/ZBFactory/*.{h,m}",
      "public_header_files": "Classes/ZBFactory/ZBFactory.h"
    },
    {
      "name": "Frameworks",
      "vendored_frameworks": "Frameworks/ZWZFramework.framework"
    },
    {
      "name": "Libraries",
      "source_files": [
        "Libraries/ZBLibrary/libZBLibrary.a",
        "Libraries/ZBLibrary/ZBLibrary.h"
      ],
      "vendored_libraries": "Libraries/ZBLibrary/libZBLibrary.a"
    },
    {
      "name": "Resources",
      "resource_bundles": {
        "ZBBundle": [
          "Resources/ZBBundle.bundle"
        ]
      }
    }
  ]
}
