{
  "name": "SwiftyContacts",
  "version": "3.0.11",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "summary": "A Swift library for Contacts framework.",
  "homepage": "https://github.com/satishbabariya/SwiftyContacts",
  "authors": {
    "Satish Babariya": "satish.babariya@gmail.com"
  },
  "source": {
    "git": "https://github.com/satishbabariya/SwiftyContacts.git",
    "tag": "3.0.11"
  },
  "platforms": {
    "ios": "9.0",
    "osx": "10.12",
    "watchos": "3.0"
  },
  "requires_arc": true,
  "default_subspecs": "Core",
  "subspecs": [
    {
      "name": "Core",
      "source_files": "Sources/Core/*.swift",
      "frameworks": [
        "Foundation",
        "Contacts"
      ]
    },
    {
      "name": "RxSwift",
      "source_files": "Sources/RxSwift/*.swift",
      "frameworks": [
        "Foundation",
        "Contacts"
      ],
      "dependencies": {
        "RxSwift": [
          "~> 4.0"
        ]
      }
    }
  ]
}
