{
  "name": "Airwallex",
  "version": "5.2.1",
  "summary": "Integrate Airwallex into your iOS app",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "homepage": "https://www.airwallex.com/docs/#overview",
  "authors": {
    "Airwallex": "pa_mobile_sdk@airwallex.com"
  },
  "source": {
    "git": "https://github.com/airwallex/airwallex-payment-ios.git",
    "tag": "5.2.1"
  },
  "platforms": {
    "ios": "11.0"
  },
  "static_framework": true,
  "default_subspecs": [
    "Core",
    "WeChatPay",
    "Card",
    "Redirect",
    "ApplePay"
  ],
  "subspecs": [
    {
      "name": "Core",
      "source_files": "Airwallex/Core/Sources/**/*.{h,m}",
      "public_header_files": "Airwallex/Core/Sources/*.h",
      "resources": [
        "Airwallex/Core/Resources/AirwallexCore.bundle"
      ]
    },
    {
      "name": "WeChatPay",
      "dependencies": {
        "Airwallex/Core": [

        ],
        "WechatOpenSDK": [
          "1.9.7"
        ]
      },
      "source_files": "Airwallex/WeChatPay/**/*.{h,m}",
      "public_header_files": "Airwallex/WeChatPay/*.h",
      "pod_target_xcconfig": {
        "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
      },
      "user_target_xcconfig": {
        "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
      }
    },
    {
      "name": "Card",
      "dependencies": {
        "Airwallex/Core": [

        ]
      },
      "source_files": "Airwallex/Card/**/*.{h,m}",
      "public_header_files": "Airwallex/Card/*.h",
      "vendored_frameworks": "TrustDefender/*.xcframework"
    },
    {
      "name": "Redirect",
      "dependencies": {
        "Airwallex/Core": [

        ]
      },
      "source_files": "Airwallex/Redirect/**/*.{h,m}",
      "public_header_files": "Airwallex/Redirect/*.h"
    },
    {
      "name": "ApplePay",
      "dependencies": {
        "Airwallex/Core": [

        ]
      },
      "source_files": "Airwallex/ApplePay/**/*.{h,m}",
      "public_header_files": "Airwallex/ApplePay/*.h"
    }
  ]
}
