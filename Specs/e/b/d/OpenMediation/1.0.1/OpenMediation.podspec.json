    "type": "LGPL-3.0",
    "file": "LICENSE"
  },
  "authors": "AdTiming",
  "source": {
    "git": "https://github.com/AdTiming/OpenMediation-iOS.git",
    "tag": "1.0.1"
  },
  "module_map": "OpenMediation/OpenMediation.modulemap",
  "libraries": [
    "c++",
    "xml2",
    "z",
    "resolv"
  ],
