{
  "name": "SyneriseSDK",
  "version": "3.8.5",
  "summary": "SyneriseSDK v. 3.8.5",
  "description": "iOS library for Synerise omnichannel platform",
  "homepage": "https://synerise.com",
  "license": {
    "type": "MIT"
  },
  "authors": "Synerise",
  "platforms": {
    "ios": "9.0"
  },
  "source": {
    "git": "https://github.com/Synerise/ios-sdk.git",
    "tag": "3.8.5"
  },
  "exclude_files": [
    "SampleAppSwift/**"
  ],
  "frameworks": [
    "UIKit",
    "Foundation"
  ],
  "vendored_frameworks": "Pod/SyneriseSDK.framework",
  "requires_arc": true,
  "xcconfig": {
    "LIBRARY_SEARCH_PATHS": "\"$(PODS_ROOT)/SyneriseSDK\""
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  }
}
