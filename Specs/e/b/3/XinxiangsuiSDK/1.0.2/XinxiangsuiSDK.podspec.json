{
  "name": "XinxiangsuiSDK",
  "version": "1.0.2",
  "summary": "Bluetooth configuration Framework of Xinxiangsui device.",
  "description": "About the Bluetooth network configuration function of Xinxiangsui device.",
  "homepage": "https://github.com/wlz0610/XinxiangsuiSDK",
  "authors": {
    "wanglizhenios@163.com": "wanglizhenios@163.com"
  },
  "platforms": {
    "ios": "9.0"
  },
  "source": {
    "git": "https://github.com/wlz0610/XinxiangsuiSDK.git",
    "tag": "1.0.2"
  },
  "ios": {
    "vendored_frameworks": "EphoneSDK.framework"
  },
  "source_files": "EphoneSDK.framework/headers/*",
  "exclude_files": "EphoneSDK.framework/*.plist",
  "frameworks": "EphoneSDK",
  "pod_target_xcconfig": {
    "VALID_ARCHS": "x86_64 armv7 arm64"
  },
  "dependencies": {
    "AFNetworking": [
      "~> 4.0.1"
    ],
    "MJExtension": [
      "~> 3.2.4"
    ],
    "YYKit": [
      "~> 1.0.9"
    ],
    "MQTTClient": [
      "~> 0.15.3"
    ]
  }
}
