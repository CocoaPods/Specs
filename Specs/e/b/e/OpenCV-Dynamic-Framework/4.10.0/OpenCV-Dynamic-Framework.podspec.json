{
  "name": "OpenCV-Dynamic-Framework",
  "version": "4.10.0",
  "summary": "OpenCV dynamic framework",
  "description": "From https://github.com/opencv/opencv",
  "homepage": "https://github.com/eddy-lau/OpenCV-Dynamic-Framework.git",
  "license": {
    "type": "3-clause BSD",
    "file": "LICENSE"
  },
  "authors": "https://opencv.org/",
  "platforms": {
    "ios": "11.0"
  },
  "source": {
    "git": "https://github.com/eddy-lau/OpenCV-Dynamic-Framework.git",
    "tag": "4.10.0"
  },
  "prepare_command": "./extract.sh",
  "preserve_paths": "opencv2.xcframework",
  "vendored_frameworks": "opencv2.xcframework",
  "requires_arc": false,
  "ios": {
    "frameworks": [
      "AssetsLibrary",
      "AVFoundation",
      "CoreGraphics",
      "CoreMedia",
      "CoreVideo",
      "Foundation",
      "QuartzCore",
      "UIKit"
    ]
  },
  "libraries": "c++",
  "pod_target_xcconfig": {
    "CLANG_CXX_LANGUAGE_STANDARD": "c++17",
    "CLANG_CXX_LIBRARY": "libc++"
  }
}
