{
  "name": "TeadsAdMobAdapter",
  "version": "5.1.8",
  "summary": "AdMob Adapter for Teads' iOS SDK",
  "module_name": "TeadsAdMobAdapter",
  "description": "Use this adapter to include AdMob as a demand source in your mediation waterfall",
  "homepage": "https://github.com/teads/TeadsSDK-iOS",
  "documentation_url": "https://support.teads.tv/support/solutions/articles/36000314767-inread-google-ad-manager-and-admob-mediation",
  "license": {
    "type": "Copyright",
    "text": "Copyright Teads 2021"
  },
  "authors": {
    "Teads": "support-sdk@teads.tv"
  },
  "source": {
    "git": "https://github.com/teads/TeadsSDK-iOS.git",
    "tag": "5.1.8"
  },
  "platforms": {
    "ios": "10.0"
  },
  "static_framework": true,
  "requires_arc": true,
  "source_files": [
    "MediationAdapters/TeadsAdMobAdapter/**/*{.swift}",
    "MediationAdapters/Common/*{.swift}"
  ],
  "exclude_files": [
    "MediationAdapters/TeadsAdMobAdapter/Exports.swift"
  ],
  "dependencies": {
    "TeadsSDK": [
      "5.1.8"
    ],
    "Google-Mobile-Ads-SDK": [
      ">= 9.0.0"
    ]
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "i386"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "i386"
  }
}
