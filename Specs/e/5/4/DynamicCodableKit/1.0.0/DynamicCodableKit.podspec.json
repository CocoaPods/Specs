{
  "name": "DynamicCodableKit",
  "version": "1.0.0",
  "homepage": "https://github.com/SwiftyLab/DynamicCodableKit",
  "summary": "DynamicCodableKit helps you to implement dynamic JSON decoding.",
  "description": "DynamicCodableKit helps you to implement dynamic JSON decoding within the constraints of Swift's sound type system by working on top of Swift's Codable implementations.",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "documentation_url": "https://swiftylab.github.io/DynamicCodableKit/1.0.0/documentation/dynamiccodablekit/",
  "social_media_url": "https://twitter.com/soumya_mahunt",
  "source": {
    "git": "https://github.com/SwiftyLab/DynamicCodableKit.git",
    "tag": "v1.0.0"
  },
  "authors": {
    "Soumya Ranjan Mahunt": "soumya.mahunt@gmail.com"
  },
  "swift_versions": "5.0",
  "platforms": {
    "ios": "8.0",
    "osx": "10.10",
    "tvos": "9.0",
    "watchos": "2.0"
  },
  "source_files": [
    "Sources/DynamicCodableKit/**/*.swift",
    "Sources/DynamicCodableKit/*.docc"
  ],
  "preserve_paths": [
    "{Sources,Tests}/DynamicCodableKit*/**/*",
    "*.md"
  ],
  "pod_target_xcconfig": {
    "CLANG_WARN_DOCUMENTATION_COMMENTS": "YES",
    "RUN_DOCUMENTATION_COMPILER": "YES"
  },
  "testspecs": [
    {
      "name": "Tests",
      "test_type": "unit",
      "source_files": "Tests/DynamicCodableKitTests/**/*.swift",
      "resources": "Tests/DynamicCodableKitTests/**/*.json"
    }
  ],
  "swift_version": "5.0"
}
