{
  "name": "SecureStorage",
  "version": "0.1.0",
  "summary": "A simple utility to store objects securely on disk or in defaults",
  "description": "A simple library that allows applications to store Objective-C/Swift objects securely on disk or in user defaults.",
  "homepage": "https://github.com/Ad1991/SecureStorage",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Adarsh Rai": "adrai75@gmail.com"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source": {
    "git": "https://github.com/Ad1991/SecureStorage.git",
    "tag": "0.1.0"
  },
  "source_files": "SecureStorage/**/*.{h,m,swift,modulemap}",
  "requires_arc": true,
  "module_map": "SecureStorage/iphonesimulator.modulemap",
  "pod_target_xcconfig": {
    "MODULEMAP_FILE[sdk=iphoneos*]": "$(PODS_ROOT)/SecureStorage/iphoneos.modulemap",
    "MODULEMAP_FILE[sdk=iphonesimulator*]": "$(PODS_ROOT)/SecureStorage/iphonesimulator.modulemap"
  }
}
