  "homepage": "http://sourceforge.net/projects/hessiankit",
  "license": {
    "type": "Apache License, Version 2.0",
    "file": "LICENSE.TXT"
  },
  "authors": {
    "Fredrik Olsson": "peylow@gmail.com"
  },
  "source": {
