{
  "name": "MRMailSDK",
  "version": "1.4.3",
  "summary": "Library for Mail.Ru OAuth2 authorization",
  "platforms": {
    "ios": "9.0"
  },
  "source": {
    "git": "https://github.com/mailru/mail-auth-sdk-ios.git",
    "tag": "1.4.3"
  },
  "resource_bundles": {
    "MRMailSDKUI": [
      "mr-mail-sdk/sources/ui/MRMailSDKUI.xcassets",
      "mr-mail-sdk/sources/ui/ru.lproj/*.strings",
      "mr-mail-sdk/sources/ui/en.lproj/*.strings"
    ]
  },
  "frameworks": [
    "Foundation",
    "UIKit",
    "SafariServices",
    "WebKit"
  ],
  "requires_arc": true,
  "license": "None",
  "homepage": "https://github.com/mailru/mail-auth-sdk-ios",
  "authors": {
    "Aleksandr Karimov": "a.karimov@corp.mail.ru"
  },
  "default_subspecs": "Core",
  "subspecs": [
    {
      "name": "Core",
      "source_files": [
        "mr-mail-sdk/sources/*.{h,m}",
        "mr-mail-sdk/sources/core/**/*.{h,m}",
        "mr-mail-sdk/sources/ui/**/*.{h,m}",
        "mr-mail-sdk/sources/utils/**/*.{h,m}"
      ],
      "public_header_files": "mr-mail-sdk/sources/*.h"
    }
  ]
}
