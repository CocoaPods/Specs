{
  "name": "AppOrderFiles",
  "version": "0.0.1",
  "summary": "Generating order files for Mach-O using Clang.",
  "description": "The easiest way to generate order files for Mach-O using Clang. Improving App Performance.",
  "homepage": "https://github.com/yulingtianxia/AppOrderFiles",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "yulingtianxia": "yulingtianxia@gmail.com"
  },
  "social_media_url": "https://twitter.com/yulingtianxia",
  "source": {
    "git": "https://github.com/yulingtianxia/AppOrderFiles.git",
    "tag": "0.0.1"
  },
  "source_files": "AppOrderFilesSample/AppOrderFiles/*.{h,m}",
  "public_header_files": "AppOrderFilesSample/AppOrderFiles/*.h",
  "platforms": {
    "ios": "8.0",
    "osx": "10.8",
    "tvos": "9.0",
    "watchos": "2.0"
  },
  "requires_arc": true,
  "user_target_xcconfig": {
    "OTHER_CFLAGS": "-fsanitize-coverage=func,trace-pc-guard",
    "OTHER_SWIFT_FLAGS": "-sanitize-coverage=func -sanitize=undefined"
  }
}
