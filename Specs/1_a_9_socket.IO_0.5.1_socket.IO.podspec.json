  "homepage": "https://github.com/pkyeck/socket.IO-objc",
  "license": "MIT",
  "authors": {
    "Philipp Kyeck": "philipp@beta-interactive.de"
  },
  "source": {
    "git": "https://github.com/pkyeck/socket.IO-objc.git",
    "tag": "0.5.1"
  },
  "source_files": "*.{h,m}",
  "platforms": {
    "ios": "5.0",
    "osx": "10.8"
  },
  "requires_arc": true,
  "dependencies": {
    "SocketRocket": [
      "~> 0.2"
    ]
