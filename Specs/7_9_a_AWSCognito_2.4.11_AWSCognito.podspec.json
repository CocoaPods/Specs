  "homepage": "http://aws.amazon.com/mobile/sdk",
  "license": "Amazon Software License",
  "authors": {
    "Amazon Web Services": "amazonwebservices"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source": {
    "git": "https://github.com/aws/aws-sdk-ios.git",
    "tag": "2.4.11"
  },
  "requires_arc": true,
  "libraries": "sqlite3",
  "dependencies": {
