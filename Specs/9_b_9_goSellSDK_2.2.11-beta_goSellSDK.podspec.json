        "TapAdditionsKit/UIKit/UIView/KeyframeAnimationOptions": [
          ">= 1.3.3"
        ],
        "TapApplication": [
          ">= 1.0.7"
        ],
        "TapBundleLocalization": [
          ">= 1.0.3"
        ],
        "TapCardValidator": [
          ">= 1.2.6"
        ],
        "TapEditableView": [
          ">= 1.0.5"
        ],
        "TapFontsKit": [
          ">= 1.0.6"
