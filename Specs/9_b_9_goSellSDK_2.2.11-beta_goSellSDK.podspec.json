        "TapAdditionsKit/Foundation/Date": [
          ">= 1.3.3"
        ],
        "TapAdditionsKit/Foundation/Locale": [
          ">= 1.3.3"
        ],
        "TapAdditionsKit/Foundation/URL": [
          ">= 1.3.3"
        ],
        "TapAdditionsKit/SwiftStandartLibrary/Bool": [
          ">= 1.3.3"
        ],
        "TapAdditionsKit/SwiftStandartLibrary/Comparable": [
          ">= 1.3.3"
