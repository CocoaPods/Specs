  "homepage": "https://github.com/Jungle-Works/Hippo-iOS-SDK",
  "documentation_url": "https://github.com/Jungle-Works/Hippo-iOS-SDK",
  "license": {
    "type": "MIT",
    "file": "FILE_LICENSE"
  },
