{
  "name": "Youbora-YouboraLib",
  "version": "5.4.5",
  "summary": "Library required by Youbora plugins.",
  "description": "<<-DESC\n                    YouboraLib is a library created by Nice People at Work. It serves\n                    as the shared logic for all Youbora plugins and it also allows you\n                    to create your own plugins.\n                     DESC",
  "homepage": "http://developer.nicepeopleatwork.com/",
  "license": {
    "type": "MIT",
    "file": "LICENSE.md"
  },
  "authors": {
    "Nice People at Work": "support@nicepeopleatwork.com"
  },
  "platforms": {
    "ios": "8.0",
    "tvos": "9.0"
  },
  "source": {
    "git": "https://github.com/NicePeopleAtWork/Youbora-YouboraLib-iOS.git",
    "tag": "5.4.5"
  },
  "default_subspecs": "default",
  "requires_arc": true,
  "subspecs": [
    {
      "name": "default",
      "ios": {
        "vendored_frameworks": "ios/static/YouboraLib.framework"
      },
      "tvos": {
        "vendored_frameworks": "tvos/static/YouboraLibTvOS.framework"
      }
    },
    {
      "name": "dynamic",
      "ios": {
        "vendored_frameworks": "ios/dynamic/YouboraLib.framework"
      },
      "tvos": {
        "vendored_frameworks": "tvos/dynamic/YouboraLibTvOS.framework"
      }
    }
  ]
}
