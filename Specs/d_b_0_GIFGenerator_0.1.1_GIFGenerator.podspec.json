{
  "name": "GIFGenerator",
  "version": "0.1.1",
  "summary": "Generate animated GIF in iOS",
  "description": "Generates a GIF file and saves it to an specific URL\n- Set frame delays\n- Set loop count",
  "homepage": "https://github.com/eduardourso/GIFGenerator",
  "license": "MIT",
  "authors": {
    "Eduardo Urso": "eduardourso@gmail.com"
  },
  "source": {
    "git": "https://github.com/eduardourso/GIFGenerator.git",
    "tag": "0.1.1"
  },
  "social_media_url": "https://twitter.com/DuUrso",
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "source_files": "Pod/Classes/**/*",
  "resource_bundles": {
    "GIFGenerator": [
      "Pod/Assets/*.png"
