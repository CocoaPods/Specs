{
  "name": "Nuke",
  "version": "0.2.1",
  "summary": "Advanced Swift framework for loading and caching images",
  "homepage": "https://github.com/kean/Nuke",
  "license": "MIT",
  "authors": "Alexander Grebenyuk",
  "source": {
    "git": "https://github.com/kean/Nuke.git",
    "tag": "0.2.1"
  },
  "social_media_url": "https://twitter.com/a_grebenyuk",
  "platforms": {
    "ios": "8.0",
    "watchos": "2.0"
  },
  "requires_arc": true,
  "default_subspecs": [
    "Core",
    "UI"
  ],
  "subspecs": [
    {
      "name": "Core",
      "source_files": "Pod/Classes/Core/**/*"
    },
    {
      "name": "UI",
      "platforms": {
        "ios": "8.0"
      },
      "dependencies": {
        "Nuke/Core": [

        ]
      },
      "ios": {
        "source_files": "Pod/Classes/UI/**/*"
      }
    },
    {
      "name": "Alamofire",
      "dependencies": {
        "Nuke/Core": [

        ],
        "Alamofire": [
          "~> 2.0"
        ]
      },
      "source_files": "Pod/Classes/Alamofire/**/*"
    },
    {
      "name": "GIF",
      "platforms": {
        "ios": "8.0"
      },
      "dependencies": {
        "Nuke/Core": [

        ],
        "Nuke/UI": [

        ],
        "FLAnimatedImage": [
          "~> 1.0"
        ]
      },
      "source_files": "Pod/Classes/GIF/**/*"
    }
  ]
}
