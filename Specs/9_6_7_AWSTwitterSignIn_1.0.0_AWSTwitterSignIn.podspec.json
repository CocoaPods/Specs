{
  "name": "AWSTwitterSignIn",
  "version": "1.0.0",
  "summary": "Amazon Web Services Twitter Sign In SDK for iOS.",
  "description": "The AWS Twitter Sign In SDK for iOS provides a library, code samples, and documentation for developers to build connected mobile applications using AWS.",
  "homepage": "https://hatboysoftware.com",
  "license": "Apache License, Version 2.0",
  "authors": {
    "Matthew Alan Gray": "mgray@hatboysoftware.com"
  },
  "platforms": {
    "ios": "9.0"
  },
  "source": {
    "git": "https://github.com/hatboysoftware/AWSTwitterSignIn.git",
    "tag": "1.0.0"
  },
  "requires_arc": true,
  "dependencies": {
    "AWSAuthCore": [
      "2.6.1"
    ],
    "AWSAuthUI": [
      "2.6.1"
    ],
    "TwitterKit": [
      "3.1.1"
    ]
  },
  "preserve_paths": "AWSTwitterSignIn/**/*",
  "source_files": "AWSTwitterSignIn/**/*.{h,m}",
  "public_header_files": "AWSTwitterSignIn/*.h",
  "frameworks": "SafariServices",
  "resource_bundles": {
    "AWSTwitterSignIn": "AWSTwitterSignIn/Images.xcassets"
  },
  "pod_target_xcconfig": {
    "FRAMEWORK_SEARCH_PATHS": "$(inherited) $(SRCROOT)/**",
    "OTHER_LDFLAGS": "$(inherited) -undefined dynamic_lookup"
  }
}
