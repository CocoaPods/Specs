{
  "name": "tealium-swift",
  "version": "1.6.0",
  "summary": "Tealium Swift Integration Library",
  "description": "Supports Tealium's iQ and UDH suite of products on iOS, MacOS, tvOS and watchOS",
  "homepage": "https://github.com/Tealium/tealium-swift",
  "license": {
    "type": "Commercial",
    "file": "LICENSE.txt"
  },
  "authors": {
    "Tealium Inc.": "tealium@tealium.com",
    "craigrouse": "craig.rouse@tealium.com"
  },
  "social_media_url": "http://twitter.com/tealium",
  "swift_version": "4.0",
  "platforms": {
    "tvos": "9.0",
    "ios": "9.0",
    "osx": "10.11",
    "watchos": "3.0"
  },
  "source": {
    "git": "https://github.com/Tealium/tealium-swift.git",
    "tag": "1.6.0"
  },
  "default_subspecs": "Core",
  "subspecs": [
    {
      "name": "Core",
      "source_files": "tealium/**/*",
      "ios": {
        "exclude_files": "tealium/crash/*"
      },
      "tvos": {
        "exclude_files": [
          "tealium/tagmanagement/TealiumTagManagementModule.swift",
          "tealium/remotecommands/*",
          "tealium/attribution/*",
          "tealium/crash/*"
        ]
      },
      "watchos": {
        "exclude_files": [
          "tealium/tagmanagement/TealiumTagManagementModule.swift",
          "tealium/autotracking/*",
          "tealium/connectivity/*",
          "tealium/remotecommands/*",
          "tealium/attribution/*",
          "tealium/crash/*"
        ]
      },
      "osx": {
        "exclude_files": [
          "tealium/tagmanagement/TealiumTagManagementModule.swift",
          "tealium/autotracking/*",
          "tealium/remotecommands/*",
          "tealium/attribution/*",
          "tealium/crash/*"
        ]
      }
    },
    {
      "name": "Crash",
      "platforms": {
        "ios": "9.0"
      },
      "ios": {
        "source_files": "tealium/crash/*",
        "dependencies": {
          "tealium-swift/Core": [

          ],
          "TealiumCrashReporter": [

          ]
        }
      },
      "tvos": {
        "exclude_files": "tealium/crash/*"
      },
      "watchos": {
        "exclude_files": "tealium/crash/*"
      },
      "osx": {
        "exclude_files": "tealium/crash/*"
      }
    }
  ]
}
