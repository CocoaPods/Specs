{
  "name": "Firebase",
  "version": "3.2.1",
  "summary": "Firebase for iOS",
  "description": "Simplify your iOS development, grow your user base, and monetize more effectively with Firebase.",
  "homepage": "https://firebase.google.com",
  "license": {
    "text": "Copyright 2016 Google Inc.",
    "type": "Copyright"
  },
  "authors": "Google Inc.",
  "source": {
    "http": "https://www.gstatic.com/cpdc/a1097eff516aa303-Firebase-3.2.1.tar.gz"
  },
  "requires_arc": true,
  "platforms": {
    "ios": "7.0"
  },
  "default_subspecs": [
    "Core"
  ],
  "subspecs": [
    {
      "name": "Core",
      "dependencies": {
        "Firebase/Analytics": [
          "3.2.1"
        ]
      }
    },
    {
      "name": "AdMob",
      "dependencies": {
        "Google-Mobile-Ads-SDK": [
          "7.8.1"
        ],
        "Firebase/Analytics": [
          "3.2.1"
        ]
      }
    },
    {
      "name": "Analytics",
      "dependencies": {
        "FirebaseAnalytics": [
          "3.2.0"
        ]
      },
      "source_files": [
        "Headers/Firebase.h"
      ],
      "public_header_files": [
        "Headers/Firebase.h"
      ],
      "preserve_paths": [
        "Headers/module.modulemap"
      ],
      "user_target_xcconfig": {
        "HEADER_SEARCH_PATHS": "$(inherited) ${PODS_ROOT}/Firebase/Headers"
      }
    },
    {
      "name": "AppIndexing",
      "dependencies": {
        "FirebaseAppIndexing": [
          "1.0.3"
        ],
        "Firebase/Analytics": [
          "3.2.1"
        ]
      }
    },
    {
      "name": "Auth",
      "dependencies": {
        "FirebaseAuth": [
          "3.0.2"
        ],
        "Firebase/Analytics": [
          "3.2.1"
        ]
      }
    },
    {
      "name": "Crash",
      "dependencies": {
        "FirebaseCrash": [
          "1.0.6"
        ],
        "Firebase/Analytics": [
          "3.2.1"
        ]
      }
    },
    {
      "name": "Database",
      "dependencies": {
        "FirebaseDatabase": [
          "3.0.1"
        ],
        "Firebase/Analytics": [
          "3.2.1"
        ]
      }
    },
    {
      "name": "DynamicLinks",
      "dependencies": {
        "FirebaseDynamicLinks": [
          "1.0.3"
        ],
        "Firebase/Analytics": [
          "3.2.1"
        ]
      }
    },
    {
      "name": "Invites",
      "dependencies": {
        "FirebaseInvites": [
          "1.0.4"
        ],
        "Firebase/Analytics": [
          "3.2.1"
        ]
      }
    },
    {
      "name": "Messaging",
      "dependencies": {
        "FirebaseMessaging": [
          "1.1.0"
        ],
        "Firebase/Analytics": [
          "3.2.1"
        ]
      }
    },
    {
      "name": "RemoteConfig",
      "dependencies": {
        "FirebaseRemoteConfig": [
          "1.1.1"
        ],
        "Firebase/Analytics": [
          "3.2.1"
        ]
      }
    },
    {
      "name": "Storage",
      "dependencies": {
        "FirebaseStorage": [
          "1.0.1"
        ],
        "Firebase/Analytics": [
          "3.2.1"
        ]
      }
    }
  ]
}
