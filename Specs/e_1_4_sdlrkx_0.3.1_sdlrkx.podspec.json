    "file": "LICENSE"
  },
  "authors": {
    "James Kizer, Cornell Tech Foundry": "jdk288 at cornell dot edu"
  },
  "source": {
    "git": "https://github.com/cornelltech/sdl-rkx.git",
    "tag": "0.3.1"
  },
  "platforms": {
    "ios": "9.0"
  },
  "pushed_with_swift_version": "3.0",
  "subspecs": [
    {
      "name": "Core",
      "source_files": "source/Core/Classes/**/*",
      "resources": [
