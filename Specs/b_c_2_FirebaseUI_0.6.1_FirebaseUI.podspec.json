        "FirebaseUI/Database": [

        ],
        "FirebaseUI/Storage": [

        ],
        "FirebaseUI/Auth": [

        ],
        "FirebaseUI/Facebook": [

        ],
        "FirebaseUI/Google": [

        ],
        "FirebaseUI/Twitter": [

        ]
      }
    },
    {
      "name": "Database",
      "vendored_frameworks": [
        "FirebaseUIFrameworks/FirebaseDatabaseUI/Frameworks/FirebaseDatabaseUI.framework"
      ],
      "dependencies": {
        "Firebase/Database": [

        ]
      }
    },
    {
      "name": "Storage",
      "vendored_frameworks": [
        "FirebaseUIFrameworks/FirebaseStorageUI/Frameworks/FirebaseStorageUI.framework"
      ],
      "dependencies": {
        "Firebase/Storage": [

        ],
        "SDWebImage": [

        ]
      }
    },
    {
      "name": "Auth",
      "vendored_frameworks": [
        "FirebaseUIFrameworks/FirebaseAuthUI/Frameworks/FirebaseAuthUI.framework"
      ],
