  "homepage": "https://github.com/Carthage/ReactiveTask",
  "license": {
    "type": "MIT",
    "file": "LICENSE.md"
  },
  "authors": "Carthage",
  "platforms": {
    "osx": "10.9"
  },
  "source": {
    "git": "https://github.com/Carthage/ReactiveTask.git",
    "tag": "0.16.0"
  },
  "source_files": "Sources/*.{swift}",
  "dependencies": {
