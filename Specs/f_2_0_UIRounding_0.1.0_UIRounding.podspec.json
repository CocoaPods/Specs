{
  "name": "UIRounding",
  "version": "0.1.0",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "homepage": "https://github.com/jankase/UIRounding",
  "authors": {
    "Jan Kase": "jan.kase@jkdev.eu"
  },
  "summary": "Simple helper for perform pixel perfect rounding of CoreGraphics elements",
  "swift_versions": "5.0",
  "platforms": {
    "ios": "8.0",
    "osx": "10.10",
    "watchos": "2.0",
    "tvos": "9.0"
  },
  "source": {
    "git": "https://github.com/jankase/UIRounding.git",
    "tag": "0.1.0"
  },
  "ios": {
    "source_files": [
      "Common/*.swift",
      "Common/iOS_tvOS/**/*.swift",
      "UIRounding_iOS/**/*.swift"
    ],
    "frameworks": [
      "CoreGraphics",
      "Foundation",
      "UIKit"
    ]
  },
  "osx": {
    "source_files": [
      "Common/*.swift",
      "UIRounding_macOS/**/*.swift"
    ],
    "frameworks": [
      "CoreGraphics",
      "Foundation",
      "Cocoa"
    ]
  },
  "tvos": {
    "source_files": [
      "Common/*.swift",
      "Common/iOS_tvOS/**/*.swift",
      "UIRounding_tvOS/**/*.swift"
    ],
    "frameworks": [
      "CoreGraphics",
      "Foundation",
      "UIKit"
    ]
  },
  "watchos": {
    "source_files": [
      "Common/*.swift",
      "UIRounding_watchOS/**/*.swift"
    ],
    "frameworks": [
      "CoreGraphics",
      "Foundation",
      "WatchKit"
    ]
  },
  "swift_version": "5.0"
}
