      "source_files": "ZFPlayer/Classes/ijkplayer/*.{h,m}",
      "public_header_files": "ZFPlayer/Classes/ijkplayer/*.h",
      "dependencies": {
        "ZFPlayer/Core": [

        ],
        "IJKMediaFramework": [

        ]
