    "tag": "0.4.0"
  },
  "platforms": {
    "ios": "10.0"
  },
  "requires_arc": true,
  "source_files": "KSOWebKit/**/*.{h,m}",
  "exclude_files": "KSOWebKit/KSOWebKit-Info.h",
  "private_header_files": "KSOWebKit/Private/*.h",
  "resource_bundles": {
    "KSOWebKit": [
