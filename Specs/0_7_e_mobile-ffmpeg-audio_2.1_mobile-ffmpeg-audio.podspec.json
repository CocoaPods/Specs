{
  "name": "mobile-ffmpeg-audio",
  "version": "2.1",
  "summary": "Mobile FFmpeg Audio Dynamic Framework",
  "description": "Includes FFmpeg v4.0.2 with chromaprint v1.4.3, lame v3.100, libilbc v2.0.2, libvorbis v1.3.6, opencore-amr v0.1.5, opus v1.2.1, shine v3.1.1, soxr v0.1.3, speex v1.2.0 and wavpack v5.1.0 libraries enabled.",
  "homepage": "https://github.com/tanersener/mobile-ffmpeg",
  "authors": {
    "Taner Sener": "tanersener@gmail.com"
  },
  "license": {
    "type": "LGPL-3.0",
    "file": "mobileffmpeg.framework/LICENSE"
  },
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "ios": {
    "frameworks": [
      "Foundation",
      "AudioToolbox",
      "CoreImage"
    ],
    "libraries": [
      "z",
      "bz2"
    ],
    "vendored_frameworks": [
      "mobileffmpeg.framework",
      "libavcodec.framework",
      "libavdevice.framework",
      "libavfilter.framework",
      "libavformat.framework",
      "libavutil.framework",
      "libswresample.framework",
      "libswscale.framework"
    ]
  },
  "source": {
    "http": "https://github.com/tanersener/mobile-ffmpeg/releases/download/v2.1/mobile-ffmpeg-audio-2.1-ios-framework.zip"
  }
