{
  "name": "AlamofireRSS",
  "version": "0.1.0",
  "summary": "An RSS parser plugin for Alamofire.",
  "description": "An RSS parser plugin for Alamofire.  Adds a \"responseRSS()\" responseHandler to Alamofire.",
  "homepage": "https://github.com/AdeptusAstartes/AlamofireRSS",
  "license": "MIT",
  "authors": {
    "Don Angelillo": "dangelillo@gmail.com"
  },
  "source": {
    "git": "https://github.com/AdeptusAstartes/AlamofireRSS.git",
    "tag": "0.1.0"
  },
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "source_files": "Pod/Classes/**/*",
  "resource_bundles": {
    "AlamofireRSS": [
      "Pod/Assets/*.png"
    ]
  },
  "dependencies": {
    "Alamofire": [
      "~> 3.0"
    ]
  }
}
