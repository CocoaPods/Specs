{
  "name": "BrainCloudCpp",
  "version": "3.2.0",
  "summary": "The C++ client library for brainCloud",
  "homepage": "http://getbraincloud.com/",
  "platforms": {
    "ios": "7.0",
    "osx": "10.9",
    "tvos": "9.0",
    "watchos": "2.0"
  },
  "license": {
    "type": "Apache License, Version 2.0",
    "file": "LICENSE"
  },
  "authors": {
    "Brad Hill": "bradleyh@bitheads.com"
  },
  "social_media_url": "https://twitter.com/braincloudbaas",
  "source": {
    "git": "https://github.com/getbraincloud/braincloud-cpp.git",
    "tag": "3.2.0"
  },
  "header_mappings_dir": "include",
  "preserve_paths": [
    "include/*",
    "include/**/*"
  ],
  "public_header_files": [
    "include/braincloud/*.h",
    "include/braincloud/internal/*.h",
    "include/braincloud/internal/mac/*.h"
  ],
  "source_files": [
    "src/*.{c,cpp}",
    "src/mac/*.{c,cpp,mm}",
    "include/braincloud/*.h",
    "include/braincloud/internal/*.h",
    "include/braincloud/internal/mac/*.h"
  ],
  "xcconfig": {
    "USER_HEADER_SEARCH_PATHS": "\"${PODS_ROOT}/BrainCloudCpp/include\""
  },
  "libraries": [
    "c++",
    "z"
  ],
  "osx": {
    "frameworks": "LDAP"
  },
  "dependencies": {
    "SSKeychain": [

    ],
    "BrainCloudJsonCpp": [

    ]
  }
}
