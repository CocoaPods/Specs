    "MJExtension": [
      "~> 3.1.0"
    ],
    "Protobuf": [
      "~> 3.9.0"
    ],
    "CocoaAsyncSocket": [
      "~> 7.6.3"
    ],
    "Reachability": [
      "~> 3.2"
    ],
    "FMDB": [
      "~> 2.7.5"
    ]
