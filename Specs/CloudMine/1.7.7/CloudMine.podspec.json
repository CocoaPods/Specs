{
  "name": "CloudMine",
  "version": "1.7.7",
  "summary": "The iOS Framework for interacting with the CloudMine platform.",
  "homepage": "https://cloudmine.me/docs/#/ios",
  "license": "MIT",
  "authors": {
    "CloudMine": "support@cloudmine.me"
  },
  "platforms": {
    "ios": "6.0"
  },
  "source": {
    "git": "https://github.com/cloudmine/cloudmine-ios.git",
    "tag": "v1.7.7"
  },
  "source_files": "ios/ios/src/**/*.{h,m}",
  "exclude_files": [
    "NSString+UUID.h",
    "NSURL+QueryParameterAdditions.h",
    "CMObject+Private.h",
    "CMObjectClassNameRegistry.h"
  ],
  "frameworks": [
    "UIKit",
    "CoreGraphics",
    "MobileCoreServices",
    "SystemConfiguration",
    "CFNetwork",
    "Foundation",
    "CoreFoundation",
    "CoreLocation",
    "Social",
    "Accounts"
  ],
  "libraries": "z",
  "requires_arc": true,
  "xcconfig": {
    "OTHER_LDFLAGS": "-ObjC"
  },
  "dependencies": {
    "AFNetworking": [
      "~> 2.5.4"
    ],
    "MAObjCRuntime": [
      "~> 0.0.1"
    ]
  },
  "prefix_header_contents": [
    "#import <SystemConfiguration/SystemConfiguration.h>",
    "#import <MobileCoreServices/MobileCoreServices.h>"
  ]
}
