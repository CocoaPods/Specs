{
  "name": "QuickJS-iOS",
  "version": "0.0.1",
  "summary": "QuickJS iOS Bridge",
  "description": "QuickJS iOS Bridge, you can invoke objc api in javascript, and invoke javascript api in objc.",
  "homepage": "https://github.com/quickjs-ios/quickjs-ios",
  "license": "MIT",
  "authors": "Sam Chang",
  "platforms": {
    "ios": "10.0"
  },
  "source": {
    "git": "https://github.com/quickjs-ios/quickjs-ios.git",
    "tag": "0.0.1"
  },
  "source_files": [
    "QuickJS-iOS/Classes",
    "QuickJS-iOS/QuickJS_iOS.h",
    "quickjs-2019-07-21/quickjs.c",
    "quickjs-2019-07-21/bjson.c",
    "quickjs-2019-07-21/cutils.c",
    "quickjs-2019-07-21/libbf.c",
    "quickjs-2019-07-21/libregexp.c",
    "quickjs-2019-07-21/libunicode.c",
    "quickjs-2019-07-21/quickjs-libc.c",
    "quickjs-2019-07-21/*.h"
  ],
  "public_header_files": [
    "QuickJS-iOS/Classes/**/*.h",
    "QuickJS-iOS/QuickJS_iOS.h",
    "quickjs-2019-07-21/quickjs.h",
    "quickjs-2019-07-21/quickjs-libc.h",
    "quickjs-2019-07-21/quickjs-atom.h"
  ],
  "xcconfig": {
    "OTHER_CFLAGS": "    $(inherited) -DCONFIG_VERSION=\\\"2019-07-21\\\"\n"
  }
}
