  "homepage": "https://github.com/auth0/Lock-Facebook.iOS",
  "license": "MIT",
  "authors": {
    "Auth0": "support@auth0.com",
    "Hernan Zalazar": "hernan@auth0.com"
  },
  "source": {
    "git": "https://github.com/auth0/Lock-Facebook.iOS.git",
    "tag": "2.2.1"
  },
