{
  "name": "GDJson",
  "version": "0.5.0",
  "license": {
    "type": "Apache 2.0",
    "file": "LICENSE"
  },
  "summary": "Minimalistic JSON library for Java/Android/iOS/GWT/J2ObjC",
  "homepage": "https://github.com/goodow/realtime-json",
  "authors": {
    "Larry Tin": "dev@goodow.com"
  },
  "source": {
