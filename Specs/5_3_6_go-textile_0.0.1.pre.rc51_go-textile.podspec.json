{
  "name": "go-textile",
  "version": "0.0.1.pre.rc51",
  "summary": "Encrypted, recoverable, schema-based, cross-application data storage built on IPFS and LibP2P",
  "description": "Objective C framework and Protobuf files generated from go-textile. You should\nnot usually use this pod directly, but instead use ios-textile.",
  "homepage": "https://github.com/textileio/go-mobile",
  "license": "MIT",
  "authors": {
    "textile.io": "contact@textile.io"
  },
  "platforms": {
    "ios": "7.0"
  },
  "source": {
    "http": "https://github.com/textileio/go-textile/releases/download/v1.0.0-rc51/go-textile_v1.0.0-rc51_ios-framework.tar.gz"
  },
  "source_files": "protos",
  "vendored_frameworks": "Mobile.framework",
  "requires_arc": false,
  "dependencies": {
    "Protobuf": [
      "~> 3.7"
    ]
  },
  "pod_target_xcconfig": {
    "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited) GPB_USE_PROTOBUF_FRAMEWORK_IMPORTS=1"
  }
}
