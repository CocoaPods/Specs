  "authors": {
    "Aleksander Zubala": "alek@zubala.com"
  },
  "source": {
    "git": "https://github.com/azubala/OCMockitoSwift.git",
    "tag": "0.1.0"
  },
  "homepage": "https://github.com/azubala/OCMockitoSwift",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source_files": "Classes/**/*.{h,m,swift}",
  "ios": {
    "frameworks": [
      "Foundation",
      "CoreFoundation"
    ]
  },
  "dependencies": {
