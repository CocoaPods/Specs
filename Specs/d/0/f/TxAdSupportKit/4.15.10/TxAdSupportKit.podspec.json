{
  "name": "TxAdSupportKit",
  "version": "4.15.10",
  "summary": "TxAdSupportKit 是一个 CSJGromre 的集成 SDK。",
  "description": "TxAdSupportKit 是一个 SDK 广告类库，方便集成穿山甲、优量汇、快手等。",
  "homepage": "https://github.com/Bogon/TxAdSupportKit",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "bogonchar": "zhangqixcu@gmail.com"
  },
  "source": {
    "git": "https://github.com/Bogon/TxAdSupportKit.git",
    "tag": "4.15.10"
  },
  "weak_frameworks": [
    "AppTrackingTransparency",
    "CoreML",
    "DeviceCheck",
    "CoreHaptics"
  ],
  "static_framework": true,
  "platforms": {
    "ios": "14.0"
  },
  "swift_versions": "4.2",
  "source_files": "TxAdSupportKit/Classes/**/*",
  "vendored_frameworks": [
    "TxAdSupportKit/ADSupportSDKs/*.xcframework"
  ],
  "resource_bundles": {
    "TxAdSupportKit": [
      "TxAdSupportKit/Assets/PrivacyInfo.xcprivacy",
      "TxAdSupportKit/Assets/*.bundle"
    ]
  },
  "dependencies": {
    "GDTMobSDK": [
      "4.15.41"
    ],
    "KSAdSDK": [
      "4.6.30.1"
    ]
  },
  "swift_version": "4.2"
}
