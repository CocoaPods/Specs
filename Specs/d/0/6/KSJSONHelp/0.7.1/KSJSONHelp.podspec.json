      "source_files": "SQLite/Source/**/*.swift",
      "dependencies": {
        "KSJSONHelp/SQL": [

        ],
        "sqlite3": [

        ]
      },
      "xcconfig": {
        "OTHER_SWIFT_FLAGS": "$(inherited) -DSQLITE_SWIFT_STANDALONE"
      }
    }
  ]
}
