{
  "name": "JY_VoiceConvert",
  "version": "0.1.4",
  "summary": "iOS音频文件转换.",
  "description": "该库从腾讯云IM的项目抽取出来的，用于WAV、AMR音频格式转换.",
  "homepage": "https://github.com/ZYHB/JY_VoiceConvert",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "zhuyuhui434@gmail.com": "zhuyuhui434@gmail.com"
  },
  "source": {
    "git": "https://github.com/ZYHB/JY_VoiceConvert.git",
    "tag": "0.1.4"
  },
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "subspecs": [
    {
      "name": "Core",
      "public_header_files": "JY_VoiceConvert/Classes/Core/*.h",
      "source_files": "JY_VoiceConvert/Classes/Core/*.{h,m,mm}",
      "frameworks": [
        "Foundation",
        "AVFoundation"
      ],
      "libraries": [
        "c",
        "c++"
      ],
      "vendored_libraries": [
        "JY_VoiceConvert/Classes/lib/libopencore-amrnb.a",
        "JY_VoiceConvert/Classes/lib/libopencore-amrwb.a"
      ]
    }
  ]
}
