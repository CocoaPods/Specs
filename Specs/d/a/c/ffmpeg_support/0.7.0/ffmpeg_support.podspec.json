{
  "name": "ffmpeg_support",
  "version": "0.7.0",
  "summary": "this library is used to transcode video for iOS",
  "description": "'you can use this library to transcode video to another video encoding'",
  "homepage": "https://github.com/DDIsFriend/ffmpeg_support_public",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "DDIsFriend": "1749579978@qq.com"
  },
  "source": {
    "git": "https://github.com/DDIsFriend/ffmpeg_support_public.git",
    "tag": "0.7.0"
  },
  "platforms": {
    "ios": "9.0"
  },
  "source_files": "ffmpeg_support/Classes/**/*",
  "frameworks": [
    "AudioToolbox",
    "CoreAudio",
    "VideoToolbox",
    "CoreMedia",
    "AVFoundation"
  ],
  "libraries": [
    "bz2",
    "iconv",
    "z"
  ],
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "${PODS_TARGET_SRCROOT}/ffmpeg_support/Classes/include"
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  }
}
