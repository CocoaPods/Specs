{
  "name": "YSTRemoteController",
  "version": "0.1.9",
  "summary": "遥控器",
  "description": "电视遥控器",
  "homepage": "http://221.131.123.204:8181/ios-mobile/modules/YSTRemoteController",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "yisRookie": "caifeng@ysten.com"
  },
  "source": {
    "git": "http://221.131.123.204:8181/ios-mobile/modules/YSTRemoteController.git",
    "tag": "0.1.9"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source_files": "YSTRemoteController/Classes/**/*.{h,pch,m}",
  "prefix_header_file": "YSTRemoteController/Classes/PrivateClass/OtherFiles/YSTRemoteModule.pch",
  "public_header_files": "YSTRemoteController/Classes/PublicClass/**/*.h",
  "resource_bundles": {
    "YSTRemoteController": [
      "YSTRemoteController/Assets/Resource-JS/*.{bundle,json,xcassets,imageset,png}",
      "YSTRemoteController/Classes/**/*.{storyboard,xib,cer,json,plist}",
      "YSTRemoteController/Assets/MB-Resource/*.{png}",
      "YSTRemoteController/Assets/JSXib/*.{xib}",
      "YSTRemoteController/Assets/LocalChannelResource/*.{plist,png}",
      "YSTRemoteController/Assets/LocalChannelResource/ChannelLogos/*.{png}"
    ]
  },
  "xcconfig": {
    "ENABLE_BITCODE": "YES",
    "GCC_PRECOMPILE_PREFIX_HEADER": "YES",
    "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited)",
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES"
  },
  "pod_target_xcconfig": {
    "OTHER_LDFLAGS": "$(inherited)",
    "SWIFT_VERSION": "4.0",
    "HEADER_SEARCH_PATHS": "$(inherited) $(SDKROOT)/usr/include/libxml2 $(SDKROOT)/usr/include/libresolv $(inherited)$(PROJECT_DIR) /usr/local/include",
    "CLANG_CXX_LANGUAGE_STANDARD": "c++11",
    "CLANG_CXX_LIBRARY": "libc++"
  },
  "vendored_libraries": "YSTRemoteController/Classes/PrivateClass/ThirdPart/*.{a}",
  "frameworks": "UIKit",
  "dependencies": {
    "Masonry": [

    ],
    "MJExtension": [

    ],
    "MJRefresh": [

    ],
    "SDWebImage": [

    ],
    "MBProgressHUD": [

    ],
    "AvoidCrash": [
      "~> 2.3.0-beta"
    ],
    "YSTNetKit": [

    ],
    "YSTMSWorkKit": [

    ],
    "YSTThirdSDK": [

    ],
    "YSTCommonKit": [

    ],
    "MGJRouter": [

    ],
    "YSTMRLog": [

    ],
    "NewUserGuide": [

    ]
  }
}
