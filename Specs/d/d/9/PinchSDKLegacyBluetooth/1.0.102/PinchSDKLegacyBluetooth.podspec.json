    "PinchSDKLegacyBluetooth.framework",
    "PinchSDKLegacyBluetooth.framework.dSYM"
  ],
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "xcconfig": {
    "FRAMEWORK_SEARCH_PATHS": "\"$(PODS_ROOT)/PinchSDKLegacyBluetooth/**\""
  }
