  "public_header_files": [
    "Sources/RFAPI/RFAPI.h",
    "Sources/RFAPI/Define/*.h",
    "Sources/RFAPI/ModelTransformer/*.h",
    "Sources/RFAPI/Compatible/*.h"
  ],
  "pod_target_xcconfig": {
  }
