{
  "name": "Kite-Print-SDK",
  "version": "7.0.0",
  "license": {
    "type": "MIT",
    "file": "Kite-SDK/LICENSE"
  },
  "homepage": "https://github.com/OceanLabs/iOS-Print-SDK",
  "authors": {
    "Deon Botha": "deon@oceanlabs.co"
  },
  "summary": "Add physical print functionality in minutes! Print magnets, photo prints, postcards, polaroids, posters, stickers, canvas', phone cases, photo books, etc",
  "source": {
    "git": "https://github.com/OceanLabs/iOS-Print-SDK.git",
    "tag": "v7.0.0"
  },
  "requires_arc": true,
  "platforms": {
    "ios": "9.0"
  },
  "social_media_url": "https://twitter.com/dbotha",
  "module_name": "KiteSDK",
  "default_subspecs": "Core",
  "dependencies": {
    "Photobook": [

    ],
    "NXOAuth2Client": [

    ]
  },
  "subspecs": [
    {
      "name": "Core",
      "source_files": [
        "Kite-SDK/PSPrintSDK/OL*.{h,m}",
        "Kite-SDK/PSPrintSDK/RMImageCropper/*.{h,m}",
        "Kite-SDK/PSPrintSDK/Categories/*.{h,m}"
      ],
      "resources": [
        "Kite-SDK/PSPrintSDK/OLKiteLocalizationResources.bundle"
      ],
      "resource_bundles": {
        "OLKiteResources": [
          "Kite-SDK/PSPrintSDK/KitePrintSDK.xcassets",
          "Kite-SDK/PSPrintSDK/Base.lproj/OLEditingToolsView.xib",
          "Kite-SDK/PSPrintSDK/Base.lproj/OLHintView.xib",
          "Kite-SDK/PSPrintSDK/kite_corrupt.jpg",
          "Kite-SDK/PSPrintSDK/Base.lproj/OLKiteStoryboard.storyboard",
          "Kite-SDK/PSPrintSDK/mug.scn"
        ]
      }
    },
    {
      "name": "Lite",
      "source_files": [
        "Kite-SDK/PSPrintSDK/OL*.{h,m}",
        "Kite-SDK/PSPrintSDK/RMImageCropper/*.{h,m}",
        "Kite-SDK/PSPrintSDK/Categories/*.{h,m}"
      ],
      "resources": [
        "Kite-SDK/PSPrintSDK/OLKiteLocalizationResources.bundle"
      ],
      "resource_bundles": {
        "OLKiteResources": [
          "Kite-SDK/PSPrintSDK/KitePrintSDK.xcassets",
          "Kite-SDK/PSPrintSDK/Base.lproj/OLEditingToolsView.xib",
          "Kite-SDK/PSPrintSDK/Base.lproj/OLHintView.xib",
          "Kite-SDK/PSPrintSDK/kite_corrupt.jpg",
          "Kite-SDK/PSPrintSDK/Base.lproj/OLKiteStoryboard.storyboard"
        ]
      },
      "xcconfig": {
        "OTHER_CFLAGS": "$(inherited) -DOL_KITE_LITE"
      }
    }
  ]
}
