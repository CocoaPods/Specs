{
  "name": "VeloniumAdMobAdapter",
  "version": "1.0.2",
  "module_name": "VeloniumAdMobAdapter",
  "summary": "AdMob mediation adapter for VeloniumSDK",
  "description": "This adapter enables AdMob mediation support for VeloniumSDK.\nIt provides banner, interstitial, native, and rewarded ad adapters\nthat work with Google Mobile Ads SDK.",
  "homepage": "https://github.com/VeloniumLTD/veloniumsdk-ios-admob-adapter",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Velonium": "support@velonium.com"
  },
  "source": {
    "git": "https://github.com/VeloniumLTD/veloniumsdk-ios-admob-adapter.git",
    "tag": "1.0.2"
  },
  "platforms": {
    "ios": "15.6"
  },
  "swift_versions": "5.0",
  "static_framework": true,
  "dependencies": {
    "VeloniumSDK": [
      "1.0.2"
    ],
    "Google-Mobile-Ads-SDK": [
      "~> 12.0"
    ]
  },
  "source_files": "AdMobMediationAdapter/**/*.{swift,h,m}",
  "public_header_files": "AdMobMediationAdapter/**/*.h",
  "requires_arc": true,
  "pod_target_xcconfig": {
    "DEFINES_MODULE": "YES",
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES"
  },
  "swift_version": "5.0"
}
