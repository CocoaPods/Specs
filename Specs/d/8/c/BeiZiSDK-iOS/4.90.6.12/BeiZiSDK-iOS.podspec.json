{
  "name": "BeiZiSDK-iOS",
  "version": "4.90.6.12",
  "summary": "BeiZiSDK 广告平台, 用于请求广告的iOS SDK.",
  "description": "支持开屏, banner, 原生, 激励, 插屏, 原生插屏, 自定义类型广告请求。",
  "homepage": "https://gitee.com/adscope_admin/bei-zi-sdk-ios.git",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "lvjunxue": "lvjunxue@hubcloud.com.cn"
  },
  "source": {
    "git": "https://gitee.com/adscope_admin/bei-zi-sdk-ios.git",
    "tag": "4.90.6.12"
  },
  "platforms": {
    "ios": "11.0"
  },
  "libraries": [
    "z",
    "c++",
    "sqlite3",
    "xml2",
    "bz2",
    "c++abi",
    "resolv.9",
    "iconv",
    "c"
  ],
  "frameworks": [
    "AdSupport",
    "AVFoundation",
    "AVKit",
    "Accelerate",
    "AudioToolbox",
    "CoreData",
    "CoreLocation",
    "CoreMedia",
    "CoreMotion",
    "CoreGraphics",
    "CoreTelephony",
    "CoreImage",
    "CoreText",
    "Foundation",
    "MediaPlayer",
    "MessageUI",
    "MobileCoreServices",
    "MapKit",
    "QuartzCore",
    "QuickLook",
    "SystemConfiguration",
    "Security",
    "StoreKit",
    "WebKit",
    "UIKit",
    "ImageIO",
    "SafariServices",
    "JavaScriptCore"
  ],
  "weak_frameworks": "AppTrackingTransparency",
  "requires_arc": true,
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "xcconfig": {
    "OTHER_LDFLAGS": "-ObjC"
  },
  "subspecs": [
    {
      "name": "BeiZiSDK-iOS",
      "vendored_frameworks": [
        "BeiZiSDK.xcframework",
        "BeiZiFoundation.xcframework"
      ]
    }
  ]
}
