      "source_files": "Sources/CoreAPI/**/*.swift",
      "frameworks": [
        "Foundation",
        "UIKit",
        "CoreLocation"
      ],
      "dependencies": {
        "ShopGunSDK/Shared": [

        ],
        "CryptoSwift": [
          "~> 0.9.0"
        ]
      }
    },
    {
      "name": "GraphAPI",
      "source_files": "Sources/GraphAPI/**/*.swift",
      "frameworks": "Foundation",
      "dependencies": {
        "ShopGunSDK/Shared": [

        ]
      }
    },
    {
      "name": "EventsTracker",
      "source_files": "Sources/EventsTracker/**/*.swift",
      "frameworks": [
        "Foundation",
        "UIKit",
        "CoreLocation"
      ],
      "dependencies": {
        "ShopGunSDK/Shared": [

        ]
      }
    },
    {
      "name": "Shared",
      "source_files": "Sources/Shared/**/*.swift",
      "frameworks": [
        "Foundation",
        "UIKit"
      ],
      "dependencies": {
        "Valet": [
          "~> 3.1.3"
        ]
      }
    }
