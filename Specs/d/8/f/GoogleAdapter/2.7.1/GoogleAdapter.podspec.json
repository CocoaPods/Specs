{
  "name": "GoogleAdapter",
  "version": "2.7.1",
  "summary": "an adapter for Google SDK",
  "description": "an adapter for Google SDK for MSP S2S",
  "homepage": "https://github.com/aimsp/msp-ios-sdk/GoogleAdapter",
  "license": "Copyright",
  "authors": {
    "huanzhiNB": "huanzhi.zhang@newsbreak.com"
  },
  "source": {
    "http": "https://github.com/ParticleMedia/msp-ios-sdk-public/releases/download/2.7.1/GoogleAdapter-2.7.1.zip",
    "type": "zip"
  },
  "platforms": {
    "ios": "13.0"
  },
  "source_files": "GoogleAdapter/GoogleAdapter/**/*.{h,m,swift}",
  "exclude_files": "Classes/Exclude",
  "dependencies": {
    "Google-Mobile-Ads-SDK": [
      "~> 12.0"
    ],
    "MSPSharedLibraries": [
      "2.5.0"
    ]
  },
  "pod_target_xcconfig": {
    "VALID_ARCHS": "x86_64 armv7 arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "static_framework": true
}
