{
  "name": "AIExternalSDK",
  "version": "0.2.0",
  "summary": "管理第三方SDK，方便项目使用",
  "description": "管理第三方SDK，方便项目使用。鉴于各平台更新不及时。",
  "homepage": "http://git.oschina.net/aicmciisc",
  "license": "MIT",
  "authors": {
    "zhaoliang": "zhaol2014@foxmail.com"
  },
  "platforms": {
    "ios": "7.0"
  },
  "source": {
    "git": "https://git.oschina.net/aicmciisc/AIExternalSDK.git",
    "tag": "0.2.0"
  },
  "subspecs": [
    {
      "name": "AlipaySDK",
      "resources": "AlipaySDK/*.bundle",
      "vendored_frameworks": "AlipaySDK/AlipaySDK.framework",
      "frameworks": [
        "QuartzCore",
        "CoreGraphics",
        "CoreText",
        "CoreTelephony",
        "SystemConfiguration",
        "CoreMotion",
        "CFNetwork"
      ],
      "libraries": [
        "z",
        "c++"
      ]
    },
    {
      "name": "WeChatSDK",
      "source_files": "WeiXin/*.h",
      "frameworks": [
        "CoreTelephony",
        "CFNetwork",
        "SystemConfiguration"
      ],
      "libraries": [
        "c++",
        "sqlite3",
        "z"
      ],
      "vendored_libraries": "WeiXin/libWeChatSDK.a",
      "pod_target_xcconfig": {
        "OTHER_LDFLAGS": "-Objc -all_load"
      }
    },
    {
      "name": "WeiboSDK",
      "source_files": "SinaWeibo/*.{h,m}",
      "resources": "SinaWeibo/*.bundle",
      "vendored_libraries": "SinaWeibo/libWeiboSDK.a",
      "frameworks": [
        "ImageIO",
        "SystemConfiguration",
        "CoreText",
        "QuartzCore",
        "Security",
        "UIKit",
        "Foundation",
        "CoreGraphics",
        "CoreTelephony"
      ],
      "libraries": [
        "sqlite3",
        "z"
      ]
    },
    {
      "name": "TencentOpenAPI",
      "resources": "TencentOpenAPI/*.bundle",
      "vendored_frameworks": "TencentOpenAPI/TencentOpenAPI.framework",
      "frameworks": [
        "CoreTelephony",
        "SystemConfiguration"
      ],
      "libraries": [
        "z",
        "c++",
        "sqlite3"
      ],
      "requires_arc": true
    }
  ]
}
