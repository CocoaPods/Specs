  "pod_target_xcconfig": {
    "OTHER_LDFLAGS": "-lObjC"
  },
  "dependencies": {
    "HexColors": [
      "4.0.0"
    ],
    "MJRefresh": [
      "3.4.3"
    ],
    "MJExtension": [
      "3.2.2"
    ],
    "Masonry": [
      "1.1.0"
    ],
    "ReactiveObjC": [
      "3.1.1"
    ],
    "SVProgressHUD": [
      "2.2.5"
    ],
    "CYLTabBarController": [
      "1.28.3"
    ],
    "JXCategoryView": [
      "1.5.5"
    ]
  },
  "subspecs": [
    {
      "name": "DKConfigure",
      "source_files": "DKProject/Classes/DKConfigure/*.{h,m}"
    },
    {
      "name": "DKNaviFixSpace",
      "source_files": "DKProject/Classes/DKNaviFixSpace/*.{h,m}"
    },
    {
      "name": "DKNetworkingTool",
      "source_files": "DKProject/Classes/DKNetworkingTool/*.{h,m}",
      "dependencies": {
        "MJExtension": [
          "3.2.2"
        ],
        "SVProgressHUD": [
          "2.2.5"
        ],
        "AFNetworking": [
          "3.2.1"
        ],
        "DKProject/DKConfigure": [

        ]
      }
    }
  ]
}
