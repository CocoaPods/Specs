{
  "name": "FlutterAdsCore",
  "version": "1.3.1",
  "summary": "A Flutter Ads plugin core library for iOS.",
  "description": "A Flutter Ads plugin core library for iOS.",
  "homepage": "https://flutterads.top/",
  "license": {
    "type": "Apache-2.0",
    "file": "LICENSE"
  },
  "authors": {
    "FlutterAds": "https://flutterads.top/"
  },
  "source": {
    "git": "https://github.com/FlutterAds/flutterads-core-ios.git",
    "tag": "v1.3.1"
  },
  "platforms": {
    "ios": "11.0"
  },
  "requires_arc": true,
  "static_framework": true,
  "vendored_frameworks": "FlutterAdsCore.xcframework",
  "dependencies": {
    "Ads-CN/BUAdSDK": [
      "7.2.1.5"
    ],
    "Ads-CN/CSJMediation-Only": [
      "7.2.1.5"
    ],
    "Ads-CN/BUAdTestMeasurement": [
      "7.2.1.5"
    ]
  },
  "configuration_pod_whitelist": {
    "Ads-CN/BUAdTestMeasurement": [
      "debug"
    ]
  },
  "frameworks": "Foundation",
  "pod_target_xcconfig": {
    "DEFINES_MODULE": "YES",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "i386",
    "VALID_ARCHS": "arm64 arm64e x86_64",
    "OTHER_LDFLAGS": "-ObjC",
    "HEADER_SEARCH_PATHS": "\"${PODS_ROOT}/Headers/Public/FlutterAdsCore\"",
    "PUBLIC_HEADERS_FOLDER_PATH": "${PRODUCT_NAME}.framework/Headers"
  }
}
