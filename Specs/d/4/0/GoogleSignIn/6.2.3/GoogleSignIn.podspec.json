{
  "name": "GoogleSignIn",
  "version": "6.2.3",
  "summary": "Enables iOS apps to sign in with Google.",
  "description": "The Google Sign-In SDK allows users to sign in with their Google account from third-party apps.",
  "homepage": "https://developers.google.com/identity/sign-in/ios/",
  "license": {
    "type": "Apache",
    "file": "LICENSE"
  },
  "authors": "Google LLC",
  "source": {
    "git": "https://github.com/google/GoogleSignIn-iOS.git",
    "tag": "6.2.3"
  },
  "platforms": {
    "ios": "9.0",
    "osx": "10.15"
  },
  "prefix_header_file": false,
  "source_files": [
    "GoogleSignIn/Sources/**/*.[mh]"
  ],
  "public_header_files": [
    "GoogleSignIn/Sources/Public/GoogleSignIn/*.h"
  ],
  "frameworks": [
    "CoreGraphics",
    "CoreText",
    "Foundation",
    "LocalAuthentication",
    "Security"
  ],
  "ios": {
    "frameworks": "UIKit"
  },
  "osx": {
    "frameworks": "AppKit"
  },
  "dependencies": {
    "AppAuth": [
      "~> 1.5"
    ],
    "GTMAppAuth": [
      "~> 1.3"
    ],
    "GTMSessionFetcher/Core": [
      "~> 1.1"
    ]
  },
  "resource_bundles": {
    "GoogleSignIn": [
      "GoogleSignIn/Sources/{Resources,Strings}/*"
    ]
  },
  "pod_target_xcconfig": {
    "GCC_PREPROCESSOR_DEFINITIONS": "GID_SDK_VERSION=6.2.3",
    "HEADER_SEARCH_PATHS": "\"${PODS_TARGET_SRCROOT}\"",
    "DEFINES_MODULE": "YES",
    "COMBINE_HIDPI_IMAGES": "NO"
  },
  "testspecs": [
    {
      "name": "unit",
      "test_type": "unit",
      "platforms": {
        "ios": "9.0",
        "osx": "10.15"
      },
      "source_files": [
        "GoogleSignIn/Tests/Unit/**/*.[mh]"
      ],
      "requires_app_host": true,
      "dependencies": {
        "OCMock": [

        ],
        "GoogleUtilities/MethodSwizzler": [
          "~> 7.2"
        ],
        "GoogleUtilities/SwizzlerTestHelpers": [
          "~> 7.2"
        ]
      }
    }
  ]
}
