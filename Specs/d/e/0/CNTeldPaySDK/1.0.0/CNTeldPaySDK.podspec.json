{
  "name": "CNTeldPaySDK",
  "version": "1.0.0",
  "summary": "支付中心",
  "homepage": "https://github.com/mrLIHUAN/CNTeldPaySDK",
  "license": "MIT",
  "authors": {
    "mrLIHUAN": "1139116598@qq.com"
  },
  "source": {
    "git": "https://github.com/mrLIHUAN/CNTeldPaySDK.git",
    "tag": "1.0.0"
  },
  "requires_arc": true,
  "default_subspecs": [
    "AliPay15.1.6",
    "SDK1.6.2",
    "pic",
    "Code"
  ],
  "platforms": {
    "ios": "8.0"
  },
  "frameworks": [
    "SystemConfiguration",
    "CoreGraphics",
    "UIKit",
    "CoreTelephony",
    "CoreText",
    "CoreMotion",
    "QuartzCore",
    "CFNetwork",
    "JavaScriptCore"
  ],
  "libraries": [
    "z",
    "c++"
  ],
  "subspecs": [
    {
      "name": "Code",
      "source_files": "CNTeldPaySDK/*.h",
      "public_header_files": "CNTeldPaySDK/*.h",
      "vendored_libraries": "CNTeldPaySDK/*.a"
    },
    {
      "name": "AliPay15.1.6",
      "ios": {
        "vendored_frameworks": "CNTeldPaySDK/AliPay15.1.6/AlipaySDK.framework"
      },
      "resources": "CNTeldPaySDK/AliPay15.1.6/AlipaySDK.bundle"
    },
    {
      "name": "SDK1.6.2",
      "resources": "CNTeldPaySDK/SDK1.6.2/*.*"
    },
    {
      "name": "pic",
      "resources": "CNTeldPaySDK/pic/*.*"
    }
  ]
}
