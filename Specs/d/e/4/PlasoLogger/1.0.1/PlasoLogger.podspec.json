  },
  "authors": {
    "Plaso Dev Team": "dev.ios@plaso.me"
  },
  "platforms": {
    "ios": "9.0"
  },
  "source": {
    "http": "https://wwwr.plaso.cn/static/sdk/ios/PlasoLogger/1.0.1/PlasoLogger.zip"
  },
  "ios": {
    "vendored_frameworks": "PlasoLogger.xcframework"
  },
  "requires_arc": true,
  "pod_target_xcconfig": {
    "VALID_ARCHS": "x86_64 armv7 arm64"
  },
