        }
      },
      "dependencies": {
        "Airship/Core": [

        ]
      }
    },
    {
      "name": "PreferenceCenter",
      "ios": {
        "source_files": "Airship/AirshipPreferenceCenter/Source/**/*.{h,m,swift}",
        "exclude_files": "Airship/AirshipPreferenceCenter/Source/AirshipPreferenceCenter.h"
      },
      "dependencies": {
        "Airship/Core": [

        ]
      }
