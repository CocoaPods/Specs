{
  "name": "THTools",
  "version": "0.0.2",
  "summary": "OC常用工具",
  "description": "一些开发中常用的工具类",
  "homepage": "https://github.com/tuhaoGavin/THTools",
  "license": "MIT",
  "authors": {
    "涂浩": "474655014@qq.com"
  },
  "source": {
    "git": "https://github.com/tuhaoGavin/THTools.git",
    "tag": "0.0.2"
  },
  "platforms": {
    "ios": "10.0"
  },
  "source_files": "THTools/*",
  "subspecs": [
    {
      "name": "Base",
      "source_files": "THTools/Base/*"
    },
    {
      "name": "Alert",
      "source_files": "THTools/Alert/*.{h,m}",
      "dependencies": {
        "Masonry": [

        ]
      }
    },
    {
      "name": "Category",
      "source_files": "THTools/Category/**/*"
    },
    {
      "name": "Macro",
      "source_files": "THTools/Macro/*.{h}"
    },
    {
      "name": "Networking",
      "source_files": "THTools/Networking/*.{h,m}",
      "dependencies": {
        "AFNetworking": [
          "~> 4.0"
        ]
      }
    },
    {
      "name": "Notification",
      "source_files": "THTools/Notification/*.{h,m}"
    },
    {
      "name": "Utilities",
      "source_files": "THTools/Utilities/*.{h,m}"
    },
    {
      "name": "Settings",
      "source_files": [
        "THTools/Settings/*.{h,m}",
        "THTools/Category/**/*"
      ],
      "resources": "THTools/Settings/Resource.bundle",
      "dependencies": {
        "Masonry": [

        ]
      }
    }
  ]
}
