  "requires_arc": true,
  "platforms": {
    "ios": "12.0"
  },
  "default_subspecs": [
    "LiveScenes",
    "Foundation",
    "Business"
  ],
  "pod_target_xcconfig": {
    "VALID_ARCHS": "x86_64 arm64"
  },
  "subspecs": [
    {
      "name": "LiveScenes",
      "vendored_frameworks": [
