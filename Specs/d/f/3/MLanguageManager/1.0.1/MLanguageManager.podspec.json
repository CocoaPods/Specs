{
  "name": "MLanguageManager",
  "version": "1.0.1",
  "summary": "专业级的 iOS 全球多语言国际化 SDK",
  "description": "专业级的 iOS 多语言国际化 SDK，支持全球 140+ 种语言动态切换。\n无需依赖系统 .lproj 文件，可配置启用指定语言，完美适配复杂多变的国际化需求。\n轻量级、高性能，助力应用快速实现全球化部署。",
  "homepage": "https://github.com/mirco1990/MLanguageManager",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "mirco1990": "your_email@example.com"
  },
  "platforms": {
    "ios": "12.0"
  },
  "source": {
    "git": "https://github.com/mirco1990/MLanguageManager.git",
    "tag": "1.0.1"
  },
  "source_files": "MLanguageManager.framework/Headers/*.h",
  "vendored_frameworks": "MLanguageManager.framework",
  "frameworks": [
    "Foundation",
    "UIKit"
  ],
  "requires_arc": true,
  "pod_target_xcconfig": {
    "ARCHS[sdk=iphoneos*]": "arm64",
    "ARCHS[sdk=iphonesimulator*]": "arm64",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "x86_64 i386",
    "VALID_ARCHS": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "x86_64 i386",
    "ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "ios": {
    "vendored_frameworks": "MLanguageManager.framework"
  }
}
