{
  "name": "MSPAmazonAdapter",
  "summary": "an adapter for Amazon SDK",
  "description": "an adapter for Amazon SDK for MSP S2S",
  "homepage": "https://github.com/aimsp/msp-ios-sdk/AmazonAdapter",
  "license": "Copyright",
  "authors": {
    "huanzhiNB": "huanzhi.zhang@newsbreak.com"
  },
  "platforms": {
    "ios": "15.0"
  },
  "swift_versions": "5.0",
  "requires_arc": true,
  "static_framework": true,
  "pod_target_xcconfig": {
    "BUILD_LIBRARY_FOR_DISTRIBUTION": "NO",
    "SWIFT_EMIT_MODULE_INTERFACE": "NO",
    "SWIFT_INSTALL_MODULE_FOR_DEPLOYMENT": "NO",
    "DEFINES_MODULE": "YES",
    "FRAMEWORK_SEARCH_PATHS": "$(inherited) $(PODS_ROOT)/../Build/XCFrameworks",
    "SWIFT_INCLUDE_PATHS": "$(inherited) $(PODS_ROOT)/../Build/XCFrameworks"
  },
  "user_target_xcconfig": {
    "FRAMEWORK_SEARCH_PATHS": "$(inherited) $(PODS_ROOT)/../Build/XCFrameworks",
    "SWIFT_INCLUDE_PATHS": "$(inherited) $(PODS_ROOT)/../Build/XCFrameworks"
  },
  "dependencies": {
    "MSPGoogleAdsTypes": [
      "1.0.0-rc.15"
    ],
    "AmazonPublisherServicesSDK": [
      "4.5.5"
    ],
    "AmazonPublisherServicesAdMobAdapter": [
      "2.2.0"
    ],
    "MSPSharedLibraries": [
      "1.0.0-rc.15"
    ],
    "MSPiOSCore": [
      "1.0.0-rc.15"
    ]
  },
  "version": "1.0.0-rc.15",
  "source": {
    "http": "https://github.com/ParticleMedia/msp-ios-sdk-public/releases/download/1.0.0-rc.15/MSPAmazonAdapter-1.0.0-rc.15.zip",
    "type": "zip",
    "sha256": "2075355b54dc7f702227743b936de652ca1fb9d4fc98cb1c31b12570d59611bd"
  },
  "vendored_frameworks": "Binary/MSPAmazonAdapter.xcframework",
  "swift_version": "5.0"
}
