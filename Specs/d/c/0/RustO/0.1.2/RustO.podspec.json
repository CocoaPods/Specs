{
  "name": "RustO",
  "version": "0.1.2",
  "summary": "RustO! - Pure Rust OCR library for iOS",
  "description": "RustO! is a high-performance OCR (Optical Character Recognition) library \nwritten in pure Rust, based on RapidOCR and powered by PaddleOCR models \nwith ONNX Runtime inference. This pod provides Swift bindings for iOS.",
  "homepage": "https://github.com/byrizki/rusto-rs",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "RustO Contributors": "support@rusto.dev"
  },
  "source": {
    "http": "https://github.com/byrizki/rusto-rs/releases/download/v0.1.2/RustO.xcframework.zip"
  },
  "platforms": {
    "ios": "12.0"
  },
  "swift_versions": "5.0",
  "source_files": "src/**/*.swift",
  "vendored_frameworks": "RustO.xcframework",
  "resources": "Resources/**/*.{mnn,txt}",
  "resource_bundles": {
    "RustOModels": [
      "Resources/**/*.{mnn,txt}"
    ]
  },
  "frameworks": "Foundation",
  "pod_target_xcconfig": {
    "ENABLE_BITCODE": "NO"
  },
  "swift_version": "5.0"
}
