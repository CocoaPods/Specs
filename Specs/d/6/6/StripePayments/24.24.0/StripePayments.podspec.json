{
  "name": "StripePayments",
  "version": "24.24.0",
  "summary": "Bindings for the Stripe Payments API.",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "homepage": "https://stripe.com/docs/mobile/ios",
  "readme": "StripePayments/README.md",
  "authors": {
    "Stripe": "support+github@stripe.com"
  },
  "source": {
