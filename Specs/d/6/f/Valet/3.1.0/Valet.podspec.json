{
  "name": "Valet",
  "version": "3.1.0",
  "license": "Apache License, Version 2.0",
  "summary": "Valet lets you securely store data in the iOS or OS X Keychain without knowing a thing about how the Keychain works. It's easy. We promise.",
  "homepage": "https://github.com/square/Valet",
  "authors": "Square",
  "source": {
    "git": "https://github.com/square/Valet.git",
    "tag": "3.1.0"
  },
  "source_files": "Sources/**/*.{swift,h}",
  "public_header_files": "Sources/*.h",
  "frameworks": "Security",
  "platforms": {
    "ios": "9.0",
    "tvos": "9.0",
    "osx": "10.11"
  },
  "testspecs": [
    {
      "name": "Tests",
      "test_type": "unit",
      "ios": {
        "requires_app_host": true,
        "source_files": "Tests/**/*.{h,m,swift}",
        "exclude_files": "Tests/MacTests.swift"
      },
      "tvos": {
        "requires_app_host": true,
        "source_files": "Tests/**/*.{h,m,swift}",
        "exclude_files": [
          "Tests/MacTests.swift",
          "Tests/*BackwardsCompatibilityTests.swift"
        ]
      },
      "osx": {
        "source_files": "Tests/**/*.{h,m,swift}"
      },
      "pod_target_xcconfig": {
        "SWIFT_OBJC_BRIDGING_HEADER": "${PODS_TARGET_SRCROOT}/Tests/ValetTests-Bridging-Header.h",
        "CLANG_WARN_UNGUARDED_AVAILABILITY": "YES"
      }
    }
  ]
}
