{
  "name": "XMPP",
  "version": "0.0.3",
  "summary": "Pure Swift XMPP library",
  "homepage": "https://github.com/FluuxIO/XMPP",
  "license": {
    "type": "Apache 2.0",
    "file": "LICENSE"
  },
  "authors": {
    "Mickaël Rémond": "contact@process-one.net",
    "ProcessOne": null
  },
  "social_media_url": "http://twitter.com/ProcessOne",
  "platforms": {
    "ios": "12.0",
    "osx": "10.14"
  },
  "swift_version": "4.2",
  "requires_arc": true,
  "compiler_flags": "-whole-module-optimization",
  "source": {
    "git": "https://github.com/FluuxIO/XMPP.git",
    "tag": "v0.0.3",
    "submodules": true
  },
  "source_files": "Sources/XMPP/**/*.{h,swift}",
  "exclude_files": "Sources/XMPP/Networking/BSDSocket/*.swift",
  "libraries": "xml2",
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "$(SDKROOT)/usr/include/libxml2"
  }
}
