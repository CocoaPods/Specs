  "homepage": "https://github.com/zenzzy/CNNImagePicker",
  "license": "MIT",
  "authors": {
    "zenzzy": "zenzzy@outlook.com"
  },
  "source": {
    "git": "https://github.com/zenzzy/CNNImagePicker.git",
    "tag": "0.2.5"
  },
  "platforms": {
    "ios": "7.0"
  },
