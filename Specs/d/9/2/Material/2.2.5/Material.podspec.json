{
  "name": "Material",
  "version": "2.2.5",
  "license": "BSD-3-Clause",
  "summary": "Material is an animation and graphics framework that is used to create beautiful applications.",
  "homepage": "http://materialswift.io",
  "social_media_url": "https://www.facebook.com/cosmicmindio",
  "authors": {
    "CosmicMind, Inc.": "support@cosmicmind.io"
  },
  "source": {
    "git": "https://github.com/CosmicMind/Material.git",
    "tag": "2.2.5"
  },
  "platforms": {
    "ios": "8.0"
  },
  "default_subspecs": "Core",
  "subspecs": [
    {
      "name": "Core",
      "platforms": {
        "ios": "8.0",
        "osx": "10.9"
      },
      "ios": {
        "source_files": "Sources/iOS/*.swift"
      },
      "osx": {
        "source_files": "Sources/macOS/**/*.swift"
      },
      "requires_arc": true,
      "resource_bundles": {
        "io.cosmicmind.material.icons": [
          "Sources/**/*.xcassets"
        ],
        "io.cosmicmind.material.fonts": [
          "Sources/**/*.ttf"
        ]
      }
    },
    {
      "name": "Capture",
      "source_files": "Sources/iOS/Capture/*.swift",
      "dependencies": {
        "Material/Core": [

        ]
      }
    },
    {
      "name": "Photos",
      "source_files": "Sources/iOS/Photos/*.swift",
      "dependencies": {
        "Material/Core": [

        ]
      }
    }
  ]
}
