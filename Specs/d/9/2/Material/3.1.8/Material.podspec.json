{
  "name": "Material",
  "version": "3.1.8",
  "swift_versions": "5.0",
  "license": "BSD-3-Clause",
  "summary": "A UI/UX framework for creating beautiful applications.",
  "homepage": "http://cosmicmind.com",
  "social_media_url": "https://www.facebook.com/cosmicmindcom",
  "authors": {
    "CosmicMind, Inc.": "support@cosmicmind.com"
  },
  "source": {
    "git": "https://github.com/CosmicMind/Material.git",
    "tag": "3.1.8"
  },
  "default_subspecs": "Core",
  "platforms": {
    "ios": "8.0"
  },
  "swift_version": "5.0",
  "subspecs": [
    {
      "name": "Core",
