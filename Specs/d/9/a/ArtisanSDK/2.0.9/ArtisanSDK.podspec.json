{
  "name": "ArtisanSDK",
  "version": "2.0.9",
  "summary": "Artisan is the first all-in-one platform for mobile app analytics, A/B testing, and personalization.",
  "homepage": "http://www.useartisan.com",
  "license": {
    "type": "Proprietary",
    "file": "FILE_LICENSE"
  },
  "authors": {
    "Kevin Jenkins": "kevin.jenkins@useartisan.com"
  },
  "source": {
    "git": "https://github.com/ArtisanMobile/ArtisanSDK.git",
    "tag": "2.0.9"
  },
  "platforms": {
    "ios": "5.0"
  },
  "resources": [
    "ArtisanSDK.bundle",
    "ArtisanAnalytics.bundle"
  ],
  "frameworks": [
    "ArtisanSDK",
    "CoreData",
    "CFNetwork",
    "Security",
    "QuartzCore",
    "SystemConfiguration",
    "CoreLocation"
  ],
  "libraries": "z",
  "requires_arc": true,
  "preserve_paths": "*.framework",
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "\"$(PODS_ROOT)\"",
    "FRAMEWORK_SEARCH_PATHS": "$(inherited) \"$(PODS_ROOT)/ArtisanSDK\"",
    "OTHER_LDFLAGS": "-ObjC, -lz"
  }
}
