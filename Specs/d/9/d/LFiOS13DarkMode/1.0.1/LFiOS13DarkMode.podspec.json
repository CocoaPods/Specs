{
  "name": "LFiOS13DarkMode",
  "version": "1.0.1",
  "summary": "easy adaptation to iOS13 dark mode",
  "homepage": "https://github.com/lincf0912/LFiOS13DarkMode",
  "license": "MIT",
  "authors": {
    "lincf0912": "dayflyking@163.com"
  },
  "platforms": {
    "ios": "7.0"
  },
  "source": {
    "git": "https://github.com/lincf0912/LFiOS13DarkMode.git",
    "tag": "1.0.1",
    "submodules": true
  },
  "requires_arc": true,
  "default_subspecs": "Core",
