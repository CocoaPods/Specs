  "description": "支持轻牛旗下SDK授权信息校验",
  "homepage": "https://github.com/YolandaQingniu/QNSDKPluginDemo",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "service@qnniu.com": "service@qnniu.com"
  },
  "source": {
    "git": "https://github.com/YolandaQingniu/QNSDKPluginDemo.git",
    "tag": "0.1.0"
  },
  "platforms": {
    "ios": "9.0"
  },
  "source_files": "QNPlugin/*.{h,m}",
