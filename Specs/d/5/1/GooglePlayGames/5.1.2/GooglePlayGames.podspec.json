  "vendored_frameworks": "gpg-cpp-sdk/ios/gpg.framework",
  "requires_arc": true,
  "xcconfig": {
    "OTHER_LDFLAGS": "-ObjC -lc++ -lz -std=gnu++11"
  },
  "dependencies": {
    "GoogleSignIn": [
      "2.4.0"
    ],
    "GooglePlusOpenSource": [
      ">= 1.7.1"
    ]
  }
