{
  "name": "UrpayCards",
  "version": "0.5.2",
  "summary": "A framework for handling Urpay cards in iOS applications.",
  "description": "UrpayCards is a framework designed for iOS applications to handle card management.",
  "homepage": "https://github.com/iAhmedWahdan/UrpayCards",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "iAhmedWahdan": "ahmednasrwahdan@gmail.com"
  },
  "source": {
    "git": "https://github.com/iAhmedWahdan/UrpayCards.git",
    "tag": "0.5.2"
  },
  "platforms": {
    "ios": "13.0"
  },
  "vendored_frameworks": "UrpayCards.xcframework",
  "swift_versions": [
    "5.0"
  ],
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64",
    "ONLY_ACTIVE_ARCH": "NO",
    "ENABLE_BITCODE": "NO"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "resource_bundles": {
    "UrpayCardsResources": [
      "UrpayCards/**/*.xcassets",
      "UrpayCards/**/*.{xib,storyboard,png,jpg,svg,pdf,json}"
    ]
  },
  "source_files": "UrpayCards/**/*.{swift}",
  "swift_version": "5.0"
}
