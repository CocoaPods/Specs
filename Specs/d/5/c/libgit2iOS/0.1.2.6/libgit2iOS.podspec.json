{
  "name": "libgit2iOS",
  "version": "0.1.2.6",
  "summary": "libgit2 for iOS",
  "swift_version": "4.0",
  "description": "libgit2 iOS pod.",
  "homepage": "https://github.com/mewehk/libgit2",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Movie": "100992222@qq.com"
  },
  "source": {
    "git": "https://github.com/mewehk/libgit2.git",
    "tag": "0.1.2.6"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source_files": "libgit2iOS/*.{swift,m,h}",
  "preserve_paths": "libgit2iOS/dependencies",
  "ios": {
    "pod_target_xcconfig": {
      "SWIFT_INCLUDE_PATHS": "\"$(PODS_ROOT)/libgit2iOS/libgit2iOS/dependencies\" \"$(SRCROOT)/libgit2iOS/libgit2iOS/dependencies/git2\"",
      "LIBRARY_SEARCH_PATHS": "\"$(PODS_ROOT)/libgit2iOS/libgit2iOS/dependencies\"",
      "HEADER_SEARCH_PATHS": "\"$(PODS_ROOT)/libgit2iOS/libgit2iOS/dependencies/git2\""
    }
  },
  "libraries": [
    "git2",
    "z",
    "iconv"
  ]
}
