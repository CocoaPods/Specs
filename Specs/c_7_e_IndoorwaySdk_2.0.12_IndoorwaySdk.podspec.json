  "license": {
    "type": "Custom",
    "file": "LICENSE"
  },
  "authors": {
    "Indoorway": "contact@indoorway.com"
  },
  "source": {
    "git": "https://github.com/indoorway/SDKforiOS.git",
    "tag": "2.0.12"
  },
  "platforms": {
