{
  "name": "JMKit",
  "version": "0.7.0",
  "summary": "UI Fast Use for iOS",
  "description": "快速创建文本，按钮，输入框，设置各种控件属性，快速完成UI",
  "homepage": "https://gitlab.com/FKFuture/jmkit",
  "license": "MIT",
  "authors": {
    "JM": "asdfgwjm@163.com"
  },
  "source": {
    "git": "https://gitlab.com/FKFuture/jmkit.git",
    "tag": "0.7.0"
  },
  "resource_bundles": {
    "JMKit": [
      "Source/Assets.xcassets/*"
    ]
  },
  "platforms": {
    "ios": "9.0"
  },
  "frameworks": [
    "Foundation",
    "UIKit"
  ],
  "subspecs": [
    {
      "name": "JMExtension",
      "source_files": "Source/JMExtension/*.{h,m}",
      "public_header_files": "Source/JMExtension/*.h"
    },
    {
      "name": "JMFactory",
      "source_files": "Source/JMFactory/*.{h,m}",
      "public_header_files": "Source/JMFactory/*.h",
      "dependencies": {
        "Masonry": [

        ]
      }
    },
    {
      "name": "JMHeader",
      "source_files": "Source/JMHeader/*.{h,m}",
      "public_header_files": "Source/JMHeader/*.h"
    },
    {
      "name": "JMTool",
      "source_files": "Source/JMTool/*.{h,m}",
      "public_header_files": "Source/JMTool/*.h"
    }
  ]
}
