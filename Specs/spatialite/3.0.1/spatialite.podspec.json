{
  "name": "spatialite",
  "version": "3.0.1",
  "summary": "SpatiaLite is an open source library intended to extend the SQLite core to support fully fledged Spatial SQL capabilities.",
  "homepage": "https://www.gaia-gis.it/fossil/libspatialite/home",
  "license": {
    "type": "Mozilla Public License v1.1",
    "file": "COPYING"
  },
  "authors": {
    " Alessandro Furieri": "a.furieri@lqt.it"
  },
  "source": {
    "http": "http://www.gaia-gis.it/gaia-sins/libspatialite-sources/libspatialite-amalgamation-3.0.1.tar.gz"
  },
  "compiler_flags": "-DVERSION=\\\"3.0.1\\\" -DOMIT_GEOCALLBACKS -DSPL_AMALGAMATION=1",
  "ios": {
    "compiler_flags": "-U__APPLE__"
  },
  "platforms": {
    "ios": "4.0",
    "osx": "10.6"
  },
  "source_files": "*.c",
  "preserve_paths": "headers/**/*.h",
  "dependencies": {
    "geos": [

    ],
    "proj4": [

    ],
    "freexl": [

    ]
  },
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "${PODS_ROOT}/spatialite/headers"
  },
  "requires_arc": false
}
