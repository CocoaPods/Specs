{
  "name": "SuperPlayer",
  "version": "2.0.0",
  "license": {
    "type": "MIT"
  },
  "homepage": "https://cloud.tencent.com/product/player",
  "authors": {
    "annidyfeng": "annidyfeng@tencent.com"
  },
  "summary": "超级播放器",
  "source": {
    "git": "https://github.com/tencentyun/SuperPlayer.git",
    "tag": "v2.0.0"
  },
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "dependencies": {
    "AFNetworking": [
      "~> 3.1"
    ],
    "SDWebImage": [
      "~> 4.4.0"
    ],
    "Masonry": [
      "~> 1.1.0"
    ],
    "CFDanmaku": [
      "~> 0.0.1"
    ]
  },
  "default_subspecs": "Player",
  "frameworks": [
    "SystemConfiguration",
    "CoreTelephony",
    "VideoToolbox",
    "CoreGraphics",
    "AVFoundation",
    "Accelerate"
  ],
  "libraries": [
    "z",
    "resolv",
    "iconv",
    "stdc++",
    "c++",
    "sqlite3"
  ],
  "subspecs": [
    {
      "name": "Player",
      "source_files": "SuperPlayer/**/*.{h,m}",
      "resources": "SuperPlayer/Resource/*",
      "dependencies": {
        "TXLiteAVSDK_Player": [
          "= 5.2.5541"
        ]
      }
    }
  ]
}
