{
  "name": "CrittercismSDK",
  "version": "5.7.1",
  "summary": "iOS and tvOS SDK for applicaton performance management.",
  "description": "Agent for monitoring iOS and tvOS app performance, including crashes, API latency, and more.\nFor more info visit http://crittercism.com",
  "homepage": "http://crittercism.com",
  "license": {
    "type": "Commercial",
    "text": "See http://www.crittercism.com/terms-services/"
  },
  "authors": {
    "crittercism": "support@crittercism.com"
  },
  "source": {
    "http": "https://app.crittercism.com/images/Crittercism_v5_7_1.zip"
  },
  "platforms": {
    "ios": "7.0",
    "tvos": "9.0"
  },
  "ios": {
    "public_header_files": "iOS/Crittercism.framework/Headers/*.h",
    "source_files": "iOS/Crittercism.framework/Headers/*.h",
    "vendored_frameworks": "iOS/Crittercism.framework"
  },
  "tvos": {
    "public_header_files": "tvOS/Crittercism.framework/Headers/*.h",
    "source_files": "tvOS/Crittercism.framework/Headers/*.h",
    "vendored_frameworks": "tvOS/Crittercism.framework"
  },
  "preserve_paths": [
    "iOS/*",
    "tvOS/*",
    "dsym_upload.sh"
  ],
  "requires_arc": true,
  "frameworks": [
    "Foundation",
    "CoreData",
    "SystemConfiguration"
  ]
}
