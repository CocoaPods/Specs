    "CoreMedia",
    "CoreGraphics",
    "AVFoundation",
    "AudioToolbox"
  ],
  "subspecs": [
    {
      "name": "Core",
      "source_files": [
        "SDK/PlayerKit/*.{h,m,mm}",
