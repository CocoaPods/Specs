      "public_header_files": "SDK/PlayerKit/*.h",
      "dependencies": {
        "IJKMediaFramework": [

        ],
        "GCDMulticastDelegate": [

        ]
      }
    },
    {
      "name": "GeneralPlayerUI",
      "source_files": [
        "SDK/GeneralPlayerUI/**/*.{h,m,mm}",
        "SDK/GeneralPlayerUI/*.{h,m,mm}",
        "SDK/Commen/*.{h,m,mm}"
      ],
      "public_header_files": "SDK/GeneralPlayerUI/*.h",
      "dependencies": {
        "MCPlayerKit/Core": [

        ],
        "SDVersion": [

        ],
        "MCStyle": [

        ],
        "SDWebImage": [

        ]
      }
    }
  ]
