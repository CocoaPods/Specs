{
  "name": "enhance",
  "version": "0.2.1",
  "summary": "A simple image viewer with zooming and UIDynamics for maximum fun",
  "description": "                       * Got some images?\n                       * This shows them in a pinch-zoomable view inside of the view controller of your choice\n                       * Fancy UIDynamics lets you swipe them out of the way or ease it back in place\n                       * Simple, direct, not trying to do everything under the sun\n",
  "homepage": "https://github.com/amco/enhance",
  "screenshots": [
    "http://i.imgur.com/mfQxIyN.jpg",
    "http://i.imgur.com/vlpMYFj.png",
    "http://i.imgur.com/98J3wNl.jpg",
    "http://i.imgur.com/r7RgeGP.jpg"
  ],
  "license": "MIT",
  "authors": {
    "Adam Yanalunas": "adamy@amcoonline.net"
  },
  "source": {
    "git": "https://github.com/amco/enhance.git",
    "tag": "0.2.1"
  },
  "social_media_url": "https://twitter.com/adamyanalunas",
  "platforms": {
    "ios": "7.0"
  },
  "requires_arc": true,
  "source_files": "Pod/Classes",
  "resource_bundles": {
    "enhance_resources": [
      "Pod/Assets/**"
    ]
  },
  "frameworks": [
    "UIKit",
    "Accelerate",
    "QuartzCore",
    "AssetsLibrary"
  ],
  "subspecs": [
    {
      "name": "Localization",
      "subspecs": [
        {
          "name": "en",
          "ios": {
            "resources": "Pod/Localization/en.lproj",
            "preserve_paths": "Pod/Localization/en.lproj"
          }
        }
      ]
    }
  ]
}
