{
  "name": "AppGuard",
  "version": "0.3.0",
  "summary": "AppGuard is a guard 💂‍♀️ for your iOS app, to check / force users to update your app or show what changed.",
  "homepage": "https://github.com/smartnsoft/AppGuard",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Smart&Soft": "contact@smartnsoft.com"
  },
  "source": {
    "git": "https://github.com/smartnsoft/AppGuard.git",
    "tag": "0.3.0"
  },
  "social_media_url": "https://twitter.com/smartnsoft",
  "swift_versions": "4.2",
  "platforms": {
    "ios": "9.0"
  },
  "frameworks": "UIKit",
  "default_subspecs": "Core",
  "static_framework": true,
  "subspecs": [
    {
      "name": "Core",
      "dependencies": {
        "Jelly": [
          "~> 1.2.0"
        ]
      },
      "source_files": [
        "AppGuard/Classes/Logic/*.{swift}",
        "AppGuard/Classes/UI/*.{swift}"
      ],
      "resource_bundles": {
        "AppGuard": [
          "AppGuard/Classes/UI/*.{xib}"
        ]
      }
    },
    {
      "name": "FirebaseRemoteConfig",
      "dependencies": {
        "AppGuard/Core": [

        ],
        "Firebase/RemoteConfig": [
          "~> 6.10"
        ]
      },
      "source_files": "AppGuard/Classes/Firebase/**/*.{swift}"
    }
  ],
  "swift_version": "4.2"
}
