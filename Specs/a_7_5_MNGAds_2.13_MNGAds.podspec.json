      "preserve_paths": "MNGAds/OMSDK_Madvertise.framework"
    },
    {
      "name": "AppsFire",
      "dependencies": {
        "MNGAds/MNGAdsStandalone": [

        ]
      }
    },
    {
      "name": "Facebook",
