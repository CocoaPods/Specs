{
  "name": "NBarSDK",
  "version": "1.0.0",
  "summary": "NBarSDK",
  "description": "NBarSDK is a barcode scanning SDK for iOS applications.",
  "homepage": "https://github.com/kartel/NBarSDK.git",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "kartel": "neilliga@gmail.com"
  },
  "source": {
    "git": "https://github.com/kartel/NBarSDK.git",
    "tag": "1.0.0"
  },
  "platforms": {
    "ios": "13.0"
  },
  "public_header_files": [
    "iphone/include/ATBarSDK/*.h",
    "include/zbar.h"
  ],
  "source_files": [
    "zbar/**/*.{h,c}",
    "include/*.h",
    "iphone/include/*.h",
    "iphone/*.{h,m}",
    "iphone/include/ATBarSDK/*.h"
  ],
  "private_header_files": [
    "iphone/include/config.h"
  ],
  "resource_bundles": {
    "NBarSDKResources": [
      "iphone/res/*"
    ]
  },
  "frameworks": [
    "AVFoundation",
    "CoreGraphics",
    "CoreMedia",
    "CoreVideo",
    "QuartzCore",
    "WebKit"
  ],
  "libraries": "iconv",
  "prefix_header_file": "iphone/include/prefix.pch",
  "requires_arc": false,
  "compiler_flags": "-w -I\"$(PODS_TARGET_SRCROOT)/iphone/include\" -I\"$(PODS_TARGET_SRCROOT)/include\" -I\"$(PODS_TARGET_SRCROOT)/zbar\"",
  "pod_target_xcconfig": {
    "EXCLUDED_SOURCE_FILE_NAMES[sdk=iphoneos*][arch=*]": "ZBarReaderViewImpl_Simulator.m",
    "EXCLUDED_SOURCE_FILE_NAMES[sdk=iphonesimulator*][arch=*]": "ZBarReaderViewImpl_Capture.m ZBarCaptureReader.m",
    "GCC_PREPROCESSOR_DEFINITIONS": "NDEBUG=1",
    "HEADER_SEARCH_PATHS": "$(inherited) $(PODS_TARGET_SRCROOT)/iphone/include $(PODS_TARGET_SRCROOT)/include $(PODS_TARGET_SRCROOT)/zbar",
    "USER_HEADER_SEARCH_PATHS": "$(inherited) $(PODS_TARGET_SRCROOT)/iphone/include $(PODS_TARGET_SRCROOT)/include $(PODS_TARGET_SRCROOT)/zbar"
  }
}
