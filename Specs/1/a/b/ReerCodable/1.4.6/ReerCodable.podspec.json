{
  "name": "ReerCodable",
  "version": "1.4.6",
  "summary": "Codable extensions using Swift Macro",
  "description": "Enhancing Swift's Codable Protocol Using Macros: A Declarative Approach to Serialization",
  "homepage": "https://github.com/reers/ReerCodable",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Asura19": "x.rhythm@qq.com"
  },
  "source": {
    "git": "https://github.com/reers/ReerCodable.git",
    "tag": "1.4.6"
  },
  "platforms": {
    "ios": "13.0",
    "osx": "10.15",
    "watchos": "6.0",
    "tvos": "13.0",
    "visionos": "1.0"
  },
  "swift_versions": "5.10",
  "source_files": "Sources/ReerCodable/**/*",
  "preserve_paths": [
    "Package.swift",
    "Sources/ReerCodableMacros",
    "Tests"
  ],
  "pod_target_xcconfig": {
    "OTHER_SWIFT_FLAGS": "-Xfrontend -load-plugin-executable -Xfrontend $(PODS_BUILD_DIR)/ReerCodable/release/ReerCodableMacros-tool#ReerCodableMacros"
  },
  "user_target_xcconfig": {
    "OTHER_SWIFT_FLAGS": "-Xfrontend -load-plugin-executable -Xfrontend $(PODS_BUILD_DIR)/ReerCodable/release/ReerCodableMacros-tool#ReerCodableMacros"
  },
  "script_phases": {
    "name": "Build ReerCodable macro plugin",
    "script": "    env -i PATH=\"$PATH\" \"$SHELL\" -l -c \"swift build -c release --package-path \\\"$PODS_TARGET_SRCROOT\\\" --build-path \\\"${PODS_BUILD_DIR}/ReerCodable\\\"\"\n",
    "execution_position": "before_compile",
    "output_files": [
      "$(PODS_BUILD_DIR)/ReerCodable/release/ReerCodableMacros-tool"
    ]
  },
  "swift_version": "5.10"
}
