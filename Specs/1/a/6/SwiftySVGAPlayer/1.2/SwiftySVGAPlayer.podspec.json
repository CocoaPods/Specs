  },
  "source": {
    "git": "https://github.com/janlionly/SwiftySVGAPlayer.git",
    "tag": "1.2"
  },
  "platforms": {
    "ios": "9.0"
  },
  "requires_arc": true,
  "vendored_frameworks": "*.framework",
