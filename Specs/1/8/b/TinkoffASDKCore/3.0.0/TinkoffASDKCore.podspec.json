{
  "name": "TinkoffASDKCore",
  "version": "3.0.0",
  "summary": "Мобильный SDK",
  "description": "Позволяет настроить прием платежей в нативной форме приложений для платформы iOS",
  "homepage": "https://github.com/TinkoffCreditSystems/AcquiringSdk_IOS",
  "changelog": "https://github.com/Tinkoff/AcquiringSdk_IOS/blob/master/CHANGELOG.md",
  "documentation_url": "https://oplata.tinkoff.ru/develop/api/payments/",
  "license": {
    "type": "Apache 2.0",
    "file": "TinkoffASDKCore/License.txt"
  },
  "authors": {
    "Tinkoff": "v.budnikov@tinkoff.ru"
  },
  "platforms": {
    "ios": "12.3"
  },
  "module_name": "TinkoffASDKCore",
  "swift_versions": "5.0",
  "source": {
    "git": "https://github.com/TinkoffCreditSystems/AcquiringSdk_IOS.git",
    "tag": "3.0.0"
  },
  "source_files": "TinkoffASDKCore/TinkoffASDKCore/**/*.swift",
  "resource_bundles": {
    "TinkoffASDKCoreResources": [
      "TinkoffASDKCore/TinkoffASDKCore/**/*.{lproj,strings,der}"
    ]
  },
  "pod_target_xcconfig": {
    "CODE_SIGN_IDENTITY": ""
  },
  "testspecs": [
    {
      "name": "Tests",
      "test_type": "unit",
      "source_files": "TinkoffASDKCore/TinkoffASDKCoreTests/**/*.swift",
      "exclude_files": [
        "TinkoffASDKCore/TinkoffASDKCoreTests/IntegrationTests.swift",
        "TinkoffASDKCore/TinkoffASDKCoreTests/FinishResponseTests.swift",
        "TinkoffASDKCore/TinkoffASDKCoreTests/CoreTests.swift"
      ],
      "resources": "TinkoffASDKCore/TinkoffASDKCoreTests/**/*.{json}"
    }
  ],
  "swift_version": "5.0"
}
