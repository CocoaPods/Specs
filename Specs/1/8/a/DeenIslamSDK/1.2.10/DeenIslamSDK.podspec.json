{
  "name": "DeenIslamSDK",
  "version": "1.2.10",
  "summary": "An Islamic SDK for MyGP iOS app",
  "description": "DeenIslamSDK provides Islamic content and features for the MyGP iOS app, including prayer times, \nQuran recitations, and other Islamic functionalities.",
  "homepage": "https://github.com/shadhin-music/DeenGPSDK-iOS.git",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Talut Mahamud Deep": "gakkdeep@gmail.com"
  },
  "platforms": {
    "ios": "13.0"
  },
  "swift_versions": "5.7",
  "source": {
    "git": "https://github.com/shadhin-music/DeenGPSDK-iOS.git",
    "tag": "1.2.10"
  },
  "ios": {
    "vendored_frameworks": "DeenIslamSDK.xcframework"
  },
  "pod_target_xcconfig": {
    "SKIP_INSTALL": "NO",
    "BUILD_LIBRARY_FOR_DISTRIBUTION": "YES",
    "ENABLE_BITCODE": "NO",
    "OTHER_LDFLAGS": "$(inherited) -ObjC"
  },
  "user_target_xcconfig": {
    "BUILD_LIBRARY_FOR_DISTRIBUTION": "YES",
    "ENABLE_BITCODE": "NO"
  },
  "requires_arc": true,
  "static_framework": true,
  "frameworks": [
    "UIKit",
    "WebKit",
    "EventKit",
    "CoreLocation"
  ],
  "swift_version": "5.7"
}
