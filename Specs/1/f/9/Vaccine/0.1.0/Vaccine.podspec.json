{
  "name": "Vaccine",
  "summary": "Make your apps immune to recompile-decease.",
  "version": "0.1.0",
  "homepage": "https://github.com/zenangst/Vaccine",
  "license": "MIT",
  "authors": {
    "Christoffer Winterkvist": "christoffer@winterkvist.com"
  },
  "source": {
    "git": "https://github.com/zenangst/Vaccine.git",
    "tag": "0.1.0"
  },
  "social_media_url": "https://twitter.com/zenangst",
  "platforms": {
    "ios": "9.0",
    "osx": "10.11",
    "tvos": "9.2",
    "watchos": "3.0"
  },
  "requires_arc": true,
  "ios": {
    "source_files": "Source/{iOS+tvOS,iOS,Shared}/**/*"
  },
  "tvos": {
    "source_files": "Source/{iOS+tvOS,tvOS,Shared}/**/*"
  },
  "osx": {
    "source_files": "Source/{macOS,Shared}/**/*"
  },
  "watchos": {
    "source_files": "Source/{watchOS,Shared}/**/*"
  },
  "pod_target_xcconfig": {
    "SWIFT_VERSION": "4.0"
  }
}
