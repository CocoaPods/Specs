    {
      "name": "Tokenization",
      "source_files": [
        "LibraryComponents/Sources/Tokenization/**/*.{swift}"
      ],
      "dependencies": {
        "SecureInputs/Networking": [

        ]
      }
    },
    {
      "name": "SecureInputs",
      "resources": [
        "LibraryComponents/Assets/*.*",
        "LibraryComponents/Assets/**/*.xcassets"
      ],
      "source_files": [
        "LibraryComponents/Sources/SecureInputs/**/*.{swift}"
      ],
      "dependencies": {
        "SecureInputs/Tokenization": [

        ],
        "AndesUI": [
          "~> 3.0"
        ]
