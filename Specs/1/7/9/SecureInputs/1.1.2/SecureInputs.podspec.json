    "http": "https://artifacts.mercadolibre.com/repository/ios-releases/SecureInputs/1.1.2/SecureInputs.zip"
  },
  "static_framework": true,
  "platforms": {
    "ios": "13.0"
  },
  "swift_versions": "5.7",
  "default_subspecs": "SecureInputs",
  "resource_bundles": {
    "SecureInputsResources": [
      "LibraryComponents/Assets/*.*"
    ]
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "testspecs": [
    {
      "name": "Tests",
      "test_type": "unit",
      "requires_app_host": false,
      "source_files": "LibraryComponents/Tests/**/*.{swift}",
      "frameworks": "XCTest"
    }
  ],
  "subspecs": [
    {
      "name": "Networking",
      "source_files": [
        "LibraryComponents/Sources/Networking/**/*.{swift}"
      ]
