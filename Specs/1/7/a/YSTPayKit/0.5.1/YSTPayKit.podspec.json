{
  "name": "YSTPayKit",
  "version": "0.5.1",
  "summary": "Vip/支付/优惠券",
  "description": "Vip/支付/优惠券 模块",
  "homepage": "http://cocomccree.cn/",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "MQL9011": "maqianli@ysten.com"
  },
  "source": {
    "git": "http://221.131.123.204:8181/ios-mobile/modules/YSTPayKit.git",
    "tag": "0.5.1"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source_files": "YSTPayKit/Classes/**/*.{h,m}",
  "prefix_header_file": "YSTPayKit/Classes/YSTPayKit.pch",
  "resource_bundles": {
    "YSTPayKit": [
      "YSTPayKit/Classes/**/*.{storyboard,xib,cer,json,plist}",
      "YSTPayKit/Assets/*.{bundle,xcassets,imageset,png}"
    ]
  },
  "public_header_files": "YSTPayKit/Classes/**/*.h",
  "xcconfig": {
    "ENABLE_BITCODE": "NO",
    "GCC_PRECOMPILE_PREFIX_HEADER": "YES",
    "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited)",
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES"
  },
  "pod_target_xcconfig": {
    "OTHER_LDFLAGS": "$(inherited)",
    "SWIFT_VERSION": "4.0",
    "HEADER_SEARCH_PATHS": "$(inherited) $(SDKROOT)/usr/include/libxml2 $(SDKROOT)/usr/include/libresolv $(inherited)$(PROJECT_DIR) /usr/local/include",
    "CLANG_CXX_LANGUAGE_STANDARD": "c++11",
    "CLANG_CXX_LIBRARY": "libc++"
  },
  "dependencies": {
    "YSTCommonKit": [

    ],
    "YSTMRLog": [

    ],
    "MJExtension": [

    ],
    "ReactiveObjC": [

    ],
    "Masonry": [

    ],
    "SDWebImage": [

    ],
    "MJRefresh": [

    ],
    "MGJRouter": [

    ]
  }
}
