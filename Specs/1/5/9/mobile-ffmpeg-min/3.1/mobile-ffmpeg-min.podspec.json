{
  "name": "mobile-ffmpeg-min",
  "version": "3.1",
  "summary": "Mobile FFmpeg Min Dynamic Framework",
  "description": "Includes FFmpeg v4.1-10 without any external libraries enabled.",
  "homepage": "https://github.com/tanersener/mobile-ffmpeg",
  "authors": {
    "Taner Sener": "tanersener@gmail.com"
  },
  "license": {
    "type": "LGPL-3.0",
    "file": "mobileffmpeg.framework/LICENSE"
  },
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "ios": {
    "frameworks": [
      "Foundation",
      "AudioToolbox",
      "CoreImage",
      "CoreMotion",
      "CoreVideo",
      "GameController",
      "VideoToolbox"
    ],
    "libraries": [
      "z",
      "bz2",
      "c++"
    ],
    "vendored_frameworks": [
      "mobileffmpeg.framework",
      "libavcodec.framework",
      "libavdevice.framework",
      "libavfilter.framework",
      "libavformat.framework",
      "libavutil.framework",
      "libswresample.framework",
      "libswscale.framework"
    ]
  },
  "source": {
    "http": "https://github.com/tanersener/mobile-ffmpeg/releases/download/v3.0/mobile-ffmpeg-min-3.0-ios-framework.zip"
  }
}
