{
  "name": "SFLiveManager",
  "version": "0.1.1",
  "summary": "A short description of SFLiveManager.",
  "description": "TODO: Add long description of the pod here.",
  "homepage": "https://github.com/Joygo-Technology/SFLiveManager",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "lee": "nemoleo@yeah.net"
  },
  "source": {
    "git": "https://github.com/Joygo-Technology/SFLiveManager.git",
    "tag": "0.1.1"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source_files": "SFLiveManager/Classes/**/*.h",
  "public_header_files": "SFLiveManager/Classes/**/*.h",
  "preserve_paths": [
    "ffmpeg/include/*.h",
    "openssl/include/*.h",
    "osl/include/*.h"
  ],
  "vendored_libraries": [
    "SFLiveManager/Library/**/*.a",
    "osl/*.a",
    "openssl/libs/*.a",
    "ffmpeg/lib/*.a"
  ],
  "frameworks": [
    "Foundation",
    "AVFoundation",
    "VideoToolbox",
    "AudioToolbox",
    "CoreTelephony",
    "OpenAL",
    "UIKit",
    "CoreMotion"
  ],
  "libraries": [
    "stdc++",
    "c++",
    "iconv",
    "resolv"
  ],
  "pod_target_xcconfig": {
    "ENABLE_BITCODE": "NO"
  },
  "user_target_xcconfig": {
    "CLANG_CXX_LANGUAGE_STANDARD": "c++11",
    "CLANG_CXX_LIBRARY": "libc++",
    "HEADER_SEARCH_PATHS": "${PODS_ROOT}/SFLiveManager/ffmpeg/include ${PODS_ROOT}/SFLiveManager/osl/include ${PODS_ROOT}/SFLiveManager/openssl/include"
  }
}
