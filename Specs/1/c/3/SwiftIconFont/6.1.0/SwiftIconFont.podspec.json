  "requires_arc": true,
  "version": "6.1.0",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Sedat CIFTCI": "me@sedat.us"
  },
  "homepage": "http://sedat.ninja",
  "source": {
    "git": "https://github.com/0x73/SwiftIconFont.git",
    "tag": "6.1.0"
  },
  "tvos": {
    "frameworks": "UIKit",
