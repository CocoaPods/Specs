{
  "name": "CYFFmpeg",
  "version": "1.0.0",
  "summary": "CYFFmpeg, a framework for ffmpeg that baseed on version is 3.4, include x264, fdk-aac, smbclient, and cmdutils for ffmpeg.",
  "description": "CYFFmpeg, \na framework for ffmpeg that baseed on version is 3.4, \ninclude x264, fdk-aac, smbclient, and cmdutils for ffmpeg.",
  "homepage": "https://github.com/yellowei/CYFFmpeg",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "yellowei": "me@yellowei.com"
  },
  "platforms": {
    "ios": "11.0"
  },
  "source": {
    "git": "https://github.com/yellowei/CYFFmpeg.git",
    "tag": "1.0.0"
  },
  "source_files": "CYFFmpeg.framework/Headers/*.{h}",
  "vendored_frameworks": "CYFFmpeg.framework",
  "public_header_files": "CYFFmpeg.framework/Headers/CYFFmpeg.h",
  "frameworks": [
    "VideoToolbox",
    "AudioToolbox",
    "OpenAL",
    "Foundation",
    "OpenGLES",
    "QuartzCore",
    "CoreAudio",
    "CoreGraphics",
    "CoreMedia",
    "AVFoundation",
    "CoreVideo"
  ],
  "libraries": [
    "iconv",
    "resolv",
    "bz2",
    "z"
  ],
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "${PODS_ROOT}/CYFFmpeg/CYFFmpeg.framework/Headers/**"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  }
}
