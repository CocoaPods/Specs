{
  "name": "BNView",
  "version": "1.4.5",
  "summary": "自定义视图库",
  "description": "官方控件无法满足需求的视图集合.",
  "homepage": "https://github.com/shang1219178163/BNView",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "BIN": "shang1219178163@gmail.com"
  },
  "platforms": {
    "ios": "9.0"
  },
  "requires_arc": true,
  "source": {
    "git": "https://github.com/shang1219178163/BNView.git",
    "tag": "1.4.5"
  },
  "source_files": [
    "BNView",
    "BNView/**/*.{h,m}"
