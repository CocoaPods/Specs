{
  "name": "ffmpeg-kit-macos-https-gpl",
  "version": "4.5",
  "summary": "FFmpeg Kit macOS Https GPL Static Framework",
  "description": "Includes FFmpeg with gmp, gnutls, libvid.stab, x264, x265 and xvidcore libraries enabled.",
  "homepage": "https://github.com/tanersener/ffmpeg-kit",
  "authors": {
    "Taner Sener": "tanersener@gmail.com"
  },
  "license": {
    "type": "GPL-3.0",
    "file": "ffmpegkit.xcframework/macos-arm64_x86_64/ffmpegkit.framework/LICENSE"
  },
  "platforms": {
    "osx": "10.15"
  },
  "requires_arc": true,
  "libraries": [
    "z",
    "bz2",
    "c++",
    "iconv"
  ],
  "source": {
    "http": "https://github.com/tanersener/ffmpeg-kit/releases/download/v4.5/ffmpeg-kit-https-gpl-4.5-macos-xcframework.zip"
  },
  "osx": {
    "frameworks": [
