{
  "name": "OpenIMSDKCore",
  "version": "3.0.1",
  "summary": "Open-IM-SDK-Core",
  "description": "OpenIM：由IM技术专家打造的基于 Go 实现的即时通讯（IM）项目，iOS版本IM SDK 可以轻松替代第三方IM云服务，打造具备聊天、社交功能的app。",
  "homepage": "https://github.com/OpenIMSDK/Open-IM-SDK-iOS",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "OpenIMSDKCore": "https://github.com/std-s/Open-IM-SDK-Core"
  },
  "source": {
    "git": "https://github.com/OpenIMSDK/openim-sdk-core-ios.git",
    "tag": "3.0.1"
  },
  "platforms": {
    "ios": "11.0"
  },
  "xcconfig": {
    "VALID_ARCHS": "armv7s arm64 x86_64"
  },
  "pod_target_xcconfig": {
    "ARCHS[sdk=iphonesimulator*]": "$(ARCHS_STANDARD_64_BIT)"
  },
  "vendored_frameworks": "Framework/*.xcframework"
}
