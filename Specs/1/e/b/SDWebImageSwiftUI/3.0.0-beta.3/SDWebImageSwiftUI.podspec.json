{
  "name": "SDWebImageSwiftUI",
  "version": "3.0.0-beta.3",
  "summary": "SwiftUI Image loading and Animation framework powered by SDWebImage",
  "description": "SDWebImageSwiftUI is a SwiftUI image loading framework, which based on SDWebImage.\nIt brings all your favorite features from SDWebImage, like async image loading, memory/disk caching, animated image playback and performances.",
  "homepage": "https://github.com/SDWebImage/SDWebImageSwiftUI",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "DreamPiggy": "lizhuoli1126@126.com"
  },
  "source": {
    "git": "https://github.com/SDWebImage/SDWebImageSwiftUI.git",
    "tag": "3.0.0-beta.3"
  },
  "platforms": {
    "ios": "14.0",
    "osx": "11.0",
    "tvos": "14.0",
    "watchos": "7.0"
  },
  "source_files": [
    "SDWebImageSwiftUI/Classes/**/*",
    "SDWebImageSwiftUI/Module/*.h"
  ],
  "pod_target_xcconfig": {
    "SUPPORTS_MACCATALYST": "YES",
    "DERIVE_MACCATALYST_PRODUCT_BUNDLE_IDENTIFIER": "NO"
  },
  "weak_frameworks": [
    "SwiftUI",
    "Combine"
  ],
  "dependencies": {
    "SDWebImage": [
      "~> 5.10"
    ]
  },
  "swift_versions": "5.3",
  "swift_version": "5.3"
}
