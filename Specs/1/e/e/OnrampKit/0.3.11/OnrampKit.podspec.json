{
  "name": "OnrampKit",
  "version": "0.3.11",
  "summary": "Pod for integration of Onramp widget in native ios applications.",
  "description": "This project contains cocoapods for integrating Onramp widget in client's native ios applications",
  "homepage": "https://github.com/Buyhatke/OnrampKit.git",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Buyhatke": "pd@buyhatke.com"
  },
  "source": {
    "git": "git@github.com:Buyhatke/OnrampKit.git",
    "tag": "0.3.11"
  },
  "platforms": {
    "ios": "12.0"
  },
  "swift_versions": "4.1",
  "source_files": "Classes/**/*.swift",
  "resource_bundles": {
    "OnrampKit": [
      "Classes/**/*.storyboard"
    ]
  },
  "vendored_frameworks": [
    "Frameworks/UdentifyCommons.xcframework",
    "Frameworks/UdentifyNFC.xcframework"
  ],
  "pod_target_xcconfig": {
    "FRAMEWORK_SEARCH_PATHS": "$(inherited) \"$(SRCROOT)/Frameworks/\""
  },
  "swift_version": "4.1"
}
