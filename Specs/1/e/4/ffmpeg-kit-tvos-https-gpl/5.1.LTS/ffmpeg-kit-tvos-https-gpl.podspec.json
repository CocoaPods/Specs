{
  "name": "ffmpeg-kit-tvos-https-gpl",
  "version": "5.1.LTS",
  "summary": "FFmpeg Kit tvOS Https GPL Shared Framework",
  "description": "Includes FFmpeg with gmp, gnutls, libvid.stab, x264, x265 and xvidcore libraries enabled.",
  "homepage": "https://github.com/arthenica/ffmpeg-kit",
  "authors": {
    "ARTHENICA": "open-source@arthenica.com"
  },
  "license": {
    "type": "GPL-3.0",
    "file": "ffmpegkit.framework/LICENSE"
  },
  "platforms": {
    "tvos": "10.0"
  },
  "requires_arc": true,
  "libraries": [
    "z",
    "bz2",
    "c++",
    "iconv"
  ],
  "source": {
    "http": "https://github.com/arthenica/ffmpeg-kit/releases/download/v5.1.LTS/ffmpeg-kit-https-gpl-5.1.LTS-tvos-framework.zip"
  },
  "tvos": {
    "frameworks": [
      "AudioToolbox",
      "CoreMedia"
    ],
    "vendored_frameworks": [
      "ffmpegkit.framework",
      "libavcodec.framework",
      "libavdevice.framework",
      "libavfilter.framework",
      "libavformat.framework",
      "libavutil.framework",
      "libswresample.framework",
      "libswscale.framework"
    ]
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=appletvsimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=appletvsimulator*]": "arm64"
  }
}
