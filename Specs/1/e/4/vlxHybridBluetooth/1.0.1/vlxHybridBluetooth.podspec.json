{
  "name": "vlxHybridBluetooth",
  "module_name": "vlxHybridBluetooth",
  "version": "1.0.1",
  "summary": "vlxHybridBluetooth iOS SDK",
  "description": "vlxHybridBluetooth SDK For IOS",
  "homepage": "https://bitbucket.org/veloxity-inc/vlxhybrid-bluetooth-ios-cocoapods",
  "license": "Proprietary License",
  "authors": {
    "veloxity": "developer@veloxity.net"
  },
  "source": {
    "git": "https://bitbucket.org/veloxity-inc/vlxhybrid-bluetooth-ios-cocoapods",
    "tag": "1.0.1"
  },
  "platforms": {
    "ios": "12.0"
  },
  "requires_arc": true,
  "source_files": "Pod/BluetoothModule.framework/Headers/*.h",
  "public_header_files": "Pod/BluetoothModule.framework/Headers/*.h",
  "preserve_paths": "Pod/BluetoothModule.framework",
  "vendored_frameworks": "Pod/BluetoothModule.framework",
  "frameworks": "BluetoothModule",
  "libraries": [
    "z",
    "sqlite3"
  ],
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "$(SDKROOT)/usr/include/libz"
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  }
}
