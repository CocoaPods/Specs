{
  "name": "LynxServiceAPI",
  "authors": "Lynx",
  "compiler_flags": [
    "-Wall",
    "-Wextra",
    "-Wno-unused-parameter",
    "-Wshorten-64-to-32",
    "-fno-rtti"
  ],
  "homepage": "https://github.com/lynx-family/lynx",
  "platforms": {
    "ios": "10.0"
  },
  "license": "Apache 2.0",
  "pod_target_xcconfig": {
    "CLANG_CXX_LANGUAGE_STANDARD": "gnu++17",
    "GCC_PREPROCESSOR_DEFINITIONS": "NDEBUG=1                                       OS_IOS=1",
    "OTHER_CPLUSPLUSFLAGS": "-fno-aligned-allocation -fno-c++-static-destructors"
  },
  "requires_arc": true,
  "source": {
    "http": "https://github.com/lynx-family/lynx/releases/download/3.5.1/LynxServiceAPI-3.5.1-dev.zip"
  },
  "summary": "Service API provided for frameworks such as Lynx/LynxBase",
  "version": "3.5.1-dev",
  "subspecs": [
    {
      "name": "Core",
      "public_header_files": [
        "platform/darwin/ios/lynx_service_api/LynxServiceLogProtocol.h",
        "platform/darwin/ios/lynx_service_api/LynxServiceProtocol.h",
        "platform/darwin/ios/lynx_service_api/LynxServiceTraceProtocol.h",
        "platform/darwin/ios/lynx_service_api/LynxServiceTrailProtocol.h",
        "platform/darwin/ios/lynx_service_api/ServiceAPI.h",
        "platform/darwin/ios/lynx_service_api/ServiceLazyLoad.h"
      ],
      "source_files": [
        "platform/darwin/ios/lynx_service_api/LynxServiceLogProtocol.h",
        "platform/darwin/ios/lynx_service_api/LynxServiceProtocol.h",
        "platform/darwin/ios/lynx_service_api/LynxServiceTraceProtocol.h",
        "platform/darwin/ios/lynx_service_api/LynxServiceTrailProtocol.h",
        "platform/darwin/ios/lynx_service_api/ServiceAPI.h",
        "platform/darwin/ios/lynx_service_api/ServiceAPI.m",
        "platform/darwin/ios/lynx_service_api/ServiceLazyLoad.h"
      ]
    }
  ]
}
