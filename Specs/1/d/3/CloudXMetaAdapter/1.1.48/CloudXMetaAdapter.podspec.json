{
  "name": "CloudXMetaAdapter",
  "version": "1.1.48",
  "summary": "Mobile SDK for CloudX iOS Meta Adapter",
  "description": "iOS adapter add-on to the CloudX iOS SDK for a Meta bidder",
  "homepage": "https://github.com/cloudx-io/cloudx-ios",
  "license": {
    "type": "Business Source License 1.1",
    "file": "LICENSE"
  },
  "authors": {
    "CloudX": "support@cloudx.com"
  },
  "platforms": {
    "ios": "14.0"
  },
  "module_name": "CloudXMetaAdapter",
  "static_framework": true,
  "source": {
    "http": "https://github.com/cloudx-io/cloudx-ios/releases/download/v1.1.48-meta/CloudXMetaAdapter-v1.1.48.xcframework.zip",
    "type": "zip",
    "flatten": false
  },
  "vendored_frameworks": "CloudXMetaAdapter.xcframework",
  "preserve_paths": "CloudXMetaAdapter.xcframework",
  "dependencies": {
    "CloudXCore": [

    ],
    "FBAudienceNetwork": [
      "~> 6.16.0"
    ]
  },
  "frameworks": [
    "AVFoundation",
    "AVKit",
    "AdSupport",
    "CoreGraphics",
    "CoreLocation",
    "CoreTelephony",
    "Foundation",
    "StoreKit",
    "SystemConfiguration",
    "UIKit"
  ],
  "weak_frameworks": [
    "Combine",
    "CryptoKit",
    "SafariServices",
    "SwiftUI",
    "WebKit",
    "FBAudienceNetwork"
  ],
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64",
    "FRAMEWORK_SEARCH_PATHS": "$(PODS_ROOT)/CloudXMetaAdapter",
    "OTHER_LDFLAGS": "-framework CloudXMetaAdapter",
    "DEFINES_MODULE": "YES",
    "CLANG_ENABLE_MODULES": "YES",
    "OTHER_CFLAGS": "-fmodules"
  },
  "user_target_xcconfig": {
    "OTHER_LDFLAGS": "-ObjC"
  },
  "requires_arc": true
}
