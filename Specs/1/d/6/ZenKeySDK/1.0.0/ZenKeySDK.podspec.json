  "homepage": "https://github.com/MyZenKey/sp-sdk-ios",
  "documentation_url": "http://developer.myzenkey.com/",
  "license": {
    "type": "custom",
    "file": "LICENSE"
  },
  "authors": {
    "ZenKey": "techsupport@myzenkey.com"
  },
