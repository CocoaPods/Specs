{
  "name": "hyperswitch-ios-hermes",
  "version": "0.79.1",
  "summary": "hermes-engine",
  "description": "hermes-engine for hyperswitch ios sdk",
  "homepage": "https://hyperswitch.io/",
  "authors": "Harshit S",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "platforms": {
    "ios": "13.4"
  },
  "source": {
    "http": "https://repo1.maven.org/maven2/com/facebook/react/react-native-artifacts/0.79.1/react-native-artifacts-0.79.1-hermes-ios-release.tar.gz"
  },
  "preserve_paths": [
    "destroot/bin/*",
    "**/*.{h,c,cpp}"
  ],
  "source_files": "destroot/include/hermes/**/*.h",
  "header_mappings_dir": "destroot/include",
  "ios": {
    "vendored_frameworks": "destroot/Library/Frameworks/universal/hermes.xcframework"
  },
  "xcconfig": {
    "CLANG_CXX_LANGUAGE_STANDARD": "c++20",
    "CLANG_CXX_LIBRARY": "compiler-default"
  }
}
