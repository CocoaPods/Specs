{
  "name": "EasyRest",
  "version": "1.2.0",
  "summary": "A simple RestClient for iOS.",
  "description": "A simple rest library for use in our iOS project's",
  "homepage": "https://github.com/lucasmpaim/EasyRest",
  "license": "MIT",
  "authors": {
    "Lucas": "lucasmpaim1@gmail.com"
  },
  "source": {
    "git": "https://github.com/lucasmpaim/EasyRest.git",
    "tag": "1.2.0"
  },
  "platforms": {
    "ios": "9.0"
  },
  "requires_arc": true,
  "default_subspecs": "Core",
  "source_files": "EasyRest/Classes/**/*",
  "dependencies": {
    "Alamofire": [
      "~> 4.8.1"
    ]
  },
  "subspecs": [
    {
      "name": "Core"
    },
    {
      "name": "PromiseKit",
      "source_files": [
        "EasyRest/Classes/**/*",
        "PromiseKit/Classes/**/*"
      ],
      "dependencies": {
        "PromiseKit": [
          "~> 6.8"
        ]
      }
    }
  ]
}
