{
  "name": "ICInputAccessory",
  "version": "2.0.1",
  "summary": "A customized token text field used in the iCook app.",
  "description": "KeyboardDismissTextField:\n* An input accessory view with a button to dismiss keyboard.\n\nTokenField:\n* A horizontal scrolling UI that groups input texts.\n* Easy to add, select and delete tokens.\n* Customizable icon and colors.\n* Supports storyboard.\n\nOptionPickerControl:\n* An easy to use UIControl that displays a UIPickerView with given options.",
  "screenshots": [
    "https://raw.githubusercontent.com/polydice/ICInputAccessory/master/screenshots/ICTokenField.png",
    "https://raw.githubusercontent.com/polydice/ICInputAccessory/master/screenshots/ICKeyboardDismissTextField.png"
  ],
  "homepage": "https://github.com/polydice/ICInputAccessory",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": [
    "bcylin",
    "trisix"
  ],
  "platforms": {
    "ios": "8.0"
  },
  "swift_version": "4.0",
  "source": {
    "git": "https://github.com/polydice/ICInputAccessory.git",
    "tag": "v2.0.1"
  },
  "requires_arc": true,
  "default_subspecs": [
    "KeyboardDismissTextField",
    "OptionPickerControl",
    "TokenField"
  ],
  "subspecs": [
    {
      "name": "KeyboardDismissTextField",
      "source_files": "Source/KeyboardDismissTextField/*.swift",
      "resources": "Source/KeyboardDismissTextField/*.xcassets"
    },
    {
      "name": "OptionPickerControl",
      "source_files": "Source/OptionPickerControl/*.swift"
    },
    {
      "name": "TokenField",
      "source_files": "Source/TokenField/*.swift"
    }
  ]
}
