{
  "name": "Flurry-iOS-SDK",
  "version": "7.0.0",
  "summary": "Flurry SDK for iOS",
  "license": {
    "type": "Commercial",
    "file": "Licenses/Flurry-LICENSE.txt"
  },
  "description": "FlurrySDK consists of: Flurry for analytics tracking and reporting. Flurry Ads for Native, Full Screen Ads integation",
  "homepage": "http://developer.yahoo.com/flurry",
  "authors": {
    "Flurry": "integration@flurry.com"
  },
  "source": {
    "git": "https://github.com/flurry/Flurry-iOS-SDK.git",
    "tag": "7.0.0"
  },
  "platforms": {
    "ios": "5.0"
  },
  "requires_arc": false,
  "default_subspecs": "FlurrySDK",
  "resource_bundles": {
    "Flurry-iOS-SDK": [
      "Pod/Assets/*.png"
    ]
  },
  "subspecs": [
    {
      "name": "FlurrySDK",
      "source_files": [
        "Flurry/Flurry.h",
        "Flurry/Empty.m"
      ],
      "frameworks": [
        "Foundation",
        "SystemConfiguration",
        "UIKit",
        "Security"
      ],
      "vendored_libraries": "Flurry/libFlurry_7.0.0.a"
    },
    {
      "name": "FlurryWatchSDK",
      "source_files": [
        "Flurry/FlurryWatch.h",
        "Flurry/Flurry.h"
      ],
      "frameworks": [
        "Foundation",
        "SystemConfiguration",
        "UIKit",
        "Security"
      ],
      "vendored_libraries": "Flurry/libFlurry_7.0.0.a"
    },
    {
      "name": "FlurryAds",
      "source_files": [
        "FlurryAds/**/*.h"
      ],
      "libraries": "z",
      "frameworks": [
        "CoreGraphics",
        "CoreMedia",
        "MediaPlayer",
        "AVFoundation"
      ],
      "weak_frameworks": [
        "AdSupport",
        "StoreKit"
      ],
      "vendored_libraries": "FlurryAds/libFlurryAds_7.0.0.a",
      "dependencies": {
        "Flurry-iOS-SDK/FlurrySDK": [

        ]
      }
    },
    {
      "name": "TumblrAPI",
      "source_files": [
        "FlurryTumblrAPI/FlurryTumblr.h",
        "FlurryTumblrAPI/FlurryTumblrDelegate.h"
      ],
      "dependencies": {
        "Flurry-iOS-SDK/FlurrySDK": [

        ]
      }
    }
  ]
}
