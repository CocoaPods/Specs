{
  "name": "UberSignature",
  "version": "1.0.3",
  "license": {
    "type": "BSD"
  },
  "homepage": "https://github.com/uber/UberSignature",
  "authors": "Uber",
  "summary": "Draw signatures in Swift and Objective-C.",
  "source": {
    "git": "https://github.com/uber/UberSignature.git",
    "tag": "1.0.3"
  },
  "default_subspecs": "Swift",
  "frameworks": [
    "CoreGraphics",
    "Foundation",
    "UIKit"
  ],
  "requires_arc": true,
  "platforms": {
    "ios": "10.0"
  },
