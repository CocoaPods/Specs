  "subspecs": [
    {
      "name": "ObjC",
      "private_header_files": "Sources/ObjC/Internal/*.h",
      "source_files": [
        "Sources/ObjC/**/*.{h,m}",
        "Sources/ObjC/Internal/*.h"
      ]
    },
    {
      "name": "Swift",
      "source_files": "Sources/Swift/**/*.swift"
    }
