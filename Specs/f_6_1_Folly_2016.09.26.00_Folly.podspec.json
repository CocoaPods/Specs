  "compiler_flags": "-DFOLLY_NO_CONFIG -DFOLLY_MOBILE=1 -DFOLLY_USE_LIBCPP=1",
  "source_files": [
    "folly/Bits.cpp",
    "folly/Conv.cpp",
    "folly/Demangle.cpp",
    "folly/StringBase.cpp",
    "folly/Unicode.cpp",
    "folly/dynamic.cpp",
    "folly/json.cpp",
    "folly/portability/BitsFunctexcept.cpp",
    "folly/detail/MallocImpl.cpp"
