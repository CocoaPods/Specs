  "authors": "Facebook",
  "source": {
    "git": "https://github.com/facebook/folly.git",
    "tag": "v2016.09.26.00"
  },
  "module_name": "folly",
  "dependencies": {
    "boost-for-react-native": [

    ],
    "DoubleConversion": [

    ],
    "glog": [

    ]
