      ]
    },
    {
      "name": "3D",
      "public_header_files": "3D/MAMapKit.framework/Versions/2.3.0.79b9c08.60159/Headers/*.h",
      "resources": "3D/MAMapKit.framework/Versions/2.3.0.79b9c08.60159/Resources/AMap.bundle",
