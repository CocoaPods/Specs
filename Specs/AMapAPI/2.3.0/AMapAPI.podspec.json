  "social_media_url": "http://lbsbbs.amap.com/forum.php?mod=forumdisplay&fid=38",
  "docset_url": "http://lbs.amap.com/api/ios-sdk/reference/",
  "source": {
    "http": "http://lbs.amap.com/wp-content/uploads/2014/06/AMap_iOS_API_Lib_V2.3.0.zip"
  },
