  "homepage": "https://github.com/Sanchew/Zombie",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "sanchew": "sanchew@mail.com"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source": {
    "git": "https://github.com/sanchew/Zombie.git",
