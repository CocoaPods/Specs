          "AWSiOSSDK/S3": [
            "~>1.7.1"
          ],
          "MyiOSHelpers/Logic/ThirdPartyHelpers/CocoaLumberjack": [
            "~>0.1.0"
          ],
          "MyiOSHelpers/Logic/Blocks": [
            "~>0.1.0"
          ]
        }
      }
    }
  ]
