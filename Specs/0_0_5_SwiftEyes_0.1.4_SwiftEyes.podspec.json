  "homepage": "https://github.com/oaleeapp/SwiftEyes",
  "license": {
    "type": "3-clause BSD",
    "file": "LICENSE"
  },
  "authors": {
    "Victor Lee": "specialvict@gmail.com"
  },
  "social_media_url": "http://twitter.com/oaleeapp",
  "platforms": {
    "ios": "9.0"
  },
  "source": {
