        "sqlite3/common": [

        ]
      },
      "pod_target_xcconfig": {
        "OTHER_CFLAGS": "$(inherited) -DSQLITE_ENABLE_JSON1=1"
      }
    },
    {
      "name": "rbu",
      "dependencies": {
        "sqlite3/common": [

        ]
      },
      "pod_target_xcconfig": {
        "OTHER_CFLAGS": "$(inherited) -DSQLITE_ENABLE_RBU=1"
      }
    },
    {
      "name": "rtree",
