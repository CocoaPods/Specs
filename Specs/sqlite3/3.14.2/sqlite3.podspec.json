        "sqlite3/common": [

        ]
      },
      "pod_target_xcconfig": {
        "OTHER_CFLAGS": "$(inherited) -DSQLITE_ENABLE_RTREE=1"
      }
    },
    {
      "name": "session",
      "dependencies": {
        "sqlite3/common": [

        ]
      },
      "pod_target_xcconfig": {
        "OTHER_CFLAGS": "$(inherited) -DSQLITE_ENABLE_PREUPDATE_HOOK=1 -DSQLITE_ENABLE_SESSION=1"
      }
    },
    {
      "name": "snapshot",
