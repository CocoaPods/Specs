{
  "name": "UrbanAirship-iOS-SDK",
  "documentation_url": "http://docs.urbanairship.com/reference/libraries/ios/8.0.1/",
  "version": "8.0.1",
  "license": "BSD",
  "platforms": {
    "ios": "8.0"
  },
  "summary": "A simple way to integrate Urban Airship services into your iOS applications.",
  "homepage": "https://github.com/urbanairship/ua-ios-pod",
  "authors": {
    "Urban Airship": "support@urbanairship.com"
  },
  "source": {
    "git": "https://github.com/urbanairship/ua-ios-pod.git",
    "tag": "8.0.1"
  },
  "module_name": "AirshipKit",
  "requires_arc": true,
  "source_files": [
    "Airship/AirshipLib.h",
    "Airship/{Common,External,Inbox,Push}/**/*.h",
    "UADummySource.m"
  ],
  "vendored_libraries": [
    "Airship/**.a"
  ],
  "libraries": [
    "z",
    "sqlite3"
  ],
  "resources": [
    "Airship/AirshipResources.bundle"
  ],
  "frameworks": [
    "UserNotifications",
    "CFNetwork",
    "CoreGraphics",
    "Foundation",
    "MobileCoreServices",
    "Security",
    "SystemConfiguration",
    "UIKit",
    "CoreTelephony",
    "CoreLocation",
    "CoreData"
  ],
  "pod_target_xcconfig": {
    "OTHER_LDFLAGS": "-ObjC -lz -lsqlite3"
  }
}
