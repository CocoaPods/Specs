{
  "name": "MFCocoaExtras",
  "version": "1.0.5",
  "summary": "MFCocoaExtras is a cross-platofrm (iOS/MacOS) Utility Library.",
  "description": "MFCocoaExtras is a cross-platofrm (iOS/MacOS) Utility Library build on Cocoa or CocoaTouch (UIKit) frameworks.\nIt includes\n- Convenient extras ( categories ) on some Cocoa classes\n- Graphics Utilities\n- Security",
  "homepage": "https://github.com/moosefactory/MFCocoaExtras",
  "license": "MIT",
  "authors": {
    "Tristan Leblanc": "tristan@moosefactory.eu"
  },
  "source": {
    "git": "https://gitlab.com/MooseFactorySoftware/MFCocoaExtras.git",
    "tag": "1.0.5"
  },
  "social_media_url": "https://twitter.com/moosefactory_eu",
  "platforms": {
    "ios": "7.0",
    "osx": "10.6"
  },
  "requires_arc": true,
  "ios": {
    "source_files": [
      "MFCocoaExtras/Sources/*",
      "MFCocoaExtras/Sources/Graphics/*",
      "MFCocoaExtras/Sources/CocoaExtras/*",
      "MFCocoaExtras/Sources/Security/*",
      "MFCocoaExtras/Sources/View/*",
      "MFCocoaExtras/Sources/CocoaTouch+Extras/*"
    ],
    "frameworks": "SystemConfiguration"
  },
  "osx": {
    "source_files": [
      "MFCocoaExtras/Sources/*",
      "MFCocoaExtras/Sources/Graphics/*",
      "MFCocoaExtras/Sources/CocoaExtras/*",
      "MFCocoaExtras/Sources/Security/*",
      "MFCocoaExtras/Sources/View/*"
    ],
    "frameworks": "SystemConfiguration"
  }
}
