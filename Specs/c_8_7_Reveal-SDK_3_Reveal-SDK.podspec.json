{
  "name": "Reveal-SDK",
  "version": "3",
  "summary": "The Reveal SDK: Runtime view debugging for iOS & tvOS apps & extensions.",
  "description": "Reveal brings powerful runtime view debugging to iOS developers.\nPlease visit our website for more details: http://revealapp.com/",
  "homepage": "https://revealapp.com/",
  "screenshots": "https://images.revealapp.com/CocoaPods/promo.jpg",
  "social_media_url": "https://twitter.com/reveal_app",
  "authors": "Itty Bitty Apps Pty Ltd",
  "source": {
    "http": "https://download.revealapp.com/RevealServer-3.zip"
  },
  "platforms": {
    "ios": "8.0",
    "tvos": "9.0"
  },
  "compiler_flags": "-ObjC",
  "frameworks": [
    "CoreGraphics",
    "CFNetwork",
    "QuartzCore"
  ],
  "libraries": "z",
  "ios": {
    "vendored_frameworks": "RevealServer-3/iOS/RevealServer.framework"
  },
  "tvos": {
    "vendored_frameworks": "RevealServer-3/tvOS/RevealServer.framework"
  },
  "xcconfig": {
    "LD_RUNPATH_SEARCH_PATHS": "$(inherited) @executable_path/Frameworks @loader_path/Frameworks"
  },
  "requires_arc": false,
  "license": "Commercial"
}
