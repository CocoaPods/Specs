  "documentation_url": "https://docs.helpcrunch.com/ios-sdk.html",
  "homepage": "https://github.com/helpcrunch/sdk-cocoapods",
  "license": {
    "type": "Apache 2.0",
    "file": "LICENSE"
  },
  "authors": {
