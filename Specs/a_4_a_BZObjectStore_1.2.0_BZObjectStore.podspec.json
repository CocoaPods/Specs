    {
      "name": "ActiveRecord",
      "dependencies": {
        "BZObjectStore/Core": [

        ]
      },
      "source_files": "BZObjectStore/ActiveRecord/*.{h,m}"
    },
    {
      "name": "Parse",
      "platforms": {
        "ios": "5.0",
        "osx": "10.8"
      },
      "ios": {
        "dependencies": {
          "BZObjectStore/Core": [

          ],
          "Parse-iOS-SDK": [

          ]
        },
        "frameworks": "Parse",
        "xcconfig": {
          "FRAMEWORK_SEARCH_PATHS": "\"$(PODS_ROOT)/Parse\""
        },
        "source_files": "BZObjectStore/BZObjectStoreParse/*.{h,m}"
      },
      "osx": {
        "dependencies": {
          "BZObjectStore/Core": [

          ],
