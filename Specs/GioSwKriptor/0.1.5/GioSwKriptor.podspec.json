    "file": "LICENSE"
  },
  "authors": {
    "Sercan Ã–zen": "sercangsu@gmail.com"
  },
  "source": {
    "git": "https://github.com/sercan5534/GioSwKriptor.git",
    "tag": "0.1.5"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source_files": "GioSwKriptor/Classes/**/*",
  "ios": {
    "source_files": "GioSwKriptor/openssl-ios/openssl/*.h",
    "preserve_paths": [
      "GioSwKriptor/openssl-ios/libcrypto.a",
      "GioSwKriptor/openssl-ios/libssl.a"
    ],
    "vendored_libraries": [
      "GioSwKriptor/openssl-ios/libcrypto.a",
      "GioSwKriptor/openssl-ios/libssl.a"
    ]
