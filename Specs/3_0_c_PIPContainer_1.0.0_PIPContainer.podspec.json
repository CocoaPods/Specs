{
  "name": "PIPContainer",
  "version": "1.0.0",
  "summary": "An easy to use interface for picture-in-picture on macOS 10.12 and later.",
  "homepage": "https://github.com/insidegui/PIPContainer",
  "description": "A view controller subclass to contain other view controllers to be presented in picture-in-picture mode",
  "license": {
    "type": "BSD"
  },
  "authors": "Guilherme Rambo",
  "social_media_url": "https://twitter.com/_inside",
  "source": {
    "git": "https://github.com/insidegui/PIPContainer.git",
    "tag": "1.0.0",
    "branch": "master"
  },
  "osx": {
    "source_files": "Sources/**/*.{h,m}",
    "private_header_files": "Sources/Private/*.h",
    "frameworks": [
      "Cocoa",
      "PIP"
    ]
  },
  "platforms": {
    "osx": "10.12"
  },
  "requires_arc": true,
  "xcconfig": {
    "FRAMEWORK_SEARCH_PATHS": "/System/Library/PrivateFrameworks"
  }
}
