{
  "name": "NaverMap",
  "module_name": "NaverMap",
  "version": "2.0.18",
  "summary": "A CocoaPods wrapper for navermap-ios-api.",
  "description": "A CocoaPods wrapper for navermap-ios-api.\nIt's not an offical CocoaPods.",
  "homepage": "https://github.com/wanbok/navermap-ios-api",
  "authors": {
    "Wanbok Choi": "choi@wanbok.com"
  },
  "source": {
    "git": "https://github.com/wanbok/navermap-ios-api.git",
    "tag": "v2.0.18"
  },
  "platforms": {
    "ios": "7.0"
  },
  "requires_arc": true,
  "resource_bundles": {
    "NaverMap": [
      "Resources/*.png"
    ]
  },
  "source_files": "Lib/**/*.h",
  "public_header_files": "Lib/**/*.h",
  "vendored_libraries": "Lib/**/*.a",
  "frameworks": [
    "UIKit",
    "CoreGraphics",
    "QuartzCore",
    "CoreLocation",
    "SystemConfiguration"
  ],
  "libraries": [
    "sqlite3",
    "xml2"
  ]
}
