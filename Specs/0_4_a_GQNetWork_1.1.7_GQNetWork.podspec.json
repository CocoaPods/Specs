  "homepage": "https://github.com/g763007297/GQNetWork",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "developer_高": "763007297@qq.com"
  },
  "platforms": {
    "ios": "5.0"
  },
  "source": {
    "git": "https://github.com/g763007297/GQNetWork.git",
    "tag": "1.1.7"
  },
  "requires_arc": true,
  "default_subspecs": "GQNetworking",
  "subspecs": [
    {
      "name": "Mapping",
      "ios": {
        "source_files": [
          "GQMapping/*.{h,m}",
