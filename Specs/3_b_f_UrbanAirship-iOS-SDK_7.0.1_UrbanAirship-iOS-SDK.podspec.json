{
  "name": "UrbanAirship-iOS-SDK",
  "documentation_url": "http://docs.urbanairship.com/reference/libraries/ios/7.0.1/",
  "version": "7.0.1",
  "license": "BSD",
  "platforms": {
    "ios": "8.0"
  },
  "summary": "A simple way to integrate Urban Airship services into your iOS applications.",
  "homepage": "https://github.com/urbanairship/ua-ios-pod",
  "authors": {
    "Urban Airship": "support@urbanairship.com"
  },
  "source": {
    "git": "https://github.com/urbanairship/ua-ios-pod.git",
    "tag": "7.0.1"
  },
  "module_name": "AirshipKit",
  "requires_arc": true,
  "source_files": [
    "Airship/AirshipLib.h",
    "Airship/{Common,External,Inbox,Push}/**/*.h"
  ],
  "vendored_libraries": [
    "Airship/**.a"
  ],
  "libraries": [
    "z",
    "sqlite3"
  ],
  "resources": [
    "Airship/AirshipResources.bundle"
  ],
  "frameworks": [
    "CFNetwork",
    "CoreGraphics",
    "Foundation",
    "MobileCoreServices",
    "Security",
    "SystemConfiguration",
    "UIKit",
    "CoreTelephony",
    "CoreLocation",
    "PassKit",
    "CoreData"
  ],
  "xcconfig": {
    "OTHER_LDFLAGS": [
      "-ObjC",
      "-lz",
      "-lsqlite3"
    ]
  }
}
