{
  "name": "SYAlipaySDK",
  "version": "1.0.0",
  "summary": "集成支付宝支付SDK，支持打包成动态库",
  "description": "1. 库版本和支付宝官方提供的SDK版本对应关系见Readme文件里面的版本说明\n2. 在支付宝App支付官方SDK包了一层，支持打包成动态库",
  "homepage": "https://github.com/isandboy/SYAlipaySDK",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "isandboy": "sandboylu@gmail.com"
  },
  "source": {
    "git": "https://github.com/isandboy/SYAlipaySDK.git",
    "tag": "1.0.0"
  },
  "platforms": {
    "ios": "8.0"
  },
  "public_header_files": "SYAlipaySDK/AlipaySDK.framework/**/*.h",
  "source_files": "SYAlipaySDK/**/*.{h,m}",
  "vendored_frameworks": "SYAlipaySDK/AlipaySDK.framework",
  "resources": "SYAlipaySDK/AlipaySDK.bundle",
  "libraries": [
    "c++",
    "z"
  ],
  "frameworks": [
    "CoreTelephony",
    "SystemConfiguration",
    "CoreTelephony",
    "QuartzCore",
    "CoreText",
    "CoreGraphics",
    "UIKit",
    "Foundation",
    "CFNetwork",
    "CoreMotion"
  ],
  "pod_target_xcconfig": {
    "OTHER_LDFLAGS": "-all_load"
  }
}
