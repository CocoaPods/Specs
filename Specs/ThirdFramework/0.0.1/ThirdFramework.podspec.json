  "authors": {
    "chunguiLiu": "chunguiLiu@126.com"
  },
  "source": {
    "git": "https://github.com/liuchungui/ThirdFramework.git",
    "tag": "0.0.1",
    "submodules": true
  },
  "platforms": {
    "ios": "7.0"
  },
  "requires_arc": true,
  "subspecs": [
    {
      "name": "Umeng",
      "source_files": "UMengSDK/*.{h,m}",
      "public_header_files": "UMengSDK/*.h",
