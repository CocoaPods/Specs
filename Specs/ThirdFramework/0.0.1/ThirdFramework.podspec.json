    },
    {
      "name": "WeChat",
      "source_files": "WeChat/*.h",
      "public_header_files": "WeChat/*.h",
      "vendored_libraries": "WeChat/libWeChatSDK.a",
      "frameworks": [
        "SystemConfiguration",
        "CoreTelephony"
      ],
      "libraries": [
        "z",
        "sqlite3.0",
        "c++"
      ]
    }
  ]
