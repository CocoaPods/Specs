{
  "name": "mobile-ffmpeg-video",
  "version": "3.1",
  "summary": "Mobile FFmpeg Video Dynamic Framework",
  "description": "Includes FFmpeg v4.1-10 with fontconfig v2.13.1, freetype v2.9.1, fribidi v1.0.5, kvazaar v1.2.0, libaom v1.0.0-1014, libass v0.14.0, libiconv v1.15, libtheora v1.1.1, libvpx v1.7.0, snappy v1.1.7 and libwebp v1.0.1 libraries enabled.",
  "homepage": "https://github.com/tanersener/mobile-ffmpeg",
  "authors": {
    "Taner Sener": "tanersener@gmail.com"
  },
  "license": {
    "type": "LGPL-3.0",
    "file": "mobileffmpeg.framework/LICENSE"
  },
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "ios": {
    "frameworks": [
      "Foundation",
      "AudioToolbox",
      "CoreImage",
      "CoreMotion",
      "CoreVideo",
      "GameController",
      "VideoToolbox"
    ],
    "libraries": [
      "z",
      "bz2",
      "c++"
    ],
    "vendored_frameworks": [
      "mobileffmpeg.framework",
      "libavcodec.framework",
      "libavdevice.framework",
      "libavfilter.framework",
      "libavformat.framework",
      "libavutil.framework",
      "libswresample.framework",
      "libswscale.framework"
    ]
  },
  "source": {
    "http": "https://github.com/tanersener/mobile-ffmpeg/releases/download/v3.1/mobile-ffmpeg-video-3.1-ios-framework.zip"
  }
}
