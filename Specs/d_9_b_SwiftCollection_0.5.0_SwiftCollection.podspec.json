    "Brad Hilton": "brad@skyvive.com"
  },
  "source": {
    "git": "https://github.com/bradhilton/SwiftCollection.git",
    "tag": "0.5.0"
  },
  "swift_versions": "5.0",
  "platforms": {
    "ios": "8.0"
  },
  "source_files": [
    "SwiftCollection",
    "SwiftCollection/**/*.{swift,h,m}"
  ],
  "requires_arc": true,
