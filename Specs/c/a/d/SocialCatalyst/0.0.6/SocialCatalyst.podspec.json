      "SocialCatalystSDK/Model.xcdatamodeld"
    ]
  },
  "preserve_paths": "SocialCatalystSDK/Info.plist",
  "dependencies": {
    "VK-ios-sdk": [

    ],
    "SDWebImage": [

    ],
    "ObjectMapper": [
      "~> 3.4"
    ],
    "MagicalRecord": [

    ],
    "SVProgressHUD": [

    ],
    "Appodeal": [

    ]
  },
  "static_framework": true,
  "pod_target_xcconfig": {
    "INFOPLIST_FILE": "${PODS_TARGET_SRCROOT}/SocialCatalystSDK/Info.plist"
  }
}
