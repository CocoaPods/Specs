{
  "name": "SocialCatalyst",
  "swift_version": "5.0",
  "version": "0.0.6",
  "summary": "SocialCatalyst - SDK for quickly creating applications based on VKontakte groups",
  "description": "Welcome to SocialCatalyst SDK! SocialCatalyst - SDK for quickly creating applications based on VKontakte groups. It's very easy to use",
  "homepage": "http://lwts.ru",
  "license": {
    "type": "GNU GPL v.3",
    "file": "LICENSE"
  },
  "authors": {
    "MIKHAIL LUTSKII": "mikhail@lwts.ru"
  },
  "social_media_url": "https://vk.com/loutskiy",
  "platforms": {
    "ios": "10.0"
  },
  "source": {
    "git": "https://github.com/loutskiy/SocialCatalyst-iOS.git",
    "tag": "0.0.6"
  },
  "source_files": [
    "SocialCatalystSDK",
    "SocialCatalystSDK/**/*.{h,m,swift,xcdatamodeld}"
