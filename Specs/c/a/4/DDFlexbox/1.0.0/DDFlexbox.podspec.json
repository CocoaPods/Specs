{
  "name": "DDFlexbox",
  "version": "1.0.0",
  "summary": "Flexbox on iOS.",
  "description": "An flexbox layout aimed at easy to use, which depend on Yoga.",
  "homepage": "https://github.com/djs66256/DDFlexbox",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Daniel": "djs66256@163.com"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source": {
    "git": "https://github.com/djs66256/DDFlexbox.git",
    "tag": "1.0.0"
  },
  "swift_versions": "4.2",
  "default_subspecs": "Swift",
  "frameworks": "UIKit",
  "dependencies": {
    "Yoga": [
      "~> 1.14"
    ]
  },
  "subspecs": [
    {
      "name": "Objc",
      "source_files": [
        "Classes",
        "Classes/objc/**/*.{h,m,mm}"
      ],
      "public_header_files": "Classes/objc/**/*.h",
      "private_header_files": "Classes/objc/Private/*.h"
    },
    {
      "name": "Swift",
      "source_files": [
        "Classes",
        "Classes/swift/**/*.{swift}"
      ]
    }
  ],
  "swift_version": "4.2"
}
