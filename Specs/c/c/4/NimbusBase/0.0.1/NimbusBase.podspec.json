{
  "name": "NimbusBase",
  "version": "0.0.1",
  "summary": "NimbusBase cloud SDK for iOS.",
  "homepage": "http://nimbusbase.com/tutorial-ios.html",
  "license": {
    "type": "Commercial",
    "text": "        Copyright (c) 2013 NimbusBase, Inc. All rights reserved.\n"
  },
  "authors": {
    "NimbusBase, Inc.": "release@nimbusbase.com"
  },
  "platforms": {
    "ios": "6.0"
  },
  "source": {
    "http": "http://nimbusbase.com/download/sdk/ios/0.0.1/NimbusBase.zip"
  },
  "source_files": "NimbusBase/Headers/*",
  "resources": "NimbusBase/Resources/NimbusBaseResources.bundle",
  "preserve_paths": "NimbusBase/libNimbusBase.a",
  "frameworks": [
    "CoreData",
    "MobileCoreServices",
    "QuartzCore",
    "Security",
    "SystemConfiguration"
