  "requires_arc": true,
  "xcconfig": {
    "LIBRARY_SEARCH_PATHS": "\"$(PODS_ROOT)/NimbusBase/NimbusBase\"",
    "OTHER_LDFLAGS": "-ObjC -force_load \"$(PODS_ROOT)/NimbusBase/NimbusBase/libNimbusBase.a\""
  }
