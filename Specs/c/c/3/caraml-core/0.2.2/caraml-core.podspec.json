{
  "name": "caraml-core",
  "version": "0.2.2",
  "summary": "Native mobile UI addon for LiquidCore",
  "description": "Provides a view that can be embedded in apps that exposes a surface API\nto LiquidCore.",
  "homepage": "https://github.com/LiquidPlayer/caraml-core",
  "license": {
    "type": "MIT",
    "file": "LICENSE.md"
  },
  "authors": {
    "Eric Lange": "eric@flicket.tv"
  },
  "platforms": {
    "ios": "11.0"
  },
  "source": {
    "git": "https://github.com/LiquidPlayer/caraml-core.git",
    "tag": "0.2.2"
  },
  "source_files": [
    "src/*.{cpp,h}",
    "caraml-core/caraml-core/*.{h,m,mm}"
  ],
  "public_header_files": [
    "caraml-core/caraml-core/LCCaramlJS.h",
    "caraml-core/caraml-core/LCCaramlSurface.h",
    "caraml-core/caraml-core/LCCaramlView.h",
    "caraml-core/caraml-core/caraml_core.h"
  ],
  "xcconfig": {
    "CLANG_WARN_DOCUMENTATION_COMMENTS": "NO",
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES",
    "OTHER_CFLAGS": "-Wno-nullability-completeness",
    "HEADER_SEARCH_PATHS": "${PODS_CONFIGURATION_BUILD_DIR}/LiquidCore-headers/LiquidCore_headers.framework/PrivateHeaders/v8 ${PODS_CONFIGURATION_BUILD_DIR}/LiquidCore-headers/LiquidCore_headers.framework/PrivateHeaders/node"
  },
  "dependencies": {
    "LiquidCore": [

    ],
    "LiquidCore-headers": [

    ]
  }
}
