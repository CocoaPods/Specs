{
  "name": "FlatbuffersCpp",
  "version": "0.0.1",
  "summary": "Flatbuffers for cpp.",
  "description": "TODO: Add long description of the pod here.",
  "homepage": "https://github.com/wanhmr/FlatbuffersCpp",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "tpphha": "tpphha@gmail.com"
  },
  "source": {
    "git": "https://github.com/wanhmr/FlatbuffersCpp.git",
    "tag": "0.0.1"
  },
  "platforms": {
    "ios": "9.0"
  },
  "vendored_libraries": "FlatbuffersCpp/lib/libflatbuffers.a",
  "source_files": "FlatbuffersCpp/**/*{.h}",
  "header_mappings_dir": "FlatbuffersCpp/include",
  "public_header_files": "FlatbuffersCpp/**/*{.h}",
  "module_map": "Module/FlatbuffersCpp.modulemap",
  "libraries": "c++",
  "static_framework": true,
  "xcconfig": {
    "CLANG_CXX_LANGUAGE_STANDARD": "c++11",
    "CLANG_CXX_LIBRARY": "libc++",
    "HEADER_SEARCH_PATHS": "\"$(inherited)\"",
    "USER_HEADER_SEARCH_PATHS": "\"$(PODS_TARGET_SRCROOT)\" \"$(PODS_TARGET_SRCROOT)/FlatbuffersCpp/include\"",
    "GCC_PREPROCESSOR_DEFINITIONS": "\"$(inherited)\" \"COCOAPODS=1\"",
    "CLANG_WARN_STRICT_PROTOTYPES": "NO",
    "CLANG_WARN_DOCUMENTATION_COMMENTS": "NO",
    "USE_HEADERMAP": "NO",
    "ALWAYS_SEARCH_USER_PATHS": "NO"
  }
}
