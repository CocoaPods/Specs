{
  "name": "PaddleOCR",
  "version": "0.2.3",
  "summary": "This is a framework for text recognition with image or camera, you can use this framework in iOS applications.",
  "description": "Your can access PaddleOCR.framework into your applications to recognition text, you can use the device camera or use photos to get some tex.",
  "homepage": "https://github.com/Leonard-iOS/PaddleOCR",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Leonard": "moyusword@gmail.com"
  },
  "platforms": {
    "ios": "9.0"
  },
  "source": {
    "git": "https://github.com/Leonard-iOS/PaddleOCR.git",
    "tag": "0.2.3"
  },
  "ios": {
    "vendored_frameworks": "PaddleOCR.framework"
  },
  "resources": "PaddleOCRResource.bundle",
  "frameworks": [
    "CoreMedia",
    "CoreVideo",
    "AVFoundation"
  ],
  "dependencies": {
    "OpenCV2": [
      "~> 3.4"
    ]
  },
  "libraries": "c++",
  "static_framework": true,
  "pod_target_xcconfig": {
    "DEFINES_MODULE": "YES",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "i386",
    "VALID_ARCHS": "x86_64 armv7 arm64"
  },
  "swift_versions": "5.0",
  "swift_version": "5.0"
}
