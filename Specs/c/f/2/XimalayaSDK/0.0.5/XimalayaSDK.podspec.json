    },
    {
      "name": "Request",
      "source_files": "SDK/include/Request/**/*.{h,m}",
      "public_header_files": "SDK/include/Request/**/*.h"
    },
    {
      "name": "Model",
      "source_files": "SDK/include/Model/**/*.{h,m}",
      "public_header_files": "SDK/include/Model/**/*.h"
    },
    {
      "name": "Utility",
