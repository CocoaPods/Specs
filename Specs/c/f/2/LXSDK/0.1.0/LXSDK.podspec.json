{
  "name": "LXSDK",
  "version": "0.1.0",
  "summary": "A short description of LXSDK.",
  "homepage": "https://gitee.com/octmon/lxsdk-ios.git",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "octmon": "octmon@qq.com"
  },
  "source": {
    "git": "https://gitee.com/octmon/lxsdk-ios.git",
    "tag": "0.1.0"
  },
  "platforms": {
    "ios": "11.0"
  },
  "default_subspecs": [
    "Core"
  ],
  "pod_target_xcconfig": {
    "OTHER_LDFLAGS": "$(inherited) -ObjC"
  },
  "xcconfig": {
    "VALID_ARCHS": "armv7 armv7s x86_64 arm64"
  },
  "subspecs": [
    {
      "name": "Core",
      "vendored_frameworks": "LXSDK.framework",
      "source_files": "LXSDK.framework/Headers/*.h",
      "public_header_files": "LXSDK.framework/Headers/*.h"
    },
    {
      "name": "IconAd",
      "dependencies": {
        "LXSDK/Core": [

        ],
        "CXHAdSDK/ChannelNoWechat": [
          "~> 1.9.2"
        ],
        "CXHAdSDK/RC": [
          "~> 1.9.2"
        ]
      }
    }
  ]
}
