{
  "name": "flagshipHorizon",
  "version": "0.1.0",
  "summary": "FlagshipHorizon OpenFeature adapter for iOS",
  "description": "A minimal OpenFeature provider (adapter) for iOS.\nUse it to register a FeatureProvider with OpenFeature and evaluate flags.",
  "homepage": "https://github.com/dream11-atharva/flagshipHorizon",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "210496608": "atharva.kothawade@dream11.com"
  },
  "source": {
    "git": "https://github.com/dream11-atharva/flagshipHorizon.git",
    "tag": "0.1.0"
  },
  "platforms": {
    "ios": "15.0"
  },
  "swift_versions": [
    "5.7",
    "5.8",
    "5.9"
  ],
  "default_subspecs": [
    "Core",
    "OpenFeature"
