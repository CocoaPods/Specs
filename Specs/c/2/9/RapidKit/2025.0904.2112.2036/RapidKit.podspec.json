  },
  "swift_versions": "5.0",
  "platforms": {
    "ios": "13.0",
    "osx": "10.14"
  },
  "ios": {
    "frameworks": [
      "CoreVideo",
      "UIKit"
    ],
    "pod_target_xcconfig": {
      "LD_RUNPATH_SEARCH_PATHS": "@executable_path/Frameworks @loader_path/Frameworks",
      "ARCHS": "arm64",
      "VALID_ARCHS": "arm64",
      "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64 x86_64 i386",
      "ONLY_ACTIVE_ARCH": "NO"
    }
  },
  "osx": {
    "frameworks": [
      "CoreVideo"
    ],
    "pod_target_xcconfig": {
      "LD_RUNPATH_SEARCH_PATHS": "@executable_path/../Frameworks @loader_path/Frameworks",
      "ARCHS": "arm64",
      "VALID_ARCHS": "arm64"
    }
  },
  "default_subspecs": "Main",
  "pod_target_xcconfig": {
    "DEFINES_MODULE": "YES",
    "HEADER_SEARCH_PATHS": "$(inherited) $(SRCROOT)/Frameworks/Sources/Public",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64 x86_64 i386"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64 x86_64 i386"
  },
  "subspecs": [
    {
      "name": "Main",
      "vendored_frameworks": "Release/RapidKit.xcframework",
      "ios": {
        "dependencies": {
          "RapidKit/WebRTC": [

          ]
        }
      },
      "libraries": "z"
    },
    {
      "name": "WebRTC",
      "vendored_frameworks": "Release/Libraries/iOS/WebRTC.framework",
      "platforms": {
        "ios": "13.0"
      }
    }
  ],
  "swift_version": "5.0"
}
