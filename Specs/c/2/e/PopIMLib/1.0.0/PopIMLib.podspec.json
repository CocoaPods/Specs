{
  "name": "PopIMLib",
  "version": "1.0.0",
  "summary": "IM聊天",
  "description": "YCIMLibSDK是一套功能完备的即时通讯开发工具包，提供稳定高效的即时消息收发、会话管理、用户状态同步、推送集成等核心能力，支持 iOS 平台的 Swift 与 Objective-C 混编项目，帮助开发者快速构建具备即时通讯功能的应用",
  "homepage": "https://github.com/luolong2018/PopIMLib",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "luolong": "15915840452@163.com"
  },
  "source": {
    "git": "https://github.com/luolong2018/PopIMLib",
    "tag": "1.0.0"
  },
  "platforms": {
    "ios": "12.0"
  },
  "swift_versions": "5.0",
  "vendored_frameworks": "PopIMLib/PopIMLib.framework",
  "static_framework": true,
  "frameworks": [
    "Foundation",
    "UIKit"
  ],
  "pod_target_xcconfig": {
    "MACH_O_TYPE": "staticlib",
    "BUILD_LIBRARY_FOR_DISTRIBUTION": "YES",
    "FRAMEWORK_SEARCH_PATHS": "$(SRCROOT)/**",
    "OTHER_LDFLAGS": "-ObjC"
  },
  "swift_version": "5.0"
}
