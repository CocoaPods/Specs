{
  "name": "ffmpeg-kit-macos-full",
  "version": "4.5.LTS",
  "summary": "FFmpeg Kit macOS Full Static Framework",
  "description": "Includes FFmpeg with dav1d, fontconfig, freetype, fribidi, gmp, gnutls, kvazaar, lame, libass, libilbc, libtheora, libvorbis, libvpx, libwebp, libxml2, opencore-amr, opus, shine, snappy, soxr, speex, twolame and vo-amrwbenc libraries enabled.",
  "homepage": "https://github.com/tanersener/ffmpeg-kit",
  "authors": {
    "Taner Sener": "tanersener@gmail.com"
  },
  "license": {
    "type": "LGPL-3.0",
    "file": "ffmpegkit.framework/LICENSE"
  },
  "platforms": {
    "osx": "10.11"
  },
  "requires_arc": true,
  "libraries": [
    "z",
    "bz2",
    "c++",
    "iconv"
  ],
  "source": {
    "http": "https://github.com/tanersener/ffmpeg-kit/releases/download/v4.5.LTS/ffmpeg-kit-full-4.5.LTS-macos-framework.zip"
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=macosx*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=macosx*]": "arm64"
  },
  "osx": {
    "frameworks": [
      "AudioToolbox",
      "CoreAudio",
      "CoreImage",
      "CoreMedia",
      "OpenCL",
      "OpenGL",
      "VideoToolbox"
    ],
    "vendored_frameworks": [
      "ffmpegkit.framework",
      "libavcodec.framework",
      "libavdevice.framework",
      "libavfilter.framework",
      "libavformat.framework",
      "libavutil.framework",
      "libswresample.framework",
      "libswscale.framework",
      "dav1d.framework",
      "expat.framework",
      "fontconfig.framework",
      "freetype.framework",
      "fribidi.framework",
      "giflib.framework",
      "gmp.framework",
      "gnutls.framework",
      "harfbuzz.framework",
      "jpeg.framework",
      "kvazaar.framework",
      "lame.framework",
      "libass.framework",
      "libhogweed.framework",
      "libilbc.framework",
      "libnettle.framework",
      "libogg.framework",
      "libopencore-amrnb.framework",
      "libpng.framework",
      "libsndfile.framework",
      "libtheora.framework",
      "libtheoradec.framework",
      "libtheoraenc.framework",
      "libvorbis.framework",
      "libvorbisenc.framework",
      "libvorbisfile.framework",
      "libvpx.framework",
      "libwebp.framework",
      "libwebpmux.framework",
      "libwebpdemux.framework",
      "libxml2.framework",
      "opus.framework",
      "shine.framework",
      "snappy.framework",
      "soxr.framework",
      "speex.framework",
      "tiff.framework",
      "twolame.framework",
      "vo-amrwbenc.framework"
    ]
  }
}
