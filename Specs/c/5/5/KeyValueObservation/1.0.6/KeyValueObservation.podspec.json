  "testspecs": [
    {
      "name": "Tests",
      "test_type": "unit",
      "platforms": {
        "ios": "9.0",
        "osx": "10.10",
        "tvos": "9.0",
        "watchos": "2.0"
      },
      "pod_target_xcconfig": {
