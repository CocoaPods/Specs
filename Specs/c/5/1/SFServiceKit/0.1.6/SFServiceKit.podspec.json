{
  "name": "SFServiceKit",
  "version": "0.1.6",
  "summary": "Some collections of tool classes (zh: 一些工具类集合).",
  "description": "Some collections of tool classes (zh: 一些工具类集合: 后台保活/显示模式/身份验证/文件管理/GCD计时器/区域监测/语言管理/位置定位/加载SFSymbol图标).",
  "homepage": "https://github.com/Sfh03031/SFServiceKit",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Sfh03031": "sfh894645252@163.com"
  },
  "source": {
    "git": "https://github.com/Sfh03031/SFServiceKit.git",
    "tag": "0.1.6"
  },
  "swift_versions": "5.0",
  "platforms": {
    "ios": "10.0"
  },
  "static_framework": true,
  "frameworks": [
    "UIKit",
    "Foundation",
    "MapKit",
    "LocalAuthentication",
    "CoreLocation",
    "AVFoundation"
  ],
  "default_subspecs": "Full",
  "subspecs": [
    {
      "name": "Bgtask",
      "source_files": "SFServiceKit/Classes/SFBackgroundTaskManager/*",
      "resource_bundles": {
        "Bgtask": [
          "SFServiceKit/Assets/SFBackgroundTaskManager/*.wav"
        ]
      }
    },
    {
      "name": "Display",
      "source_files": "SFServiceKit/Classes/SFDisplayModeManager/*"
    },
    {
      "name": "Author",
      "source_files": "SFServiceKit/Classes/SFAuthenticationManager/*"
    },
    {
      "name": "File",
      "source_files": "SFServiceKit/Classes/SFFileManager/*"
    },
    {
      "name": "Timer",
      "source_files": "SFServiceKit/Classes/SFGCDTimer/*"
    },
    {
      "name": "IBeacon",
      "source_files": "SFServiceKit/Classes/SFIBeaconManager/*"
    },
    {
      "name": "Language",
      "source_files": "SFServiceKit/Classes/SFLanguageManager/*"
    },
    {
      "name": "Location",
      "source_files": "SFServiceKit/Classes/SFLocationManager/*"
    },
    {
      "name": "Symbol",
      "source_files": "SFServiceKit/Classes/SFSymbolManager/*"
    },
    {
      "name": "IAP",
      "source_files": "SFServiceKit/Classes/SFIAPManager/*"
    },
    {
      "name": "Full",
      "dependencies": {
        "SFServiceKit/Bgtask": [

        ],
        "SFServiceKit/Display": [

        ],
        "SFServiceKit/Author": [

        ],
        "SFServiceKit/File": [

        ],
        "SFServiceKit/Timer": [

        ],
        "SFServiceKit/IBeacon": [

        ],
        "SFServiceKit/Language": [

        ],
        "SFServiceKit/Location": [

        ],
        "SFServiceKit/Symbol": [

        ],
        "SFServiceKit/IAP": [

        ]
      }
    }
  ],
  "swift_version": "5.0"
}
