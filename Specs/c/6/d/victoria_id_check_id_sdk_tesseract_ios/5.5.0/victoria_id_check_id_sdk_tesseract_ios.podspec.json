{
  "name": "victoria_id_check_id_sdk_tesseract_ios",
  "header_dir": "TesseractOCR",
  "version": "5.5.0",
  "summary": "Use Tesseract OCR in iOS projects written in either Objective-C or Swift.",
  "homepage": "https://github.com/victoria-id/Tesseract-OCR-iOS/",
  "documentation_url": "https://github.com/victoria-id/Tesseract-OCR-iOS/blob/master/README.md",
  "license": {
    "type": "MIT",
    "file": "LICENSE.md"
  },
  "authors": {
    "Victoria-ID": "support@victoria-id.com"
  },
  "source": {
    "git": "https://github.com/victoria-id/Tesseract-OCR-iOS.git",
    "tag": "5.5.0"
  },
  "platforms": {
    "ios": "15.0"
  },
  "requires_arc": true,
  "frameworks": [
    "UIKit",
    "Foundation"
  ],
  "ios": {
    "vendored_frameworks": [
      "TesseractOCR/xcframework/TesseractOCR.xcframework",
      "TesseractOCR/xcframework/Tesseract.xcframework",
      "TesseractOCR/xcframework/Leptonica.xcframework",
      "TesseractOCR/xcframework/JPEG.xcframework",
      "TesseractOCR/xcframework/PNG.xcframework",
      "TesseractOCR/xcframework/TIFF.xcframework"
    ]
  },
  "xcconfig": {
    "OTHER_LDFLAGS": "-lz",
    "CLANG_CXX_LIBRARY": "compiler-default"
  }
}
