{
  "name": "Reveal-SDK",
  "version": "20",
  "summary": "The Reveal SDK: Runtime view debugging for iOS & tvOS apps & extensions.",
  "description": "Reveal brings powerful runtime view debugging to iOS developers.\nPlease visit our website for more details: http://revealapp.com/",
  "homepage": "https://revealapp.com/",
  "license": {
    "type": "Commercial",
    "file": "RevealServer-20/LICENSE.md"
  },
  "authors": {
    "Itty Bitty Apps Pty Ltd": "support@revealapp.com"
  },
  "source": {
    "http": "https://download.revealapp.com/RevealServer-20.zip"
  },
  "social_media_url": "https://twitter.com/reveal_app",
  "screenshots": [
    "https://images.revealapp.com/CocoaPods/banner.jpg"
  ],
  "platforms": {
    "ios": "8.0",
    "tvos": "9.0"
  },
  "requires_arc": false,
  "compiler_flags": "-ObjC",
  "frameworks": [
    "CoreGraphics",
    "CFNetwork",
    "QuartzCore"
  ],
  "libraries": "z",
  "ios": {
    "vendored_frameworks": "RevealServer-20/iOS/RevealServer.framework"
  },
  "tvos": {
    "vendored_frameworks": "RevealServer-20/tvOS/RevealServer.framework"
  },
  "xcconfig": {
    "LD_RUNPATH_SEARCH_PATHS": "$(inherited) @executable_path/Frameworks @loader_path/Frameworks"
  }
}
