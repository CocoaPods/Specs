{
  "name": "EMASCurl",
  "version": "1.4.3",
  "summary": "Aliyun EMASCurl iOS SDK with HTTP/2 support.",
  "homepage": "https://www.aliyun.com/product/httpdns",
  "authors": {
    "xiaoyu": "yx456323@alibaba-inc.com"
  },
  "platforms": {
    "ios": "10.0"
  },
  "source": {
    "git": "https://github.com/aliyun/alicloud-ios-sdk-emascurl.git",
    "tag": "1.4.3"
  },
  "source_files": "EMASCurl/*.{h,m}",
  "public_header_files": [
    "EMASCurl/EMASCurl.h",
    "EMASCurl/EMASCurlLogger.h",
    "EMASCurl/EMASCurlProtocol.h",
    "EMASCurl/EMASCurlConfiguration.h"
  ],
  "requires_arc": true,
  "frameworks": "Foundation",
  "vendored_frameworks": "precompiled/libcurl-HTTP2.xcframework",
  "xcconfig": {
    "OTHER_LDFLAGS": "$(inherited) -ObjC -lz",
    "HEADER_SEARCH_PATHS": "$(inherited) ${PODS_ROOT}/EMASCurlHttp2/EMASCurl"
  }
}
