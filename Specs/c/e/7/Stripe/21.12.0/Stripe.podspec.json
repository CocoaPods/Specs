{
  "name": "Stripe",
  "version": "21.12.0",
  "summary": "Accept online payments using Stripe.",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "homepage": "https://stripe.com/docs/mobile/ios",
  "authors": {
    "Stripe": "support+github@stripe.com"
  },
  "source": {
    "git": "https://github.com/stripe/stripe-ios.git",
    "tag": "21.12.0"
  },
  "frameworks": [
    "Foundation",
    "Security",
    "WebKit",
    "PassKit",
    "Contacts",
    "CoreLocation",
    "UIKit"
  ],
  "requires_arc": true,
  "platforms": {
    "ios": "11.0"
  },
  "swift_versions": "5.0",
  "weak_frameworks": "SwiftUI",
  "source_files": [
    "Stripe/*.swift",
    "Stripe/PanModal/**/*.swift"
  ],
  "ios": {
    "resource_bundles": {
      "Stripe": "Stripe/Resources/**/*.{lproj,json,png,xcassets}"
    }
  },
  "dependencies": {
    "StripeCore": [
      "21.12.0"
    ],
    "StripeUICore": [
      "21.12.0"
    ],
    "StripeApplePay": [
      "21.12.0"
    ]
  },
  "subspecs": [
    {
      "name": "Stripe3DS2",
      "source_files": "Stripe3DS2/Stripe3DS2/**/*.{h,m}",
      "resource_bundles": {
        "Stripe3DS2": [
          "Stripe3DS2/Stripe3DS2/Resources/**/*.{lproj,png}"
        ]
      }
    }
  ],
  "swift_version": "5.0"
}
