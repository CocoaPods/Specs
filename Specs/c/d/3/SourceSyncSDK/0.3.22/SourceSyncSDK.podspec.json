{
  "name": "SourceSyncSDK",
  "version": "0.3.22",
  "summary": "A framework for handling activation details in iOS apps.",
  "homepage": "https://github.com/Source-Digital/sourcesync-sdk-ui-ios",
  "license": {
    "type": "MIT",
    "file": "LICENSE.md"
  },
  "authors": {
    "Source Digital": "dev@sourcedigital.net"
  },
  "platforms": {
    "ios": "14.0"
  },
  "swift_versions": "5.7",
  "source": {
    "git": "https://github.com/Source-Digital/sourcesync-sdk-ui-ios.git",
    "tag": "0.3.22"
  },
  "source_files": [
    "Sources/**/*.{h,m,swift}",
    "SourceSyncSDK/Sources/**/*.{h,m,swift}"
  ],
  "exclude_files": [
    "Package.swift",
    "TestApp/**/*",
    "Tests/**/*",
    "Example/**/*"
  ],
  "dependencies": {
    "DivKit": [
      "~> 31.13.0"
    ],
    "DivKitExtensions": [
      "~> 31.13.0"
    ]
  },
  "frameworks": [
    "UIKit",
    "Foundation"
  ],
  "ios": {
    "pod_target_xcconfig": {
      "ARCHS": "$(ARCHS_STANDARD)",
      "VALID_ARCHS": "arm64 x86_64 arm64e",
      "EXCLUDED_ARCHS": "",
      "BUILD_LIBRARY_FOR_DISTRIBUTION": "NO",
      "SWIFT_VERSION": "5.7",
      "ENABLE_BITCODE": "NO"
    }
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS": "",
    "FRAMEWORK_SEARCH_PATHS": "$(inherited)"
  },
  "requires_arc": true,
  "module_name": "SourceSyncSDK",
  "swift_version": "5.7"
}
