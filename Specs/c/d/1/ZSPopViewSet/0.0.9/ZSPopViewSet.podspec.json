  "homepage": "https://github.com/safiriGitHub/ZSPopViewSet",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "safiri": "safiri@163.com"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source": {
    "git": "https://github.com/safiriGitHub/ZSPopViewSet.git",
    "tag": "0.0.9"
  },
