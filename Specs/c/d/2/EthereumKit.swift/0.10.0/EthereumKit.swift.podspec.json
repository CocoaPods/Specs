  },
  "dependencies": {
    "OpenSslKit.swift": [
      "~> 1.0"
    ],
    "Secp256k1Kit.swift": [
      "~> 1.0"
    ],
    "HdWalletKit.swift": [
      "~> 1.5"
    ],
    "HsToolKit.swift": [
      "~> 1.0"
    ],
    "RxSwift": [
      "~> 5.0"
    ],
    "BigInt": [
      "~> 5.0"
    ],
    "GRDB.swift": [
      "~> 4.0"
    ],
    "BlueSocket": [
      "~> 1.0"
    ]
  },
  "swift_version": "5"
}
