{
  "name": "IronSourceAdMobAdapter",
  "version": "5.0.0.0",
  "summary": "AdMob Adapter",
  "description": "Use this adapter to show AdMob ads",
  "homepage": "http://www.is.com/",
  "license": {
    "type": "Commercial",
    "text": "https://platform.ironsrc.com/partners/terms-and-conditions-new-user"
  },
  "authors": {
    "IronSource": "http://www.is.com/contact/"
  },
  "source": {
    "http": "https://raw.githubusercontent.com/ironsource-mobile/iOS-adapters/master/admob-adapter/5.0.0/ISAdMobAdapter5.0.0.zip"
  },
  "source_files": "ISAdMobAdapter/ISAdMobAdapter.xcframework/**/*.{h,m}",
  "resource_bundles": {
    "ISAdMobResources": [
      "ISAdMobAdapter/ISAdMobAdapter.xcframework/Assets/**/*.nib"
    ]
  },
  "public_header_files": "ISAdMobAdapter/ISAdMobAdapter.xcframework/**/*.h",
  "preserve_paths": "ISAdMobAdapter/ISAdMobAdapter.xcframework",
  "platforms": {
    "ios": "12.0"
  },
  "pod_target_xcconfig": {
    "VALID_ARCHS": "arm64 x86_64"
  },
  "user_target_xcconfig": {
    "OTHER_LDFLAGS": "-ObjC"
  },
  "vendored_frameworks": "ISAdMobAdapter/ISAdMobAdapter.xcframework",
  "dependencies": {
    "IronSourceSDK/Ads": [
      "~> 9.0"
    ],
    "Google-Mobile-Ads-SDK": [
      "= 12.9.0"
    ]
  },
  "swift_versions": "5.0",
  "swift_version": "5.0"
}
