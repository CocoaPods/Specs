{
  "name": "FFSwiftExtension",
  "version": "0.0.3",
  "summary": "这是一个总结性的FFSwiftExtension.",
  "description": "构建初衷，希望可以不断总结一些扩展，便于开发",
  "homepage": "https://github.com/cocoanerd/FFSwiftExtension.git",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "cocoanerd": "cocoanerd@163.com"
  },
  "source": {
    "git": "https://github.com/cocoanerd/FFSwiftExtension.git",
    "tag": "0.0.3"
  },
  "platforms": {
    "ios": "8.0"
  },
  "swift_versions": "5.0",
  "resource_bundles": {
    "FFSwiftExtension": [
      "FFSwiftExtension/Assets/*"
    ]
  },
  "frameworks": [
    "UIKit",
    "Foundation"
  ],
  "subspecs": [
    {
      "name": "UIKit",
      "source_files": "FFSwiftExtension/UI{Application,Button,Color,Label,Pasteboard,Screen,View,ViewController}+Ex.swift",
      "dependencies": {
        "FFSwiftExtension/Views": [

        ],
        "MBProgressHUD": [

        ]
      }
    },
    {
      "name": "Foundation",
      "source_files": [
        "FFSwiftExtension/{String,AttributedString,CGFloat,Double,Int}+Ex.swift",
        "FFSwiftExtension/UserDefaults.swift"
      ]
    },
    {
      "name": "Safe",
      "source_files": "FFSwiftExtension/{Array,Dictionary}+Safe.swift"
    },
    {
      "name": "Views",
      "source_files": "FFSwiftExtension/HUDView.swift"
    },
    {
      "name": "WKCrashSDK",
      "source_files": "FFSwiftExtension/WKCrashSDK/**/*.{h,m}"
    }
  ],
  "swift_version": "5.0"
}
