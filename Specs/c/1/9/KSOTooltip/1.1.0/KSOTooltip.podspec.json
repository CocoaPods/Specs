    "William Towe": "willbur1984@gmail.com"
  },
  "source": {
    "git": "https://github.com/Kosoku/KSOTooltip.git",
    "tag": "1.1.0"
  },
  "platforms": {
    "ios": "10.0"
  },
  "requires_arc": true,
  "source_files": "KSOTooltip/**/*.{h,m}",
