{
  "name": "KKInitializer",
  "version": "0.0.4",
  "summary": "基础控件的便利构造器，包含UILabel+KKInitializer、UIButton+KKInitializer, UIImage+KKInitializer, 持续更新ing...",
  "homepage": "https://github.com/cocoZ/KKInitializer",
  "license": "MIT",
  "authors": {
    "cocoa": "cocoa.zkk@foxmail.com"
  },
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "source": {
    "git": "https://github.com/cocoZ/KKInitializer.git",
    "tag": "0.0.4"
  },
  "source_files": [
    "Initializer",
    "Initializer/*.{h,m}"
  ],
  "exclude_files": "KKInitializer/**/*.plist",
  "public_header_files": "Initializer/*.h",
  "subspecs": [
    {
      "name": "UILabel+KKInitializer",
      "source_files": [
        "UILabel",
        "UILabel/*.{h,m}"
      ]
    },
    {
      "name": "UIButton+KKInitializer",
      "source_files": [
        "UIButton",
        "UIButton/*.{h,m}"
      ]
    },
    {
      "name": "UIImage+KKInitializer",
      "source_files": [
        "UIImage",
        "UIImage/*.{h,m}"
      ]
    }
  ]
}
