  },
  "authors": {
    "John E. Bailey": "john@devmode.com",
    "Sean M. Duncan": "sean@devmode.com"
  },
  "social_media_url": "http://twitter.com/DevModeInc",
  "platforms": {
    "ios": 6.0
  },
  "source": {
    "git": "https://github.com/devmode/dm-image.git",
    "tag": "0.0.3"
  },
  "source_files": [
    "Classes",
    "Classes/**/*.{h,m}"
  ],
  "public_header_files": "Classes/**/*.h",
  "dependencies": {
    "DMFoundation": [
      "~> 0.1.0"
    ],
    "DMRequest": [
      "~> 0.2.0"
    ]
  },
