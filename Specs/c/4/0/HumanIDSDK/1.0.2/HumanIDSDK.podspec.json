{
  "name": "HumanIDSDK",
  "version": "1.0.2",
  "summary": "HumanIDSDK for iOS.",
  "description": "Bluenumber Foundation HumanID SDK for iOS platform.",
  "homepage": "https://github.com/bluenumberfoundation/humanid-ios-sdk",
  "license": {
    "type": "GPL",
    "file": "LICENSE"
  },
  "authors": {
    "Bluenumber Foundation": "developers@human-id.org"
  },
