{
  "name": "CryptoXC",
  "version": "0.1",
  "source": {
    "git": "https://github.com/tareq3/CrytoXCFramework.git",
    "tag": "0.1"
  },
  "summary": "Cryptography in Swift. SHA, MD5, CRC, PBKDF, Poly1305, HMAC, CMAC, HDKF, Scrypt, ChaCha20, Rabbit, Blowfish, AES, RSA.",
  "description": "Cryptography functions and helpers for Swift implemented in Swift. SHA-1, SHA-2, SHA-3, MD5, PBKDF1, PBKDF2, Scrypt, CRC, Poly1305, HMAC, ChaCha20, Rabbit, Blowfish, AES, RSA",
  "homepage": "https://github.com/tareq3/CrytoXCFramework",
  "license": {
    "type": "MIT"
  },
  "authors": {
    "Marcin Krzy≈ºanowski": "marcin@krzyzanowskim.com"
  },
  "requires_arc": true,
  "platforms": {
    "ios": "11.0"
  },
  "xcconfig": {
    "FRAMEWORK_SEARCH_PATHS": "$(inherited)"
  },
  "default_subspecs": "Debug",
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "i386"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "i386"
  },
  "subspecs": [
    {
      "name": "Debug",
      "vendored_frameworks": "CryptoXC.xcframework"
    },
    {
      "name": "Release",
      "vendored_frameworks": "CryptoXC.xcframework"
    }
  ]
}
