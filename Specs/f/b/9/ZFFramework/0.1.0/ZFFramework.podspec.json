      "name": "ZFCore",
      "public_header_files": "ZF/ZFCore/zfsrc/**/*.{h,hpp}",
      "header_mappings_dir": "ZF/ZFCore/zfsrc",
      "source_files": [
        "ZF/ZFCore/zfsrc/**/*.{h,hpp,c,cpp,m,mm}",
        "ZF/ZFCore/zfsrc_ext/**/*.{h,hpp,c,cpp,m,mm}"
      ],
      "resources": [
        "ZF/ZFCore/zfres"
      ],
      "exclude_files": "ZF/**/ZF_PUT_RES_FILES_HERE",
      "preserve_paths": "**/*"
