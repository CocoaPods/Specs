{
  "name": "SwifterSwift",
  "version": "5.0.0",
  "summary": "A handy collection of more than 500 native Swift extensions to boost your productivity.",
  "description": "SwifterSwift is a collection of over 500 native Swift extensions, with handy methods, syntactic sugar, and performance improvements for wide range of primitive data types, UIKit and Cocoa classes –over 500 in 1– for iOS, macOS, tvOS and watchOS.",
  "homepage": "https://github.com/SwifterSwift/SwifterSwift",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Omar Albeik": "omaralbeik@gmail.com"
  },
  "social_media_url": "http://twitter.com/omaralbeik",
  "screenshots": "https://raw.githubusercontent.com/SwifterSwift/SwifterSwift/master/Assets/logo.png",
  "documentation_url": "http://swifterswift.com/docs",
  "platforms": {
    "ios": "8.0",
    "osx": "10.10",
    "tvos": "9.0",
    "watchos": "2.0"
  },
  "swift_version": "5.0",
  "requires_arc": true,
  "source": {
    "git": "https://github.com/SwifterSwift/SwifterSwift.git",
    "tag": "5.0.0"
  },
  "source_files": "Sources/SwifterSwift/**/*.swift",
  "subspecs": [
    {
      "name": "SwiftStdlib",
      "source_files": "Sources/SwifterSwift/SwiftStdlib/*.swift"
    },
    {
      "name": "Foundation",
      "source_files": "Sources/SwifterSwift/Foundation/*.swift"
    },
    {
      "name": "UIKit",
      "source_files": [
        "Sources/SwifterSwift/Shared/*.swift",
        "Sources/SwifterSwift/UIKit/*.swift"
      ]
    },
    {
      "name": "AppKit",
      "source_files": [
        "Sources/SwifterSwift/Shared/*.swift",
        "Sources/SwifterSwift/AppKit/*.swift"
      ]
    },
    {
      "name": "CoreGraphics",
      "source_files": "Sources/SwifterSwift/CoreGraphics/*.swift"
    },
    {
      "name": "CoreLocation",
      "source_files": "Sources/SwifterSwift/CoreLocation/*.swift"
    },
    {
      "name": "MapKit",
      "source_files": "Sources/SwifterSwift/MapKit/*.swift"
    },
    {
      "name": "SpriteKit",
      "source_files": "Sources/SwifterSwift/SpriteKit/*.swift"
    },
    {
      "name": "Dispatch",
      "source_files": "Sources/SwifterSwift/Dispatch/*.swift"
    }
  ]
}
