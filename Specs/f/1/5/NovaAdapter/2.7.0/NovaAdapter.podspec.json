{
  "name": "NovaAdapter",
  "version": "2.7.0",
  "summary": "an adapter for Google SDK",
  "description": "an adapter for Google SDK for MSP S2S",
  "homepage": "https://github.com/aimsp/msp-ios-sdk/GoogleAdapter",
  "license": "Copyright",
  "authors": {
    "huanzhiNB": "huanzhi.zhang@newsbreak.com"
  },
  "source": {
    "http": "https://github.com/ParticleMedia/msp-ios-sdk-public/releases/download/2.7.0/NovaAdapter-2.7.0.zip",
    "type": "zip"
  },
  "platforms": {
    "ios": "13.0"
  },
  "source_files": "NovaAdapter/NovaAdapter/**/*.{h,m,swift}",
  "exclude_files": "Classes/Exclude",
  "dependencies": {
    "MSPSharedLibraries": [
      "2.5.0"
    ],
    "MSPOMSDK": [
      "2.3.0"
    ]
  },
  "vendored_frameworks": "NovaAdapter/NovaCore.xcframework",
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "static_framework": true
}
