  "platforms": {
    "ios": "8.0",
    "osx": "10.10",
    "watchos": "2.0",
    "tvos": "9.0"
  },
  "swift_version": "4.2",
  "default_subspecs": [
    "Alert",
    "BaseViewController",
    "General",
    "PageViewController",
    "Progress",
    "Protocols",
    "OnlineUI",
    "VersionUpdate"
  ],
  "subspecs": [
    {
      "name": "Alert",
      "platforms": {
        "ios": "8.0"
      },
      "ios": {
        "source_files": [
          "Alert/*.swift",
          "Public/TableViewCellable.swift"
        ]
      },
      "frameworks": "UIKit"
    },
    {
      "name": "BaseViewController",
      "platforms": {
        "ios": "8.0"
      },
      "ios": {
        "source_files": [
          "BaseViewController/*.swift",
          "BaseViewController/*.{h,m}"
        ]
      },
      "frameworks": "UIKit"
    },
    {
      "name": "General",
      "platforms": {
        "ios": "8.0"
      },
      "ios": {
        "source_files": "General/**/*.swift"
      },
      "frameworks": "UIKit",
      "dependencies": {
        "StarKit/Protocols": [

        ],
        "Kingfisher": [
          "~> 4.10.0"
        ]
      }
    },
    {
      "name": "PageViewController",
      "platforms": {
        "ios": "8.0"
      },
      "ios": {
        "source_files": "PageViewController/*.{h,m,swift}",
        "public_header_files": "PageViewController/*.h"
      },
      "requires_arc": true,
      "frameworks": "UIKit"
    },
    {
      "name": "Progress",
      "platforms": {
        "ios": "8.0"
      },
      "ios": {
        "source_files": "Progress/**/*.swift"
      },
      "frameworks": "UIKit"
    },
    {
      "name": "Protocols",
      "platforms": {
        "ios": "8.0"
      },
      "ios": {
        "source_files": "Protocols/*.swift"
      },
      "frameworks": "UIKit"
    },
    {
      "name": "OnlineUI",
      "platforms": {
        "ios": "8.0"
      },
      "ios": {
        "source_files": [
          "OnlineUI/**/*.swift",
          "Public/TableViewCellable.swift"
        ]
      },
      "frameworks": "UIKit",
      "dependencies": {
        "StarKit/BaseViewController": [

        ],
        "OLMaterialComponents/Typography": [
          "~> 1.0.0"
        ],
        "OLMaterialComponents/Font": [
          "~> 1.0.0"
        ],
        "StarKit/General": [

        ],
        "StarKit/Protocols": [

        ],
        "OLMaterialComponents/TextFields": [
          "~> 1.0.0"
        ],
        "OLMaterialComponents/Dialogs": [
          "~> 1.0.0"
        ]
      },
      "resource_bundles": {
        "UIView": [
          "OnlineUI/Assets/*.*"
        ]
      }
    },
    {
      "name": "VersionUpdate",
      "platforms": {
        "ios": "8.0"
      },
      "ios": {
        "source_files": "VersionUpdate/*.swift"
      },
      "frameworks": "UIKit",
      "dependencies": {
        "StarKit/Alert": [

        ]
      }
    }
  ]
}
