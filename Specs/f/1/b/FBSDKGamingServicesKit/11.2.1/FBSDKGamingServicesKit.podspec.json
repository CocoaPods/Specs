  "summary": "Official Facebook SDK for iOS to access Facebook Gaming Services",
  "description": "The Facebook SDK for iOS GamingKit framework provides:\n* Friend Finder.",
  "homepage": "https://developers.facebook.com/docs/ios/",
  "license": {
    "type": "Facebook Platform License",
    "file": "LICENSE"
  },
  "authors": "Facebook",
  "platforms": {
    "ios": "9.0"
  },
  "swift_versions": "5.0",
  "source": {
    "git": "https://github.com/facebook/facebook-ios-sdk.git",
    "tag": "v11.2.1"
  },
  "weak_frameworks": [
    "Foundation",
    "CoreGraphics"
  ],
  "requires_arc": true,
  "pod_target_xcconfig": {
