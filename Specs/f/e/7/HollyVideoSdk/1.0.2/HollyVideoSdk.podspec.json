{
  "name": "HollyVideoSdk",
  "version": "1.0.2",
  "summary": "合力视频客服SDK",
  "description": "用于手机移动端快速接入合力视频客服",
  "homepage": "https://gitee.com/hunterxman/holly-video-sdk.git",
  "license": {
    "type": "MIT"
  },
  "authors": {
    "hunterxman": "906162426@qq.com"
  },
  "source": {
    "git": "https://gitee.com/hunterxman/holly-video-sdk.git",
    "tag": "1.0.2"
  },
  "platforms": {
    "ios": "13.0"
  },
  "source_files": "HollyVideoSdk.framework/headers/*.h",
  "public_header_files": "HollyVideoSdk.framework/headers/*.h",
  "resources": "HollyVideo.bundle",
  "vendored_frameworks": "HollyVideoSdk.framework",
  "frameworks": [
    "Foundation",
    "UIKit",
    "AVFoundation"
  ],
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64",
    "VALID_ARCHS": "arm64 x86_64",
    "HEADER_SEARCH_PATHS": "${PODS_ROOT}/HollyVideoSdk/"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64",
    "VALID_ARCHS": "arm64 x86_64",
    "HEADER_SEARCH_PATHS": "${PODS_ROOT}/HollyVideoSdk/"
  },
  "dependencies": {
    "TXLiteAVSDK_TRTC": [
      "~>12.2"
    ],
    "SDWebImage": [

    ]
  },
  "static_framework": true
}
