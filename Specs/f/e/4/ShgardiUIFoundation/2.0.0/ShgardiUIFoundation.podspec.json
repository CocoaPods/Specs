      "platforms": {
        "ios": "13.0"
      },
      "dependencies": {
        "ShgardiUIFoundation/Core": [

        ],
        "ShgardiFoundation/Rx": [

        ]
      }
    }
  ],
  "swift_version": "5.0"
}
