      "LeakChecker/Classes/Core/**/*",
      "LeakChecker/Classes/Utils/**/*",
      "LeakChecker/Classes/UIKit-Utils/**/*",
      "LeakChecker/Classes/UIKit-UI/**/*"
    ]
