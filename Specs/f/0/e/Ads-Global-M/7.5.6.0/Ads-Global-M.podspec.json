{
  "name": "Ads-Global-M",
  "version": "7.5.6.0",
  "summary": "pangle mediation",
  "description": "pangle mediation",
  "homepage": "https://github.com/bytedance/Bytedance-UnionAD.git",
  "license": {
    "type": "MIT",
    "file": "SDK/LICENSE"
  },
  "authors": {
    "zhangtianhao": "zhangtianhao.1230@bytedance.com"
  },
  "platforms": {
    "ios": "12.0"
  },
  "source": {
    "http": "https://sf16-fe-tos-sg.i18n-pglstatp.com/obj/pangle-sdk-static-va/7.5.6.0/SDK.zip"
  },
  "frameworks": [
    "UIKit",
    "CoreFoundation",
    "WebKit",
    "StoreKit",
    "CoreServices",
    "MediaPlayer",
    "CoreMedia",
    "AVFoundation",
    "CoreTelephony",
    "SystemConfiguration",
    "AdSupport",
    "CoreMotion",
    "Accelerate"
  ],
  "weak_frameworks": "AppTrackingTransparency",
  "libraries": [
    "z",
    "bz2",
    "resolv.9",
    "c++",
    "sqlite3",
    "xml2",
    "c++abi"
  ],
  "pod_target_xcconfig": {
    "OTHER_LDFLAGS": "-ObjC",
    "COMPILER_INDEX_STORE_ENABLE": "NO",
    "LLVM_LTO[config=Debug][sdk=*][arch=*]": "NO",
    "LLVM_LTO[config=Release][sdk=*][arch=*]": "NO",
    "GCC_OPTIMIZATION_LEVEL[config=Debug][sdk=*][arch=*]": "0",
    "GCC_OPTIMIZATION_LEVEL[config=Release][sdk=*][arch=*]": "z",
    "ASSETCATALOG_COMPILER_OPTIMIZATION": "space",
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES",
    "CODE_SIGNING_ALLOWED": "NO",
    "VALID_ARCHS": "x86_64 arm64"
  },
  "requires_arc": true,
  "default_subspecs": [
    "Standard"
  ],
  "subspecs": [
    {
      "name": "Standard",
      "dependencies": {
        "Ads-Global-M/PAGAdSDK": [

        ],
        "Ads-Global-M/PAGMediation": [

        ]
      }
    },
    {
      "name": "PAGAdSDK",
      "vendored_frameworks": [
        "SDK/PAGAdSDK.xcframework"
      ],
      "preserve_paths": "SDK/PAGAdSDK.xcframework",
      "resources": "SDK/PAGAdSDK.bundle",
      "resource_bundles": {
        "AdsGlobalSDK": [
          "SDK/PAGAdSDK.xcframework/ios-arm64/PAGAdSDK.framework/PrivacyInfo.xcprivacy"
        ]
      }
    },
    {
      "name": "PAGMediation",
      "vendored_frameworks": [
        "SDK/PAGMediation.xcframework"
      ],
      "preserve_paths": "SDK/PAGMediation.xcframework"
    },
    {
      "name": "PAGMTestSuite",
      "vendored_frameworks": [
        "SDK/PAGMTestSuite.xcframework"
      ],
      "preserve_paths": "SDK/PAGMTestSuite.xcframework",
      "resources": "SDK/PAGMTestSuite.bundle"
    }
  ]
}
