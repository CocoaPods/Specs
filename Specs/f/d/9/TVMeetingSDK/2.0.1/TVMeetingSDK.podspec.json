{
  "name": "TVMeetingSDK",
  "version": "2.0.1",
  "summary": "TVMeetingSDK for IOS",
  "description": "TVMeetingSDK.framework一款用于开启即时视频会议的sdk",
  "homepage": "https://github.com/loneKiss/TVMeetingSDK",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "loneKiss": "775865767@qq.com"
  },
  "platforms": {
    "ios": "9.0"
  },
  "source": {
    "git": "https://github.com/loneKiss/TVMeetingSDK.git",
    "tag": "2.0.1"
  },
  "resources": [
    "TVMeetingSDKReadme.md"
  ],
  "vendored_frameworks": "TVMeetingSDK/Frameworks/*.framework",
  "source_files": "TVMeetingSDK/Headers/*.h",
  "resource_bundles": {
    "TVMeetingSDK": [
      "TVMeetingSDK/Resources/*"
    ]
  },
  "requires_arc": true,
  "pod_target_xcconfig": {
    "FRAMEWORK_SEARCH_PATHS": "$(inherited) $(PODS_ROOT)/**",
    "HEADER_SEARCH_PATHS": "$(inherited) $(PODS_ROOT)/**",
    "VALID_ARCHS": "armv7 arm64 x86_64",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  }
}
