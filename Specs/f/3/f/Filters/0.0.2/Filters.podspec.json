{
  "name": "Filters",
  "version": "0.0.2",
  "summary": "A Bloom/Cuckoo Filter implementation in Swift.",
  "description": "A Bloom/Cuckoo Filter implementation in Swift. with scalable and high performance.",
  "homepage": "https://github.com/panicfrog/Filters.git",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "yongping": "burnedfrog@163.com"
  },
  "platforms": {
    "ios": "11.0",
    "osx": "10.13"
  },
  "source": {
    "git": "https://github.com/panicfrog/Filters.git",
    "tag": "0.0.2"
  },
  "source_files": [
    "Sources/Filters/**/*.{swift}",
    "Sources/Cmurmur3/**/*.{c,h}",
    "Sources/Cxxh/**/*.{c,h}"
  ],
  "public_header_files": [
    "Sources/Cmurmur3/include/*.h",
    "Sources/Cxxh/include/xxhash.h"
  ],
  "preserve_paths": [
    "Sources/Cmurmur3/module.modulemap",
    "Sources/Cxxh/module.modulemap"
  ],
  "pod_target_xcconfig": {
    "SWIFT_INCLUDE_PATHS": "$(SRCROOT)/Filters/Sources/Cmurmur3/include/**$(SRCROOT)/Filters/Sources/Cxxh/include/**"
  },
  "swift_versions": "5.7",
  "swift_version": "5.7"
}
