{
  "name": "MoboreIosSdk",
  "version": "0.2.0",
  "summary": "Mobore iOS SDK for Real User Monitoring",
  "description": "A comprehensive iOS SDK for real user monitoring, crash reporting, and performance tracking.",
  "homepage": "https://github.com/mobore/mobore-ios-sdk",
  "license": {
    "type": "Commercial",
    "text": "Copyright 2025 Mobore.com"
  },
  "authors": {
    "Mobore": "team@mobore.com"
  },
  "documentation_url": "https://docs.mobore.com",
  "platforms": {
    "ios": "16.0"
  },
  "swift_versions": "5.10",
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "i386 x86_64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "i386 x86_64"
  },
  "source": {
    "git": "https://github.com/mobore/mobore-ios-sdk.git",
    "tag": "0.2.0"
  },
  "source_files": "Sources/mobore-ios-sdk/**/*.{swift,h,m}",
  "resource_bundles": {
    "MoboreIosSdk": [
      "Sources/mobore-ios-sdk/Resources/**/*"
    ]
  },
  "requires_arc": true,
  "static_framework": true,
  "dependencies": {
    "OpenTelemetry-Swift-Sdk": [
      "~> 2.1.0"
    ],
    "OpenTelemetry-Swift-SdkResourceExtension": [
      "~> 2.1.0"
    ],
    "OpenTelemetry-Swift-PersistenceExporter": [
      "~> 2.1.0"
    ],
    "OpenTelemetry-Swift-Instrumentation-URLSession": [
      "~> 2.1.0"
    ],
    "OpenTelemetry-Swift-Protocol-Exporter-Http": [
      "~> 2.1.0"
    ],
    "OpenTelemetry-Swift-Instrumentation-NetworkStatus": [
      "~> 2.1.0"
    ],
    "Kronos": [
      "~> 4.2"
    ],
    "PLCrashReporter": [
      "~> 1.12"
    ]
  },
  "swift_version": "5.10"
}
