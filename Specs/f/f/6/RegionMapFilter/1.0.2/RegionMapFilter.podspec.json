{
  "name": "RegionMapFilter",
  "version": "1.0.2",
  "summary": "Region-based filtering for Google Maps iOS",
  "description": "RegionMapFilter provides utilities for filtering and managing regions\nusing Google Maps SDK on iOS. Includes customizable UI components and\nsupports multiple region selection with polygon overlay.",
  "homepage": "https://github.com/Ahmedramadaan333/RegionMapFilter",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Ahmed Ramadan": "12ramadan.ail@gmail.com"
  },
  "source": {
    "git": "https://github.com/Ahmedramadaan333/RegionMapFilter.git",
    "tag": "1.0.2"
  },
  "platforms": {
    "ios": "13.0"
  },
  "swift_versions": "5.5",
  "static_framework": true,
  "source_files": "RegionMapFilter/RegionMapFilter.docc/Resources/RegionMapFilter/**/*.swift",
  "resource_bundles": {
    "RegionMapFilter": [
      "RegionMapFilter/RegionMapFilter.docc/Resources/RegionMapFilter/GeojsonFile/*.geojson"
    ]
  },
  "exclude_files": "RegionMapFilter/RegionMapFilter.docc/Package.swift",
  "dependencies": {
    "GoogleMaps": [
      "~> 7.0"
    ]
  },
  "frameworks": [
    "UIKit",
    "MapKit"
  ],
  "swift_version": "5.5"
}
