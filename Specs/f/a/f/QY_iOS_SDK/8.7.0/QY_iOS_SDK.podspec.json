{
  "name": "QY_iOS_SDK",
  "version": "8.7.0",
  "license": {
    "type": "Copyright",
    "text": " Copyright 2021 Netease \n"
  },
  "summary": "网易七鱼客服访客端 iOS SDK",
  "homepage": "https://qiyukf.com",
  "authors": {
    "qiyukf": "yunshangfu@126.com"
  },
  "source": {
    "git": "https://github.com/qiyukf/QY_iOS_SDK.git",
    "tag": "8.7.0"
  },
  "platforms": {
    "ios": "9.0"
  },
  "frameworks": [
    "AVFoundation",
    "UIKit",
    "SystemConfiguration",
    "MobileCoreServices",
    "WebKit",
    "CoreTelephony",
    "CoreText",
    "CoreMedia",
    "ImageIO",
    "CoreMotion",
    "AudioToolbox",
    "Photos",
    "AssetsLibrary",
    "Accelerate"
  ],
  "libraries": [
    "c++",
    "z",
    "sqlite3.0",
    "xml2"
  ],
  "resources": [
    "**/Resources/QYResource.bundle",
    "**/Resources/QYLanguage.bundle",
    "**/Resources/QYCustomResource.bundle"
  ],
  "default_subspecs": "Default",
  "pod_target_xcconfig": {
    "VALID_ARCHS": "armv7 x86_64 arm64"
  },
  "requires_arc": true,
  "subspecs": [
    {
      "name": "Default",
      "resources": [
        "**/Resources/QYResource.bundle",
        "**/Resources/QYLanguage.bundle",
        "**/Resources/QYCustomResource.bundle"
      ],
      "vendored_frameworks": "**/QYSDK.xcframework",
      "dependencies": {
        "NIMSDK_LITE": [
          "8.9.110"
        ]
      }
    },
    {
      "name": "Abroad",
      "resources": [
        "**/Resources/QYResource.bundle",
        "**/Resources/QYLanguage.bundle",
        "**/Resources/QYCustomResource.bundle",
        "**/Resources/QYConfigResource.bundle"
      ],
      "vendored_frameworks": "**/QYSDK.xcframework",
      "frameworks": "QYSDK",
      "dependencies": {
        "NIMSDK_LITE": [
          "9.2.8"
        ]
      }
    },
    {
      "name": "Video",
      "resources": [
        "**/Resources/QYResource.bundle",
        "**/Resources/QYLanguage.bundle",
        "**/Resources/QYCustomResource.bundle",
        "**/Resources/QYVideoResource.bundle"
      ],
      "vendored_frameworks": [
        "**/QYSDK.xcframework",
        "**/QYVideoService.xcframework"
      ],
      "dependencies": {
        "NIMSDK_LITE": [
          "8.9.110"
        ],
        "NERtcSDK": [
          "4.6.20"
        ]
      }
    }
  ]
}
