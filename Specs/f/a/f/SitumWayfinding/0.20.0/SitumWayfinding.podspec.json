{
  "name": "SitumWayfinding",
  "version": "0.20.0",
  "summary": "Indoor Location for iOS.",
  "static_framework": true,
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "description": "With Situm IPS platform you can develop your wayfinding solution from zero, and with the module Situm WYF you can easily integrate guiding functionality in an existing APP to improve your visitors experience, whether in hospitals, malls, airports, corporate headquarters or convention centers.",
  "homepage": "http://developers.situm.es/pages/mobile/ios/",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Name": "support@situm.es"
  },
  "source": {
    "git": "https://github.com/situmtech/ios-swift-library-wayfinding.git",
    "tag": "0.20.0"
  },
  "platforms": {
    "ios": "12.0"
  },
  "swift_versions": "5.0",
  "source_files": "SitumWayfinding/Classes/**/*",
  "resources": [
    "SitumWayfinding/Assets/*.storyboard",
    "SitumWayfinding/Assets/Images/**/*.png",
    "SitumWayfinding/Assets/Fonts/**/*.ttf"
  ],
  "resource_bundles": {
    "SitumWayfinding": [
      "SitumWayfinding/Localizations/**/*",
      "SitumWayfinding/Classes/situm_google_maps_style.json",
      "SitumWayfinding/Classes/situm_google_maps_style_dark.json",
      "SitumWayfinding/Assets/Images/**/*.xcassets",
      "SitumWayfinding/Assets/Fonts/**/*.ttf"
    ]
  },
  "dependencies": {
    "GoogleMaps": [
      "~> 4.2.0"
    ],
    "SitumSDK": [
      "~> 3.7.2"
    ],
    "Google-Maps-iOS-Utils": [
      "~> 4.1.0"
    ]
  },
  "deprecated_in_favor_of": "SitumSDK",
  "swift_version": "5.0"
}
