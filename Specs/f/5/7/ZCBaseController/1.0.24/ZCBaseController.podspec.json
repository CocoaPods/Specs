{
  "name": "ZCBaseController",
  "version": "1.0.24",
  "summary": "a ZCBaseController for ios.",
  "description": "It is a ZCBaseController used on iOS, which implement by Objective-C.",
  "homepage": "https://github.com/ChenZhenChun/ZCBaseController",
  "license": "MIT",
  "authors": {
    "ChenZhenChun": "346891964@qq.com"
  },
  "source": {
    "git": "https://github.com/ChenZhenChun/ZCBaseController.git",
    "tag": "1.0.24",
    "submodules": true
  },
  "platforms": {
    "ios": "9.0"
  },
  "requires_arc": true,
  "frameworks": [
    "Foundation",
    "UIKit"
  ],
  "subspecs": [
    {
