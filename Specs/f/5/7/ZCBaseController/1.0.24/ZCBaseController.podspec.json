        "Macros": [
          "~> 1.0"
        ],
        "Categorys": [
          "~> 1.0"
        ],
        "MJRefresh": [
          "3.2.0"
        ],
        "UITableView+FDTemplateLayoutCell": [
          "1.6.0"
