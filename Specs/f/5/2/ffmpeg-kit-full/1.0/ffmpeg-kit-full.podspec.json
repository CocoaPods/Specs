{
  "name": "ffmpeg-kit-full",
  "version": "1.0",
  "summary": "FFmpeg Kit iOS Full Shared Framework",
  "description": "Full iOS build of FFmpeg Kit with all default libraries.",
  "homepage": "https://github.com/voyageh/ffmpeg-kit",
  "authors": {
    "ARTHENICA": "open-source@arthenica.com"
  },
  "license": {
    "type": "LGPL-3.0",
    "file": "ffmpegkit.xcframework/ios-arm64/ffmpegkit.framework/LICENSE"
  },
  "platforms": {
    "ios": "12.1"
  },
  "requires_arc": true,
  "libraries": [
    "z",
    "bz2",
    "c++",
    "iconv"
  ],
  "source": {
    "http": "https://github.com/voyageh/ffmpeg-kit/releases/download/v6.0/ffmpeg-kit-full.zip"
  },
  "ios": {
    "frameworks": [
      "AudioToolbox",
      "AVFoundation",
      "CoreMedia",
      "VideoToolbox"
    ]
  },
  "vendored_frameworks": [
    "ffmpegkit.xcframework",
    "libavcodec.xcframework",
    "libavdevice.xcframework",
    "libavfilter.xcframework",
    "libavformat.xcframework",
    "libavutil.xcframework",
    "libswresample.xcframework",
    "libswscale.xcframework"
  ]
}
