    "commit": "b4e29e1384f16328d6801476d3ca98cfad77a260"
  },
  "platforms": {
    "ios": "9.0"
  },
  "source_files": [
    "MWSplitPhotoBrowser",
    "Pod/Classes/**/*"
  ],
  "resource_bundles": {
    "MWSplitPhotoBrowser": [
      "Pod/Assets/*.png"
    ]
  },
  "requires_arc": true,
  "frameworks": [
    "ImageIO",
    "QuartzCore",
    "AssetsLibrary",
    "MediaPlayer"
  ],
  "weak_frameworks": "Photos",
