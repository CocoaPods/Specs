{
  "name": "DJISDKExtension",
  "version": "0.4.0",
  "summary": "Extension for DJISDK-iOS.",
  "description": "Extension for DJISDK-iOS, only support Swift.",
  "homepage": "https://github.com/gzkiwiinc/DJISDKExtension",
  "license": "MIT",
  "authors": {
    "Kyle": "lacklock@gmail.com",
    "Pandara": "wen.pandara@gmail.com"
  },
  "swift_version": "4.2",
  "platforms": {
    "ios": "9.0"
  },
  "requires_arc": true,
  "xcconfig": {
    "VALID_ARCHS": "arm64 arm64e x86_64"
  },
  "source": {
    "git": "https://github.com/gzkiwiinc/DJISDKExtension.git",
    "tag": "0.4.0"
  },
  "source_files": [
    "DJISDKExtension/*.swift",
    "DJISDKExtension/**/*.swift"
  ],
  "dependencies": {
    "DJI-SDK-iOS": [
      "~> 4.8"
    ],
    "PromiseKit": [
      "~> 6.4"
    ]
  },
  "pod_target_xcconfig": {
    "ENABLE_BITCODE": "NO"
  }
}
