{
  "name": "icu4c-numberformatter",
  "version": "0.1.0",
  "summary": "A pre-built set of ICU binaries",
  "description": "A pre-built set of ICU binaries used to provide the functionality of short number formatters.",
  "homepage": "https://github.com/kerrmarin/icu4c-numberformatter",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "kerrmarin": "kerr@kerrmarin.com"
  },
  "source": {
    "git": "https://github.com/kerrmarin/icu4c-numberformatter.git",
    "tag": "0.1.0"
  },
  "platforms": {
    "ios": "14.0"
  },
  "static_framework": true,
  "source_files": "product/include/**/*.{h}",
  "header_mappings_dir": "product/include",
  "public_header_files": "product/include/**/*.{h}",
  "vendored_frameworks": [
    "product/frameworks/icudata.xcframework",
    "product/frameworks/icui18n.xcframework",
    "product/frameworks/icuio.xcframework",
    "product/frameworks/icuuc.xcframework"
  ]
}
