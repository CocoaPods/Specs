{
  "name": "MiloSdk",
  "version": "8.0",
  "summary": "Milo SDK",
  "description": "For Milo SDK implementation steps, check README.md.",
  "homepage": "https://github.com/Glynk/Milo-iOS-SDK.git",
  "license": "MIT",
