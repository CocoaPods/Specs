    "Fabric": [
      "1.9.0"
    ],
    "Google/Analytics": [
      "2.0.4"
    ],
    "IQKeyboardManagerSwift": [
      "6.2.0"
    ],
    "Kingfisher": [
      "5.1.1"
    ],
    "lottie-ios": [
      "2.5.2"
    ],
    "ObjectMapper": [
      "3.4.2"
    ],
    "Socket.IO-Client-Swift": [
      "14.0.0"
    ]
  },
  "xcconfig": {
