{
  "name": "CocoaLUT",
  "version": "0.2.23",
  "summary": "LUTs (1D and 3D color lookup tables) for Cocoa applications.",
  "homepage": "http://github.com/videovillage/CocoaLUT",
  "license": "MIT",
  "authors": {
    "Wil Gieseler": "wil@wilgieseler.com",
    "Greg Cotten": "greg@gregcotten.com"
  },
  "source": {
