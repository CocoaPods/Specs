{
  "name": "STBaseProject",
  "version": "1.0.3",
  "summary": "Project infrastructure, common tools. The new project can inherit.",
  "homepage": "https://github.com/i-stack/STBaseProject",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "songMW": "songshoubing7664@163.com"
  },
  "source": {
    "git": "https://github.com/i-stack/STBaseProject.git",
    "tag": "1.0.3"
  },
  "platforms": {
    "ios": "12.0"
  },
  "swift_versions": [
    "5"
  ],
  "default_subspecs": "STBaseModule",
  "subspecs": [
    {
      "name": "STBaseModule",
      "subspecs": [
        {
          "name": "STBaseViewController",
          "source_files": [
            "STBaseProject/Classes/STBaseModule/STBaseViewController/*.swift"
          ]
        },
        {
          "name": "STBaseView",
          "source_files": [
            "STBaseProject/Classes/STBaseModule/STBaseView/*.swift"
          ]
        },
        {
          "name": "STBaseViewModel",
          "source_files": [
            "STBaseProject/Classes/STBaseModule/STBaseViewModel/*.swift"
          ]
        },
        {
          "name": "STBaseModel",
          "source_files": [
            "STBaseProject/Classes/STBaseModule/STBaseModel/*.swift"
          ]
        }
      ]
    },
    {
      "name": "STBaseConfig",
      "source_files": [
        "STBaseProject/Classes/STBaseConfig/*.swift"
      ]
    },
    {
      "name": "STScanner",
      "source_files": [
        "STBaseProject/Classes/STScanner/*.swift"
      ]
    },
    {
      "name": "STScreenshot",
      "source_files": [
        "STBaseProject/Classes/STScreenshot/*.swift"
      ]
    }
  ],
  "swift_version": "5"
}
