      "dependencies": {
        "KVOController": [

        ]
      }
    },
    {
      "name": "Bridge",
      "source_files": "XMNWeb/Classes/Bridge/**/*",
      "public_header_files": "XMNWeb/Classes/Bridge/*.h",
      "resource_bundles": {
        "XMNWebBridge": [
          "XMNWeb/Assets/Bridge/**/*"
        ]
      },
      "dependencies": {
        "XMNWeb/Core": [

        ]
      },
      "pod_target_xcconfig": {
        "GCC_PREPROCESSOR_DEFINITIONS": "XMNBRIDGE_ENABLED=1"
      },
      "user_target_xcconfig": {
        "GCC_PREPROCESSOR_DEFINITIONS": "XMNBRIDGE_ENABLED=1"
      }
    }
  ]
