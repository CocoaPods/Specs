    "Kin Foundation": "kin@kik.com"
  },
  "source": {
    "git": "https://github.com/kinfoundation/StellarKit.git",
    "tag": "0.2.1",
    "submodules": true
  },
  "dependencies": {
    "KinUtil": [
      "0.0.2"
    ]
  },
  "platforms": {
    "ios": "8.0"
  },
  "swift_version": "3.2",
  "preserve_paths": "StellarKit/source/CCommonCrypto/module.modulemap",
  "source_files": "StellarKit/source/**/*.swift",
  "pod_target_xcconfig": {
    "SWIFT_INCLUDE_PATHS": "$(PODS_TARGET_SRCROOT)/StellarKit/source/CCommonCrypto"
  }
}
