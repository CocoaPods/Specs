{
  "name": "StellarKit",
  "version": "0.2.1",
  "license": {
    "type": "MIT"
  },
  "homepage": "https://github.com/kinfoundation/StellarKit.git",
  "summary": "A framework for communicating with a Stellar Horizon node",
