{
  "name": "Pingpp",
  "version": "2.0.3",
  "summary": "Pingplusplus iOS SDK",
  "description": "                   移动应用支付接口。\n                   开发者不再需要编写冗长的代码，简单几步就可以使你的应用获得支付功能。\n                   让你的移动应用接入支付像大厦接入电力一样简单，方便，和温暖。\n                   支持微信支付，公众账号支付，支付宝钱包，百度钱包，银联手机支付。\n",
  "homepage": "https://pingxx.com",
  "license": "COMMERCIAL",
  "authors": {
    "Afon Weng": "xufeng.weng@pingxx.com"
  },
  "platforms": {
    "ios": "5.1.1"
  },
  "source": {
    "git": "https://github.com/PingPlusPlus/pingpp-ios.git",
    "tag": "2.0.3_all"
  },
  "source_files": [
    "lib/Pingpp.h",
    "lib/Dependencies/**/*.{h,m}"
  ],
  "public_header_files": "lib/Pingpp.h",
  "private_header_files": "lib/Dependencies/**/*.h",
  "vendored_libraries": "lib/**/*.a",
  "ios": {
    "vendored_frameworks": "lib/Channels/AlipaySDK.framework",
    "libraries": [
      "c++",
      "stdc++",
      "z"
    ]
  },
  "resources": "lib/**/*.bundle",
  "frameworks": [
    "CFNetwork",
    "SystemConfiguration",
    "Security",
    "CoreTelephony",
    "AddressBook",
    "AddressBookUI",
    "AudioToolbox",
    "CoreAudio",
    "CoreGraphics",
    "ImageIO",
    "MapKit",
    "MessageUI",
    "MobileCoreServices",
    "QuartzCore"
  ],
  "xcconfig": {
    "OTHER_LDFLAGS": "-lObjC"
  }
}
