  "homepage": "https://developers.google.com/identity/sign-in/ios/",
  "license": {
    "type": "Copyright",
    "text": "Copyright 2015 Google Inc. All Rights Reserved."
  },
  "authors": "Google Inc.",
  "platforms": {
