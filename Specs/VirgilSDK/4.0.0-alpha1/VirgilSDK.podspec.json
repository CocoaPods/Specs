{
  "name": "VirgilSDK",
  "version": "4.0.0-alpha1",
  "summary": "Virgil SDK for Apple devices and languages.",
  "cocoapods_version": ">= 0.36",
  "homepage": "https://github.com/VirgilSecurity/virgil-sdk-x/tree/v4",
  "license": {
