{
  "name": "OpenTokSDK-WebRTC",
  "version": "2.1.5",
  "authors": "TokBox",
  "summary": "The OpenTok iOS SDK lets you use OpenTok video sessions in apps you build for iPad, iPhone, and iPod touch devices.",
  "homepage": "https://github.com/opentok/opentok-ios-sdk-webrtc/",
  "license": {
    "type": "Apache",
    "file": "LICENSE.txt"
  },
  "source": {
    "git": "https://github.com/opentok/opentok-ios-sdk-webrtc.git",
    "tag": "2.1.5"
  },
  "platforms": {
    "ios": "5.0"
  },
  "source_files": "Opentok.framework/Versions/A/Headers/*.h",
  "resources": "Opentok.framework/Versions/A/Resources/opentok.bundle",
  "frameworks": [
    "Opentok",
    "UIKit",
    "CoreMedia",
    "CoreVideo",
    "CoreAudio",
    "CoreTelephony",
    "CFNetwork",
    "AVFoundation",
    "SystemConfiguration",
    "Security",
    "QuartzCore",
    "AudioToolbox",
    "OpenGLES",
    "MobileCoreServices"
  ],
  "libraries": [
    "stdc++",
    "z"
  ],
  "xcconfig": {
    "FRAMEWORK_SEARCH_PATHS": "\"$(PODS_ROOT)/OpenTokSDK-WebRTC/\""
  },
  "preserve_paths": "Opentok.framework",
  "requires_arc": false
}
