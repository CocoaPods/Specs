{
  "name": "ZerzuraKit",
  "version": "0.8.0",
  "license": {
    "type": "MIT"
  },
  "homepage": "https://github.com/VictorLi08/ZerzuraKit",
  "authors": {
    "Victor Li": "victorli08@outlook.com"
  },
  "summary": "Versatile general-purpose Swift library.",
  "source": {
    "git": "https://github.com/VictorLi08/ZerzuraKit.git",
    "tag": "v0.8.0"
  },
  "swift_versions": "5.1",
  "platforms": {
    "ios": "10.0",
    "tvos": "10.0",
    "watchos": "2.0",
    "osx": "10.10"
  },
  "source_files": "Common/*.swift",
  "ios": {
    "source_files": [
      "Common/Mobile/*.swift",
      "Common/iOS_tvOS/*.swift",
      "iOS/*.swift"
    ],
    "frameworks": "UIKit"
  },
  "tvos": {
    "source_files": [
      "Common/Mobile/*.swift",
      "Common/iOS_tvOS/*.swift",
      "tvOS/*.swift"
    ],
    "frameworks": "UIKit"
  },
  "watchos": {
    "source_files": [
      "Common/Mobile/*.swift",
      "watchOS/*.swift"
    ]
  },
  "osx": {
    "source_files": "macOS/*.swift",
    "frameworks": "Cocoa"
  },
  "frameworks": "CoreLocation",
  "swift_version": "5.1"
}
