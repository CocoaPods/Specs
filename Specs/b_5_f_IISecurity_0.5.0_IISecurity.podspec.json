      "name": "Exchange",
      "source_files": "IISecurity/Classes/Exchange/*.{h,m}"
    },
    {
      "name": "IISecurity",
      "source_files": "IISecurity/Classes/IISecurity/**/*.swift"
    },
    {
      "name": "DeviceSafeTool",
      "source_files": "IISecurity/Classes/DeviceSafeTool/**/*.{h,m}"
