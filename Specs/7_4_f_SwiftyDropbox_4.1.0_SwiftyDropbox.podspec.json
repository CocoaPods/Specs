{
  "name": "SwiftyDropbox",
  "version": "4.1.0",
  "summary": "Dropbox Swift SDK for API v2",
  "homepage": "https://dropbox.com/developers/",
  "license": "MIT",
  "authors": {
    "Stephen Cobbe": "scobbe@dropbox.com"
  },
  "source": {
    "git": "https://github.com/dropbox/SwiftyDropbox.git",
    "tag": "4.1.0"
  },
  "osx": {
    "source_files": [
      "Source/SwiftyDropbox/PlatformNeutral/**/*.{swift,h}",
      "Source/SwiftyDropbox/SwiftyDropbox_macOS/**/*.{swift,h}"
    ],
    "frameworks": [
      "AppKit",
      "Webkit",
      "SystemConfiguration",
      "Foundation"
    ]
  },
  "ios": {
    "source_files": [
      "Source/SwiftyDropbox/PlatformNeutral/**/*.{swift,h}",
      "Source/SwiftyDropbox/SwiftyDropbox_iOS/**/*.{swift,h}"
    ],
    "frameworks": [
      "UIKit",
      "Webkit",
      "SystemConfiguration",
      "Foundation"
    ]
  },
  "requires_arc": true,
  "platforms": {
    "osx": "10.11",
    "ios": "9.0"
  },
  "dependencies": {
    "Alamofire": [
      "~> 4.0.0"
    ]
  }
}
