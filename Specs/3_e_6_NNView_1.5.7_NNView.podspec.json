    ],
    "TZImagePickerController": [

    ],
    "PPNumberButton": [

    ],
    "Masonry": [

    ]
  },
  "pod_target_xcconfig": {
    "GCC_PREPROCESSOR_DEFINITIONS": "MAS_SHORTHAND_GLOBALS=1, MAS_SHORTHAND=1"
  },
  "user_target_xcconfig": {
    "GCC_PREPROCESSOR_DEFINITIONS": "MAS_SHORTHAND_GLOBALS=1, MAS_SHORTHAND=1"
  }
