{
  "name": "MHUIKIT",
  "version": "0.0.8",
  "summary": "iOS一套UI开发框架",
  "homepage": "https://github.com/hepassion/MHUIKIT",
  "license": "MIT",
  "authors": {
    "zhaomh": "hepassion@163.com"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source": {
    "git": "https://github.com/hepassion/MHUIKIT.git",
    "tag": "0.0.8"
  },
  "source_files": "Classes/*.h",
  "requires_arc": true,
  "frameworks": [
    "Foundation",
    "UIKit"
  ],
  "description": "MHUIKIT,包含了一些控件封装，工具类等，可加快开发速度。",
  "resource_bundles": {
    "MHUIKIT": [
      "Assets/*.png"
    ]
  },
  "subspecs": [
    {
      "name": "MHDubgger",
      "requires_arc": true,
      "source_files": "Classes/MHDubgger/*.{h,m}",
      "dependencies": {
        "Aspects": [
          "~> 1.4.1"
        ]
      }
    },
    {
      "name": "Encryption",
      "requires_arc": true,
      "source_files": "Classes/Encryption/*.{h,m}"
    },
    {
      "name": "Utils",
      "requires_arc": true,
      "source_files": "Classes/Utils/*.{h,m}"
    },
    {
      "name": "Observer",
      "requires_arc": true,
      "source_files": "Classes/Observer/*.{h,m}",
      "dependencies": {
        "MHUIKIT/Encryption": [

        ],
        "MHUIKIT/Utils": [

        ]
      }
    },
    {
      "name": "MHCoreKit",
      "requires_arc": true,
      "source_files": "Classes/MHCoreKit/*.{h,m}",
      "dependencies": {
        "MHUIKIT/Encryption": [

        ],
        "MHUIKIT/Utils": [

        ],
        "MHUIKIT/Observer": [

        ]
      }
    }
  ]
}
