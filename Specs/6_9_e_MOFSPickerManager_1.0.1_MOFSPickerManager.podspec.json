{
  "name": "MOFSPickerManager",
  "version": "1.0.1",
  "summary": "PickerManager for iOS",
  "description": "iOS PickerView整合，一行代码调用（省市区三级联动+日期选择+普通选择）",
  "homepage": "https://github.com/memoriesofsnows/MOFSPickerManagerDemo.git",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "memoriesofsnows": "luoyuant@163.com"
  },
  "social_media_url": "http://www.jianshu.com/u/f4284f2cc646",
  "platforms": {
    "ios": "7.0"
  },
  "source": {
    "git": "https://github.com/memoriesofsnows/MOFSPickerManagerDemo.git",
    "tag": "1.0.1"
  },
  "source_files": [
    "MOFSPickerManagerDemo/MOFSPickerManager/**/*.{h,m}",
    "MOFSPickerManagerDemo/GDataXMLNode/**/*.{h,m}"
  ],
  "exclude_files": "Classes/Exclude",
  "libraries": "xml2",
  "requires_arc": [
    "MOFSPickerManagerDemo/MOFSPickerManager/**/*.{h,m}"
  ],
  "module_name": "MOFSPickerManager",
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "$(SDKROOT)/usr/include/libxml2",
    "OTHER_LINKER_FLAGS": "-ObjC",
    "WEAK_REFERENCES_IN_MANUAL_RETAIN_RELEASE": "Yes"
  }
}
