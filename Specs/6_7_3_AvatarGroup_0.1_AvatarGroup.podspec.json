  "screenshots": "https://raw.githubusercontent.com/xflagstudio/AvatarGroup/master/screenshots/demo.png",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "lm2343635": "lm2343635@126.com"
  },
  "source": {
