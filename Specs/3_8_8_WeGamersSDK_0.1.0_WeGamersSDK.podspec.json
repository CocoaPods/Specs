{
  "name": "WeGamersSDK",
  "version": "0.1.0",
  "summary": "用于游戏接入的各类SDK",
  "description": "用于游戏接入的各类SDK。当前有4个：",
  "homepage": "https://github.com/dev8ios/WeGamersSDK",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "dev8ios": "iggdev8@gmail.com"
  },
  "source": {
    "git": "https://github.com/dev8ios/WeGamersSDK.git",
    "tag": "0.1.0"
  },
  "platforms": {
    "ios": "8.0"
  },
  "default_subspecs": "Core",
  "static_framework": true,
  "subspecs": [
    {
      "name": "Core",
      "vendored_frameworks": "WeGamersSDK/Core/Frameworks/*.framework",
      "frameworks": "AdSupport",
      "libraries": "c++",
      "resources": "WeGamersSDK/Core/Bundles/*.bundle"
    },
    {
      "name": "AuthKit",
      "source_files": "WeGamersSDK/Classes/AuthKit/**/*"
    },
    {
      "name": "GameReplayKit",
      "source_files": "WeGamersSDK/Classes/GameReplayKit/**/*"
    },
    {
      "name": "GameLiveKit",
      "source_files": "WeGamersSDK/Classes/GameLiveKit/**/*"
    },
    {
      "name": "CommunityKit",
      "source_files": "WeGamersSDK/Classes/CommunityKit/**/*"
    }
  ]
}
