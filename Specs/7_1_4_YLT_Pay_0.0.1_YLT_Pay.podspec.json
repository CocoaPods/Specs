{
  "name": "YLT_Pay",
  "version": "0.0.1",
  "summary": "支付方式集成",
  "description": "AliPay、WeChatPay、UnionPay、ApplePay等支付方式集成",
  "homepage": "https://github.com/YLTTeam/YLT_Pay.git",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "xphaijj0305@126.com": "xianggong@126.com"
  },
  "source": {
    "git": "https://github.com/YLTTeam/YLT_Pay.git",
    "tag": "0.0.1"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source_files": [
    "YLT_Pay/Classes/*.{h,m}",
    "YLT_Pay/Classes/AliPay/*.{h,m}",
    "YLT_Pay/Classes/AliPay/Util/*.{h,m}",
    "YLT_Pay/Classes/WeChatPay/*.{h,m}",
    "YLT_Pay/Classes/UnionPay/*.{h,m,mm}",
    "YLT_Pay/Classes/ApplePay/*.{h,m}",
    "YLT_Pay/Classes/IapPay/*.{h,m}"
  ],
  "public_header_files": [
    "YLT_Pay/Classes/*.h",
    "YLT_Pay/Classes/AliPay/*.h",
    "PHPayLib/Classes/AliPay/Util/*.h",
    "YLT_Pay/Classes/WeChatPay/*.h",
    "YLT_Pay/Classes/UnionPay/*.h",
    "YLT_Pay/Classes/ApplePay/*.h",
    "YLT_Pay/Classes/IapPay/*.h"
  ],
  "frameworks": "CoreMotion",
  "libraries": [
    "z",
    "sqlite3.0",
    "c++"
  ],
  "xcconfig": {
    "CLANG_CXX_LANGUAGE_STANDARD": "c++11",
    "CLANG_CXX_LIBRARY": "libc++"
  },
  "vendored_frameworks": "YLT_Pay/Classes/Alipay/*.framework",
  "vendored_libraries": [
    "YLT_Pay/Classes/WeChatPay/*.a",
    "YLT_Pay/Classes/UnionPay/*.a"
  ],
  "resources": "YLT_Pay/Classes/Alipay/*.bundle",
  "dependencies": {
    "YLT_BaseLib": [

    ]
  }
}
