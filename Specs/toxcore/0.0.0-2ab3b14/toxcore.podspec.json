{
  "name": "toxcore",
  "version": "0.0.0-2ab3b14",
  "summary": "Cocoapods wrapper for toxcore",
  "homepage": "https://github.com/Antidote-for-Tox/toxcore",
  "license": "GPLv3",
  "authors": {
    "Dmytro Vorobiov": "d@dvor.me"
  },
  "source": {
    "git": "https://github.com/Antidote-for-Tox/toxcore.git",
    "tag": "0.0.0-2ab3b14",
    "submodules": true
  },
  "platforms": {
    "ios": "7.0",
    "osx": "10.9"
  },
  "requires_arc": true,
  "ios": {
    "vendored_frameworks": "VPX.framework"
  },
  "osx": {
    "vendored_frameworks": "VPX.framework"
  },
  "xcconfig": {
    "FRAMEWORK_SEARCH_PATHS": "\"${PODS_ROOT}\""
  },
  "source_files": [
    "toxcore/toxcore/*.{c,h}",
    "toxcore/toxencryptsave/*.{c,h}",
    "toxcore/toxav/*.{c,h}",
    "toxcore/toxdns/*.{c,h}"
  ],
  "public_header_files": [
    "toxcore/toxcore/*.h",
    "toxcore/toxav/*.h",
    "toxcore/toxdns/*.h"
  ],
  "dependencies": {
    "libsodium": [
      "~> 1.0.1"
    ],
    "libopus-patched-config": [
      "~> 1.1"
    ]
  }
}
