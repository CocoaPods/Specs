{
  "name": "AMPSAdSDK",
  "version": "5.1.0.5",
  "summary": "AdScope 聚合广告管理平台.",
  "description": "可以兼容多家广告形式，一键式集成多家广告SDK，可自定义渠道接入，统一管理多个渠道。",
  "homepage": "https://gitee.com/adscope/amps-ad-sdk.git",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Cookie": "lvjunxue@hubcloud.com.cn"
  },
  "source": {
    "git": "https://gitee.com/adscope/amps-ad-sdk.git",
    "tag": "5.1.0.5"
  },
  "platforms": {
    "ios": "11.0"
  },
  "default_subspecs": "AMPSAdSDK",
  "libraries": [
    "z",
    "c++",
    "sqlite3",
    "c"
  ],
  "frameworks": [
    "AdSupport",
    "AVFoundation",
    "Accelerate",
    "AppTrackingTransparency",
    "CoreTelephony",
    "Foundation",
    "SystemConfiguration",
    "Security",
    "StoreKit",
    "WebKit",
    "UIKit",
    "SafariServices"
  ],
  "xcconfig": {
    "OTHER_LDFLAGS": "-ObjC"
  },
  "subspecs": [
    {
      "name": "AMPSAdSDK",
      "vendored_frameworks": [
        "AMPSAdSDK.xcframework"
      ],
      "dependencies": {
        "AdScopeFoundation": [
          "~>5.1.0.4"
        ]
      }
    }
  ]
}
