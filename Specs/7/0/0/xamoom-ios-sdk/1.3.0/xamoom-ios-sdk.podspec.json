{
  "name": "xamoom-ios-sdk",
  "version": "1.3.0",
  "summary": "xamoom-ios-sdk is a framework for the xamoom-cloud api. So you can write your own applications for the xamoom-cloud.",
  "homepage": "http://xamoom.github.io/xamoom-ios-sdk/",
  "license": "GNU",
  "authors": {
    "Raphael Seher": "raphael@xamoom.com"
  },
  "source": {
    "git": "https://github.com/xamoom/xamoom-ios-sdk.git",
    "tag": "1.3.0"
  },
  "social_media_url": "https://twitter.com/xamoom",
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "source_files": "src/**/*.{h,m}",
  "resource_bundles": {
    "xamoom-ios-sdk": [
      "Pod/Assets/*.*"
    ]
  },
  "resources": [
    "src/ContentBlocks/Blocks/*.xib",
    "Pod/Assets/*.*"
  ],
  "vendored_libraries": "src/lib/SVGKit/libSVGKit-iOS.1.2.0.a",
  "public_header_files": [
    "src/lib/SVGKit/usr/local/include/*.h",
    "src/*.h",
    "src/mapping/*.h",
    "src/ContentBlocks/*.h",
    "src/ContentBlocks/Blocks/*.h",
    "src/ContentBlocks/MapViews/*.h"
  ],
  "frameworks": [
    "CoreText",
    "CoreImage",
    "QuartzCore",
    "CoreGraphics",
    "MobileCoreServices",
    "Security",
    "CFNetwork",
    "SystemConfiguration"
  ],
  "libraries": "xml2",
  "dependencies": {
    "RestKit": [
      "~> 0.24.1"
    ],
    "QRCodeReaderViewController": [
      "~> 2.0.0"
    ],
    "SMCalloutView": [

    ],
    "youtube-ios-player-helper": [
      "~> 0.1.1"
    ],
    "SDWebImage": [
      "~>3.7"
    ]
  }
}
