    "HEADER_SEARCH_PATHS": "\"${PODS_ROOT}/ZSWebSDK\"",
    "VALID_ARCHS": "arm64 x86_64 armv7"
  },
  "pod_target_xcconfig": {
    "ARCHS[sdk=iphonesimulator*]": "$(ARCHS_STANDARD_64_BIT)"
