{
  "name": "YSTLiveModule",
  "version": "0.5.0",
  "summary": "直播模块",
  "description": "Ysten 直播模块",
  "homepage": "http://192.168.1.10:8181/ios-mobile/modules/YSTLiveModule.git",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Allen": "caifeng@ysten.com"
  },
  "source": {
    "git": "http://192.168.1.10:8181/ios-mobile/modules/YSTLiveModule.git",
    "tag": "0.5.0"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source_files": "YSTLiveModule/Classes/**/*.{h,pch,m}",
  "prefix_header_file": "YSTLiveModule/Classes/Private/OtherFiles/LiveModule.pch",
  "public_header_files": "YSTLiveModule/Classes/Public/**/*.h",
  "resource_bundles": {
    "YSTLiveModule": [
      "YSTLiveModule/Assets/JiangSu/*.{xcassets,imageset,png}",
      "YSTLiveModule/Assets/JiangSuTheme/*.h",
      "YSTLiveModule/Assets/JiangSuXib/*.xib",
      "YSTLiveModule/Classes/**/*.{storyboard,xib,cer,json,plist}",
      "YSTLiveModule/Assets/MBReosurce/*.{bundle,xcassets,imageset,png}"
    ]
  },
  "xcconfig": {
    "ENABLE_BITCODE": "NO",
    "GCC_PRECOMPILE_PREFIX_HEADER": "YES",
    "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited)",
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES"
  },
  "pod_target_xcconfig": {
    "OTHER_LDFLAGS": "$(inherited)",
    "SWIFT_VERSION": "4.0",
    "HEADER_SEARCH_PATHS": "$(inherited) $(SDKROOT)/usr/include/libxml2 $(SDKROOT)/usr/include/libresolv $(inherited)$(PROJECT_DIR) /usr/local/include",
    "CLANG_CXX_LANGUAGE_STANDARD": "c++11",
    "CLANG_CXX_LIBRARY": "libc++"
  },
  "frameworks": "UIKit",
  "dependencies": {
    "Masonry": [

    ],
    "MJExtension": [

    ],
    "MJRefresh": [

    ],
    "SDWebImage": [

    ],
    "YSTNetKit": [

    ],
    "AvoidCrash": [
      "~> 2.3.0-beta"
    ],
    "YSTPlayer": [

    ],
    "MGJRouter": [

    ],
    "YSTMRLog": [
      "~> 1.1.0"
    ],
    "YSTCommonKit": [

    ],
    "TYPagerController": [

    ]
  }
}
