      "frameworks": "MobileCoreServices",
      "requires_arc": false,
      "compiler_flags": "-DBZ_USE_SBJSON",
      "dependencies": {
        "SBJson": [

        ]
      }
    },
    {
      "name": "Core",
      "platforms": {
        "ios": "5.0"
      },
      "source_files": "BZFoursquare/*.{h,m}",
      "frameworks": "MobileCoreServices",
      "requires_arc": false
    }
  ]
