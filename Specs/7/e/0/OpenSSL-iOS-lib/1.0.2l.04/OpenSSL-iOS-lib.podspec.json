{
  "name": "OpenSSL-iOS-lib",
  "version": "1.0.2l.04",
  "summary": "根据最新官方源码编译的 OpenSSL iOS 库, 官方网址: https://www.openssl.org/source/",
  "authors": {
    "707689817@qq.com": "707689817@qq.com"
  },
  "homepage": "https://github.com/LengYi/OpenSSLRepo",
  "source": {
    "git": "https://github.com/LengYi/OpenSSLRepo.git",
    "tag": "1.0.2l.04"
  },
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source_files": "include/openssl/**/*.h",
  "public_header_files": "include/**/*.h",
  "ios": {
    "vendored_libraries": [
      "lib/libcrypto.a",
      "lib/libssl.a"
    ]
  },
  "libraries": [
    "ssl",
    "crypto"
  ],
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "${PODS_ROOT}/OpenSSL-iOS-lib/OpenSSL-iOS-lib/include/**"
  },
  "requires_arc": false
}
