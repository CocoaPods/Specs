{
  "name": "TencentOpenApiSDK-XCFramework",
  "version": "3.5.17.6",
  "summary": "The Official iOS SDK of Tencent Open API.",
  "homepage": "http://wiki.open.qq.com",
  "license": {
    "type": "Copyright",
    "text": "      Copyright (c) 2014 Tencent. All rights reserved.\n"
  },
  "authors": {
    "OpenQQ": "opensupport@qq.com"
  },
  "platforms": {
    "ios": null
  },
  "requires_arc": true,
  "source": {
    "git": "https://gitee.com/chengssir/tencent-open-api.git",
    "tag": "3.5.17.6"
  },
  "xcconfig": {
    "FRAMEWORK_SEARCH_PATHS": "$(inherited)"
  },
  "libraries": [
    "iconv",
    "z",
    "c++",
    "sqlite3"
  ],
  "frameworks": [
    "Security",
    "SystemConfiguration",
    "CoreGraphics",
    "CoreTelephony"
  ],
  "default_subspecs": "Basic",
  "subspecs": [
    {
      "name": "bundle",
      "vendored_frameworks": "bundle/TencentOpenAPI.framework",
      "source_files": "bundle/TencentOpenAPI.framework/Headers/**/*.h",
      "resource_bundles": {
        "TencentOpenAPI": [
          "bundle/TencentOpenApi_IOS_Bundle.bundle"
        ]
      }
    },
    {
      "name": "Basic",
      "vendored_frameworks": "Basic/TencentOpenAPI.xcframework",
      "source_files": "Basic/TencentOpenAPI.xcframework/ios-arm64_armv7/TencentOpenAPI.framework/Headers/**/*.h"
    }
  ]
}
