{
  "name": "scrypt.c",
  "version": "0.1.1",
  "platforms": {
    "ios": "8.0",
    "osx": "10.10",
    "tvos": "9.0",
    "watchos": "2.0"
  },
  "license": {
    "type": "Apache License 2.0",
    "file": "License.md"
  },
  "summary": "scrypt module for web3swift",
  "homepage": "https://github.com/bankex/web3swift",
  "authors": "Bankex Foundation",
  "source": {
    "git": "https://github.com/v57/scrypt.c.git",
    "tag": "0.1.1"
  },
  "pod_target_xcconfig": {
    "OTHER_CFLAGS": "-pedantic -Wall -Wextra -Wcast-align -Wnested-externs -Wshadow -Wstrict-prototypes -Wno-shorten-64-to-32 -Wno-conditional-uninitialized -Wno-unused-function -Wno-long-long -Wno-overlength-strings -O3",
    "HEADER_SEARCH_PATHS": "\"${PODS_ROOT}/scrypt\""
  },
  "source_files": "scrypt/**/*.{c,h}",
  "public_header_files": "scrypt/include/*.h",
  "module_name": "scrypt"
