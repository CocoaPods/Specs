{
  "name": "ShopifyCheckoutSheetKit",
  "version": "3.4.0-rc.8",
  "summary": "Enables Swift apps to embed the Shopify's highest converting, customizable, one-page checkout.",
  "authors": "Shopify Inc.",
  "homepage": "https://github.com/Shopify/checkout-sheet-kit-swift",
  "readme": "https://github.com/Shopify/checkout-sheet-kit-swift/blob/main/README.md",
  "changelog": "https://github.com/Shopify/checkout-sheet-kit-swift/releases",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "source": {
    "git": "https://github.com/Shopify/checkout-sheet-kit-swift.git",
    "tag": "3.4.0-rc.8"
  },
  "swift_versions": "5.0",
  "platforms": {
    "ios": "13.0"
  },
  "pod_target_xcconfig": {
    "OTHER_SWIFT_FLAGS": "-package-name ShopifyCheckoutSheetKit -DCOCOAPODS"
  },
  "default_subspecs": "Core",
  "subspecs": [
    {
      "name": "Core",
      "source_files": "Sources/ShopifyCheckoutSheetKit/**/*.swift",
      "resource_bundles": {
        "ShopifyCheckoutSheetKit": [
          "Sources/ShopifyCheckoutSheetKit/Assets.xcassets"
        ]
      }
    },
    {
      "name": "AcceleratedCheckouts",
      "source_files": "Sources/ShopifyAcceleratedCheckouts/**/*.swift",
      "dependencies": {
        "ShopifyCheckoutSheetKit/Core": [

        ]
      },
      "resource_bundles": {
        "ShopifyAcceleratedCheckouts": [
          "Sources/ShopifyAcceleratedCheckouts/Localizable.xcstrings",
          "Sources/ShopifyAcceleratedCheckouts/Media.xcassets"
        ]
      }
    }
  ],
  "swift_version": "5.0"
}
