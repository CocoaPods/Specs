  },
  "summary": "iOS原生扫码控制器,导入时使用pod 'AZQrCodeScanController/swift' 或 pod 'AZQrCodeScanController/oc'",
  "homepage": "https://github.com/CoderAzreal/AZQrCodeScanController",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "AZReal": "tianfengyu@foxmail.com"
  },
  "source": {
    "git": "https://github.com/CoderAzreal/AZQrCodeScanController.git",
    "tag": "1.0.9"
  },
  "requires_arc": true,
  "subspecs": [
    {
      "name": "swift",
      "source_files": "AZQrCodeScanController/AZQrCodeScanController-Swift/*.{swift}",
