{
  "name": "FirebaseAI",
  "version": "12.2.0",
  "summary": "Firebase AI SDK",
  "description": "Build AI-powered apps and features with the Gemini API using the Firebase AI SDK.",
  "homepage": "https://firebase.google.com",
  "license": {
    "type": "Apache-2.0",
    "file": "LICENSE"
  },
  "authors": "Google, Inc.",
  "source": {
    "git": "https://github.com/firebase/firebase-ios-sdk.git",
    "tag": "CocoaPods-12.2.0"
  },
  "social_media_url": "https://twitter.com/Firebase",
  "platforms": {
    "ios": "15.0",
    "osx": "12.0",
    "tvos": "15.0",
    "watchos": "8.0"
  },
  "cocoapods_version": ">= 1.12.0",
  "prefix_header_file": false,
  "source_files": [
    "FirebaseAI/Sources/**/*.swift"
  ],
  "swift_versions": "5.9",
  "frameworks": "Foundation",
  "ios": {
    "frameworks": "UIKit"
  },
  "osx": {
    "frameworks": "AppKit"
  },
  "tvos": {
    "frameworks": "UIKit"
  },
  "watchos": {
    "frameworks": "WatchKit"
  },
  "dependencies": {
    "FirebaseAppCheckInterop": [
      "~> 12.2.0"
    ],
    "FirebaseAuthInterop": [
      "~> 12.2.0"
    ],
    "FirebaseCore": [
      "~> 12.2.0"
    ],
    "FirebaseCoreExtension": [
      "~> 12.2.0"
    ]
  },
  "testspecs": [
    {
      "name": "unit",
      "test_type": "unit",
      "scheme": {
        "code_coverage": true
      },
      "platforms": {
        "ios": "15.0",
        "osx": "12.0",
        "tvos": "15.0"
      },
      "source_files": [
        "FirebaseAI/Tests/Unit/**/*.swift"
      ],
      "exclude_files": [
        "FirebaseAI/Tests/Unit/Snippets/**/*.swift"
      ],
      "resources": [
        "FirebaseAI/Tests/Unit/vertexai-sdk-test-data/mock-responses",
        "FirebaseAI/Tests/Unit/Resources/**/*"
      ]
    }
  ],
  "swift_version": "5.9"
}
