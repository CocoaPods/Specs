  "homepage": "https://github.com/Janyau/HOFA",
  "license": "MIT",
  "authors": {
    "Janyau": "janyaugoo@gmail.com"
  },
  "platforms": {
    "ios": "9.0"
  },
  "source": {
    "git": "https://github.com/Janyau/HOFA.git",
    "tag": "1.0.5"
  },
  "source_files": "HOFA/**/*",
  "public_header_files": "HOFA/*.h",
