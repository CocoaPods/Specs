      "frameworks": "Foundation",
      "platforms": {
        "ios": "9.0"
      }
    },
    {
      "name": "ProtoType",
      "source_files": "HOFA/ProtoType/**/*",
      "frameworks": "Foundation",
      "platforms": {
        "ios": "9.0"
      }
    }
  ]
}
