{
  "name": "StrapiSwift",
  "version": "1.0.0",
  "platforms": {
    "ios": "12.4"
  },
  "swift_versions": [
    "5.0",
    "5.1",
    "5.2"
  ],
  "authors": "Ricardo Rauber Pereira",
  "license": "MIT",
  "homepage": "http://ricardorauber.com",
  "source": {
    "git": "https://github.com/ricardorauber/StrapiSwift.git",
    "tag": "1.0.0"
  },
  "summary": "A Swift toolkit to connect to your Strapi backend",
  "pod_target_xcconfig": {
    "ENABLE_TESTABILITY": "YES"
  },
  "source_files": "Classes/**/*",
  "dependencies": {
    "RestService": [
      "1.0.1"
    ],
    "KeyValueStorage": [
      "1.0.0"
    ]
  },
  "testspecs": [
    {
      "name": "Tests",
      "test_type": "unit",
      "requires_app_host": true,
      "source_files": "Tests/**/*",
      "dependencies": {
        "Nimble": [
          "8.0.7"
        ],
        "OHHTTPStubs/Swift": [
          "9.0.0"
        ],
        "Quick": [
          "2.2.0"
        ]
      }
    }
  ],
  "swift_version": "5.2"
}
