{
  "name": "EDTSUI",
  "version": "0.0.4",
  "summary": "EDTSUI is an iOS SDK for Design Layout",
  "description": "EDTSUI is a library based on iOS Swift language that help developer to design their layout more easier and according to EDTS Design System standardized.",
  "homepage": "https://github.com/elevenia/edts-ui-ios",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Victor Agung Sukarno": "victor.sukarno@sg-edts.com"
  },
  "platforms": {
    "ios": "11.0"
  },
  "swift_versions": "5.0",
  "source": {
    "git": "https://github.com/elevenia/edts-ui-ios.git",
    "tag": "0.0.4"
  },
  "source_files": [
    "EDTSUI",
    "EDTSUI/**/*.{h,m}"
  ],
  "resources": [
    "EDTSUI/**/*.{xib}"
  ],
  "subspecs": [
    {
      "name": "Color",
      "source_files": "EDTSUI/Atoms/Color"
    },
    {
      "name": "Font",
      "source_files": "EDTSUI/Atoms/Font"
    },
    {
      "name": "OTP",
      "source_files": "EDTSUI/Components/OTP",
      "resources": [
        "EDTSUI/Components/OTP/*.{xib}"
      ]
    },
    {
      "name": "Textfield",
      "source_files": "EDTSUI/Components/Textfield",
      "resources": [
        "EDTSUI/Components/Textfield/*.{xib}"
      ]
    },
    {
      "name": "PopUp",
      "source_files": "EDTSUI/Modules/PopUp",
      "resources": [
        "EDTSUI/Modules/PopUp/*.{xib}"
      ]
    },
    {
      "name": "Onboarding",
      "source_files": "EDTSUI/Modules/Onboarding",
      "resources": [
        "EDTSUI/Modules/Onboarding/*.{xib}"
      ]
    }
  ],
  "swift_version": "5.0"
}
