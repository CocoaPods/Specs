{
  "name": "IJKPlayerBySDK",
  "version": "0.1.1",
  "summary": "IJK播放器",
  "description": "IJK播放器，用户播放rtmp视频流，高品质立体声",
  "homepage": "https://fijkplayer.befovy.com/docs/zh/replace-ijkplayer.html",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "seedmorn_gf": "yangfeng@liuliangwan.com"
  },
  "source": {
    "git": "https://git.code.tencent.com/IJKSDK/IJKPlayerBySDK.git",
    "tag": "0.1.1"
  },
  "platforms": {
    "ios": "10.0"
  },
  "static_framework": true,
  "swift_versions": "5.0",
  "source_files": "IJKPlayerBySDK/Classes/**/*.swift",
  "vendored_frameworks": "IJKPlayerBySDK/Classes/*.framework",
  "frameworks": [
    "AudioToolbox",
    "AVFoundation",
    "CoreGraphics",
    "CoreMedia",
    "CoreVideo",
    "MediaPlayer",
    "MobileCoreServices",
    "OpenGLES",
    "QuartzCore",
    "UIKit",
    "VideoToolbox"
  ],
  "libraries": [
    "bz2",
    "c++",
    "z"
  ],
  "xcconfig": {
    "GCC_PREPROCESSOR_DEFINITIONS": "IJK_IOS=1"
  },
  "pod_target_xcconfig": {
    "ENABLE_BITCODE": "NO"
  },
  "user_target_xcconfig": {
    "ENABLE_BITCODE": "NO"
  },
  "swift_version": "5.0"
}
