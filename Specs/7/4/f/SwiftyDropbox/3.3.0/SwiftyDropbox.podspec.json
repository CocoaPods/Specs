{
  "name": "SwiftyDropbox",
  "version": "3.3.0",
  "summary": "Dropbox Swift SDK for API v2",
  "homepage": "https://dropbox.com/developers/",
  "license": "MIT",
  "authors": {
    "Stephen Cobbe": "scobbe@dropbox.com"
  },
  "source": {
    "git": "https://github.com/dropbox/SwiftyDropbox.git",
    "tag": "3.3.0"
  },
  "osx": {
    "source_files": [
      "Source/SwiftyDropbox/SwiftyDropbox_macOS/SwiftyDropbox_macOS.h",
      "Source/Source/*.{h,m,swift}",
      "Source/Source/macOS/*.{h,m,swift}"
    ],
    "frameworks": [
      "AppKit",
      "Foundation"
    ]
  },
  "ios": {
    "source_files": [
      "Source/SwiftyDropbox/SwiftyDropbox/SwiftyDropbox.h",
      "Source/Source/*.{h,m,swift}",
      "Source/Source/iOS/*.{h,m,swift}"
    ],
    "frameworks": [
      "UIKit",
      "Foundation"
    ]
  },
  "requires_arc": true,
  "platforms": {
    "osx": "10.10",
    "ios": "8.0"
  },
  "dependencies": {
    "Alamofire": [
      "~> 3.5"
    ]
  }
}
