{
  "name": "ConvivaSDK",
  "version": "4.0.9",
  "summary": "Conviva SDK for iOS/tvOS.",
  "description": "Conviva SDK Framework for custom integration.",
  "homepage": "https://github.com/Conviva/ConvivaSDK",
  "license": {
    "type": "Commercial",
    "text": "LICENSE.md"
  },
  "authors": {
    "Conviva": "www.conviva.com"
  },
  "source": {
    "git": "https://github.com/Conviva/ConvivaSDK.git",
    "tag": "4.0.9"
  },
  "platforms": {
    "ios": "9.0",
    "tvos": "9.0"
  },
  "ios": {
    "vendored_frameworks": "Framework/iOS/ConvivaSDK.framework",
    "frameworks": "CoreTelephony",
    "pod_target_xcconfig": {
      "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
    },
    "user_target_xcconfig": {
      "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
    }
  },
  "tvos": {
    "vendored_frameworks": "Framework/tvOS/ConvivaSDK.framework",
    "pod_target_xcconfig": {
      "EXCLUDED_ARCHS[sdk=appletvsimulator*]": "arm64"
    },
    "user_target_xcconfig": {
      "EXCLUDED_ARCHS[sdk=appletvsimulator*]": "arm64"
    }
  },
  "frameworks": [
    "CoreMedia",
    "SystemConfiguration",
    "MobileCoreServices",
    "CoreGraphics",
    "Security",
    "AdSupport"
  ],
  "weak_frameworks": [
    "Network"
  ]
}
