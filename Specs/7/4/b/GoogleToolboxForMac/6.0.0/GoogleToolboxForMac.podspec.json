    "Core",
    "GeometryUtils",
    "KVO",
    "Logger",
    "StringEncoding",
    "NSData+zlib",
    "NSFileHandle+UniqueName",
    "NSString+HTML",
    "NSString+XML",
    "NSThread+Blocks"
  ],
  "subspecs": [
