{
  "name": "AdfurikunSDK-iOS",
  "version": "3.8.1",
  "summary": "An iOS SDK for ADFURIKUN Movie Reward Ads",
  "homepage": "https://adfurikun.jp/adfurikun/",
  "license": {
    "type": "Copyright",
    "text": "Copyright Glossom Inc. All rights reserved."
  },
  "authors": "Glossom Inc.",
  "platforms": {
    "ios": "10.0"
  },
  "source": {
    "git": "https://github.com/glossom-dev/AdfurikunSDK-iOS",
    "tag": "3.8.1"
  },
  "default_subspecs": "All",
  "static_framework": true,
  "xcconfig": {
    "VALID_ARCHS": "armv7 armv7s x86_64 arm64"
  },
  "subspecs": [
    {
      "name": "Core",
      "vendored_frameworks": "**/ADFMovieReward.framework",
      "frameworks": [
        "AdSupport",
        "AVFoundation",
        "CoreGraphics",
        "CoreMedia",
        "CoreTelephony",
        "MediaPlayer",
        "StoreKit",
        "SystemConfiguration",
        "SafariServices",
        "UIKit",
        "WebKit"
      ],
      "pod_target_xcconfig": {
        "OTHER_LDFLAGS": [
          "-ObjC",
          "-fobjc-arc"
        ]
      }
    },
    {
      "name": "AdColony",
      "dependencies": {
        "AdfurikunSDK-iOS/Core": [

        ],
        "AdColony": [
          "4.5.0"
        ]
      },
      "source_files": "**/adnetworks/AdColony/*.{h,m}",
      "resources": "**/adnetworks/AdColony/*.txt"
    },
    {
      "name": "AdMob",
      "dependencies": {
        "AdfurikunSDK-iOS/Core": [

        ],
        "Google-Mobile-Ads-SDK": [
          "8.1.0"
        ]
