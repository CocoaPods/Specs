{
  "name": "MbSecureCrypto",
  "version": "0.2.0",
  "summary": "A secure cryptography library for iOS and macOS",
  "description": "MbSecureCrypto provides secure cryptographic operations \nincluding random string generation and more. Built with \nApple's Security framework integration for optimal platform security.",
  "homepage": "https://github.com/mavbozo/MbSecureCrypto",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Maverick Bozo": "mavbozo@pm.me"
  },
  "platforms": {
    "ios": "15.6",
    "osx": "12.4"
  },
  "source": {
    "git": "https://github.com/mavbozo/MbSecureCrypto.git",
    "tag": "0.2.0"
  },
  "source_files": "MbSecureCrypto/**/*.{h,m}",
  "public_header_files": "MbSecureCrypto/**/*.h",
  "frameworks": "Security",
  "requires_arc": true,
  "pod_target_xcconfig": {
    "CLANG_WARN_OBJC_IMPLICIT_RETAIN_SELF": "YES",
    "GCC_TREAT_WARNINGS_AS_ERRORS": "YES",
    "OTHER_CFLAGS": "-fstack-protector-strong"
  }
}
