{
  "name": "TestJoyTalk",
  "version": "1.0.0",
  "summary": "TestJoyTalk - 测试 SDK for iOS",
  "description": "TestJoyTalk 是一个用于 iOS 平台的测试 SDK。\n该 SDK 支持真机和模拟器，使用 xcframework 格式分发。",
  "homepage": "https://github.com/Wangyaxiong/TestJoyTalk",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Wangyaxiong": "https://github.com/Wangyaxiong"
  },
  "source": {
    "git": "https://github.com/Wangyaxiong/TestJoyTalk.git",
    "tag": "v1.0.0"
  },
  "platforms": {
    "ios": "15.0"
  },
  "vendored_frameworks": "JoyTalkSDK.xcframework",
  "public_header_files": "JoyTalkSDK/Public/*.h",
  "dependencies": {
    "AFNetworking": [
      "~> 4.0.1"
    ],
    "MJExtension": [

    ],
    "FMDB": [
      "~> 2.7.12"
    ],
    "SocketRocket": [
      "~> 0.7.1"
    ]
  },
  "frameworks": [
    "Foundation",
    "UIKit",
    "SystemConfiguration",
    "Security"
  ],
  "libraries": [
    "sqlite3",
    "z"
  ],
  "static_framework": true,
  "pod_target_xcconfig": {
    "VALID_ARCHS": "arm64 x86_64",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "i386"
  }
}
