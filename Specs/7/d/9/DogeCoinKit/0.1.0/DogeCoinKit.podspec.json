  "homepage": "https://github.com/tweakdeveloper/DogeCoinKit",
  "license": "MIT",
  "authors": {
    "Nolan Clark": "tweakdeveloper@gmail.com"
  },
  "source": {
    "git": "https://github.com/tweakdeveloper/DogeCoinKit.git",
