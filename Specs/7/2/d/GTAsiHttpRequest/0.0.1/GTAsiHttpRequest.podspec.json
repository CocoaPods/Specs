{
  "name": "GTAsiHttpRequest",
  "version": "0.0.1",
  "summary": "Just Building AsiHttpRequest To A Static Framework.",
  "description": "1. static framework of AsiHttpRequest.\n2. don't change any src.\n3. don't include ASIWebPageRequest and CloudFiles function.\n4. just support Objective-C.",
  "homepage": "https://github.com/kaikai5545/GTAsiHttpRequest",
  "license": "MIT",
  "authors": {
    "WRK": "kaikai5545@163.com"
  },
  "platforms": {
    "ios": "7.0"
  },
  "source": {
    "git": "https://github.com/kaikai5545/GTAsiHttpRequest.git",
    "tag": "0.0.1"
  },
  "source_files": [
    "GTAsiHttpRequest/*",
    "GTAsiHttpRequest/**/*"
  ],
  "public_header_files": "GTAsiHttpRequest/*.h",
  "frameworks": [
    "CFNetwork",
    "SystemConfiguration",
    "MobileCoreServices",
    "CoreGraphics"
  ],
  "libraries": "z.1.2.5",
  "requires_arc": false,
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "$(SDKROOT)/usr/include/libz"
  }
}
