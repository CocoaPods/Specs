{
  "name": "KJBannerView",
  "version": "3.0.2",
  "summary": "KJBannerView是一款自带图片下载和缓存轮播Banner，支持动态图和网图混合显示",
  "homepage": "https://github.com/yangKJ/KJBannerViewDemo",
  "description": "https://github.com/yangKJ/KJBannerViewDemo/blob/master/README.md",
  "license": "Copyright (c) 2018 yangkejun",
  "authors": {
    "77": "ykj310@126.com"
  },
  "source": {
    "git": "https://github.com/yangKJ/KJBannerViewDemo.git",
    "tag": "3.0.2"
  },
  "platforms": {
    "ios": "9.0"
  },
  "social_media_url": "https://www.jianshu.com/p/47b29be42a49",
  "requires_arc": true,
  "ios": {
    "pod_target_xcconfig": {
      "PRODUCT_BUNDLE_IDENTIFIER": "com.yangkejun.KJBannerViewDemo"
    },
    "source_files": "Sources/KJBannerHeader.h"
  },
  "default_subspecs": "BannerView",
  "subspecs": [
    {
      "name": "BannerView",
      "source_files": [
        "Sources/KJBannerView/*.{h,m}",
        "Sources/PageControl/*.{h,m}"
      ],
      "resources": "Sources/KJBannerView/*.{bundle}"
    },
    {
      "name": "Downloader",
      "source_files": "Sources/WebImage/*.{h,m}"
    }
  ]
}
