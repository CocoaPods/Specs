  "public_header_files": "Sparkle.framework/Versions/B/Headers/*.h",
  "vendored_frameworks": "Sparkle.framework",
  "xcconfig": {
    "FRAMEWORK_SEARCH_PATHS": "\"${PODS_ROOT}/Sparkle\"",
    "LD_RUNPATH_SEARCH_PATHS": "@loader_path/../Frameworks"
  },
  "requires_arc": true
