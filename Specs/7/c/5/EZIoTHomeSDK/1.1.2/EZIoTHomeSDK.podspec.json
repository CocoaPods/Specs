{
  "name": "EZIoTHomeSDK",
  "version": "1.1.2",
  "summary": "Ezviz IoT Home SDK for iOS.",
  "description": "The HOME SDK is used to quickly integrate the open capabilities of the Ezviz IoT platform for developers.",
  "homepage": "https://github.com/Ezviz-OpenBiz/EZIoT-HomeSDK-iOS",
  "authors": "yuqian",
  "platforms": {
    "ios": "9.0"
  },
  "source": {
    "git": "https://github.com/Ezviz-OpenBiz/EZIoT-HomeSDK-iOS.git",
    "tag": "1.1.2"
  },
  "vendored_frameworks": "Frameworks/**.framework",
  "frameworks": [
    "OpenAL",
    "VideoToolbox"
  ],
  "libraries": [
    "iconv.2.4.0",
    "c++",
    "resolv.9"
  ],
  "requires_arc": true,
  "resources": "Frameworks/Resources/com.hri.hpc.mobile.ios.player.metallib",
  "pod_target_xcconfig": {
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES",
    "ENABLE_BITCODE": "NO",
    "OTHER_LDFLAGS": [
      "-ObjC"
    ],
    "VALID_ARCHS[sdk=iphonesimulator*]": ""
  },
  "user_target_xcconfig": {
    "ENABLE_BITCODE": "NO"
  },
  "dependencies": {
    "AFNetworking": [
      "~> 4.0.1"
    ],
    "GTMBase64": [
      "~> 1.0.1"
    ],
    "MJExtension": [
      "~> 3.3.0"
    ],
    "Reachability": [
      "~> 3.2"
    ],
    "ReactiveObjC": [
      "~> 3.1.1"
    ],
    "SSKeychain": [
      "~> 1.4.1"
    ],
    "KVOController": [
      "~> 1.2.0"
    ],
    "YYCache": [
      "~> 1.0.4"
    ],
    "FMDB": [
      "~> 2.7.5"
    ],
    "Realm": [
      "~> 10.19.0"
    ]
  }
}
