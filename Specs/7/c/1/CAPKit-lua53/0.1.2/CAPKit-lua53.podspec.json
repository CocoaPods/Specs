{
  "name": "CAPKit-lua53",
  "version": "0.1.2",
  "summary": "lua53 library.",
  "description": "lua5.3.3 library.",
  "homepage": "https://github.com/successinfo-org/CAPKit-lua53",
  "license": "MIT",
  "authors": {
    "samchang": "sam.chang@me.com"
  },
  "platforms": {
    "ios": "5.1.1"
  },
  "source": {
    "git": "https://github.com/successinfo-org/CAPKit-lua53.git",
    "tag": "v0.1.2"
  },
  "source_files": [
    "lua-5.3.3/src/*.{h,c}",
    "lua53/*.{h,c}"
  ],
  "exclude_files": [
    "lua-5.3.3/src/lua.c",
    "lua-5.3.3/src/luac.c"
  ],
  "compiler_flags": [
    "-DLUA_COMPAT_5_1",
    "-DLUA_COMPAT_5_2",
    "-DLUA_COMPAT_FLOATSTRING",
    "-DLUA_FLOORN2I=1"
  ],
  "header_dir": "lua53",
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "$(PODS_ROOT)/CAPKit-lua53/lua-5.3.3/src $(PODS_ROOT)/CAPKit-lua53/lua53"
  }
}
