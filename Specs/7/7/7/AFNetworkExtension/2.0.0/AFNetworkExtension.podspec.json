{
  "name": "AFNetworkExtension",
  "version": "2.0.0",
  "license": "MIT",
  "summary": "AFNetworking 3/4 Extensions",
  "homepage": "https://github.com/ElfSundae/AFNetworkExtension",
  "social_media_url": "https://twitter.com/ElfSundae",
  "authors": {
    "Elf Sundae": "https://0x123.com"
  },
  "source": {
    "git": "https://github.com/ElfSundae/AFNetworkExtension.git",
    "tag": "2.0.0"
  },
  "platforms": {
    "ios": "9.0",
    "tvos": "9.0",
    "osx": "10.10",
    "watchos": "2.0"
  },
  "ios": {
    "pod_target_xcconfig": {
      "PRODUCT_BUNDLE_IDENTIFIER": "com.0x123.AFNetworkExtension"
    }
  },
  "tvos": {
    "pod_target_xcconfig": {
      "PRODUCT_BUNDLE_IDENTIFIER": "com.0x123.AFNetworkExtension"
    }
  },
  "osx": {
    "pod_target_xcconfig": {
      "PRODUCT_BUNDLE_IDENTIFIER": "com.0x123.AFNetworkExtension"
    }
  },
  "watchos": {
    "pod_target_xcconfig": {
      "PRODUCT_BUNDLE_IDENTIFIER": "com.0x123.AFNetworkExtension-watchOS"
    }
  },
  "source_files": "AFNetworkExtension/AFNetworkExtension.h",
  "subspecs": [
    {
      "name": "Logger",
      "source_files": "AFNetworkExtension/Logger/*.{h,m}",
      "dependencies": {
        "AFNetworking/NSURLSession": [
          ">= 3.0"
        ],
        "AFNetworkActivityLogger": [
          ">= 3.0"
        ],
        "ESFramework": [
          "~> 3.22"
        ]
      }
    },
    {
      "name": "NSURLSession",
      "source_files": "AFNetworkExtension/NSURLSession/*.{h,m}",
      "dependencies": {
        "AFNetworking/NSURLSession": [
          ">= 3.0"
        ]
      }
    },
    {
      "name": "Reachability",
      "platforms": {
        "ios": "9.0",
        "tvos": "9.0",
        "osx": "10.10"
      },
      "source_files": "AFNetworkExtension/Reachability/*.{h,m}",
      "dependencies": {
        "AFNetworking/Reachability": [
          ">= 3.0"
        ]
      }
    },
    {
      "name": "Serialization",
      "source_files": "AFNetworkExtension/Serialization/*.{h,m}",
      "dependencies": {
        "AFNetworking/Serialization": [
          ">= 3.0"
        ],
        "ESFramework": [
          "~> 3.22"
        ]
      }
    }
  ]
}
