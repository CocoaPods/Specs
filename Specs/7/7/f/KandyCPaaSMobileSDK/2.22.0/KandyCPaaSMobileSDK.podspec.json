  },
  "dependencies": {
    "KandyWebRTC": [

    ]
  },
  "libraries": [
    "c++",
    "icucore",
    "z",
    "sqlite3",
    "stdc++"
  ],
  "ios": {
    "frameworks": [
      "AVFoundation",
      "SystemConfiguration",
      "CFNetwork",
      "Security",
      "OpenGLES",
      "QuartzCore",
      "CoreAudio",
      "CoreMedia",
      "CoreVideo",
