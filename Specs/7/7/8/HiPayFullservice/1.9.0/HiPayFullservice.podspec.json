{
  "name": "HiPayFullservice",
  "version": "1.9.0",
  "summary": "HiPay SDK for iOS lets you accept payments in your iOS application.",
  "description": "HiPay is a new generation of payment platform optimized for todaysâ€™ e-tailers.\n\nThis SDK leverages the HiPay platform to let you accept payments in your iOS application.\n\nYou can either take advantage of the turnkey payment screen or make a custom implement by using the core wrapper.",
  "homepage": "https://developer.hipay.com/doc/hipay-fullservice-sdk-ios/",
  "license": "Apache-2.0",
  "authors": {
    "Aymeric BERTHELOT": "aberthelot@hipay.com",
    "Morgan BAUMARD": "mbaumard@hipay.com"
  },
  "source": {
    "git": "https://github.com/hipay/hipay-fullservice-sdk-ios.git",
    "tag": "1.9.0"
  },
  "platforms": {
    "ios": "9.0"
  },
  "requires_arc": true,
  "pod_target_xcconfig": {
    "TARGETED_DEVICE_FAMILY": "1,2"
  },
  "source_files": "HiPayFullservice/*.{m,h}",
  "default_subspecs": [
    "Core",
    "Payment-Screen"
  ],
  "public_header_files": "HiPayFullservice/*.h",
  "subspecs": [
    {
      "name": "Core",
      "source_files": [
        "HiPayFullservice/Core/**/*.{h,m}",
        "HiPayFullservice/Device Print/**/*.h",
        "HiPayFullservice/Utilities/**/*.{h,m}"
      ],
      "public_header_files": [
        "HiPayFullservice/Core/**/*.h",
        "HiPayFullservice/Utilities/**/*.h"
      ],
      "resource_bundles": {
        "HPFCoreLocalization": [
          "HiPayFullservice/Core/**/*.lproj"
        ]
      },
      "libraries": "c++"
    },
    {
      "name": "Utilities",
      "source_files": "HiPayFullservice/Utilities/**/*.{h,m}",
      "public_header_files": "HiPayFullservice/Utilities/**/*.h",
      "resource_bundles": {
        "HPFUtilitiesResources": [
          "HiPayFullservice/Utilities/**/*.{plist}"
        ]
      },
      "dependencies": {
        "HiPayFullservice/Core": [

        ]
      }
    },
    {
      "name": "Device-Print",
      "vendored_frameworks": "HiPayFullservice/Device Print/iovation.framework",
      "frameworks": [
        "CoreTelephony",
        "SystemConfiguration",
        "ExternalAccessory"
      ]
    },
    {
      "name": "Payment-Screen",
      "source_files": [
        "HiPayFullservice/Payment Screen/**/*.{h,m}"
      ],
      "public_header_files": "HiPayFullservice/Payment Screen/**/*.h",
      "resource_bundles": {
        "HPFPaymentScreenViews": [
          "HiPayFullservice/Payment Screen/**/*.{xib,png,storyboard}"
        ],
        "HPFPaymentScreenLocalization": [
          "HiPayFullservice/Payment Screen/**/*.lproj"
        ]
      },
      "dependencies": {
        "HiPayFullservice/Core": [

        ],
        "HiPayFullservice/Utilities": [

        ]
      },
      "weak_frameworks": "WebKit",
      "frameworks": [
        "UIKit",
        "Accelerate",
        "AudioToolbox",
        "AVFoundation",
        "CoreLocation",
        "CoreMedia",
        "MessageUI",
        "MobileCoreServices",
        "SystemConfiguration"
      ],
      "vendored_libraries": [
        "HiPayFullservice/Payment Screen/CardIO/*.a"
      ],
      "compiler_flags": "-fmodules",
      "xcconfig": {
        "OTHER_LDFLAGS": "-lc++ -ObjC"
      }
    },
    {
      "name": "CardIO",
      "dependencies": {
        "CardIO": [
          "~> 5.4.1"
        ]
      }
    },
    {
      "name": "Datecs-POS",
      "source_files": [
        "HiPayFullservice/POS/*.{h,m}"
      ],
      "vendored_frameworks": "HiPayFullservice/POS/Datecs/DatecsLibrary.framework",
      "xcconfig": {
        "OTHER_LDFLAGS": "-lresolv"
      },
      "frameworks": [
        "SystemConfiguration",
        "Security",
        "CoreTelephony"
      ],
      "libraries": "c++"
    }
  ]
}
