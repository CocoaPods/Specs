{
  "name": "PayPal.Checkout",
  "version": "3.1.0",
  "summary": "Add PayPal Checkout to your iOS application.",
  "description": "Provides API for PayPal Checkout for iOS. Provides a native in context checkout experience, as well as a simple Safari View Controller integration.",
  "homepage": "https://github.com/paypal/paypalcheckout-ios",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Derek Anderson": "dereanderson@paypal.com"
  },
  "source": {
    "git": "https://github.com/paypal/paypalcheckout-ios.git",
    "tag": "3.1.0"
  },
  "platforms": {
    "ios": "9.0"
  },
  "ios": {
    "resource_bundles": {
      "PYPLCheckout": [
        "pod/assets/PYPLCheckout.bundle/*",
        "pod/assets/PYPLCheckout/**/*.png"
      ]
    },
    "vendored_frameworks": [
      "pod/lib/AppAuth.framework",
      "pod/lib/NativeCheckout.framework"
    ]
  },
  "vendored_libraries": [
    "pod/lib/libPPRiskComponent.a",
    "pod/lib/libAuthCore.a",
    "pod/lib/libNativeCheckout.a"
