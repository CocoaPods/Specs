{
  "name": "KalturaPlayerSDK",
  "version": "2.6.5",
  "summary": "The Kaltura player-sdk-native component enables embedding the kaltura player into native environments.",
  "homepage": "https://github.com/kaltura/player-sdk-native-ios",
  "license": {
    "type": "AGPLv3",
    "text": "AGPLv3"
  },
  "authors": {
    "Eliza Sapir": "eliza.sapir@gmail.com",
    "Nissim Pardo": "nissim.pardo@kaltura.com"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source": {
    "git": "https://github.com/kaltura/player-sdk-native-ios.git",
    "tag": "v2.6.5"
  },
  "libraries": [
    "stdc++",
    "z",
    "System",
    "stdc++.6",
    "xml2.2",
    "c++",
    "stdc++.6.0.9",
    "xml2"
  ],
  "frameworks": [
    "MediaPlayer",
    "SystemConfiguration",
    "QuartzCore",
    "CoreFoundation",
    "AVFoundation",
    "AudioToolbox",
    "CFNetwork",
    "AdSupport",
    "WebKit",
    "MessageUI",
    "Social",
    "MediaAccessibility",
    "Foundation",
    "CoreGraphics",
    "UIKit"
  ],
  "requires_arc": true,
  "deprecated": true,
  "subspecs": [
    {
      "name": "Core",
      "source_files": [
        "**/*.{h,m}",
        "PlayerSDK/KALTURAPlayerSDK/**/*.{h,m}"
      ],
      "resource_bundles": {
        "KALTURAPlayerSDKResources": "KALTURAPlayerSDK/*.{xib,plist}"
      }
    },
    {
      "name": "Widevine",
      "libraries": "WViPhoneAPI",
      "vendored_libraries": "libWViPhoneAPI.a",
      "pod_target_xcconfig": {
        "ENABLE_BITCODE": "NO",
        "GCC_PREPROCESSOR_DEFINITIONS": "WIDEVINE_ENABLED=1"
      }
    },
    {
      "name": "GoogleCast",
      "pod_target_xcconfig": {
        "GCC_PREPROCESSOR_DEFINITIONS": "GOOGLE_CAST_ENABLED=1"
      },
      "dependencies": {
        "google-cast-sdk": [
          "~> 3.2.0"
        ]
      }
    }
  ]
}
