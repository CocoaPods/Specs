{
  "name": "Johnny",
  "version": "1.0",
  "summary": "Melodic Caching in Swift",
  "homepage": "https://github.com/zolomatok/Johnny",
  "license": {
    "type": "MIT"
  },
  "authors": {
    "Zoltán Matók": "bellamycpt@gmail.com"
  },
  "source": {
    "git": "https://github.com/zolomatok/Johnny.git",
    "tag": "1.0"
  },
  "requires_arc": true,
  "module_name": "Johnny",
  "platforms": {
    "osx": "10.10",
    "ios": "8.0",
    "tvos": "9.0",
    "watchos": "2.0"
  },
  "osx": {
    "source_files": [
      "Johnny/*.swift",
      "Johnny/Extensions/common/*.swift",
      "Johnny/Extensions/macOS/*.swift"
    ],
    "frameworks": [
      "AppKit",
      "Foundation"
    ]
  },
  "ios": {
    "source_files": [
      "Johnny/*.swift",
      "Johnny/Extensions/*.swift",
      "Johnny/Extensions/common/*.swift"
    ],
    "frameworks": [
      "UIKit",
      "Foundation"
    ]
  },
  "tvos": {
    "source_files": [
      "Johnny/*.swift",
      "Johnny/Extensions/*.swift",
      "Johnny/Extensions/common/*.swift"
    ],
    "frameworks": [
      "UIKit",
      "Foundation"
    ]
  },
  "watchos": {
    "source_files": [
      "Johnny/*.swift",
      "Johnny/Extensions/common/*.swift",
      "Johnny/Extensions/UIColor.swift",
      "Johnny/Extensions/UIImage.swift"
    ]
  },
  "dependencies": {
    "AsyncSwift": [

    ]
  }
}
