{
  "name": "cineio-broadcast-ios",
  "version": "0.7.2",
  "summary": "cine.io Broadcast iOS SDK",
  "description": "                          iOS SDK for Broadcasting using the cine.io API.\n",
  "homepage": "https://github.com/cine-io/cineio-broadcast-ios",
  "license": "MIT",
  "authors": {
    "Jeffrey Wescott": "jeffrey@cine.io"
  },
  "source": {
    "git": "https://github.com/cine-io/cineio-broadcast-ios.git",
    "tag": "0.7.2"
  },
  "requires_arc": true,
  "header_dir": "cineio",
  "source_files": [
    "cineio-broadcast-ios/cineio-broadcast-ios/*.h",
    "cineio-broadcast-ios/cineio-broadcast-ios/*.m*",
    "cineio-broadcast-ios/cineio-broadcast-ios/*.c*"
  ],
  "frameworks": [
    "Foundation",
    "UIKit",
    "QuartzCore",
    "CoreGraphics",
    "CoreAudio",
    "CoreMedia",
    "AudioToolbox",
    "AVFoundation",
    "MediaPlayer"
  ],
  "dependencies": {
    "AFNetworking": [
      "~> 2.5"
    ],
    "VideoCore": [
      "~> 0.3"
    ]
  },
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "${PODS_ROOT}/boost"
  },
  "platforms": {
    "ios": "8.0"
  }
}
