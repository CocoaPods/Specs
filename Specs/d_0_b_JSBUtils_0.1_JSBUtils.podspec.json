{
  "name": "JSBUtils",
  "version": "0.1",
  "summary": "Small classes I reuse in most project",
  "homepage": "https://github.com/jeffreybergier/swiftutils",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Your Name": "cocoapod@jeffburg.com"
  },
  "social_media_url": "http://twitter.com/jeffburg",
  "requires_arc": true,
  "source": {
    "git": "https://github.com/jeffreybergier/SwiftUtils.git",
    "tag": "v0.1",
    "submodules": false
  },
  "platforms": {
    "ios": "8.0",
    "osx": "10.9",
    "tvos": "9.0",
    "watchos": "2.0"
  },
  "ios": {
    "source_files": [
      "JSBUtils/Common/*.{h,swift}",
      "JSBUtils/JSBUtils_iOS/*.{h,swift}"
    ]
  },
  "osx": {
    "source_files": [
      "JSBUtils/Common/*.{h,swift}",
      "JSBUtils/JSBUtils_macOS/*.{h,swift}"
    ]
  },
  "tvos": {
    "source_files": [
      "JSBUtils/Common/*.{h,swift}",
      "JSBUtils/JSBUtils_tvOS/*.{h,swift}"
    ]
  },
  "watchos": {
    "source_files": [
      "JSBUtils/Common/*.{h,swift}",
      "JSBUtils/JSBUtils_watchOS/*.{h,swift}"
    ]
  }
}
