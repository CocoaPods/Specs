{
  "name": "MDCViewPager",
  "version": "0.1.0",
  "summary": "A simple view pager for Swift.",
  "description": "A simple view pager for Swift. Compatible with iOS 8.0 or later.",
  "homepage": "https://github.com/jigang-duan/MDCViewPager",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "jigang-duan": "djg4055108@126.com"
  },
  "source": {
    "git": "https://github.com/jigang-duan/MDCViewPager.git",
    "tag": "0.1.0"
  },
  "default_subspecs": "Core",
  "platforms": {
    "ios": "8.0"
  },
  "subspecs": [
    {
      "name": "Core",
      "source_files": "MDCViewPager/Classes/**/*",
      "dependencies": {
        "MaterialComponents/PageControl": [
          "~> 44.3"
        ],
        "MaterialComponents/Ink": [
          "~> 44.3"
        ],
        "Kingfisher": [
          "~> 4.6"
        ]
      },
      "frameworks": "UIKit",
      "resource_bundles": {
        "MDCViewPager": [
          "MDCViewPager/Assets/*.png"
        ]
      }
    },
    {
      "name": "RxSwift",
      "source_files": "MDCViewPager/RxSwift/**/*",
      "dependencies": {
        "MDCViewPager/Core": [

        ],
        "RxCocoa": [
          "~> 4.0"
        ]
      }
    }
  ]
}
