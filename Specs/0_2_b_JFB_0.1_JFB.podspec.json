{
  "name": "JFB",
  "version": "0.1",
  "summary": "Jera's new way to build form quickly and easily",
  "description": "Jera Form Builder (JFB) bring a new way to build form using *Eureka* enginee",
  "homepage": "https://github.com/vitormesquita/JFB",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Vitor Mesquita": "vitor.mesquita09@gmail.com"
  },
  "source": {
    "git": "https://github.com/vitormesquita/JFB.git",
    "tag": "0.1"
  },
  "platforms": {
    "ios": "9.0"
  },
  "default_subspecs": "Core",
  "resource_bundles": {
    "JFB": [
      "Assets/*.png"
    ]
  },
  "subspecs": [
    {
      "name": "Core",
      "source_files": "Source/**/*.swift",
      "dependencies": {
        "Eureka": [
          "~> 4"
        ],
        "Material": [
          "~> 2"
        ],
        "NSStringMask": [
          "~> 1.2"
        ]
      }
    }
  ]
}
