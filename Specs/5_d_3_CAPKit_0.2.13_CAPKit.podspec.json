{
  "name": "CAPKit",
  "version": "0.2.13",
  "summary": "CAPKit Framework.",
  "description": "CAPKit Framework, Cloud Application Platform.",
  "homepage": "https://github.com/successinfo-org/CAPKit",
  "license": "MIT",
  "authors": {
    "samchang": "sam.chang@me.com"
  },
  "platforms": {
    "ios": "7.0.0"
  },
  "source": {
    "git": "https://github.com/successinfo-org/CAPKit.git",
    "tag": "v0.2.13"
  },
  "frameworks": [
    "AssetsLibrary",
    "MessageUI",
    "AddressBookUI",
    "AddressBook",
    "Accelerate",
    "MapKit",
    "AudioToolbox",
    "CoreTelephony",
    "QuickLook",
    "Social",
    "CoreLocation"
  ],
  "weak_frameworks": "WebKit",
  "libraries": "resolv",
  "default_subspecs": "Release",
  "dependencies": {
    "DTCoreText": [
      "~> 1.6.12"
    ],
    "TouchJSON": [
      "~> 1.1"
    ],
    "ASIHTTPRequest": [
      "~> 1.8.2"
    ],
    "FMDB/FTS": [
      "~> 2.6.2"
    ],
    "Reachability": [
      "~> 3.1.1"
    ],
    "ZipArchive": [
      "~> 1.4.0"
    ],
    "UIDevice-Hardware": [
      "~> 0.1.3"
    ],
    "NSData+Base64": [
      "~> 1.0.0"
    ],
    "ZBarSDK": [
      "~> 1.3.1"
    ],
    "GZIP": [
      "~> 1.0.2"
    ],
    "SSKeychain": [
      "~> 1.2.2"
    ],
    "AnimatedGIFImageSerialization-fork": [
      "~> 0.2.1"
    ],
    "Bugly": [
      "~> 2.4.8"
    ],
    "CAPKit-iOS-WebP": [
      "~> 0.4"
    ],
    "CAPKit-lua53": [
      "~> 0.1.0"
    ],
    "CAPKit-3rdparty-libs": [
      "~> 0.1.0"
    ],
    "CAPKit-sqlcipher": [
      "~> 0.1.0"
    ],
    "CAPKit-OpenCV24x": [
      "~> 2.4.10"
    ],
    "CAPKit-Box2D": [
      "~> 0.1.0"
    ],
    "luafan": [
      "~> 0.6.3"
    ]
  },
  "subspecs": [
    {
      "name": "Release",
      "source_files": "extension/**/*.*",
      "resources": "Release/builtin",
      "ios": {
        "vendored_frameworks": "Release/CAPKit.framework"
      }
    },
    {
      "name": "Resource",
      "resources": "Release/builtin"
    },
    {
      "name": "Debug",
      "source_files": "extension/**/*.*",
      "resources": "Debug/builtin",
      "ios": {
        "vendored_frameworks": "Debug/CAPKit.framework"
      }
    }
  ]
}
