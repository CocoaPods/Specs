{
  "name": "ZKPayMediator",
  "version": "0.1.0",
  "summary": "A short description of ZKPayMediator.",
  "homepage": "https://github.com/kaiser143/ZKPayMediator",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
