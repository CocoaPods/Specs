{
  "name": "RxRequester",
  "version": "0.3.0",
  "summary": "Simple & Clean RxJava requester for Android",
  "description": "Simple & Clean RxJava requester for Android",
  "homepage": "https://github.com/ShabanKamell/RxRequester-Swift",
  "license": {
    "type": "Apache 2.0",
    "file": "LICENSE"
  },
  "authors": {
    "ShabanKamell": "sh3ban.kamel@gmail.com"
  },
  "source": {
    "git": "https://github.com/ShabanKamell/RxRequester-Swift.git",
    "tag": "0.3.0"
  },
  "social_media_url": "https://twitter.com/ShaAhKa",
  "platforms": {
    "ios": "11.0"
  },
  "swift_versions": "5.1.2",
  "default_subspecs": "Core",
  "subspecs": [
    {
      "name": "Core",
      "source_files": "Sources/RxRequester/**/*",
      "exclude_files": "Sources/RxRequester/Tests/**/*",
      "frameworks": "Foundation",
      "dependencies": {
        "RxSwift": [
          "~> 5.0"
        ]
      }
    },
    {
      "name": "Alamofire",
      "source_files": "Sources/RxRequesterAlamofire/**/*",
      "exclude_files": "Sources/RxRequesterAlamofire/Tests/**/*",
      "dependencies": {
        "RxRequester/Core": [

        ],
        "Alamofire": [
          "~> 5.0.0-rc.3"
        ]
      }
    },
    {
      "name": "Moya",
      "source_files": "Sources/RxRequesterMoya/**/*",
      "exclude_files": "Sources/RxRequesterMoya/Tests/**/*",
      "dependencies": {
        "RxRequester/Core": [

        ],
        "Moya/RxSwift": [
          "~> 14.0.0-beta.5"
        ]
      }
    }
  ],
  "swift_version": "5.1.2"
}
