{
  "name": "FirebaseUI",
  "version": "0.5.1",
  "summary": "UI binding libraries for Firebase.",
  "homepage": "https://github.com/firebase/FirebaseUI-iOS",
  "license": {
    "type": "Apache 2.0",
    "file": "FirebaseUIFrameworks/LICENSE"
  },
  "authors": "Firebase",
  "source": {
    "http": "https://github.com/firebase/FirebaseUI-iOS/releases/download/v0.5.1/FirebaseUIFrameworks.zip"
  },
  "platforms": {
    "ios": "8.0"
  },
  "ios": {
    "frameworks": "UIKit",
    "vendored_frameworks": "FirebaseUIFrameworks/*/Frameworks/*.framework"
  },
  "requires_arc": true,
  "default_subspecs": "All",
  "subspecs": [
    {
      "name": "All",
      "dependencies": {
        "FirebaseUI/Database": [

        ],
        "FirebaseUI/Auth": [

        ]
      }
    },
    {
      "name": "Database",
      "vendored_frameworks": [
        "FirebaseUIFrameworks/FirebaseDatabaseUI/Frameworks/FirebaseDatabaseUI.framework"
      ],
      "dependencies": {
        "Firebase/Database": [
          "~> 3.0"
        ]
      }
    },
    {
      "name": "Auth",
      "dependencies": {
        "FirebaseUI/AuthBase": [

        ],
        "FirebaseUI/Facebook": [

        ],
        "FirebaseUI/Google": [

        ]
      }
    },
    {
      "name": "AuthBase",
      "vendored_frameworks": [
        "FirebaseUIFrameworks/FirebaseAuthUI/Frameworks/FirebaseAuthUI.framework"
      ],
      "dependencies": {
        "Firebase/Analytics": [
          "~> 3.0"
        ],
        "Firebase/Auth": [
          "~> 3.0"
        ]
      }
    },
    {
      "name": "Facebook",
      "vendored_frameworks": [
        "FirebaseUIFrameworks/FirebaseFacebookAuthUI/Frameworks/FirebaseFacebookAuthUI.framework"
      ],
      "dependencies": {
        "FirebaseUI/AuthBase": [

        ],
        "FBSDKLoginKit": [
          "~> 4.0"
        ]
      }
    },
    {
      "name": "Google",
      "vendored_frameworks": [
        "FirebaseUIFrameworks/FirebaseGoogleAuthUI/Frameworks/FirebaseGoogleAuthUI.framework"
      ],
      "dependencies": {
        "FirebaseUI/AuthBase": [

        ],
        "GoogleSignIn": [
          "~> 4.0"
        ]
      }
    }
  ]
}
