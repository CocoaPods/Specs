{
  "name": "DarklyEventSource",
  "version": "1.3.0",
  "summary": "HTML5 Server-Sent Events in your Cocoa app.",
  "homepage": "https://github.com/launchdarkly/ios-eventsource",
  "license": "MIT (see LICENSE.txt)",
  "authors": {
    "Neil Cowburn": "git@neilcowburn.com"
  },
  "source": {
    "git": "https://github.com/launchdarkly/ios-eventsource.git",
    "tag": "1.3.0"
  },
  "source_files": [
    "EventSource",
    "EventSource/EventSource.{h,m}"
  ],
  "ios": {
    "xcconfig": {
      "FRAMEWORK_SEARCH_PATHS": "$(inherited) \"$(SDKROOT)/Developer/Library/Frameworks\" \"$(DEVELOPER_LIBRARY_DIR)/Frameworks\""
    }
  },
  "osx": {
    "xcconfig": {
      "FRAMEWORK_SEARCH_PATHS": "$(inherited) \"$(DEVELOPER_LIBRARY_DIR)/Frameworks\""
    }
  },
  "platforms": {
    "ios": "8.0",
    "osx": "10.7",
    "watchos": "2.0",
    "tvos": "9.0"
  },
  "requires_arc": true,
  "xcconfig": {
    "OTHER_LDFLAGS": "-lobjc"
  }
}
