{
  "name": "vMAT",
  "version": "0.0.1",
  "summary": "The vMAT library implements a grab-bag of mathematical functions inspired by MATLAB.",
  "homepage": "http://kaelin.github.io/vMAT/",
  "license": "BSD 2-Clause",
  "authors": {
    "Kaelin Colclasure": "kaelin@acm.org"
  },
  "source": {
    "git": "https://github.com/kaelin/vMAT.git",
    "tag": "0.0.1"
  },
  "platforms": {
    "osx": "10.8"
  },
  "source_files": [
    "vMAT",
    "vMAT/**/*.{h,m}"
  ],
  "frameworks": [
    "Accelerate",
    "Foundation"
  ],
  "requires_arc": true,
  "xcconfig": {
    "CLANG_CXX_LANGUAGE_STANDARD": "gnu++0x",
    "CLANG_CXX_LIBRARY": "libc++"
  },
  "dependencies": {
    "BlocksKit": [
      "~> 1.8.1"
    ],
    "eigen": [
      "~> 3.1.2"
    ]
  }
}
