{
  "name": "Reveal-iOS",
  "version": "4",
  "summary": "The Reveal SDK: Runtime view debugging for iOS.",
  "description": "Reveal brings powerful runtime view debugging to iOS developers.",
  "homepage": "https://github.com/xiaolei0808/Reveal-iOS",
  "authors": {
    "金小白": "leixjin@gmail.com"
  },
  "source": {
    "git": "https://github.com/xiaolei0808/Reveal-iOS.git",
    "tag": "4"
  },
  "platforms": {
    "ios": "8.0"
  },
  "frameworks": [
    "CoreGraphics",
    "CFNetwork",
    "QuartzCore"
  ],
  "vendored_frameworks": "Reveal-iOS/Assets/*.framework",
  "compiler_flags": "-ObjC",
  "requires_arc": "false",
  "libraries": "z",
  "xcconfig": {
    "LD_RUNPATH_SEARCH_PATHS": "$(inherited) @executable_path/Frameworks @loader_path/Frameworks"
  }
}
