{
  "name": "TK-BaiduNaviSDK-iOS",
  "version": "4.1.3",
  "summary": "BaiduNaviSDK for iOS",
  "homepage": "http://lbsyun.baidu.com/index.php?title=ios-navsdk",
  "authors": {
    "Baidu": "http://lbsyun.baidu.com/index.php?title=ios-navsdk"
  },
  "source": {
    "git": "https://git.dev.tencent.com/geek_cz/TK-BaiduNaviSDK-iOS.git",
    "tag": "4.1.3"
  },
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "source_files": "BaiduNaviSDK/inc/*.{h,m}",
  "resources": "BaiduNaviSDK/resource",
  "resource_bundles": {
    "BaiduNavi": [
      "BaiduNaviSDK/*.{bundle,xcassets}"
    ]
  },
  "vendored_libraries": "BaiduNaviSDK/*.a",
  "frameworks": [
    "AdSupport",
    "MobileCoreServices",
    "AssetsLibrary",
    "UserNotifications",
    "AudioToolbox",
    "AVFoundation",
    "CoreBluetooth",
    "CoreMotion",
    "CoreLocation",
    "ImageIO",
    "MediaPlayer"
  ],
  "libraries": [
    "iconv",
    "c++",
    "sqlite3.0"
  ],
  "pod_target_xcconfig": {
    "OTHER_LDFLAGS": "-lObjC"
  }
}
