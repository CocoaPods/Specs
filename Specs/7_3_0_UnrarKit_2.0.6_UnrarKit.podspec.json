{
  "name": "UnrarKit",
  "version": "2.0.6",
  "summary": "Provides a port of Unrar library to iOS and Mac platforms",
  "license": "BSD",
  "homepage": "https://github.com/abbeycode/UnrarKit",
  "authors": {
    "Dov Frankel": "dov@abbey-code.com"
  },
