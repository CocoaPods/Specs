{
  "name": "libx264-iOS",
  "version": "1.2",
  "license": "MIT",
  "summary": "libx264 build for iOS",
  "homepage": "https://github.com/chenddcoder/libx264-iOS",
  "authors": {
    "chenddcoder": "chenddcoder@foxmail.com"
  },
  "source": {
    "git": "https://github.com/chenddcoder/libx264-iOS.git",
    "tag": "1.2",
    "submodules": true
  },
  "requires_arc": false,
  "platforms": {
    "ios": "7.0",
    "osx": "10.8"
  },
  "public_header_files": "include/{*}.h",
  "vendored_libraries": "lib/libx264.a",
  "libraries": "x264",
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "${PODS_ROOT}/libx264-iOS/include/**",
    "LIBRARY_SEARCH_PATHS": "${PODS_ROOT}/libx264-iOS/lib/**"
  }
}
