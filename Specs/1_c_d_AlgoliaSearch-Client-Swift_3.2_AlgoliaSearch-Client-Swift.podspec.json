{
  "name": "AlgoliaSearch-Client-Swift",
  "module_name": "AlgoliaSearch",
  "version": "3.2",
  "license": "MIT",
  "summary": "Algolia Search API Client for iOS & OS X written in Swift.",
  "homepage": "https://github.com/algolia/algoliasearch-client-swift",
  "authors": {
    "Algolia": "contact@algolia.com"
  },
  "source": {
    "git": "https://github.com/algolia/algoliasearch-client-swift.git",
    "tag": "3.2"
  },
  "platforms": {
    "ios": "8.0"
  },
  "default_subspecs": "Online",
  "subspecs": [
    {
      "name": "Online",
      "platforms": {
        "ios": "8.0",
        "osx": "10.10"
      },
      "source_files": [
        "Source/*.swift",
        "Source/Helpers/*.swift"
      ]
    },
    {
      "name": "Offline",
      "dependencies": {
        "AlgoliaSearchOfflineCore-iOS": [
          "0.1"
        ]
      },
      "pod_target_xcconfig": {
        "GCC_PREPROCESSOR_DEFINITIONS": "ALGOLIA_SDK=1",
        "OTHER_SWIFT_FLAGS": "-DALGOLIA_SDK"
      },
      "source_files": [
        "Source/*.swift",
        "Source/Helpers/*.swift",
        "Source/Offline/*.swift"
      ]
    }
  ]
}
